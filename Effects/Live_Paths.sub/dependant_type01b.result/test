
Init File

int main()
{
   int i, result = 0;
   int size;
   size = 10;
   int a[size];

   for(i = 0; i <= 9; i += 1)
      a[i] = i;

   for(i = 0; i <= 9; i += 1)
      result += a[i];

   return result;
}

