PROPER_EFFECTS

int main()
{
//             <    is written>: result
//             <    is referenced>: result
//             <    is declared>: i result
   int i, result = 0;
//             <    is declared>: size
   int size;
//             <    is written>: size
//             <    is referenced>: size
   size = 10;
//             <    is read   >: size
//             <    is referenced>: size
//             <    is declared>: a
   int a[size];
//             <    is written>: i
//             <    is referenced>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: a[i]
//             <    is referenced>: a i
      a[i] = i;
//             <    is written>: i
//             <    is referenced>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: a[i] i result
//             <    is written>: result
//             <    is referenced>: a i result
      result += a[i];
//             <    is read   >: result
//             <    is referenced>: result

   return result;
}

CUMULATED_EFFECTS

int main()
{
//             <    is written>: result
//             <    is referenced>: result
//             <    is declared>: i result
   int i, result = 0;
//             <    is declared>: size
   int size;
//             <    is written>: size
//             <    is referenced>: size
   size = 10;
//             <    is read   >: size
//             <    is referenced>: size
//             <    is declared>: a
   int a[size];
//             <may be written>: a[*]
//             <    is read   >: i
//             <    is written>: i
//             <    is referenced>: a i

   for(i = 0; i <= 9; i += 1)
//             <may be written>: a[*]
//             <    is read   >: i
//             <    is referenced>: a i
      a[i] = i;
//             <may be read   >: a[*]
//             <    is read   >: i result
//             <    is written>: i result
//             <    is referenced>: a i result

   for(i = 0; i <= 9; i += 1)
//             <may be read   >: a[*]
//             <    is read   >: i result
//             <    is written>: result
//             <    is referenced>: a i result
      result += a[i];
//             <    is read   >: result
//             <    is referenced>: result

   return result;
}

IN_EFFECTS

int main()
{
   int i, result = 0;
   int size;
   size = 10;
//             <    is imported>: size
   int a[size];

   for(i = 0; i <= 9; i += 1)
//             <    is imported>: i
      a[i] = i;
//             <may be imported>: a[*]
//             <    is imported>: result

   for(i = 0; i <= 9; i += 1)
//             <may be imported>: a[*]
//             <    is imported>: i result
      result += a[i];
//             <    is imported>: result

   return result;
}

OUT_EFFECTS

int main()
{
//             <    is exported>: result
   int i, result = 0;
   int size;
   size = 10;
   int a[size];
//             <may be exported>: a[*]

   for(i = 0; i <= 9; i += 1)
//             <may be exported>: a[*]
      a[i] = i;
//             <    is exported>: result

   for(i = 0; i <= 9; i += 1)
//             <    is exported>: result
      result += a[i];

   return result;
}

LIVE_IN_PATHS

int main()
{
   int i, result = 0;
//             <    is alive (in)>: result
   int size;
//             <    is alive (in)>: result
   size = 10;
//             <    is alive (in)>: result size
   int a[size];
//             <may be alive (in)>: a[*]
//             <    is alive (in)>: result

   for(i = 0; i <= 9; i += 1)
//             <may be alive (in)>: a[*]
//             <    is alive (in)>: i result
      a[i] = i;
//             <may be alive (in)>: a[*]
//             <    is alive (in)>: result

   for(i = 0; i <= 9; i += 1)
//             <may be alive (in)>: a[*]
//             <    is alive (in)>: i result
      result += a[i];
//             <    is alive (in)>: result

   return result;
}

LIVE_OUT_PATHS

int main()
{
//             <    is alive (out)>: result
   int i, result = 0;
//             <    is alive (out)>: result
   int size;
//             <    is alive (out)>: result size
   size = 10;
//             <may be alive (out)>: a[*]
//             <    is alive (out)>: result
   int a[size];
//             <may be alive (out)>: a[*]
//             <    is alive (out)>: result

   for(i = 0; i <= 9; i += 1)
//             <may be alive (out)>: a[*]
//             <    is alive (out)>: i result
      a[i] = i;
//             <    is alive (out)>: result

   for(i = 0; i <= 9; i += 1)
//             <may be alive (out)>: a[*] result
//             <    is alive (out)>: i
      result += a[i];

   return result;
}

