
      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0005
      DO I = 1, N                                                       0006
         DO J = 1, N                                                    0007
            WORK(J,K) = J+K                                             0008
         ENDDO

         CALL INC1(K)                                                   0011

         DO J = 1, N                                                    0013
            WORK(J,K) = J*J-K*K                                         0014
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END

C  T(K) {0<=K, N<=K}


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)

C  T(K) {0<=K}

      K = M*M                                                           0005

C  T(I,J,K) {I+K#init==K+1, 1<=I, I<=N}

      DO I = 1, N                                                       0006

C  T(J) {1<=J, J<=N}

         DO J = 1, N                                                    0007

C  T() {}

            WORK(J,K) = J+K                                             0008
         ENDDO

C  T(K) {K==K#init+1}


         CALL INC1(K)                                                   0011

C  T(J) {1<=J, J<=N}


         DO J = 1, N                                                    0013

C  T() {}

            WORK(J,K) = J*J-K*K                                         0014

C  T() {}

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END

C  <A(PHI1)-R-EXACT-{1<=PHI1, PHI1<=N}>
C  <A(PHI1)-W-EXACT-{1<=PHI1, PHI1<=N}>


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0005

C  <A(PHI1)-R-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>
C  <A(PHI1)-W-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>
C  <WORK(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2, PHI2<=K+N,
C    0<=K, 1<=N}>
C  <WORK(PHI1,PHI2)-W-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2, PHI2<=K+N,
C    0<=K, 1<=N}>

      DO I = 1, N                                                       0006

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI2==K, 1<=PHI1, PHI1<=N, 1<=I, I<=K+1,
C    I<=N}>

         DO J = 1, N                                                    0007

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K+1, I<=N,
C    1<=J, J<=N}>

            WORK(J,K) = J+K                                             0008
         ENDDO

         CALL INC1(K)                                                   0011

C  <A(PHI1)-R-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=N}>
C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=N}>
C  <WORK(PHI1,PHI2)-R-EXACT-{1<=PHI1, PHI1<=N, K<=PHI2+1, PHI2<=K,
C    1<=I, I<=K, I<=N}>
C  <WORK(PHI1,PHI2)-W-EXACT-{PHI2==K, 1<=PHI1, PHI1<=N, 1<=I, I<=K,
C    I<=N, 1<=N}>


         DO J = 1, N                                                    0013

C  <WORK(PHI1,PHI2)-W-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K, I<=N, 1<=J,
C    J<=N, 1<=N}>

            WORK(J,K) = J*J-K*K                                         0014

C  <A(PHI1)-R-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N, 1<=N}>
C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N, 1<=N}>
C  <WORK(PHI1,PHI2)-R-EXACT-{PHI1==J, 1<=PHI1, PHI1<=N, K<=PHI2+1,
C    PHI2<=K, 1<=I, I<=K, I<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END

C  <A(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=N}>


      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0005

C  <A(PHI1)-IN-EXACT-{1<=PHI1, PHI1<=N, 0<=K}>

      DO I = 1, N                                                       0006
         DO J = 1, N                                                    0007
            WORK(J,K) = J+K                                             0008
         ENDDO

         CALL INC1(K)                                                   0011

C  <A(PHI1)-IN-EXACT-{PHI1==I, 1<=I, I<=K, I<=N}>
C  <WORK(PHI1,PHI2)-IN-EXACT-{PHI2==K-1, 1<=PHI1, PHI1<=N, 1<=I, I<=K,
C    I<=N}>


         DO J = 1, N                                                    0013
            WORK(J,K) = J*J-K*K                                         0014

C  <A(PHI1)-IN-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J<=N, 1<=N}>
C  <WORK(PHI1,PHI2)-IN-EXACT-{PHI1==J, 1<=PHI1, PHI1<=N, K<=PHI2+1,
C    PHI2<=K, 1<=I, I<=K, I<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END

      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0005
      DO I = 1, N                                                       0006

C  <WORK(PHI1,PHI2)-OUT-EXACT-{PHI2==K, 1<=PHI1, PHI1<=N, 1<=I,
C    I<=K+1, I<=N}>

         DO J = 1, N                                                    0007

C  <WORK(PHI1,PHI2)-OUT-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K+1, I<=N,
C    1<=J, J<=N}>

            WORK(J,K) = J+K                                             0008
         ENDDO

         CALL INC1(K)                                                   0011

         DO J = 1, N                                                    0013

C  <WORK(PHI1,PHI2)-OUT-EXACT-{PHI1==J, PHI2==K, 1<=I, I<=K, I<=N,
C    1<=J, J<=N, 1<=N}>

            WORK(J,K) = J*J-K*K                                         0014

C  <A(PHI1)-OUT-EXACT-{PHI1==I, 1<=I, I<=K, I<=N, 1<=J, J+1<=N, 1<=N}>

            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END

      SUBROUTINE RENPAR6(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K = M*M                                                           0005
      DO I = 1, N                                                       0006
         DOALL J = 1, N                                                 0007
            WORK(J,K) = J+K                                             0008
         ENDDO

         CALL INC1(K)                                                   0011
         DOALL J = 1, N                                                 0013
            WORK(J,K) = J*J-K*K                                         0014
         ENDDO
         DO J = 1, N                                                    0013
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0015
         ENDDO
      ENDDO
      END


      SUBROUTINE RENPAR6_2(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K0 = M*M                                                          0006
      DO I = 1, N                                                       0007
         K = K0+I-1                                                     0008
         DO J = 1, N                                                    0009
            WORK(J,K) = J+K                                             0010
         ENDDO

         CALL INC1(K)                                                   0013

         DO J = 1, N                                                    0015
            WORK(J,K) = J*J-K*K                                         0016
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0017
         ENDDO
      ENDDO
      END


      SUBROUTINE RENPAR6_2(A,N,K,M)
      INTEGER N,K,M,A(N)
      DIMENSION WORK(100,100)
      K0 = M*M                                                          0006
      DOALL I = 1, N                                                    0007
         PRIVATE J,K,WORK(1:100, 1:100)
         K = K0+I-1                                                     0008
         DOALL J = 1, N                                                 0009
            WORK(J,K) = J+K                                             0010
         ENDDO

         CALL INC1(K)                                                   0013
         DOALL J = 1, N                                                 0015
            WORK(J,K) = J*J-K*K                                         0016
         ENDDO
         DO J = 1, N                                                    0015
            A(I) = A(I)+WORK(J,K)+WORK(J,K-1)                           0017
         ENDDO
      ENDDO
      END
