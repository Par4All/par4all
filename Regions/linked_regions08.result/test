//  T(main) {main==0}

int main()
{

//  T(N) {N==100}

   int N = 100;

//  T() {}

   double A[100][100];
   {

//  T(0`1`i) {}

      int i;

//  T(0`1`i) {1<=0`1`i, 0`1`i<=N}

      for(i = 1; i <= N; i += 1) {

//  T(0`1`2`3`j) {}

         int j;

//  T(0`1`2`3`j) {1<=0`1`2`3`j, 0`1`2`3`j<=N}

         for(j = 1; j <= N; j += 1) {

//  T(x0) {}

            int x0 = i*j;

//  T() {}

            double x1 = (double) N/2;

//  T() {}

            if (x0<x1) {

//  T() {}

               A[N-i-1][i+j-1] = 1.0;

//  T() {}

               A[i-1][N-i-j-1] = 1.0;
            }

//  T() {}

            if (i==j)

//  T() {}

               A[i-1][j-1] = 1.0;
         }
      }
   }
   {

//  T(0`7`i) {}

      int i;

//  T(0`7`i) {0<=0`7`i, 0`7`i+1<=N}

      for(i = 0; i <= N-1; i += 1) {

//  T(0`7`8`j) {}

         int j;

//  T(0`7`8`j) {0<=0`7`8`j, 0`7`8`j+1<=N}

         for(j = 0; j <= N-1; j += 1)

//  T() {}

            printf("%f\n", A[i][j]);
      }
   }

//  T(main) {main==0}


   return 0;
}

