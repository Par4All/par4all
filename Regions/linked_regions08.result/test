//  P(0`1`i,N) {N==100, 1<=0`1`i, 0`1`i<=100}

         int j;

//  P(0`1`2`3`j,0`1`i,N) {N==100, 1<=0`1`i, 0`1`i<=100}

         for(j = 1; j <= N; j += 1) {

//  P(0`1`2`3`j,0`1`i,N) {N==100, 1<=0`1`2`3`j, 0`1`2`3`j<=100,
//    1<=0`1`i, 0`1`i<=100}

