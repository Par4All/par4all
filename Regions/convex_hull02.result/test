int main()
{

//  T(i,k) {}

   int i, k;

//  T(N) {N==100}

   int N = 100;

//  T() {}

   double A[N][N];

//  T(i,k) {i+1<=N, 0<=i}


   for(i = 0; i <= N-1; i += 1)

//  T(k) {k+1<=N, 0<=k}

      for(k = 0; k <= N-1; k += 1)

//  T() {}

         if (1) {

//  T() {}

            A[i][k] = 1.;

//  T() {}

            A[N/2][N/2] = 0.;
         }

//  T(main) {main==0}


   return 0;
}

