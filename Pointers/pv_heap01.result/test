
PROPER  EFFECTS

int main()
{
   int *p, *q, *r;
//             <    is written>: n
   int a, n = 10;
   void *s;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(2*sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: q _MALLOC_EFFECTS:_MALLOC_
   q = (int *) malloc(2*sizeof(a));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: r _MALLOC_EFFECTS:_MALLOC_
   r = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: s _MALLOC_EFFECTS:_MALLOC_
   s = malloc(15);
//             <    is read   >: n _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
   p = (int *) malloc(n*sizeof(int));
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p, *q, *r;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a, n = 10;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   void *s;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   p = (int *) malloc(2*sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   q = (int *) malloc(2*sizeof(a));

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_9[0] , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = (int *) malloc(sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_9[0] , EXACT
// r -> *HEAP*_l_10 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   s = malloc(15);

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_9[0] , EXACT
// r -> *HEAP*_l_10 , EXACT
// s -> *HEAP*_l_11[0] , EXACT

   p = (int *) malloc(n*sizeof(int));

// Points To:
// p -> *HEAP*_l_12[0] , EXACT
// q -> *HEAP*_l_9[0] , EXACT
// r -> *HEAP*_l_10 , EXACT
// s -> *HEAP*_l_11[0] , EXACT

   return 0;
}
