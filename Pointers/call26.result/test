
Points-to for "call26"


// Points To IN:
// ppp -> *NULL_POINTER* , MAY
// ppp -> _ppp_1[0] , MAY


// Points To OUT:
// _ppp_1[*] -> *NULL_POINTER* , MAY
// _ppp_1[0] -> *NULL_POINTER* , EXACT
// ppp -> _ppp_1[0] , EXACT

void call26(int ***ppp)
{

// Points To:
// ppp -> *NULL_POINTER* , MAY
// ppp -> _ppp_1[0] , MAY

   *ppp = (void *) 0;

// Points To:
// _ppp_1[*] -> *NULL_POINTER* , MAY
// _ppp_1[0] -> *NULL_POINTER* , EXACT
// ppp -> _ppp_1[0] , EXACT

   return;
}

Points-to for "main"


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p = &i;

// Points To:
// p -> i , EXACT

   int **pp = &p;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT

   int ***ppp = &pp;

// Points To:
// p -> i , EXACT
// pp -> p , EXACT
// ppp -> pp , EXACT

   call26(ppp);

// Points To:
// p -> i , EXACT
// pp -> *NULL_POINTER* , EXACT
// ppp -> pp , EXACT

   return 0;
}
