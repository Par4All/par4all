
PROPER  EFFECTS

int main()
{
//             <    is read   >: a

   if (a>1) {
      static int *p = &a;
      static int *q;
//             <    is written>: q
      q = &b;
//             <    is read   >: a
//             <    is written>: a
      a = a-1;
   }
   else {
      static int *r = &a;
      static int *s;
//             <    is written>: s
      s = &b;
//             <    is read   >: a
//             <    is written>: a
      a = a+2;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT:
// p -> a , MAY
// q -> b , MAY
// r -> a , MAY
// s -> b , MAY

int main()
{

// Points To: none


   if (a>1) {

// Points To: none

      static int *p = &a;

// Points To:
// p -> a , EXACT

      static int *q;

// Points To:
// p -> a , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      q = &b;

// Points To:
// p -> a , EXACT
// q -> b , EXACT

      a = a-1;
   }
   else {

// Points To: none

      static int *r = &a;

// Points To:
// r -> a , EXACT

      static int *s;

// Points To:
// r -> a , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      s = &b;

// Points To:
// r -> a , EXACT
// s -> b , EXACT

      a = a+2;
   }

// Points To:
// p -> a , MAY
// q -> b , MAY
// r -> a , MAY
// s -> b , MAY

   return 0;
}
