
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 3;
   int *p;
   int a[10];
//             <    is read   >: i

   if (i>1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
//             <    is written>: i
      i = 0;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 3;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int a[10];

// Points To:
// p -> undefined , EXACT


   if (i>1) {

// Points To:
// p -> undefined , EXACT

      int *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT

      q = &a[i];

// Points To:
// p -> undefined , EXACT
// q -> a[*] , EXACT

      p = q;

// Points To:
// p -> a[*] , EXACT
// q -> a[*] , EXACT

      i = 0;
   }

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY

   return 0;
}
