
PROPER  EFFECTS

int main()
{
   int i;
   int *p;
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1) {
      int *q;
//             <    is read   >: i
//             <    is written>: q
      q = &a[i];
//             <    is read   >: q
//             <    is written>: p
      p = q;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int a[10];

// Points To:
// p -> undefined , EXACT


   for(i = 0; i <= 9; i += 1) {

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY

      int *q;

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY
// q -> undefined , EXACT

      q = &a[i];

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY
// q -> a[*] , EXACT

      p = q;
   }

// Points To:
// p -> undefined , MAY
// p -> a[*] , MAY

   return 0;
}
