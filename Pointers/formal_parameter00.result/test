
PROPER  EFFECTS

void formal_parameter00(int *pi)
{
   int *q;
   int i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is written>: pi
   pi = &i;
   return 0;
}

POINTS-TO


// Points To IN:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY


// Points To OUT: none

void formal_parameter00(int *pi)
{

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY

   int *q;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   i = 0;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = pi;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

   pi = &i;

// Points To:
// pi -> i , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

   return 0;
}
