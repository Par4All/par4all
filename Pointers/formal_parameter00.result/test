
PROPER EFFECTS

void formal_parameter00(int *pi)
{
   int *q;
   int i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <    is written>: pi
   pi = &i;
   return 0;
}

PROPER POINTER EFFECTS

void formal_parameter00(int *pi)
{
   int *q;
   int i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is read   >: q
//             <    is written>: q
   q++;
//             <    is written>: pi
   pi = &i;
   return 0;
}

POINTS TO


// Points To IN:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY


// Points To OUT:
// pi -> undefined , EXACT

void formal_parameter00(int *pi)
{

// Points To: none

   int *q;

// Points To:
// q -> undefined , EXACT

   int i;

// Points To:
// q -> undefined , EXACT


   i = 0;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> undefined , EXACT

   q = pi;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _pi_1[0] , MAY

   q++;

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> _pi_1[1] , EXACT

   pi = &i;

// Points To:
// pi -> i , EXACT
// q -> _pi_1[1] , EXACT

   return 0;
}
