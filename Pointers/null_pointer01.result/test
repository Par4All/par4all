
PROPER  EFFECTS

int main()
{
   int i;
//             <    is written>: p
   int *p = 0;
//             <    is written>: q
   int **q = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p

   *p = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   *q = &i;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p = 0;

// Points To:
// p -> *NULL_POINTER* , EXACT

   int **q = 0;

// Points To:
// p -> *NULL_POINTER* , EXACT
// q -> *NULL_POINTER* , EXACT


   *p = 1;

// Points To: unreachable

   *q = &i;

// Points To: unreachable


   return 0;
}
