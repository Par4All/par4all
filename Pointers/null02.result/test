
PROPER  EFFECTS

int main()
{
   int *p;
//             <    is written>: p
   p = 0;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   p = 0;

// Points To:
// p -> *NULL* , EXACT


   return 0;
}
