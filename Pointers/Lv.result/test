void Lv(_Complex v[nms])
{

// Points To: none

   if (1) {

// Points To: none

      int nm, nvh, i;

// Points To: none

      _Complex z;

// Points To: none


      nm = nms;

// Points To: none

      nvh = nvhs;

// Points To: none


      for(i = 0; i <= nm-1; i += 1) {

// Points To:
// cs1 -> _cs1_320080[0] , EXACT
// vs -> _vs_320040[0][0][0] , EXACT

         z = vprod(nvh, 0, (*vs)[i], v);

// Points To:
// cs1 -> _cs1_320080[0] , MAY
