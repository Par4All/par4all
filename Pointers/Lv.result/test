         cs1[i] = (double _Complex) z;
      }

// Points To:
// cs1 -> _cs1_320080[0] , EXACT
// vs -> _vs_320040[0][0][0] , EXACT


      sum_vprod(nm);

// Points To:
// cs1 -> _cs1_320080[0] , EXACT
// vs -> _vs_320040[0][0][0] , EXACT

      cmat_vec_dble(nm, *avd, cs2, cs1);

// Points To:
// avd -> _avd_320072[0][0][0] , EXACT
// cs1 -> _cs1_320080[0] , EXACT
// vs -> _vs_320040[0][0][0] , EXACT


