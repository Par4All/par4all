
Proper effects for "dependence10"

void dependence10(int flag, int *a)
{
   int *p;
   int b[10];
   int i;
//             <    is read   >: flag
   
   // Two paths
   if (flag)
//             <    is read   >: a
//             <    is written>: p
      p = a;
   else
//             <    is written>: p
      p = b;
//             <    is written>: i
   
   // p may points to a[0] or b[0];
   
   for(i = 0; i <= 9; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = 0;
}

