int main()
{
   char buf[20];
   size_t nbytes;
   ssize_t bytes_read;
   int fd;

   nbytes = sizeof(buf);
   bytes_read = read(fd, buf, nbytes);
}
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*]
int main()
{
   char buf[20];
   size_t nbytes;
   ssize_t bytes_read;
   int fd;
//               <must be written>: nbytes

   nbytes = sizeof(buf);
//               <may be read    >: __IO_EFFECTS:LUNS[*]
//               <may be written >: __IO_EFFECTS:LUNS[*] buf[*]
//               <must be read   >: fd nbytes
//               <must be written>: bytes_read
   bytes_read = read(fd, buf, nbytes);
}

POINTS TO

//  {}
int main()
{
//  points to = {}
   char buf[20];
//  points to = {}
   size_t nbytes;
//  points to = {}
   ssize_t bytes_read;
//  points to = {}
   int fd;
//  points to = {}

   nbytes = sizeof(buf);
//  points to = {}
   bytes_read = read(fd, buf, nbytes);
}
