
PROPER  EFFECTS

int main()
{
   int *p;
   int i, j;
//             <    is read   >: i j

   if (i==j)
//             <    is written>: p
      p = &i;
   else
//             <    is written>: p
      p = &j;
//             <    is written>: p
   p = (void *) 0;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT

   int i, j;

// Points To:
// p -> undefined , EXACT


   if (i==j)

// Points To:
// p -> undefined , EXACT

      p = &i;
   else

// Points To:
// p -> undefined , EXACT

      p = &j;

// Points To:
// p -> i , MAY
// p -> j , MAY

   p = (void *) 0;

// Points To:
// p -> *NULL_POINTER* , EXACT


   return 0;
}
