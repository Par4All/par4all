
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(int argc, char **argv)
{

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT


   if (argc>0)

// Points To:
// p -> undefined , EXACT

      p = (void *) 0;
   else if (argc<0)

// Points To:
// p -> undefined , EXACT

      p = &argc;

// Points To:
// p -> undefined , MAY
// p -> *NULL_POINTER* , MAY
// p -> argc , MAY

   
   // Here, p must point toward undefined, NULL or argc
   
   *p = 1;

// Points To:
// p -> argc , EXACT

   
   // Here, p must point toward argc
   
   return 0;
}
