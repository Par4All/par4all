
Proper effects for "dependence11"

void dependence11(struct my_struct *s)
{
   int *b, *a;
   int i;
//             <    is read   >: s
//             <    is written>: a
   a = s->a;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: b
   // a points_to s[0][1]
   b = s->b;
//             <    is written>: i
   // b points_to s[0][2]?
   
   
   for(i = 0; i <= 9; i += 1) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: a i
      a[i] = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: b i
      b[i] = 1;
   }
}

