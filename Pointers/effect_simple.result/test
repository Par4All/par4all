
PROPER  EFFECTS

int main()
{
   int *p, *q;
   int i, j;
//             <    is written>: p
   p = &i;
//             <    is written>: q
   q = &j;
//             <    is read   >: q
//             <    is written>: p
   p = q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = 1;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i, j;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &i;

// Points To:
// p -> i , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &j;

// Points To:
// p -> i , EXACT
// q -> j , EXACT

   p = q;

// Points To:
// p -> j , EXACT
// q -> j , EXACT

   *p = 1;

// Points To:
// p -> j , EXACT
// q -> j , EXACT

   return 0;
}
