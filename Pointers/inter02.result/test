
PROPER  EFFECTS

int main()
{
   int *x;
   int tab[10];
   int *tab2[10];
   int tab3[10][10];
   int **tab4;
   int y;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x
   
   
   foo(x);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   foo(tab);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: tab2[4]
   foo(tab2[4]);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   foo(tab3[5]);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   foo(tab4[6]);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   foo(&y);
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   foo(&tab[1]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]

   printf("%d\n", *x);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab[0] __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[0]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab2[4][0]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab3[5][0] 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab3[5][0]);
//             <may be read   >: stdout[0] *ANY_MODULE*:*ANYWHERE*
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab4[6][0]);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", y);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout tab[1] __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d\n", tab[1]);
   return 1;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *x;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int tab[10];

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *tab2[10];

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int tab3[10][10];

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int **tab4;

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int y;

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   
   
   foo(x);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(tab);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(tab2[4]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(tab3[5]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(tab4[6]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(&y);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   foo(&tab[1]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   printf("%d\n", *x);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", tab[0]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", tab2[4][0]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", tab3[5][0]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", tab4[6][0]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", y);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   printf("%d\n", tab[1]);

// Points To:
// tab4 -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 1;
}
