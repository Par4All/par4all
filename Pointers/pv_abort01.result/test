
PROPER  EFFECTS

int main()
{
   int **a;
   int b;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: a
   a[0] = &b;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int **a;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a[0] = &b;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> b , EXACT
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}
