
PROPER  EFFECTS

void arithmetic09(int a[10])
{
//             <    is written>: q
   int *q = &a[9];
   int *p;
//             <    is read   >: q
//             <    is written>: p q

   p = q -= 1;
   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void arithmetic09(int a[10])
{

// Points To: none

   int *q = &a[9];

// Points To:
// q -> a[9] , EXACT

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> a[9] , EXACT


   p = q -= 1;

// Points To:
// p -> a[8] , EXACT
// q -> a[8] , EXACT

   return;
}
