
PROPER  EFFECTS

int main()
{
   int *p, *q, *r;
   int a;
   void *s;
//             <    is written>: p
   p = (int *) calloc(2, sizeof(int));
//             <    is written>: q
   q = (int *) calloc(2, sizeof(a));
//             <    is written>: r
   r = (int *) calloc(1, sizeof(int));
//             <    is written>: s
   s = calloc(1, 15);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *p, *q, *r;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   void *s;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   p = (int *) calloc(2, sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , MAY
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   q = (int *) calloc(2, sizeof(a));

// Points To:
// p -> *HEAP*_l_8[0] , MAY
// q -> *HEAP*_l_9[0] , MAY
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   r = (int *) calloc(1, sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , MAY
// q -> *HEAP*_l_9[0] , MAY
// r -> *HEAP*_l_10[0] , MAY
// s -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   s = calloc(1, 15);

// Points To:
// p -> *HEAP*_l_8[0] , MAY
// q -> *HEAP*_l_9[0] , MAY
// r -> *HEAP*_l_10[0] , MAY
// s -> *HEAP*_l_11[0] , MAY

   return 0;
}
