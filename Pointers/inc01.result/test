
Proper effects for "inc01"

int inc01(int *pi)
{
   int *q;
//             <    is written>: i
   int i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: i
   i = *q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i q
   *q = i++;
   return 0;
}

