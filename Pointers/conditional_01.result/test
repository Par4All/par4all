
PROPER  EFFECTS

int main()
{
   int *q;
   int *p;
//             <    is written>: i j
   int i = 0, j = 1;
//             <    is written>: q
   q = &i;
//             <may be read   >: q
//             <    is read   >: i
//             <    is written>: p
   p = i>0?&j:q;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: p stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("address of p =%p\n", p);
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: q stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("address of q =%p\n", q);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *q;

// Points To:
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i = 0, j = 1;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &i;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> i , EXACT

   p = i>0?&j:q;

// Points To:
// p -> i , MAY
// p -> j , MAY
// q -> i , EXACT

   printf("address of p =%p\n", p);

// Points To:
// p -> i , MAY
// p -> j , MAY
// q -> i , EXACT

   printf("address of q =%p\n", q);

// Points To:
// p -> i , MAY
// p -> j , MAY
// q -> i , EXACT

   return 0;
}
