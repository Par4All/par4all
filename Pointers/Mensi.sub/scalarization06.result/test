
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(int argc, char **argv)
{

// Points To: none

   // BEGIN BLOCK

// Points To: none

   int i, n = 100;

// Points To: none

   int *x, *y, *t;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE* , EXACT
// x -> *ANY_MODULE*:*NOWHERE* , EXACT
// y -> *ANY_MODULE*:*NOWHERE* , EXACT


   x = (int *) malloc(sizeof(int));

// Points To:
// t -> *ANY_MODULE*:*NOWHERE* , EXACT
// x -> *HEAP*_l_18 , MAY
// y -> *ANY_MODULE*:*NOWHERE* , EXACT

   y = (int *) malloc(sizeof(int));

// Points To:
// t -> *ANY_MODULE*:*NOWHERE* , EXACT
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

   t = (int *) malloc(sizeof(int));

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY


   for(i = 0; i <= n-1; i += 1)

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

      scanf("%d %d", &x[i], &y[i]);

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY


   for(i = 0; i <= n-1; i += 1) {

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

      // BEGIN BLOCK

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

      t[1] = x[i];

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

      x[i] = y[i];

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

      y[i] = t[1];
      // END BLOCK
   }

// Points To:
// t -> *HEAP*_l_20 , MAY
// x -> *HEAP*_l_18 , MAY
// y -> *HEAP*_l_19 , MAY

   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}

PROPER  EFFECTS WITH POINTS TO

int main(int argc, char **argv)
{
   // BEGIN BLOCK
//             <    is written>: n
   int i, n = 100;
   int *x, *y, *t;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: x _MALLOC_EFFECTS:_MALLOC_

   x = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: y _MALLOC_EFFECTS:_MALLOC_
   y = (int *) malloc(sizeof(int));
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: t _MALLOC_EFFECTS:_MALLOC_
   t = (int *) malloc(sizeof(int));
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1)
//             <may be read   >: stdin[0]
//             <may be written>: *HEAP*_l_18 *HEAP*_l_19 stdin[0]
//             <    is read   >: i n stdin __IO_EFFECTS:LUNS[0]
//             <    is written>: __IO_EFFECTS:LUNS[0]
      scanf("%d %d", &x[i], &y[i]);
//             <    is read   >: n
//             <    is written>: i

   for(i = 0; i <= n-1; i += 1) {
      // BEGIN BLOCK
//             <may be read   >: *HEAP*_l_18
//             <may be written>: *HEAP*_l_20
//             <    is read   >: i n t x
      t[1] = x[i];
//             <may be read   >: *HEAP*_l_19
//             <may be written>: *HEAP*_l_18
//             <    is read   >: i n x y
      x[i] = y[i];
//             <may be read   >: *HEAP*_l_20
//             <may be written>: *HEAP*_l_19
//             <    is read   >: i n t y
      y[i] = t[1];
      // END BLOCK
   }
//             <may be read   >: *HEAP*_l_18 *HEAP*_l_19 stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: n stdout x y __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}

RW regions

int main(int argc, char **argv)
{
   // BEGIN BLOCK
   int i, n = 100;
   int *x, *y, *t;

//  <x-W-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>


   x = (int *) malloc(sizeof(int));

//  <y-W-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

   y = (int *) malloc(sizeof(int));

//  <t-W-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-R-EXACT-{}>
//  <_MALLOC_EFFECTS:_MALLOC_-W-EXACT-{}>

   t = (int *) malloc(sizeof(int));

   for(i = 0; i <= n-1; i += 1)

//  <stdin-R-EXACT-{}>
//  <stdin[PHI1]-R-MAY-{PHI1==0}>
//  <stdin[PHI1]-W-MAY-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==0}>

      scanf("%d %d", &x[i], &y[i]);

   for(i = 0; i <= n-1; i += 1) {
      // BEGIN BLOCK

//  <t-R-EXACT-{}>
//  <x-R-EXACT-{}>

      t[1] = x[i];

//  <x-R-EXACT-{}>
//  <y-R-EXACT-{}>

      x[i] = y[i];

//  <t-R-EXACT-{}>
//  <y-R-EXACT-{}>

      y[i] = t[1];
      // END BLOCK
   }

//  <stdout-R-EXACT-{}>
//  <stdout[PHI1]-R-MAY-{PHI1==0}>
//  <stdout[PHI1]-W-MAY-{PHI1==0}>
//  <x-R-EXACT-{}>
//  <y-R-EXACT-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-R-EXACT-{PHI1==1}>
//  <__IO_EFFECTS:LUNS[PHI1]-W-EXACT-{PHI1==1}>

   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}

IN regions


//  <stdin-IN-EXACT-{}>
//  <stdin[PHI1]-IN-MAY-{PHI1==0}>
//  <stdout-IN-EXACT-{}>
//  <stdout[PHI1]-IN-MAY-{PHI1==0}>
//  <_MALLOC_EFFECTS:_MALLOC_-IN-EXACT-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-MAY-{0<=PHI1, PHI1<=1}>

int main(int argc, char **argv)
{

//  <stdin-IN-EXACT-{}>
//  <stdin[PHI1]-IN-MAY-{PHI1==0}>
//  <stdout-IN-EXACT-{}>
//  <stdout[PHI1]-IN-MAY-{PHI1==0}>
//  <_MALLOC_EFFECTS:_MALLOC_-IN-EXACT-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-MAY-{0<=PHI1, PHI1<=1}>

   // BEGIN BLOCK
   int i, n = 100;
   int *x, *y, *t;

//  <_MALLOC_EFFECTS:_MALLOC_-IN-EXACT-{}>


   x = (int *) malloc(sizeof(int));

//  <_MALLOC_EFFECTS:_MALLOC_-IN-EXACT-{}>

   y = (int *) malloc(sizeof(int));

//  <_MALLOC_EFFECTS:_MALLOC_-IN-EXACT-{}>

   t = (int *) malloc(sizeof(int));

//  <stdin-IN-EXACT-{}>
//  <stdin[PHI1]-IN-MAY-{PHI1==0, 1<=n}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-MAY-{PHI1==0, 1<=n}>


   for(i = 0; i <= n-1; i += 1)

//  <stdin-IN-EXACT-{}>
//  <stdin[PHI1]-IN-MAY-{PHI1==0}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==0}>

      scanf("%d %d", &x[i], &y[i]);

//  <t-IN-EXACT-{}>
//  <x-IN-EXACT-{}>
//  <y-IN-EXACT-{}>


   for(i = 0; i <= n-1; i += 1) {

//  <t-IN-EXACT-{}>
//  <x-IN-EXACT-{}>
//  <y-IN-EXACT-{}>

      // BEGIN BLOCK

//  <t-IN-EXACT-{}>
//  <x-IN-EXACT-{}>

      t[1] = x[i];

//  <x-IN-EXACT-{}>
//  <y-IN-EXACT-{}>

      x[i] = y[i];

//  <t-IN-EXACT-{}>
//  <y-IN-EXACT-{}>

      y[i] = t[1];
      // END BLOCK
   }

//  <stdout-IN-EXACT-{}>
//  <stdout[PHI1]-IN-MAY-{PHI1==0}>
//  <x-IN-EXACT-{}>
//  <y-IN-EXACT-{}>
//  <__IO_EFFECTS:LUNS[PHI1]-IN-EXACT-{PHI1==1}>

   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}

Privatized regions

int main(int argc, char **argv)
{
   // BEGIN BLOCK
   int i, n = 100;
   int *x, *y, *t;

   x = (int *) malloc(sizeof(int));
   y = (int *) malloc(sizeof(int));
   t = (int *) malloc(sizeof(int));

#pragma omp for private(i)
   for(i = 0; i <= n-1; i += 1)
      scanf("%d %d", &x[i], &y[i]);

#pragma omp for private(i)
   for(i = 0; i <= n-1; i += 1) {
      // BEGIN BLOCK
      t[1] = x[i];
      x[i] = y[i];
      y[i] = t[1];
      // END BLOCK
   }
   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}

Code after scalarization

int main(int argc, char **argv)
{
   // BEGIN BLOCK
   int i, n = 100;
   int *x, *y, *t;

   x = (int *) malloc(sizeof(int));
   y = (int *) malloc(sizeof(int));
   t = (int *) malloc(sizeof(int));

   for(i = 0; i <= n-1; i += 1)
      scanf("%d %d", &x[i], &y[i]);

   for(i = 0; i <= n-1; i += 1) {
      // BEGIN BLOCK
      t[1] = x[i];
      x[i] = y[i];
      y[i] = t[1];
      // END BLOCK
   }
   printf("%d %d", x[n-1], y[n-1]);
   // END BLOCK
}
