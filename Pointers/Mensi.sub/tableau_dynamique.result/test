//             <may be read   >: _pa_1.data
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _pa_1.dim pa
void init_array(parray_t pa)
{
   int i;
//             <may be read   >: _pa_1.data i
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _pa_1.dim pa
   for(i = 0; i <= pa->dim-1; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _pa_1.data _pa_1.dim i pa
      (pa->data)[i] = (float) i;

   return;
}
