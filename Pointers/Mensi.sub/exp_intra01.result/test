
PROPER  EFFECTS 

int main()
{
//             <    is written>: i
   int i = 0, *p, *q;
//             <    is written>: p
   p = &i;
//             <    is read   >: p
//             <    is written>: q
   q = p;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0, *p, *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT

   p = &i;

// Points To:
// p -> i , EXACT
// q -> undefined , EXACT

   q = p;

// Points To:
// p -> i , EXACT
// q -> i , EXACT

   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0, *p, *q;
//             <    is written>: p
   p = &i;
//             <    is read   >: p
//             <    is written>: q
   q = p;
   return 0;
}

CUMULATED  EFFECTS WITH POINTS TO

int main()
{
//             <    is written>: i
   int i = 0, *p, *q;
//             <    is written>: p
   p = &i;
//             <    is read   >: p
//             <    is written>: q
   q = p;
   return 0;
}
