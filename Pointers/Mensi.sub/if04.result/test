
INLINING "init"

int main()
{
   int init_p = 1;
   int *q = (void *) 0;
   if (init_p)
      q = (int *) malloc(4*sizeof(int));
   if (q==(void *) 0)
      exit(1);
   else
      *q = 0;
   return 0;
}

