
PROPER  EFFECTS 

int main()
{

//             <    is written>: m n
   int n = 4, m = 3;
//             <    is read   >: m n
   int a[n][m];
//             <    is read   >: m
//             <    is written>: p
   int (*p)[m] = a;
//             <    is read   >: p
//             <    is written>: p

   p += 1;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{


// Points To: none

   int n = 4, m = 3;

// Points To: none

   int a[n][m];

// Points To: none

   int (*p)[m] = a;

// Points To:
// p -> a[0] , EXACT


   p += 1;

// Points To:
// p -> a[1] , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{

//             <    is written>: m n
   int n = 4, m = 3;
//             <    is read   >: m n
   int a[n][m];
//             <    is read   >: m
//             <    is written>: p
   int (*p)[m] = a;
//             <    is read   >: p
//             <    is written>: p

   p += 1;

   return 0;
}

CUMULATED  EFFECTS WITH POINTS TO

int main()
{

//             <    is written>: m n
   int n = 4, m = 3;
//             <    is read   >: m n
   int a[n][m];
//             <    is read   >: m
//             <    is written>: p
   int (*p)[m] = a;
//             <    is read   >: p
//             <    is written>: p

   p += 1;

   return 0;
}
