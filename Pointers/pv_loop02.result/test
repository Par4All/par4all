
PROPER  EFFECTS

int main()
{
   int i;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = i;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = (int *) malloc(10*sizeof(int));

// Points To:
// p -> *HEAP*_l_7[0] , EXACT


   for(i = 0; i <= 9; i += 1)

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

      p[i] = i;

// Points To:
// p -> *HEAP*_l_7[0] , EXACT

   return 0;
}
