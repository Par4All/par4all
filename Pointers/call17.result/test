
PROPER  EFFECTS

void call17(int *x)
{
   /* check aliases */
//             <    is read   >: x
//             <    is written>: y
   int *y = x;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: y
   y[0] = 1;
   return;
}

POINTS-TO


// Points To IN:
// x -> *NULL* , MAY
// x -> _x_1[0] , MAY


// Points To OUT:
// x -> *NULL* , MAY
// x -> _x_1[0] , MAY

void call17(int *x)
{
   /* check aliases */

// Points To:
// x -> *NULL* , MAY
// x -> _x_1[0] , MAY

   int *y = x;

// Points To:
// x -> *NULL* , MAY
// x -> _x_1[0] , MAY
// y -> *NULL* , MAY
// y -> _x_1[0] , MAY

   y[0] = 1;

// Points To:
// x -> *NULL* , MAY
// x -> _x_1[0] , MAY
// y -> _x_1[0] , EXACT

   return;
}
