
PROPER  EFFECTS

void comma01()
{
   int i;
   int *p;
   int *q;
   int *r;
//             <    is written>: p q r
   p = &i, q = &i, r = &i;
//             <    is written>: i
   
   
   i = 2;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void comma01()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int *r;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &i, q = &i, r = &i;

// Points To:
// p -> i , EXACT
// q -> i , EXACT
// r -> i , EXACT

   
   
   i = 2;
}
