
PROPER  EFFECTS

int main(void)
{
   int i, j;
   int **pp;
   {
//             <    is written>: pi
      int *pi = &i;
//             <    is written>: pp
      pp = &pi;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      **pp = 1;
   }
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   // pp points to a dead value in the stack
   // should generate an error
   **pp = 0;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pp
   // segfault?
   *pp = &j;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int i, j;

// Points To: none

   int **pp;
   {

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int *pi = &i;

// Points To:
// pi -> i , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi;

// Points To:
// pi -> i , EXACT
// pp -> pi , EXACT

      **pp = 1;
   }

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   
   // pp points to a dead value in the stack
   // should generate an error
   **pp = 0;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   // segfault?
   *pp = &j;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> j , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   return 0;
}
