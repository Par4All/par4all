
PROPER  EFFECTS

int main()
{
   int y[10];
   char z[10];
   float v[10];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.first
   *x.first = &y[5];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.second
   *x.second = &z[6];
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x.third
   *x.third = &v[7];

   return 0;
}

POINTS-TO


// Points To IN:
// x.first -> *NULL_POINTER* , MAY
// x.second -> *NULL_POINTER* , MAY
// x.third -> *NULL_POINTER* , MAY
// x.first -> _x_1__0 , MAY
// x.second -> _x_2__0 , MAY
// x.third -> _x_3__0 , MAY


// Points To OUT:
// _x_1__0 -> y[5] , EXACT
// _x_2__0 -> z[6] , EXACT
// _x_3__0 -> v[7] , EXACT
// x.first -> *NULL_POINTER* , MAY
// x.second -> *NULL_POINTER* , MAY
// x.third -> *NULL_POINTER* , MAY
// x.first -> _x_1__0 , MAY
// x.second -> _x_2__0 , MAY
// x.third -> _x_3__0 , MAY

int main()
{

// Points To: none

   int y[10];

// Points To: none

   char z[10];

// Points To: none

   float v[10];

// Points To: none

   *x.first = &y[5];

// Points To:
// _x_1__0 -> y[5] , EXACT
// x.first -> *NULL_POINTER* , MAY
// x.first -> _x_1__0 , MAY

   *x.second = &z[6];

// Points To:
// _x_1__0 -> y[5] , EXACT
// _x_2__0 -> z[6] , EXACT
// x.first -> *NULL_POINTER* , MAY
// x.second -> *NULL_POINTER* , MAY
// x.first -> _x_1__0 , MAY
// x.second -> _x_2__0 , MAY

   *x.third = &v[7];

// Points To:
// _x_1__0 -> y[5] , EXACT
// _x_2__0 -> z[6] , EXACT
// _x_3__0 -> v[7] , EXACT
// x.first -> *NULL_POINTER* , MAY
// x.second -> *NULL_POINTER* , MAY
// x.third -> *NULL_POINTER* , MAY
// x.first -> _x_1__0 , MAY
// x.second -> _x_2__0 , MAY
// x.third -> _x_3__0 , MAY


   return 0;
}
