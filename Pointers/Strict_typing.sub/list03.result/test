int main(void)
{

// Points To:
// nil -> *NULL_POINTER* , EXACT

   list l = nil;

// Points To:
// l -> *NULL_POINTER* , EXACT
// nil -> *NULL_POINTER* , EXACT

   int n;

// Points To:
// l -> *NULL_POINTER* , EXACT
// nil -> *NULL_POINTER* , EXACT

   fscanf(stdin, "%d", &n);

// Points To:
// l -> *NULL_POINTER* , EXACT
// nil -> *NULL_POINTER* , EXACT

   while (n--)

// Points To:
// l -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// l -> *NULL_POINTER* , MAY
// nil -> *NULL_POINTER* , EXACT

      l = list_cons((double) n, l);

// Points To:
// l -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// l -> *NULL_POINTER* , MAY
// nil -> *NULL_POINTER* , EXACT

   n = list_len(l);

// Points To:
// l -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// l -> *NULL_POINTER* , MAY
// nil -> *NULL_POINTER* , EXACT

   fprintf(stdout, "n=%d", n);

// Points To:
// l -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// l -> *NULL_POINTER* , MAY
// nil -> *NULL_POINTER* , EXACT

   list_free(l), l = nil;

// Points To:
// l -> *NULL_POINTER* , EXACT
// nil -> *NULL_POINTER* , EXACT

   return 0;
}
