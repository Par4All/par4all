double list_value(list l)
{
   /* if (l==nil) */
   /*   return 0.0; */
   /* else */
   /*   return l->value; */

// Points To: none

   double v;

// Points To:
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

   if (l==nil)

// Points To:
// l -> *NULL_POINTER* , EXACT
// nil -> *NULL_POINTER* , EXACT

      v = 0.0;
   else

// Points To:
// l -> _l_1 , EXACT
// nil -> *NULL_POINTER* , EXACT

      v = l->value;

// Points To:
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

   return v;
}

