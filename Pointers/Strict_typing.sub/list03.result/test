int list_len(list l)
{

// Points To: none

   int n = 0;

// Points To:
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY

   list c = l;

// Points To:
// c -> *NULL_POINTER* , MAY
// c -> _l_1 , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

   while (c!=nil) {

// Points To:
// c -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// c -> _l_1 , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

      // we know that c!=nil...
      c = list_next(c);

// Points To:
// c -> *ANY_MODULE*:*ANYWHERE*_b2 , MAY
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

      n++;
   }

// Points To:
// c -> *NULL_POINTER* , EXACT
// l -> *NULL_POINTER* , MAY
// l -> _l_1 , MAY
// nil -> *NULL_POINTER* , EXACT

   return n;
}

