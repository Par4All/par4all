// Points To OUT:
// pi -> _pi_84[1] , EXACT
// q -> _q_92[0] , EXACT

int global03a()
{

// Points To: none

   int *p = i;

// Points To:
// p -> i[0] , EXACT
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_84[0] , MAY

   *pi = 1;

// Points To:
// p -> i[0] , EXACT
// pi -> _pi_84[0] , EXACT

   pi++;

// Points To:
// p -> i[0] , EXACT
// pi -> _pi_84[1] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _q_92[0] , MAY

   q->one = 1;

// Points To:
// p -> i[0] , EXACT
// pi -> _pi_84[1] , EXACT
// q -> _q_92[0] , EXACT

   (q->two)[4] = 2;

// Points To:
// p -> i[0] , EXACT
// pi -> _pi_84[1] , EXACT
// q -> _q_92[0] , EXACT

   return *p;
}

