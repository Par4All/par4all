            k1 ^= s->rNToGo==1?1:0;
            s->nblock_used++;
            if (s->nblock_used==s->save_nblock+1)
               ;
            else if (k1!=s->k0)
               s->k0 = k1;
            else {

               s->state_out_len = 3;
               s->tPos = (s->tt)[s->tPos];
