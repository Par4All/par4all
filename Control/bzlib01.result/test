         s->tPos = (s->tt)[s->tPos];
         k1 = (UChar) (s->tPos&0xff);
         s->tPos >>= 8;
         if (s->rNToGo==0) {
            s->rNToGo = BZ2_rNums[s->rTPos];
            s->rTPos++;
            if (s->rTPos==512)
               s->rTPos = 0;
         }
         s->rNToGo--;
         k1 ^= s->rNToGo==1?1:0;
         s->nblock_used++;
         if (s->nblock_used==s->save_nblock+1)
            ;
         else if (k1!=s->k0)
            s->k0 = k1;
         else {

            s->state_out_len = 2;
            s->tPos = (s->tt)[s->tPos];
            k1 = (UChar) (s->tPos&0xff);
            s->tPos >>= 8;
