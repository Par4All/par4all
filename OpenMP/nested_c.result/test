// OMP style
int main(void)
{
   int k, i;                                                            /*0006*/
   k = 0;
   i = 0;
   float sum;                                                           /*0007*/
   sum = 0;
   float array[size][size];                                             /*0008*/


#pragma omp parallel for private(i)
   for(k = 0; k <= size-1; k += 1)
#pragma omp parallel for 
      for(i = 0; i <= size-1; i += 1)
         array[k][i] = i+k;                                             /*0012*/


   for(i = 0; i <= size-1; i += 1)
      for(k = 0; k <= size-1; k += 1)
         printf("array[%d][%d] = %f", i, k, array[k][i]);               /*0018*/

   return 0;                                                            /*0022*/
}
int main(void)
{
   int k, i;                                                            /*0006*/
   k = 0;
   i = 0;
   float sum;                                                           /*0007*/
   sum = 0;
   float array[size][size];                                             /*0008*/


#pragma omp parallel for private(i)
   for(k = 0; k <= size-1; k += 1)
#pragma omp parallel for
      for(i = 0; i <= size-1; i += 1)
         array[k][i] = i+k;                                             /*0012*/


   for(i = 0; i <= size-1; i += 1)
      for(k = 0; k <= size-1; k += 1)
         printf("array[%d][%d] = %f", i, k, array[k][i]);               /*0018*/

   return 0;                                                            /*0022*/
}
