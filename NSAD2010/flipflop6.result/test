
TRANSFORMERS


//  T() {}

int main()
{

//  T() {}

   double x[2][10];

//  T(i,new,old,t) {new==1, old==0}

   int old = 0, new = 1, i, t;

//  T(t) {new==1, old==0, t==0}


   t = 0;

//  T(i,new,old,t) {new#init==1, old#init==0, t#init==0, 0<=new,
//    new<=1, 1<=new+t, 0<=old, old<=1, old<=t, 0<=t, t<=999}

   while (t<1000) {

//  T(i) {0<=i, t<=999}

      for(i = 0; i <= 9; i += 1)

//  T() {0<=i, i<=9, t<=999}

         x[new][i] = g(x[old][i]);

//  T(new) {i==10, 0<=new, new<=1, t<=999}

      new = (new+1)%2;

//  T(old) {i==10, 0<=new, new<=1, 0<=old, old<=1, t<=999}

      old = (old+1)%2;

//  T(t) {i==10, t==t#init+1, 0<=new, new<=1, 0<=old, old<=1, t<=1000}

      t++;
   }
}

PRECONDITIONS


//  P() {}

int main()
{

//  P() {}

   double x[2][10];

//  P() {}

   int old = 0, new = 1, i, t;

//  P(i,new,old,t) {new==1, old==0}


   t = 0;

//  P(i,new,old,t) {new==1, old==0, t==0}

   while (t<1000) {

//  P(i,new,old,t) {0<=new, new<=1, 1<=new+t, 0<=old, old<=1, old<=t,
//    t<=999}

      for(i = 0; i <= 9; i += 1)

//  P(i,new,old,t) {0<=i, i<=9, 0<=new, new<=1, 1<=new+t, 0<=old,
//    old<=1, old<=t, t<=999}

         x[new][i] = g(x[old][i]);

//  P(i,new,old,t) {i==10, 0<=new, new<=1, 1<=new+t, 0<=old, old<=1,
//    old<=t, t<=999}

      new = (new+1)%2;

//  P(i,new,old,t) {i==10, 0<=new, new<=1, 0<=old, old<=1, old<=t,
//    0<=t, t<=999}

      old = (old+1)%2;

//  P(i,new,old,t) {i==10, 0<=new, new<=1, 0<=old, old<=1, 0<=t,
//    t<=999}

      t++;
   }
}
