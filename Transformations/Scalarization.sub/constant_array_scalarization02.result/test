{
   int b[3];

   a[0] = 1;
   a[1] = 1;
   a[2] = 1;

   c[0] = 1;
   c[1] = 1;
   c[2] = 1;

   b[2] = 1;
   b[1] = 1;
   b[0] = 1;
}
void constant_array_scalarization02(int a[3])
{
   int b[3];
   //PIPS generated variable
   int b0, b1, b2;

   a[0] = 1;
   a[1] = 1;
   a[2] = 1;

   c[0] = 1;
   c[1] = 1;
   c[2] = 1;

   b0 = 1;
   b1 = 1;
   b2 = 1;
}

