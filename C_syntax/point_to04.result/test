
parsed printed file

void point_to04()
{
   struct s {
      int foo;
   };
   typedef struct s a_t;
   typedef a_t *p_t;
   a_t a;
   p_t p;

   (p+2)->foo = 1;
   
   /* Floating point values are not allowed in pointer arithmetic */
   /* (p+2.)->foo = 1; */

   (p+(int) 2.)->foo = 1;
   (2+p)->foo = 1;
   (p-2)->foo = 1;
   
   /* This one is forbidden */
   /*(2-p)->foo = 1; */

   p++->foo = 1;
   p--->foo = 1;
   ++p->foo = 1;
   --p->foo = 1;
}
