main()
{
   enum fleurs {rose=0x0001, marguerite=0, jasmin};
   enum legumes {carotte=rose+50, haricot};
   enum fleurs ma1, ma2, ma3;
   enum legumes mon1, mon2;
   int i,  a1,  a2,  a3,  on1,  on2,  j;

   ma1 = rose;
   ma2 = marguerite;
   ma3 = jasmin;
   mon1 = carotte;
   mon2 = haricot;

   i = ma1+ma2+ma3+mon1+mon2;

   a1 = rose;
   a2 = marguerite;
   a3 = jasmin;
   on1 = carotte;
   on2 = haricot;

   j = a1+a2+a3+on1+on2;
}

//  P() {}

main()
{
   enum fleurs {rose=0x0001, marguerite=0, jasmin};
   enum legumes {carotte=rose+50, haricot};
   enum fleurs ma1, ma2, ma3;
   enum legumes mon1, mon2;
   int i,  a1,  a2,  a3,  on1,  on2,  j;

//  P() {}


   ma1 = rose;

//  P(ma1) {ma1==1}

   ma2 = marguerite;

//  P(ma1,ma2) {ma1==1, ma2==0}

   ma3 = jasmin;

//  P(ma1,ma2,ma3) {ma1==1, ma2==0, ma3==1}

   mon1 = carotte;

//  P(ma1,ma2,ma3,mon1) {ma1==1, ma2==0, ma3==1, mon1==51}

   mon2 = haricot;

//  P(ma1,ma2,ma3,mon1,mon2) {ma1==1, ma2==0, ma3==1, mon1==51,
//    mon2==52}


   i = ma1+ma2+ma3+mon1+mon2;

//  P(i,ma1,ma2,ma3,mon1,mon2) {ma1==1, ma2==0, ma3==1, mon1==51,
//    mon2==52}


   a1 = rose;

//  P(a1,i,ma1,ma2,ma3,mon1,mon2) {a1==1, ma1==1, ma2==0, ma3==1,
//    mon1==51, mon2==52}

   a2 = marguerite;

//  P(a1,a2,i,ma1,ma2,ma3,mon1,mon2) {a1==1, a2==0, ma1==1, ma2==0,
//    ma3==1, mon1==51, mon2==52}

   a3 = jasmin;

//  P(a1,a2,a3,i,ma1,ma2,ma3,mon1,mon2) {a1==1, a2==0, a3==1, ma1==1,
//    ma2==0, ma3==1, mon1==51, mon2==52}

   on1 = carotte;

//  P(a1,a2,a3,i,ma1,ma2,ma3,mon1,mon2,on1) {a1==1, a2==0, a3==1,
//    ma1==1, ma2==0, ma3==1, mon1==51, mon2==52, on1==51}

   on2 = haricot;

//  P(a1,a2,a3,i,ma1,ma2,ma3,mon1,mon2,on1,on2) {a1==1, a2==0, a3==1,
//    ma1==1, ma2==0, ma3==1, mon1==51, mon2==52, on1==51, on2==52}


   j = a1+a2+a3+on1+on2;
}
