@INPROCEEDINGS{Nguy:95a,
	AUTHOR = {Nguyen,Trung and Gu, Jungie and Li, Zhiyuan},
	TITLE ={An Interprocedural Parallelizing Compiler and Its Support
		  for Memory Hierarchy Research},
	INDEX ={\indexbib{}},
	BOOKTITLE = lcpc,
	YEAR = 1995,
	MONTH = aug,
	PAGES = {7.1--7.15},
	SUMMARY = {
		  
		  This article is  an overview of several analyses performed
 		  by the  Panorama  compiler developed at the  university of
 		  Minnesota. The first   technique  is an array  data   flow
 		  analysis  (GARs), based on the interprocedural hierachical
 		  control flow graph    of the program.  The second,  called
 		  'static task   profil{\'e},  helps in  deciding  whether  the
 		  parallel    loops    must be   scheduled   statically   or
 		  dynamically. The  third one, handles  task assignment  and
 		  data allocation   at   the    same  time,  by   using    a
 		  heuristic.  

		  The main part is dedicated to GARs, but it only consists
 		  of RSDs with bounds and strides, with a guard, approach
 		  already used by other authors.

