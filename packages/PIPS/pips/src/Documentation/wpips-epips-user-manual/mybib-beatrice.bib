@INPROCEEDINGS{Wen:95a,
	AUTHOR = {Wen, Renhua. and Gao, Guang and Dongen, Vincent},
	TITLE ={The Design and Implementation of the Accurate Array Data
		  Flow Analysis in the {HPC} Compiler},
	INDEX ={\indexbib{}},
	BOOKTITLE = hpcs,
	YEAR = 1995,
	MONTH = jul,
	PAGES = {144--155},
	SUMMARY = {Encore une version des l'{\em array data flow\/} \`{a} la
		  Feautrier. Cette fois-ci, ils calculent des {\em source
		  tree} qui sont des arbres \`{a} deux {\'e}tages. La racine est
		  le consommateur, les feuilles sont les producteurs. Les
		  conditions sous lesquelles une r{\'e}f{\'e}rence est bien
		  producteur est stock{\'e}e au niveau de la feuille et non
		  dans des niveaux interm{\'e}diaires de l'arbre. Ceci permet
		  une repr{\'e}sentation plus compacte. En plus, les
		  producteurs potentiels sont examin{\'e}s dans l'ordre
		  lexicographique inverse, ceux de niveau plus {\'e}lev{\'e}
		  (profondeur de d{\'e}pendance) avant ceux de niveau moins
		  {\'e}lev{\'e}. Et la recherche s'ar{\^e}te d\`{e}s que toutes les
		  feuilles ont trouv{\'e} un producteur correspondant.
		  
		  Il y a plus. La m{\'e}thode de Feautrier s'applique
		  normalement \`{a} un nid de boucles. Les auteurs l'{\'e}tendent
		  \`{a} la s{\'e}quence d'instructions complexes, sans introduire
		  de boucle imaginaire englobante. Mais si les r{\'e}sultats
		  exp{\'e}rimentaux sont plut{\^o}t bons pour les nids de boucles,
		  il semble qu'ils ne soient pas \`{a} la hauteur pour
		  l'analyse globale, car les codes du Perfect Club ne sont
		  pas assez {\em r{\'e}guliers\/} et il y a des appels de
		  proc{\'e}dure
		  
		  Le compilateur utilise PIP et Chernikova.
		  
		  \references{16}}
}

		  
