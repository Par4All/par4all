
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T(d,t,v,x) {}

   int v, t, x, d;

//  T(d,t,v,x) {d==0, t==0, v==0, x==0}

   v = t = x = d = 0;

//  T() {d==0, t==0, v==0, x==0}

   if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

      checking_error();

//  T(d,t,v,x) {d+t#init<=d#init+t,
//    d#init+t+5v#init+5x<=d+t#init+5v+5x#init, d<=t, t+5x<=d+5v,
//    0<=t, t#init<=t, 2v<=t+10, t<=5v+10, 0<=v, v#init<=v}

   while (rand_b())

//  T(d,t,v,x) {d+11t#init+10v#init==d#init+11t+10v-10,
//    d#init+10v<=d+10v#init+10, d+v<=d#init+v#init+1,
//    2d#init+13v#init+11x+2<=2d+13v+11x#init, 2v<=t+10, t<=5v+10,
//    v#init<=v}

      if (rand_b()) {

//  T() {x<=4}

         if (!(x<=4))

//  T() {0==-1}

            deadlock();

//  T(x) {x==x#init+1, x<=5}

         x++;

//  T(v) {v==v#init+1, x<=5}

         v++;

//  T() {2v<=t+10, t<=5v+10, x<=5}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {d<=9}

         if (!(d<=9))

//  T() {0==-1}

            deadlock();

//  T(d) {d==d#init+1, d<=10}

         d++;

//  T(t) {t==t#init+1, d<=10}

         t++;

//  T() {d<=10, 2v<=t+10, t<=5v+10}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {d==10, 2<=x}

         if (!(d==10&&x>=2))

//  T() {0==-1}

            deadlock();

//  T(x) {d==10, x==0, 2<=x#init}

         x = 0;

//  T(d) {d==0, d#init==10, x==0}

         d = 0;

//  T() {d==0, x==0, 2v<=t+10, t<=5v+10}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T(d,t,v,x) {}

   int v, t, x, d;

//  T(d,t,v,x) {d==0, t==0, v==0, x==0}

   v = t = x = d = 0;

//  T() {d==0, t==0, v==0, x==0}

   if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

      checking_error();

//  T(d,t,v,x) {d#init+9t#init<=d+9t, d+t#init<=d#init+t,
//    d#init+t+5v#init+5x<=d+t#init+5v+5x#init,
//    d+t#init+10x#init<=d#init+t+10x, 0<=d+9t, d<=t, t+5x<=d+5v,
//    d+2v<=12t+10x, d<=t+10x, 2v<=t+10, t<=5v+10}

   while (rand_b())

//  T(d,t,v,x) {d#init+9t#init<=d+9t, d+t#init<=d#init+t,
//    d#init+t<=d+t#init+10, d#init+t+5v#init+5x<=d+t#init+5v+5x#init,
//    3d#init+4t+10v#init+15x<=3d+8t#init+5v+15x#init+65,
//    d#init+2t+5x<=d+t#init+5v+5x#init+15,
//    12d#init+12t+35v#init+60x<=12d+37t#init+60x#init+375,
//    9d#init+9t+15v#init+40x<=9d+24t#init+40x#init+275,
//    7d#init+7t+15v#init+30x<=7d+22t#init+30x#init+265,
//    3d#init+3t+10x<=3d+3t#init+15v#init+10x#init+125,
//    2d#init+2t+10v#init+10x<=2d+7t#init+10x#init+55,
//    2d+2t#init+15x#init<=2d#init+2t+15v#init+15x+100,
//    6d+11t#init+30x#init<=6d#init+6t+25v#init+30x+45,
//    d+t#init+10x#init<=d#init+t+10x,
//    d+11t#init+10x#init+10<=d#init+11t+10x, 2v<=t+10, t<=5v+10,
//    t#init<=5v#init+15}

      if (rand_b()) {

//  T() {x==0}

         if (!(x==0))

//  T() {0==-1}

            deadlock();

//  T(x) {x==1, x#init==0}

         x++;

//  T(v) {v==v#init+1, x==1}

         v++;

//  T() {x==1, 2v<=t+10, t<=5v+10}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {d<=8}

         if (!(d<=8))

//  T() {0==-1}

            deadlock();

//  T(d) {d==d#init+1, d<=9}

         d++;

//  T(t) {t==t#init+1, d<=9}

         t++;

//  T() {d<=9, 2v<=t+10, t<=5v+10}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T(d,t,v,x) {d==10, d#init+t==t#init+10, v+x#init==v#init+x,
//    t#init+1<=t, t+10v#init<=5t#init+5v+15x#init+35, 2v<=t+10,
//    t<=5v+5x#init+5, t#init<=5v#init+15,
//    3v#init<=3t#init+8x#init+37, 2v#init<=t#init+2x#init+7,
//    v#init+2<=v+x#init, 0<=3v#init+2x#init+19, x#init<=1}

         if (rand_b()) {

//  T() {d==9}

            if (!(d==9))

//  T() {0==-1}

               deadlock();

//  T(d) {d==10, d#init==9}

            d++;

//  T(t) {d==10, t==t#init+1}

            t++;

//  T() {d==10, 2v<=t+10, t<=5v+10}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();

//  T(v,x) {d==10, v+x#init==v#init+x, 2v<=t+10, t<=5v+10, v#init<=v}

            while (rand_b()) {

//  T() {d==10, x==0, 0<=t+23}

               if (!(x==0))

//  T() {0==-1}

                  deadlock();

//  T(x) {d==10, x==1, x#init==0, 0<=t+23}

               x++;

//  T(v) {d==10, v==v#init+1, x==1, 0<=t+23}

               v++;

//  T() {d==10, x==1, 0<=t+23, 2v<=t+10, t<=5v+10}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();
            }

//  T() {d==10, x==1, 0<=t+23, t<=5v+10}

            if (!(x==1))

//  T() {0==-1}

               deadlock();

//  T(x) {d==10, x==2, x#init==1, 0<=t+23, t<=5v+10}

            x++;

//  T(v) {d==10, v==v#init+1, x==2, 0<=t+23, t<=5v+5}

            v++;

//  T() {d==10, x==2, 0<=t+23, 2v<=t+10, t<=5v+5}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();
         }
         else {

//  T() {x==1}

            if (!(x==1))

//  T() {0==-1}

               deadlock();

//  T(x) {x==2, x#init==1}

            x++;

//  T(v) {v==v#init+1, x==2}

            v++;

//  T() {x==2, 2v<=t+10, t<=5v+10}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();

//  T(d,t,v,x) {d+t#init==d#init+t, v+x#init==v#init+x, d#init<=d,
//    2v<=t+10, t<=5v+10, v#init<=v, x<=v+8, 2<=x}

            while (rand_b())

//  T(d,t,v,x) {d+v==d#init+v#init+1, t+v==t#init+v#init+1,
//    v+x#init==v#init+x, 2v<=t+10, t<=5v+10, v#init<=v, v<=v#init+1}

               if (rand_b()) {

//  T() {x<=3}

                  if (!(x<=3))

//  T() {0==-1}

                     deadlock();

//  T(x) {x==x#init+1, x<=4}

                  x++;

//  T(v) {v==v#init+1, x<=4}

                  v++;

//  T() {2v<=t+10, t<=5v+10, x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }
               else {

//  T() {d<=8}

                  if (!(d<=8))

//  T() {0==-1}

                     deadlock();

//  T(d) {d==d#init+1, d<=9}

                  d++;

//  T(t) {t==t#init+1, d<=9}

                  t++;

//  T() {d<=9, 2v<=t+10, t<=5v+10}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }

//  T() {d==9, 2v<=t+2x+6, x<=v+8, 2<=x}

            if (!(d==9))

//  T() {0==-1}

               deadlock();

//  T(d) {d==10, d#init==9, 2v<=t+2x+6, x<=v+8, 2<=x}

            d++;

//  T(t) {d==10, t==t#init+1, 2v<=t+2x+5, x<=v+8, 2<=x}

            t++;

//  T() {d==10, 2v<=t+10, t<=5v+10, 2v<=t+2x+5, x<=v+8, 2<=x}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();
         }

//  T(v,x) {d==10, v+x#init==v#init+x, 2v<=t+10, t<=5v+10, v<=t+x+13,
//    x<=v+8, 2<=x, x#init<=x}

         while (rand_b()) {

//  T() {d==10, 0<=t+21, x<=3}

            if (!(x<=3))

//  T() {0==-1}

               deadlock();

//  T(x) {d==10, x==x#init+1, 0<=t+21, x<=4}

            x++;

//  T(v) {d==10, v==v#init+1, 0<=t+21, x<=4}

            v++;

//  T() {d==10, 0<=t+21, 2v<=t+10, t<=5v+10, x<=4}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();
         }

//  T() {d==10, 0<=t+23, t<=5v+10, 20v<=13t+20x+120, 5v<=4t+10x+35,
//    10x<=t+10v+110, v<=t+x+13, 2v<=t+2x+6, t+15x<=20v+140, x<=v+8,
//    2<=x}

         if (!(d==10&&x>=2))

//  T() {0==-1}

            deadlock();

//  T(x) {d==10, x==0, 0<=t+23, t<=5v+10, 20v<=13t+20x#init+120,
//    5v<=4t+10x#init+35, 10x#init<=t+10v+110, v<=t+x#init+13,
//    2v<=t+2x#init+6, t+15x#init<=20v+140, x#init<=v+8, 2<=x#init}

         x = 0;

//  T(d) {d==0, d#init==10, x==0, 0<=t+21, t<=80v+500, t<=30v+170,
//    t<=20v+110, t<=5v+10, 2t<=85v+535, 0<=v+6}

         d = 0;

//  T() {d==0, x==0, 0<=t+21, 2v<=t+10, t<=80v+500, t<=30v+170,
//    t<=20v+110, t<=5v+10, 2t<=85v+535, 0<=v+6}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T(d,t,v,x) {d==10, d#init+t==t#init+10, v+x#init==v#init+5, x==5,
//    t#init+1<=t, t<=5v#init+30, 2v#init<=t+2x#init,
//    t#init<=5v#init+15, 2v#init<=t#init+2x#init+6,
//    0<=3v#init+2x#init+19, x#init<=1}

         if (rand_b()) {

//  T() {d==9}

            if (!(d==9))

//  T() {0==-1}

               deadlock();

//  T(d) {d==10, d#init==9}

            d++;

//  T(t) {d==10, t==t#init+1}

            t++;

//  T() {d==10, 2v<=t+10, t<=5v+10}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();

//  T(v,x) {d==10, v+x#init==v#init+x, 2v<=t+10, t<=5v+10, v#init<=v}

            while (rand_b()) {

//  T() {d==10, x==0, 0<=t+23}

               if (!(x==0))

//  T() {0==-1}

                  deadlock();

//  T(x) {d==10, x==1, x#init==0, 0<=t+23}

               x++;

//  T(v) {d==10, v==v#init+1, x==1, 0<=t+23}

               v++;

//  T() {d==10, x==1, 0<=t+23, 2v<=t+10, t<=5v+10}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();
            }

//  T() {d==10, x==1, 0<=t+23, t<=5v+10}

            if (!(x==1))

//  T() {0==-1}

               deadlock();

//  T(x) {d==10, x==2, x#init==1, 0<=t+23, t<=5v+10}

            x++;

//  T(v) {d==10, v==v#init+1, x==2, 0<=t+23, t<=5v+5}

            v++;

//  T() {d==10, x==2, 0<=t+23, 2v<=t+10, t<=5v+5}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();

//  T(v,x) {d==10, v+x#init==v#init+x, 2v<=t+10, t+5x<=5v+15, 2<=x,
//    x<=4, x#init<=x}

            while (rand_b()) {

//  T() {d==10, 0<=t+20, x<=3}

               if (!(x<=3))

//  T() {0==-1}

                  deadlock();

//  T(x) {d==10, x==x#init+1, 0<=t+20, x<=4}

               x++;

//  T(v) {d==10, v==v#init+1, 0<=t+20, x<=4}

               v++;

//  T() {d==10, 0<=t+20, 2v<=t+10, t<=5v+10, x<=4}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();
            }

//  T() {d==10, x==4, 0<=t+23, 2v<=t+14, t+5<=5v}

            if (!(x==4))

//  T() {0==-1}

               deadlock();

//  T(x) {d==10, x==5, x#init==4, 0<=t+23, 2v<=t+14, t+5<=5v}

            x++;

//  T(v) {d==10, v==v#init+1, x==5, 0<=t+23, 2v<=t+16, t+10<=5v}

            v++;

//  T() {d==10, x==5, 0<=t+23, 2v<=t+10, t+10<=5v}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();
         }
         else {

//  T() {x==1}

            if (!(x==1))

//  T() {0==-1}

               deadlock();

//  T(x) {x==2, x#init==1}

            x++;

//  T(v) {v==v#init+1, x==2}

            v++;

//  T() {x==2, 2v<=t+10, t<=5v+10}

            if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

               checking_error();

//  T(d,t,v,x) {d+t#init==d#init+t, v+x#init==v#init+x, d#init<=d,
//    2v<=t+10, t<=5v+10, v#init<=v, x<=v+8, 2<=x}

            while (rand_b())

//  T(d,t,v,x) {d+v==d#init+v#init+1, t+v==t#init+v#init+1,
//    v+x#init==v#init+x, 2v<=t+10, t<=5v+10, v#init<=v, v<=v#init+1}

               if (rand_b()) {

//  T() {x<=3}

                  if (!(x<=3))

//  T() {0==-1}

                     deadlock();

//  T(x) {x==x#init+1, x<=4}

                  x++;

//  T(v) {v==v#init+1, x<=4}

                  v++;

//  T() {2v<=t+10, t<=5v+10, x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }
               else {

//  T() {d<=8}

                  if (!(d<=8))

//  T() {0==-1}

                     deadlock();

//  T(d) {d==d#init+1, d<=9}

                  d++;

//  T(t) {t==t#init+1, d<=9}

                  t++;

//  T() {d<=9, 2v<=t+10, t<=5v+10}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }

//  T(d,t,v,x) {d==10, d#init+t==t#init+10, v+x#init==v#init+5, x==5,
//    d#init<=9, 17v<=d#init+10t#init+110,
//    6t#init+10v<=6d#init+55v#init+100,
//    2t#init+5v<=2d#init+15v#init+15, 10v<=d#init+4t#init+5v#init+55,
//    2v<=t#init+11, 0<=v+3, v#init+1<=v, v<=v#init+3}

            if (rand_b()) {

//  T() {x==4, 2v<=t+14, 0<=v+4}

               if (!(x==4))

//  T() {0==-1}

                  deadlock();

//  T(x) {x==5, x#init==4, 2v<=t+14, 0<=v+4}

               x++;

//  T(v) {v==v#init+1, x==5, 2v<=t+16, 0<=v+3}

               v++;

//  T() {x==5, 2v<=t+10, t<=5v+10, 0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();

//  T(d,t) {d+t#init==d#init+t, x==5, d#init<=d, 2v<=t+10, t<=5v+10,
//    0<=v+3}

               while (rand_b()) {

//  T() {x==5, d<=8, 0<=v+3}

                  if (!(d<=8))

//  T() {0==-1}

                     deadlock();

//  T(d) {d==d#init+1, x==5, d<=9, 0<=v+3}

                  d++;

//  T(t) {t==t#init+1, x==5, d<=9, 0<=v+3}

                  t++;

//  T() {x==5, d<=9, 2v<=t+10, t<=5v+10, 0<=v+3}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }

//  T() {d==9, x==5, 2v<=t+10, 0<=v+3}

               if (!(d==9))

//  T() {0==-1}

                  deadlock();

//  T(d) {d==10, d#init==9, x==5, 2v<=t+10, 0<=v+3}

               d++;

//  T(t) {d==10, t==t#init+1, x==5, 2v<=t+9, 0<=v+3}

               t++;

//  T() {d==10, x==5, 2v<=t+9, t<=5v+10, 0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();
            }
            else {

//  T() {d==9, 2v<=t+2x+6, x<=v+8, 2<=x}

               if (!(d==9))

//  T() {0==-1}

                  deadlock();

//  T(d) {d==10, d#init==9, 2v<=t+2x+6, x<=v+8, 2<=x}

               d++;

//  T(t) {d==10, t==t#init+1, 2v<=t+2x+5, x<=v+8, 2<=x}

               t++;

//  T() {d==10, 2v<=t+10, t<=5v+10, 2v<=t+2x+5, x<=v+8, 2<=x}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();

//  T(v,x) {d==10, v+x#init==v#init+x, 2v<=t+10, t<=5v+10, 2v<=t+2x+5,
//    x<=v+8, 2<=x, x#init<=x}

               while (rand_b()) {

//  T() {d==10, 0<=t+21, x<=3}

                  if (!(x<=3))

//  T() {0==-1}

                     deadlock();

//  T(x) {d==10, x==x#init+1, 0<=t+21, x<=4}

                  x++;

//  T(v) {d==10, v==v#init+1, 0<=t+21, x<=4}

                  v++;

//  T() {d==10, 0<=t+21, 2v<=t+10, t<=5v+10, x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                     checking_error();
               }

//  T() {d==10, x==4, 0<=t+23, 2v<=t+13, t<=5v+10, 0<=v+4}

               if (!(x==4))

//  T() {0==-1}

                  deadlock();

//  T(x) {d==10, x==5, x#init==4, 0<=t+23, 2v<=t+13, t<=5v+10, 0<=v+4}

               x++;

//  T(v) {d==10, v==v#init+1, x==5, 0<=t+23, 2v<=t+15, t<=5v+5,
//    0<=v+3}

               v++;

//  T() {d==10, x==5, 0<=t+23, 2v<=t+10, t<=5v+5, 0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

                  checking_error();
            }
         }

//  T() {d==10, x==5, 2v<=t+10, t<=5v+10, 0<=v+3}

         if (!(d==10&&x>=2))

//  T() {0==-1}

            deadlock();

//  T(x) {d==10, x==0, x#init==5, 2v<=t+10, t<=5v+10, 0<=v+3}

         x = 0;

//  T(d) {d==0, d#init==10, x==0, 2v<=t+10, t<=5v+10, 0<=v+3}

         d = 0;

//  T() {d==0, x==0, 2v<=t+10, t<=5v+10, 0<=v+3}

         if (!(2*v<=t+10&&5*v>=t-10))

//  T() {0==-1}

            checking_error();
      }
}

PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int v, t, x, d;

//  P(d,t,v,x) {}

   v = t = x = d = 0;

//  P(d,t,v,x) {d==0, t==0, v==0, x==0}

   if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

      checking_error();

//  P(d,t,v,x) {d==0, t==0, v==0, x==0}

   while (rand_b())

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v}

      if (rand_b()) {

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v}

         if (!(x<=4))

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v,
//    5<=x}

            deadlock();

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v,
//    x<=4}

         x++;

//  P(d,t,v,x) {d<=t, t+5x<=d+5v+5, 0<=t, 2v<=t+10, t<=5v+10, 0<=v,
//    x<=5}

         v++;

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+12, t<=5v+5, 1<=v, x<=5}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+12, t<=5v+5, t+11<=2v,
//    1<=v, x<=5}

            checking_error();
      }
      else if (rand_b()) {

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v}

         if (!(d<=9))

//  P(d,t,v,x) {10<=d, d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10,
//    0<=v}

            deadlock();

//  P(d,t,v,x) {d<=9, d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10,
//    0<=v}

         d++;

//  P(d,t,v,x) {d<=10, d<=t+1, t+5x+1<=d+5v, 0<=t, 2v<=t+10, t<=5v+10,
//    0<=v}

         t++;

//  P(d,t,v,x) {d<=10, d<=t, t+5x<=d+5v, 1<=t, 2v<=t+9, t<=5v+11,
//    0<=v}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {t==5v+11, d<=10, 5x+11<=d, 0<=v}

            checking_error();
      }
      else {

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v}

         if (!(d==10&&x>=2))

//  P(d,t,v,x) {d<=t, t+5x<=d+5v, 0<=t, 2v<=t+10, t<=5v+10, 0<=v}

            deadlock();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, t+5x<=5v+10, 0<=v,
//    2<=x}

         x = 0;

//  P(d,t,v,x) {d==10, x==0, 10<=t, t<=5v, 2v<=t+10, 0<=v}

         d = 0;

//  P(d,t,v,x) {d==0, x==0, 10<=t, t<=5v, 2v<=t+10, 0<=v}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int v, t, x, d;

//  P(d,t,v,x) {}

   v = t = x = d = 0;

//  P(d,t,v,x) {d==0, t==0, v==0, x==0}

   if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

      checking_error();

//  P(d,t,v,x) {d==0, t==0, v==0, x==0}

   while (rand_b())

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

      if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

         if (!(x==0))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    d+5<=t+5x, 2v<=t+10, t<=5v+10}

            deadlock();

//  P(d,t,v,x) {x==0, 0<=d+9t, d<=t, t<=d+5v, d+2v<=12t, 2v<=t+10,
//    t<=5v+10}

         x++;

//  P(d,t,v,x) {x==1, 0<=d+9t, d<=t, t<=d+5v, d+2v<=12t, 2v<=t+10,
//    t<=5v+10}

         v++;

//  P(d,t,v,x) {x==1, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+2, 2v<=t+12,
//    t<=5v+5}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {x==1, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+2, 2v<=t+12,
//    t<=5v+5, t+11<=2v}

            checking_error();
      }
      else if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

         if (!(d<=8))

//  P(d,t,v,x) {9<=d, 0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x,
//    d<=t+10x, 2v<=t+10, t<=5v+10}

            deadlock();

//  P(d,t,v,x) {d<=8, 0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x,
//    d<=t+10x, 2v<=t+10, t<=5v+10}

         d++;

//  P(d,t,v,x) {d<=9, 1<=d+9t, d<=t+1, t+5x+1<=d+5v, d+2v<=12t+10x+1,
//    d<=t+10x+1, 2v<=t+10, t<=5v+10}

         t++;

//  P(d,t,v,x) {d<=9, d<=t, 10<=d+9t, t+5x<=d+5v, d+2v+11<=12t+10x,
//    d<=t+10x, 2v<=t+9, t<=5v+11}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {t==5v+11, d<=9, 0<=d+45v+89, d<=5v+11, d<=58v+10x+121,
//    d<=5v+10x+11, 5x+11<=d, 0<=v+6}

            checking_error();
      }
      else if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

         if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

            if (!(d==9))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

               deadlock();

//  P(d,t,v,x) {d==9, 9<=t, 2v<=t+10, t<=5v+10, v+5<=6t+5x,
//    t+5x<=5v+9, 9<=t+10x}

            d++;

//  P(d,t,v,x) {d==10, 9<=t, 2v<=t+10, t<=5v+10, v+5<=6t+5x,
//    t+5x<=5v+9, 9<=t+10x}

            t++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+11, v+11<=6t+5x,
//    t+5x<=5v+10, 10<=t+10x}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, t==5v+11, 0<=v, 0<=v+2x, 0<=29v+5x+55, x+1<=0}

               checking_error();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+10, v+11<=6t+5x,
//    t+5x<=5v+10, 10<=t+10x}

            while (rand_b()) {

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 10<=t+10x}

               if (!(x==0))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 2t+5x<=10v+15, 10<=t+10x,
//    15<=t+5x}

                  deadlock();

//  P(d,t,v,x) {d==10, x==0, 10<=t, 85v<=44t+415, 20v<=11t+80,
//    2v<=t+10, t<=5v+10}

               x++;

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+415, 20v<=11t+80,
//    2v<=t+10, t<=5v+10}

               v++;

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+500, 20v<=11t+100,
//    2v<=t+12, t<=5v+5}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+500, 20v<=11t+100,
//    2v<=t+12, t<=5v+5, t+11<=2v}

                  checking_error();
            }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 10<=t+10x}

            if (!(x==1))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 340v<=187t+75x+1360,
//    85v<=44t+5x+395, 85v+5x<=44t+415, 20v<=11t+10x+80, t+5x<=5v+10,
//    10<=t+10x}

               deadlock();

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+410, 20v<=11t+90,
//    2v<=t+10, t<=5v+5}

            x++;

//  P(d,t,v,x) {d==10, x==2, 10<=t, 85v<=44t+410, 20v<=11t+90,
//    2v<=t+10, t<=5v+5}

            v++;

//  P(d,t,v,x) {d==10, x==2, 10<=t, t<=5v, 85v<=44t+495, 20v<=11t+110,
//    2v<=t+12}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==2, 10<=t, t<=5v, 85v<=44t+495, 20v<=11t+110,
//    2v<=t+12, t+11<=2v}

               checking_error();
         }
         else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

            if (!(x==1))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x,
//    d+2v<=12t+5x, d<=t+10x, 2v<=t+10, t<=5v+10}

               deadlock();

//  P(d,t,v,x) {x==1, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+10,
//    2v<=t+10, t<=5v+10}

            x++;

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+10,
//    2v<=t+10, t<=5v+10}

            v++;

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+12, t<=5v+5}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+12, t<=5v+5, t+11<=2v}

               checking_error();

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+10, t<=5v+5}

            while (rand_b())

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

               if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

                  if (!(x<=3))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 4<=x}

                     deadlock();

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x,
//    x<=3}

                  x++;

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v+5, 2v<=t+10, t<=5v+10, 3<=x,
//    x<=4}

                  v++;

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+12, t<=5v+5, 3<=x,
//    x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+12, t<=5v+5,
//    t+11<=2v, 3<=x, x<=4}

                     checking_error();
               }
               else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

                  if (!(d<=8))

//  P(d,t,v,x) {9<=d, 0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10,
//    2<=x}

                     deadlock();

//  P(d,t,v,x) {d<=8, 0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10,
//    2<=x}

                  d++;

//  P(d,t,v,x) {d<=9, 1<=d+9t, d<=t+1, t+5x+1<=d+5v, 2v<=t+10,
//    t<=5v+10, 2<=x}

                  t++;

//  P(d,t,v,x) {d<=9, d<=t, 10<=d+9t, t+5x<=d+5v, 2v<=t+9, t<=5v+11,
//    2<=x}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

                     checking_error();
               }

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 120v+30x<=d+69t+720,
//    20v+20x<=d+19t+180, 3d+100v+10x<=53t+540, 24d+t+205x<=125v+945,
//    10x<=d+9t+40, 3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25,
//    2<=x}

            if (!(d==9))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 6t+1230x<=43d+750v+3800,
//    120v+30x<=d+69t+720, 20v+20x<=d+19t+180, 82x<=d+2t+50v+248,
//    3d+100v+10x<=53t+540, 4d+120v+30x<=69t+760, 4d+20v+20x<=19t+220,
//    24d+t+205x<=125v+945, 10x<=d+9t+40, d+8x<=3t+52, 2d+30x<=9t+200,
//    3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25, 2<=x}

               deadlock();

//  P(d,t,v,x) {d==9, 9<=t, 2v<=t+10, t<=5v+5, 100v+10x<=53t+513,
//    40v+10x<=23t+243, 20v+20x<=19t+189, t+205x<=125v+729,
//    t+5x<=5v+9, 10x<=9t+49, 10x<=3t+63, 2<=x}

            d++;

//  P(d,t,v,x) {d==10, 9<=t, 2v<=t+10, t<=5v+5, 100v+10x<=53t+513,
//    40v+10x<=23t+243, 20v+20x<=19t+189, 2v<=t+2x+6,
//    t+205x<=125v+729, t+5x<=5v+9, 10x<=9t+49, 10x<=3t+63, x<=v+8,
//    2<=x}

            t++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+6, 100v+10x<=53t+460,
//    40v+10x<=23t+220, 20v+20x<=19t+170, 2v<=t+2x+5,
//    t+205x<=125v+730, t+5x<=5v+10, 10x<=9t+40, 10x<=3t+60, x<=v+8,
//    2<=x}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

               checking_error();
         }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 35v+5x<=19t+170, t+5x<=5v+10,
//    2<=x}

         while (rand_b()) {

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, t+5x<=5v+10,
//    2<=x}

            if (!(x<=3))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, t+5x<=5v+10,
//    4<=x}

               deadlock();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, t+5x<=5v+10,
//    2<=x, x<=3}

            x++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+135, t+5x<=5v+15,
//    3<=x, x<=4}

            v++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, 25v+5x<=14t+160, t+5x<=5v+10,
//    3<=x, x<=4}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, t+11<=2v, 25v+5x<=14t+160,
//    t+5x<=5v+10, 3<=x, x<=4}

               checking_error();
         }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, t+5x<=5v+10,
//    2<=x}

         if (!(d==10&&x>=2))

//  P() {0==-1}

            deadlock();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, t+5x<=5v+10,
//    2<=x}

         x = 0;

//  P(d,t,v,x) {d==10, x==0, 10<=t, t<=5v, 55v<=32t+295, 40v<=23t+213,
//    30v<=19t+195, 25v<=14t+120, 20v<=11t+96, 2v<=t+10, t<=20v+110,
//    0<=v+6}

         d = 0;

//  P(d,t,v,x) {d==0, x==0, 10<=t, t<=5v, 55v<=32t+295, 40v<=23t+213,
//    30v<=19t+195, 25v<=14t+120, 20v<=11t+96, 2v<=t+10, t<=80v+500,
//    t<=30v+170, t<=20v+110, 2t<=85v+535, 0<=v+6}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

         if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

            if (!(d==9))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

               deadlock();

//  P(d,t,v,x) {d==9, 9<=t, 2v<=t+10, t<=5v+10, v+5<=6t+5x,
//    t+5x<=5v+9, 9<=t+10x}

            d++;

//  P(d,t,v,x) {d==10, 9<=t, 2v<=t+10, t<=5v+10, v+5<=6t+5x,
//    t+5x<=5v+9, 9<=t+10x}

            t++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+11, v+11<=6t+5x,
//    t+5x<=5v+10, 10<=t+10x}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, t==5v+11, 0<=v, 0<=v+2x, 0<=29v+5x+55, x+1<=0}

               checking_error();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+10, v+11<=6t+5x,
//    t+5x<=5v+10, 10<=t+10x}

            while (rand_b()) {

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 10<=t+10x}

               if (!(x==0))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 2t+5x<=10v+15, 10<=t+10x,
//    15<=t+5x}

                  deadlock();

//  P(d,t,v,x) {d==10, x==0, 10<=t, 85v<=44t+415, 20v<=11t+80,
//    2v<=t+10, t<=5v+10}

               x++;

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+415, 20v<=11t+80,
//    2v<=t+10, t<=5v+10}

               v++;

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+500, 20v<=11t+100,
//    2v<=t+12, t<=5v+5}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+500, 20v<=11t+100,
//    2v<=t+12, t<=5v+5, t+11<=2v}

                  checking_error();
            }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 85v+5x<=44t+415,
//    20v<=11t+10x+80, t+5x<=5v+10, 10<=t+10x}

            if (!(x==1))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t<=5v+10, 340v<=187t+75x+1360,
//    85v<=44t+5x+395, 85v+5x<=44t+415, 20v<=11t+10x+80, t+5x<=5v+10,
//    10<=t+10x}

               deadlock();

//  P(d,t,v,x) {d==10, x==1, 10<=t, 85v<=44t+410, 20v<=11t+90,
//    2v<=t+10, t<=5v+5}

            x++;

//  P(d,t,v,x) {d==10, x==2, 10<=t, 85v<=44t+410, 20v<=11t+90,
//    2v<=t+10, t<=5v+5}

            v++;

//  P(d,t,v,x) {d==10, x==2, 10<=t, t<=5v, 85v<=44t+495, 20v<=11t+110,
//    2v<=t+12}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==2, 10<=t, t<=5v, 85v<=44t+495, 20v<=11t+110,
//    2v<=t+12, t+11<=2v}

               checking_error();

//  P(d,t,v,x) {d==10, x==2, 10<=t, t<=5v, 85v<=44t+495, 20v<=11t+110,
//    2v<=t+10}

            while (rand_b()) {

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t+5x<=5v+10, 2<=x, x<=4}

               if (!(x<=3))

//  P(d,t,v,x) {d==10, x==4, 10<=t, 2v<=t+10, t+10<=5v}

                  deadlock();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t+5x<=5v+10, 2<=x, x<=3}

               x++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t+5x<=5v+15, 3<=x, x<=4}

               v++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, t+5x<=5v+10, 3<=x, x<=4}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, t+11<=2v, t+5x<=5v+10, 3<=x,
//    x<=4}

                  checking_error();
            }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t+5x<=5v+10, 2<=x, x<=4}

            if (!(x==4))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, t+5x<=5v+10, 2<=x, x<=3}

               deadlock();

//  P(d,t,v,x) {d==10, x==4, 10<=t, 2v<=t+10, t+10<=5v}

            x++;

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+10, t+10<=5v}

            v++;

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+12, t+15<=5v}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+12, t+15<=5v, t+11<=2v}

               checking_error();
         }
         else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x, d<=t+10x,
//    2v<=t+10, t<=5v+10}

            if (!(x==1))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, d+2v<=12t+10x,
//    d+2v<=12t+5x, d<=t+10x, 2v<=t+10, t<=5v+10}

               deadlock();

//  P(d,t,v,x) {x==1, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+10,
//    2v<=t+10, t<=5v+10}

            x++;

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+5<=d+5v, d+2v<=12t+10,
//    2v<=t+10, t<=5v+10}

            v++;

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+12, t<=5v+5}

            if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+12, t<=5v+5, t+11<=2v}

               checking_error();

//  P(d,t,v,x) {x==2, 0<=d+9t, d<=t, t+10<=d+5v, d+2v<=12t+12,
//    2v<=t+10, t<=5v+5}

            while (rand_b())

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

               if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

                  if (!(x<=3))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 4<=x}

                     deadlock();

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x,
//    x<=3}

                  x++;

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v+5, 2v<=t+10, t<=5v+10, 3<=x,
//    x<=4}

                  v++;

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+12, t<=5v+5, 3<=x,
//    x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+12, t<=5v+5,
//    t+11<=2v, 3<=x, x<=4}

                     checking_error();
               }
               else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10, 2<=x}

                  if (!(d<=8))

//  P(d,t,v,x) {9<=d, 0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10,
//    2<=x}

                     deadlock();

//  P(d,t,v,x) {d<=8, 0<=d+9t, d<=t, t+5x<=d+5v, 2v<=t+10, t<=5v+10,
//    2<=x}

                  d++;

//  P(d,t,v,x) {d<=9, 1<=d+9t, d<=t+1, t+5x+1<=d+5v, 2v<=t+10,
//    t<=5v+10, 2<=x}

                  t++;

//  P(d,t,v,x) {d<=9, d<=t, 10<=d+9t, t+5x<=d+5v, 2v<=t+9, t<=5v+11,
//    2<=x}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

                     checking_error();
               }

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 120v+30x<=d+69t+720,
//    20v+20x<=d+19t+180, 3d+100v+10x<=53t+540, 24d+t+205x<=125v+945,
//    10x<=d+9t+40, 3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25,
//    2<=x}

            if (rand_b()) {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 120v+30x<=d+69t+720,
//    20v+20x<=d+19t+180, 3d+100v+10x<=53t+540, 24d+t+205x<=125v+945,
//    10x<=d+9t+40, 3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25,
//    2<=x}

               if (!(x==4))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 120v+15x<=d+69t+645,
//    120v+30x<=d+69t+720, 20v+10x<=d+19t+130, 20v+20x<=d+19t+180,
//    3d+100v+5x<=53t+515, 3d+100v+10x<=53t+540, 24d+t+205x<=125v+945,
//    48d+2t+205x<=250v+865, 5x<=d+9t+15, 10x<=d+9t+40, 3d+10x<=3t+90,
//    2v<=t+10, t<=5v+5, t+5x<=5v+25, 2t+5x<=10v+25, 2<=x}

                  deadlock();

//  P(d,t,v,x) {x==4, 0<=d+9t, d<=t, 120v<=d+69t+600, 20v<=d+19t+100,
//    t+20<=d+5v, 3d+100v<=53t+500, 24d+t<=125v+125, 2v<=t+10,
//    t<=5v+5}

               x++;

//  P(d,t,v,x) {x==5, 0<=d+9t, d<=t, 120v<=d+69t+600, 20v<=d+19t+100,
//    t+20<=d+5v, 3d+100v<=53t+500, 24d+t<=125v+125, 2v<=t+10,
//    t<=5v+5, 0<=v+4}

               v++;

//  P(d,t,v,x) {x==5, 0<=d+9t, d<=t, 24d+t<=125v, 120v<=d+69t+720,
//    20v<=d+19t+120, t+25<=d+5v, 3d+100v<=53t+600, t<=5v, 2v<=t+12,
//    0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {x==5, 0<=d+9t, d<=t, 24d+t<=125v, 120v<=d+69t+720,
//    20v<=d+19t+120, t+25<=d+5v, 3d+100v<=53t+600, t<=5v, 2v<=t+12,
//    t+11<=2v, 0<=v+3}

                  checking_error();

//  P(d,t,v,x) {x==5, 0<=d+9t, d<=t, 24d+t<=125v, 120v<=d+69t+720,
//    20v<=d+19t+120, t+25<=d+5v, 3d+100v<=53t+600, t<=5v, 2v<=t+10,
//    0<=v+3}

               while (rand_b()) {

//  P(d,t,v,x) {x==5, d<=t, t+25<=d+5v, t<=5v, 2v<=t+10}

                  if (!(d<=8))

//  P(d,t,v,x) {x==5, 9<=d, d<=t, t+25<=d+5v, t<=5v, 2v<=t+10}

                     deadlock();

//  P(d,t,v,x) {x==5, d<=8, d<=t, t+25<=d+5v, t<=5v, 2v<=t+10}

                  d++;

//  P(d,t,v,x) {x==5, d<=9, d<=t+1, t+26<=d+5v, t<=5v, 2v<=t+10,
//    0<=v+3}

                  t++;

//  P(d,t,v,x) {x==5, d<=9, d<=t, t+25<=d+5v, 2v<=t+9, t<=5v+1,
//    0<=v+3}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

                     checking_error();
               }

//  P(d,t,v,x) {x==5, d<=t, t+25<=d+5v, t<=5v, 2v<=t+10}

               if (!(d==9))

//  P(d,t,v,x) {x==5, d<=t, t+25<=d+5v, t<=5v, 2v<=t+10}

                  deadlock();

//  P(d,t,v,x) {d==9, x==5, 9<=t, 2v<=t+10, t+16<=5v}

               d++;

//  P(d,t,v,x) {d==10, x==5, 9<=t, 2v<=t+10, t+16<=5v, 0<=v+3}

               t++;

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+9, t+15<=5v, 0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

                  checking_error();
            }
            else {

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 120v+30x<=d+69t+720,
//    20v+20x<=d+19t+180, 3d+100v+10x<=53t+540, 24d+t+205x<=125v+945,
//    10x<=d+9t+40, 3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25,
//    2<=x}

               if (!(d==9))

//  P(d,t,v,x) {0<=d+9t, d<=t, t+5x<=d+5v, 6t+1230x<=43d+750v+3800,
//    120v+30x<=d+69t+720, 20v+20x<=d+19t+180, 82x<=d+2t+50v+248,
//    3d+100v+10x<=53t+540, 4d+120v+30x<=69t+760, 4d+20v+20x<=19t+220,
//    24d+t+205x<=125v+945, 10x<=d+9t+40, d+8x<=3t+52, 2d+30x<=9t+200,
//    3d+10x<=3t+90, 2v<=t+10, t<=5v+5, t+5x<=5v+25, 2<=x}

                  deadlock();

//  P(d,t,v,x) {d==9, 9<=t, 2v<=t+10, t<=5v+5, 100v+10x<=53t+513,
//    40v+10x<=23t+243, 20v+20x<=19t+189, t+205x<=125v+729,
//    t+5x<=5v+9, 10x<=9t+49, 10x<=3t+63, 2<=x}

               d++;

//  P(d,t,v,x) {d==10, 9<=t, 2v<=t+10, t<=5v+5, 100v+10x<=53t+513,
//    40v+10x<=23t+243, 20v+20x<=19t+189, 2v<=t+2x+6,
//    t+205x<=125v+729, t+5x<=5v+9, 10x<=9t+49, 10x<=3t+63, x<=v+8,
//    2<=x}

               t++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+6, 100v+10x<=53t+460,
//    40v+10x<=23t+220, 20v+20x<=19t+170, 2v<=t+2x+5,
//    t+205x<=125v+730, t+5x<=5v+10, 10x<=9t+40, 10x<=3t+60, x<=v+8,
//    2<=x}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

                  checking_error();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+9, t<=5v+6, 100v+10x<=53t+460,
//    40v+10x<=23t+220, 20v+20x<=19t+170, 2v<=t+2x+5,
//    t+205x<=125v+730, t+5x<=5v+10, 10x<=9t+40, 10x<=3t+60, x<=v+8,
//    2<=x}

               while (rand_b()) {

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, 2v<=t+x+7,
//    t+5x<=5v+10, 2<=x}

                  if (!(x<=3))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, 2v<=t+x+7,
//    t+5x<=5v+10, 4<=x}

                     deadlock();

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, 2v<=t+x+7,
//    t+5x<=5v+10, 2<=x, x<=3}

                  x++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+135, 2v<=t+x+6,
//    t+5x<=5v+15, 3<=x, x<=4}

                  v++;

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, 25v+5x<=14t+160, 2v<=t+x+8,
//    t+5x<=5v+10, 3<=x, x<=4}

                  if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+12, t+11<=2v, 25v+5x<=14t+160,
//    2v<=t+x+8, t+5x<=5v+10, 3<=x, x<=4}

                     checking_error();
               }

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 25v+5x<=14t+130, 2v<=t+x+7,
//    t+5x<=5v+10, 2<=x}

               if (!(x==4))

//  P(d,t,v,x) {d==10, 10<=t, 2v<=t+10, 50v+5x<=28t+235,
//    25v+5x<=14t+130, 2v<=t+x+7, t+5x<=5v+10, 2<=x}

                  deadlock();

//  P(d,t,v,x) {d==10, x==4, 10<=t, 25v<=14t+110, 2v<=t+10, t+10<=5v}

               x++;

//  P(d,t,v,x) {d==10, x==5, 10<=t, 25v<=14t+110, 2v<=t+10, t+10<=5v,
//    0<=v+4}

               v++;

//  P(d,t,v,x) {d==10, x==5, 10<=t, 25v<=14t+135, 2v<=t+12, t+15<=5v,
//    0<=v+3}

               if (!(2*v<=t+10&&5*v>=t-10))

//  P(d,t,v,x) {d==10, x==5, 10<=t, 25v<=14t+135, 2v<=t+12, t+15<=5v,
//    t+11<=2v, 0<=v+3}

                  checking_error();
            }
         }

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+10, t+15<=5v}

         if (!(d==10&&x>=2))

//  P() {0==-1}

            deadlock();

//  P(d,t,v,x) {d==10, x==5, 10<=t, 2v<=t+10, t+15<=5v}

         x = 0;

//  P(d,t,v,x) {d==10, x==0, 10<=t, 2v<=t+10, t+15<=5v, 0<=v+3}

         d = 0;

//  P(d,t,v,x) {d==0, x==0, 10<=t, 2v<=t+10, t+15<=5v, 0<=v+3}

         if (!(2*v<=t+10&&5*v>=t-10))

//  P() {0==-1}

            checking_error();
      }
}
