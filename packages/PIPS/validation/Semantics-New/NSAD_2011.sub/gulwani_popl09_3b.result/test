
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T(c,n,s,x) {}

   int n, x, c, s;

//  T(n) {0<=n}

   n = rand();

//  T(c,x) {c==0, x==0, 0<=n}

   x = c = 0;

//  T(s) {c==0, s==1, x==0, 0<=n}

   s = 1;

//  T() {c==0, s==1, x==0, 0<=n}

   if (!(c<=n))

//  T() {0==-1}

      checking_error();

//  T(c,s,x) {c==x, c#init==x#init, 0<=c, c#init<=c, c<=n, 1<=s, s<=2,
//    s#init<=s}

   while (rand_b())

//  T(c,s,x) {c+s==c#init+s#init+1, c+x#init==c#init+x, c<=c#init+1,
//    c#init+2<=c+s, c<=n, 0<=n, s<=2}

      if (rand_b()) {

//  T() {s==1, 0<=n, x+1<=n}

         if (!(s==1&&x<n))

//  T() {0==-1}

            deadlock();

//  T(x) {s==1, x==x#init+1, 0<=n, x<=n}

         x++;

//  T(c) {c==c#init+1, s==1, 0<=n, x<=n}

         c++;

//  T() {s==1, c<=n, 0<=n, x<=n}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {s==1, 0<=n}

         if (!(s==1))

//  T() {0==-1}

            deadlock();

//  T(s) {s==2, s#init==1, 0<=n}

         s = 2;

//  T() {s==2, c<=n, 0<=n}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {s==2, 0<=n, x+1<=n}

         if (!(s==2&&x<n))

//  T() {0==-1}

            deadlock();

//  T(x) {s==2, x==x#init+1, 0<=n, x<=n}

         x++;

//  T(c) {c==c#init+1, s==2, 0<=n, x<=n}

         c++;

//  T() {s==2, c<=n, 0<=n, x<=n}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T(c,n,s,x) {}

   int n, x, c, s;

//  T(n) {0<=n}

   n = rand();

//  T(c,x) {c==0, x==0, 0<=n}

   x = c = 0;

//  T(s) {c==0, s==1, x==0, 0<=n}

   s = 1;

//  T() {c==0, s==1, x==0, 0<=n}

   if (!(c<=n))

//  T() {0==-1}

      checking_error();

//  T(c,s,x) {c==n, c==x, c#init==0, s==2, s#init==1, x#init==0, 0<=c}

   if (x>=n) {

//  T() {c==0, n==0, s==1, x==0}

      if (!(s==1))

//  T() {0==-1}

         deadlock();

//  T(s) {c==0, n==0, s==2, s#init==1, x==0}

      s = 2;

//  T() {c==0, n==0, s==2, x==0}

      if (!(c<=n))

//  T() {0==-1}

         checking_error();
   }
   else {

//  T(c,x) {c==x, c#init==x#init, s==1, 0<=c, c+1<=n, x#init<=c}

      while (rand_b()) {

//  T() {s==1, 1<=n, x+2<=n}

         if (!(s==1&&x<n-1))

//  T() {0==-1}

            deadlock();

//  T(x) {s==1, x==x#init+1, 1<=n, x+1<=n}

         x++;

//  T(c) {c==c#init+1, s==1, 1<=n, x+1<=n}

         c++;

//  T() {s==1, c<=n, 1<=n, x+1<=n}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }

//  T(c,s,x) {c==n, c==x, c#init==x#init, s==2, s#init==1,
//    c#init+1<=c, 0<=c#init}

      if (rand_b()) {

//  T() {c==n-1, c==x, s==1, 0<=c}

         if (!(s==1&&x==n-1))

//  T() {0==-1}

            deadlock();

//  T(x) {c==n-1, c==x-1, c==x#init, s==1, 0<=c}

         x++;

//  T(c) {c==c#init+1, c==n, c==x, s==1, 1<=c}

         c++;

//  T() {c==n, c==x, s==1, 1<=c}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();

//  T() {c==n, c==x, s==1, 1<=c}

         if (!(s==1))

//  T() {0==-1}

            deadlock();

//  T(s) {c==n, c==x, s==2, s#init==1, 1<=c}

         s = 2;

//  T() {c==n, c==x, s==2, 1<=c}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {c==x, s==1, 0<=c, 1<=n}

         if (!(s==1))

//  T() {0==-1}

            deadlock();

//  T(s) {c==x, s==2, s#init==1, 0<=c, 1<=n}

         s = 2;

//  T() {c==x, s==2, 0<=c, c<=n, 1<=n}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();

//  T(c,x) {c==x, c#init==x#init, s==2, 0<=c, c<=n, x#init<=c, 1<=n}

         while (rand_b()) {

//  T() {s==2, 1<=n, x+2<=n}

            if (!(s==2&&x<n-1))

//  T() {0==-1}

               deadlock();

//  T(x) {s==2, x==x#init+1, 1<=n, x+1<=n}

            x++;

//  T(c) {c==c#init+1, s==2, 1<=n, x+1<=n}

            c++;

//  T() {s==2, c<=n, 1<=n, x+1<=n}

            if (!(c<=n))

//  T() {0==-1}

               checking_error();
         }

//  T() {c==n-1, c==x, s==2, 0<=c}

         if (!(s==2&&x==n-1))

//  T() {0==-1}

            deadlock();

//  T(x) {c==n-1, c==x-1, c==x#init, s==2, 0<=c}

         x++;

//  T(c) {c==c#init+1, c==n, c==x, s==2, 1<=c}

         c++;

//  T() {c==n, c==x, s==2, 1<=c}

         if (!(c<=n))

//  T() {0==-1}

            checking_error();
      }
   }
}

PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int n, x, c, s;

//  P(c,n,s,x) {}

   n = rand();

//  P(c,n,s,x) {0<=n}

   x = c = 0;

//  P(c,n,s,x) {c==0, x==0, 0<=n}

   s = 1;

//  P(c,n,s,x) {c==0, s==1, x==0, 0<=n}

   if (!(c<=n))

//  P() {0==-1}

      checking_error();

//  P(c,n,s,x) {c==0, s==1, x==0, 0<=n}

   while (rand_b())

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

      if (rand_b()) {

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

         if (!(s==1&&x<n))

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

            deadlock();

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

         x++;

//  P(c,n,s,x) {c==x-1, s==1, 0<=c, c+1<=n, 0<=n}

         c++;

//  P(c,n,s,x) {c==x, s==1, 1<=c, c<=n, 0<=n}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

         if (!(s==1))

//  P(c,n,s,x) {c==x, s==2, 0<=c, c<=n}

            deadlock();

//  P(c,n,s,x) {c==x, s==1, 0<=c, c<=n}

         s = 2;

//  P(c,n,s,x) {c==x, s==2, 0<=c, c<=n, 0<=n}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

         if (!(s==2&&x<n))

//  P(c,n,s,x) {c==x, 0<=c, c<=n, 1<=s, s<=2}

            deadlock();

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+1<=n}

         x++;

//  P(c,n,s,x) {c==x-1, s==2, 0<=c, c+1<=n, 0<=n}

         c++;

//  P(c,n,s,x) {c==x, s==2, 1<=c, c<=n, 0<=n}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int n, x, c, s;

//  P(c,n,s,x) {}

   n = rand();

//  P(c,n,s,x) {0<=n}

   x = c = 0;

//  P(c,n,s,x) {c==0, x==0, 0<=n}

   s = 1;

//  P(c,n,s,x) {c==0, s==1, x==0, 0<=n}

   if (!(c<=n))

//  P() {0==-1}

      checking_error();

//  P(c,n,s,x) {c==0, s==1, x==0, 0<=n}

   if (x>=n) {

//  P(c,n,s,x) {c==0, n==0, s==1, x==0}

      if (!(s==1))

//  P() {0==-1}

         deadlock();

//  P(c,n,s,x) {c==0, n==0, s==1, x==0}

      s = 2;

//  P(c,n,s,x) {c==0, n==0, s==2, x==0}

      if (!(c<=n))

//  P() {0==-1}

         checking_error();
   }
   else {

//  P(c,n,s,x) {c==0, s==1, x==0, 1<=n}

      while (rand_b()) {

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

         if (!(s==1&&x<n-1))

//  P(c,n,s,x) {c==n-1, c==x, s==1, 0<=c}

            deadlock();

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+2<=n}

         x++;

//  P(c,n,s,x) {c==x-1, s==1, 0<=c, c+2<=n, 1<=n}

         c++;

//  P(c,n,s,x) {c==x, s==1, 1<=c, c+1<=n, 1<=n}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

      if (rand_b()) {

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

         if (!(s==1&&x==n-1))

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+2<=n}

            deadlock();

//  P(c,n,s,x) {c==n-1, c==x, s==1, 0<=c}

         x++;

//  P(c,n,s,x) {c==n-1, c==x-1, s==1, 0<=c}

         c++;

//  P(c,n,s,x) {c==n, c==x, s==1, 1<=c}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();

//  P(c,n,s,x) {c==n, c==x, s==1, 1<=c}

         if (!(s==1))

//  P() {0==-1}

            deadlock();

//  P(c,n,s,x) {c==n, c==x, s==1, 1<=c}

         s = 2;

//  P(c,n,s,x) {c==n, c==x, s==2, 1<=c}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

         if (!(s==1))

//  P() {0==-1}

            deadlock();

//  P(c,n,s,x) {c==x, s==1, 0<=c, c+1<=n}

         s = 2;

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+1<=n, 1<=n}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+1<=n, 1<=n}

         while (rand_b()) {

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+1<=n}

            if (!(s==2&&x<n-1))

//  P(c,n,s,x) {c==n-1, c==x, s==2, 0<=c}

               deadlock();

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+2<=n}

            x++;

//  P(c,n,s,x) {c==x-1, s==2, 0<=c, c+2<=n, 1<=n}

            c++;

//  P(c,n,s,x) {c==x, s==2, 1<=c, c+1<=n, 1<=n}

            if (!(c<=n))

//  P() {0==-1}

               checking_error();
         }

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+1<=n}

         if (!(s==2&&x==n-1))

//  P(c,n,s,x) {c==x, s==2, 0<=c, c+2<=n}

            deadlock();

//  P(c,n,s,x) {c==n-1, c==x, s==2, 0<=c}

         x++;

//  P(c,n,s,x) {c==n-1, c==x-1, s==2, 0<=c}

         c++;

//  P(c,n,s,x) {c==n, c==x, s==2, 1<=c}

         if (!(c<=n))

//  P() {0==-1}

            checking_error();
      }
   }
}
