
Transformers for slope11


//  T() {}

void slope11()
{

//  T(k,x,y) {}

   int x, y, k;

//  T() {0<=x+198, x<=99}

   if (!(-198<=x&&x<=99))

//  T() {0==-1}

      abort();

//  T(k) {x+y==k}


   if (-1551*x+76799*y-7525551<=0)

//  T(k) {x+y==k}

      /* Apparently, there are *three* intermediate points between
             (-198,93) and (99,99): (-197,94), (-148,95) and (50,99) */
      k = x+y;
   else

//  T(k) {x+y==k}

      k = x+y;

//  T() {}


   return;
}

