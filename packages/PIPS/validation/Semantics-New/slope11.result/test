//  P() {}

void slope11()
{

//  P() {}

   int x, y, k;

//  P(k,x,y) {}

   if (!(-198<=x&&x<=99))

//  P(k,x,y) {}

      abort();

//  P(k,x,y) {0<=x+198, x<=99}


   if (-1551*x+76799*y-7525551<=0)

//  P(k,x,y) {0<=x+198, x<=99, 99y<=2x+9701, y<=x+291, 49y<=x+4803,
//    y<=99}

      /* Apparently, there are *three* intermediate points between
             (-198,93) and (99,99): (-197,94), (-148,95) and (50,99) */
      k = x+y;
   else

//  P(k,x,y) {0<=x+198, x<=99, 2x+9702<=99y}

      k = x+y;

//  P(k,x,y) {x+y==k, 0<=x+198, x<=99}


   return;
}
