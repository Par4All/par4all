
TRANSFORMERS


//  T(dowhile02) {dowhile02==0}

int dowhile02()
{

//  T(i) {}

   int i, a[10], b[10];

//  T(i) {}

   do {

//  T(i) {0<=i, i<=9}

      for(i = 0; i <= 9; i += 1)

//  T() {0<=i, i<=9}

         a[i] = i;
   }
   while (0);

//  T(dowhile02) {dowhile02==0, i==10}


   return 0;
}

PRECONDITIONS


//  P() {}

int dowhile02()
{

//  P() {}

   int i, a[10], b[10];

//  P(i) {}

   do {

//  P(i) {}

      for(i = 0; i <= 9; i += 1)

//  P(i) {0<=i, i<=9}

         a[i] = i;
   }
   while (0);

//  P(i) {i==10}


   return 0;
}
