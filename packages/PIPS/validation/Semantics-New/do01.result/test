
Source code with transformers


//  T() {}

int main()
{

//  T(i) {i==0}

   int i = 0;

//  T() {i==0}

   int a[20];

//  T(n) {i==0}

   int n;

//  T(m) {i==0}

   int m;

//  T(k) {i==0, k<=m, k<=n}

   int k = m<=n?m:n;

//  T(n) {i==0, k<=m, k<=n#init}


   scanf("%d", &n);

//  T(m) {i==0, k<=m#init}

   scanf("%d", &m);

//  T(i) {i#init==0, 0<=i}


   for(i = 0; i <= n<=m?n:m; i += 1)

//  T() {0<=i, i<=m, i<=n}

      a[i] = i;

//  T() {0<=i}

   printf("%d\n", i);
}

Source code with preconditions


//  P() {}

int main()
{

//  P() {}

   int i = 0;

//  P(i) {i==0}

   int a[20];

//  P(i) {i==0}

   int n;

//  P(i,n) {i==0}

   int m;

//  P(i,m,n) {i==0}

   int k = m<=n?m:n;

//  P(i,k,m,n) {i==0, k<=m, k<=n}


   scanf("%d", &n);

//  P(i,k,m,n) {i==0, k<=m}

   scanf("%d", &m);

//  P(i,k,m,n) {i==0}


   for(i = 0; i <= n<=m?n:m; i += 1)

//  P(i,k,m,n) {0<=i, i<=m, i<=n}

      a[i] = i;

//  P(i,k,m,n) {0<=i}

   printf("%d\n", i);
}
