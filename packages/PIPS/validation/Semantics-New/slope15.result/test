
Transformers for slope15


//  T() {}

void slope15()
{

//  T(k,x,y) {}

   int x, y, k;

//  T() {2<=x, x<=11}

   if (!(2<=x&&x<=11))

//  T() {0==-1}

      abort();

//  T(k) {x+y==k}


   if (12*y<=5*x)

//  T(k) {x+y==k}

      /* (2,0), (3,1), (5,2), (10,4), (11,4) */
      k = x+y;
   else

//  T(k) {x+y==k}

      k = x+y;

//  T() {}


   return;
}

Preconditions for slope15


//  P() {}

void slope15()
{

//  P() {}

   int x, y, k;

//  P(k,x,y) {}

   if (!(2<=x&&x<=11))

//  P(k,x,y) {}

      abort();

//  P(k,x,y) {2<=x, x<=11}


   if (12*y<=5*x)

//  P(k,x,y) {2<=x, x<=11, 5y<=2x, y+2<=x, 2y+1<=x, y<=4}

      /* (2,0), (3,1), (5,2), (10,4), (11,4) */
      k = x+y;
   else

//  P(k,x,y) {2<=x, x<=11, x<=2y+1, 2x+1<=5y}

      k = x+y;

//  P(k,x,y) {x+y==k, 2<=x, x<=11}


   return;
}
