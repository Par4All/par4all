
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(x,y) {}

   int x, y;

//  T(x) {0<=x}

   x = rand();

//  T(y) {0<=x, 0<=y}

   y = rand();

//  T() {0<=x, 0<=y}

   if (x>=5&&x<=8&&y>=0&&y<=3)

//  T(x,y) {2<=x, x<=8, x+y#init<=x#init+y, 20<=4x+3y, 7x+5y<=71,
//    5<=x#init, x#init<=8, 0<=y, y<=10, 0<=y#init, y#init<=3}


      while (1) {

//  T() {}

         if (x==1&&y==9)

//  T() {0==-1}

            checking_error();

//  T(x,y) {x+y#init<=x#init+y}

         while (flip())

//  T(x,y) {x<=6, 6<=x+x#init, x#init<=x+2, x#init+2y<=x+18,
//    x+6<=x#init+2y, 2x<=x#init+y+2, x+y#init<=x#init+y, x+y<=14,
//    y<=y#init+2}

            if (flip())

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=6, x<=y, x+y<=14, 4<=y,
//    y<=10}

               if (x>=0&&x<=4&&y>=2&&y<=8&&y>=x&&y<=10-x&&x>=2) {

//  T(x) {x==x#init+2, 4<=x, x<=6, x<=y+2, x+y<=12, 2<=y, y<=8}

                  x += 2;

//  T(y) {y==y#init+2, 4<=x, x<=6, x<=y, x+y<=14, 4<=y, y<=10}

                  y += 2;

//  T() {4<=x, x<=6, x<=y, x+y<=14, 4<=y, y<=10}

                  if (x==1&&y==9)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;
            else if (flip())

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=6, 2<=y, y<=8}

               if (x>=2&&x<=8&&y>=4&&y<=10&&x>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=6, 4<=y, y<=10}

                  x -= 2;

//  T(y) {y==y#init-2, 2<=x, x<=6, 2<=y, y<=8}

                  y -= 2;

//  T() {2<=x, x<=6, 2<=y, y<=8}

                  if (x==1&&y==9)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;
            else if (x>=2&&x<=8&&y>=0&&y<=6&&x>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=6, 0<=y, y<=6}

               x -= 2;

//  T(y) {y==y#init+2, 2<=x, x<=6, 2<=y, y<=8}

               y += 2;

//  T() {2<=x, x<=6, 2<=y, y<=8}

               if (x==1&&y==9)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;

//  T(x,y) {x==x#init-2, 0<=x, x<=1, 2<=y, y<=8, y#init<=y+2,
//    y<=y#init+2}

         if (flip())

//  T(x,y) {x==x#init-2, y==y#init-2, 0<=x, x<=1, 2<=y, y<=8}

            if (x>=2&&x<=8&&y>=4&&y<=10&&x<=3) {

//  T(x) {x==x#init-2, 0<=x, x<=1, 4<=y, y<=10}

               x -= 2;

//  T(y) {y==y#init-2, 0<=x, x<=1, 2<=y, y<=8}

               y -= 2;

//  T() {0<=x, x<=1, 2<=y, y<=8}

               if (x==1&&y==9)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;
         else if (x>=2&&x<=8&&y>=0&&y<=6&&x<=3) {

//  T(x) {x==x#init-2, 0<=x, x<=1, 0<=y, y<=6}

            x -= 2;

//  T(y) {y==y#init+2, 0<=x, x<=1, 2<=y, y<=8}

            y += 2;

//  T() {0<=x, x<=1, 2<=y, y<=8}

            if (x==1&&y==9)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=3, x<=y, x+y<=14, 4<=y,
//    y<=10}

         if (x>=0&&x<=4&&y>=2&&y<=8&&y>=x&&y<=10-x&&x<=1) {

//  T(x) {x==x#init+2, 2<=x, x<=3, x<=y+2, x+y<=12, 2<=y, y<=8}

            x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=3, x<=y, x+y<=14, 4<=y, y<=10}

            y += 2;

//  T() {2<=x, x<=3, x<=y, x+y<=14, 4<=y, y<=10}

            if (x==1&&y==9)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {0==-1}

            while (1)

//  T() {0==-1}

               ;
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y;

//  P(x,y) {}

   x = rand();

//  P(x,y) {0<=x}

   y = rand();

//  P(x,y) {0<=x, 0<=y}

   if (x>=5&&x<=8&&y>=0&&y<=3)

//  P(x,y) {5<=x, x<=8, 0<=y, y<=3}


      while (1) {

//  P(x,y) {2<=x, x<=8, 20<=4x+3y, 7x+5y<=71, 0<=y, y<=10}

         if (x==1&&y==9)

//  P() {0==-1}

            checking_error();

//  P(x,y) {2<=x, x<=8, 20<=4x+3y, 7x+5y<=71, 0<=y, y<=10}

         while (flip())

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=10}

            if (flip())

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=10}

               if (x>=0&&x<=4&&y>=2&&y<=8&&y>=x&&y<=10-x&&x>=2) {

//  P(x,y) {2<=x, x<=4, 10<=2x+3y, x<=y, x+y<=10, 5x+2y<=46, 2<=y,
//    y<=8}

                  x += 2;

//  P(x,y) {4<=x, x<=6, 14<=2x+3y, x<=y+2, x+y<=12, 5x+2y<=56, 2<=y,
//    y<=8}

                  y += 2;

//  P(x,y) {4<=x, x<=6, 20<=2x+3y, x<=y, x+y<=14, 5x+2y<=60, 4<=y,
//    y<=10}

                  if (x==1&&y==9)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=8, 101<=37x+3y, 10<=2x+3y, x+y<=14, 5x+2y<=46,
//    0<=y, y<=10}

                  while (1)

//  P(x,y) {2<=x, x<=8, 101<=37x+3y, 10<=2x+3y, x+y<=14, 5x+2y<=46,
//    0<=y, y<=10}

                     ;
            else if (flip())

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=10}

               if (x>=2&&x<=8&&y>=4&&y<=10&&x>=4) {

//  P(x,y) {4<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 4<=y, y<=10}

                  x -= 2;

//  P(x,y) {2<=x, x<=6, 6<=2x+3y, x+y<=12, 5x+2y<=36, 4<=y, y<=10}

                  y -= 2;

//  P(x,y) {2<=x, x<=6, 0<=2x+3y, x+y<=10, 5x+2y<=32, 2<=y, y<=8}

                  if (x==1&&y==9)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 7x+5y<=71,
//    0<=y, y<=10}

                  while (1)

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 7x+5y<=71,
//    0<=y, y<=10}

                     ;
            else if (x>=2&&x<=8&&y>=0&&y<=6&&x>=4) {

//  P(x,y) {4<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=6}

               x -= 2;

//  P(x,y) {2<=x, x<=6, 6<=2x+3y, x+y<=12, 5x+2y<=36, 0<=y, y<=6}

               y += 2;

//  P(x,y) {2<=x, x<=6, 12<=2x+3y, x+y<=14, 5x+2y<=40, 2<=y, y<=8}

               if (x==1&&y==9)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x<=3y+11, 5x+2y<=46,
//    0<=y, y<=10}

               while (1)

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x<=3y+11, 5x+2y<=46,
//    0<=y, y<=10}

                  ;

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=10}

         if (flip())

//  P(x,y) {2<=x, x<=8, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=10}

            if (x>=2&&x<=8&&y>=4&&y<=10&&x<=3) {

//  P(x,y) {2<=x, x<=3, 10<=2x+3y, x+y<=14, 5x+2y<=46, 4<=y, y<=10}

               x -= 2;

//  P(x,y) {0<=x, x<=1, 6<=2x+3y, x+y<=12, 5x+2y<=36, 4<=y, y<=10}

               y -= 2;

//  P(x,y) {0<=x, x<=1, 0<=2x+3y, x+y<=10, 5x+2y<=32, 2<=y, y<=8}

               if (x==1&&y==9)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {2<=x, x<=8, 2y+8<=7x, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y,
//    y<=10}

               while (1)

//  P(x,y) {2<=x, x<=8, 2y+8<=7x, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y,
//    y<=10}

                  ;
         else if (x>=2&&x<=8&&y>=0&&y<=6&&x<=3) {

//  P(x,y) {2<=x, x<=3, 10<=2x+3y, x+y<=14, 5x+2y<=46, 0<=y, y<=6}

            x -= 2;

//  P(x,y) {0<=x, x<=1, 6<=2x+3y, x+y<=12, 5x+2y<=36, 0<=y, y<=6}

            y += 2;

//  P(x,y) {0<=x, x<=1, 12<=2x+3y, x+y<=14, 5x+2y<=40, 2<=y, y<=8}

            if (x==1&&y==9)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,y) {2<=x, x<=8, 80<=19x+6y, 10<=2x+3y, x+y<=14, 5x+2y<=46,
//    0<=y, y<=10}

            while (1)

//  P(x,y) {2<=x, x<=8, 80<=19x+6y, 10<=2x+3y, x+y<=14, 5x+2y<=46,
//    0<=y, y<=10}

               ;

//  P(x,y) {0<=x, x<=1, 2<=y, y<=8}

         if (x>=0&&x<=4&&y>=2&&y<=8&&y>=x&&y<=10-x&&x<=1) {

//  P(x,y) {0<=x, x<=1, x<=y, x+y<=10, 2<=y, y<=8}

            x += 2;

//  P(x,y) {2<=x, x<=3, x<=y+2, x+y<=12, 2<=y, y<=8}

            y += 2;

//  P(x,y) {2<=x, x<=3, x<=y, x+y<=14, 4<=y, y<=10}

            if (x==1&&y==9)

//  P() {0==-1}

               checking_error();
         }
         else

//  P() {0==-1}

            while (1)

//  P() {0==-1}

               ;
      }
}
