
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(x,y) {}

   int x, y;

//  T(x) {0<=x}

   x = rand();

//  T(y) {0<=x, 0<=y}

   y = rand();

//  T() {0<=x, 6<=x+y, 0<=y}

   if (x>=0&&x<=5&&y>=0&&y<=5&&y<=5-x)

//  T(x,y) {x+y#init==x#init+y, y<=x+5, x<=y+5, 0<=x#init, x#init<=5,
//    x#init+y#init<=5, 0<=y#init, y#init<=5}


      while (1) {

//  T() {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

         if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

            assert_error();

//  T(x,y) {x+y#init==x#init+y, y<=x+y#init, x+y#init<=y+5,
//    x+2y#init<=y+8, 0<=y#init, y#init<=5}

         if (flip())

//  T(x,y) {x+y#init==x#init+y, 0<=x#init, x#init<=5, x#init+y<=8,
//    5y<=x#init+6y#init, y#init<=y, y<=y#init+1, 0<=y#init,
//    y#init<=5}

            if (flip()) {

//  T(x,y) {x==6, x#init==5, y==y#init+1, 1<=y, y<=3}

               if (x>=0&&x<=5&&y>=0&&y<=5&&x==5&&y<=2) {

//  T(x) {x==6, x#init==5, 0<=y, y<=2}

                  x++;

//  T(y) {x==6, y==y#init+1, 1<=y, y<=3}

                  y++;

//  T() {x==6, 1<=y, y<=3}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {x==6, 1<=y, y<=3}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=5, x+y<=8, 3x<=y+12, 0<=y, y<=5}

                  while (1)

//  T() {0<=x, x<=5, x+y<=8, 3x<=y+12, 0<=y, y<=5}

                     ;

//  T() {x==6, 1<=y, y<=3}

               "deadlock";

//  T() {x==6, 1<=y, y<=3}

               if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();
            }
            else if (flip()) {

//  T(x,y) {x==x#init+1, y==6, y#init==5, 1<=x, x<=3}

               if (x>=0&&x<=5&&y>=0&&y<=5&&x<=2&&y==5) {

//  T(x) {x==x#init+1, y==5, 1<=x, x<=3}

                  x++;

//  T(y) {y==6, y#init==5, 1<=x, x<=3}

                  y++;

//  T() {y==6, 1<=x, x<=3}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {y==6, 1<=x, x<=3}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=5, 3y<=x+12, x+y<=8, 0<=y, y<=5}

                  while (1)

//  T() {0<=x, x<=5, 3y<=x+12, x+y<=8, 0<=y, y<=5}

                     ;

//  T() {y==6, 1<=x, x<=3}

               "deadlock";

//  T() {y==6, 1<=x, x<=3}

               if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();
            }
         else if (flip())

//  T(x,y) {x+y#init==x#init+y, 0<=x, y<=x+5, x<=y+5, 0<=y, y#init<=y}

            while (flip())

//  T(x,y) {x+y#init==x#init+y, y#init<=y, y<=y#init+1}

               if (flip()) {

//  T(x,y) {x==x#init+1, y==y#init+1, 1<=x, x<=3, 1<=y, y<=5}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x<=2&&y<=4) {

//  T(x) {x==x#init+1, 1<=x, x<=3, 0<=y, y<=4}

                     x++;

//  T(y) {y==y#init+1, 1<=x, x<=3, 1<=y, y<=5}

                     y++;

//  T() {1<=x, x<=3, 1<=y, y<=5}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {1<=x, x<=3, 1<=y, y<=5}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {1<=x, x<=3, 1<=y, y<=5}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();
               }
               else if (flip()) {

//  T(x,y) {x==x#init+1, y==y#init+1, 1<=x, x<=5, 1<=y, y<=3}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x<=4&&y<=2) {

//  T(x) {x==x#init+1, 1<=x, x<=5, 0<=y, y<=2}

                     x++;

//  T(y) {y==y#init+1, 1<=x, x<=5, 1<=y, y<=3}

                     y++;

//  T() {1<=x, x<=5, 1<=y, y<=3}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {1<=x, x<=5, 1<=y, y<=3}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {1<=x, x<=5, 1<=y, y<=3}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();
               }
         else if (flip()) {

//  T(x,y) {x==x#init+1, y==y#init+1, 4<=x, x<=5, x+y<=9, 4<=y, y<=5}

            if (x>=0&&x<=5&&y>=0&&y<=5&&x>=3&&x<=4&&y>=3&&y<=4&&y<=7-x) {

//  T(x) {x==x#init+1, 4<=x, x<=5, x+y<=8, 3<=y, y<=4}

               x++;

//  T(y) {y==y#init+1, 4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               y++;

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (x==2&&y==7)

//  T() {0==-1}

                  checking_error();

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (x==5&&y==7)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

               while (1)

//  T() {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

                  ;

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

            if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

               assert_error();

//  T(x,y) {x+y#init==x#init+y, 4<=x, x<=5, 4<=y, y<=5}

            while (flip())

//  T(x,y) {x+x#init==9, x==y, x+y#init==9, 4<=x, x<=5}

               if (flip()) {

//  T(x,y) {x==5, x#init==4, y==5, y#init==4}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x==4&&y==4) {

//  T(x) {x==5, x#init==4, y==4}

                     x++;

//  T(y) {x==5, y==5, y#init==4}

                     y++;

//  T() {x==5, y==5}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {x==5, y==5}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {x==5, y==5}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();
               }
               else {

//  T(x,y) {x==4, x#init==5, y==4, y#init==5}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x==5&&y==5) {

//  T(x) {x==4, x#init==5, y==5}

                     x--;

//  T(y) {x==4, y==4, y#init==5}

                     y--;

//  T() {x==4, y==4}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {x==4, y==4}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {x==4, y==4}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();
               }

//  T(x,y) {x==x#init-1, y==y#init-1, 3<=x, x<=8, y<=x+1, x<=y+1,
//    x+y<=7, 3<=y, y<=8}

            if (x>=4&&x<=9&&y>=4&&y<=9&&y<=9-x) {

//  T(x) {x==x#init-1, 3<=x, x<=8, y<=x+2, x<=y, x+y<=8, 4<=y, y<=9}

               x--;

//  T(y) {y==y#init-1, 3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

               y--;

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

               if (x==2&&y==7)

//  T() {0==-1}

                  checking_error();

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

               if (x==5&&y==7)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {y<=x+1, x<=y+1}

               while (1)

//  T() {y<=x+1, x<=y+1}

                  ;

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

            if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

               assert_error();
         }
         else {

//  T(x,y) {x+y#init==x#init+y, 3<=x#init, y#init+28<=3x#init+4y,
//    x#init+y<=y#init+6, 3x#init+2y+y#init<=26, y<=6, 3<=y#init}

            if (flip())

//  T(x,y) {x==x#init+1, x+y==10, x+y#init==9, 4<=x, x<=6}

               if (flip())

//  T(x,y) {x==4, x#init==3, y==6, y#init==5}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x==3&&y==5) {

//  T(x) {x==4, x#init==3, y==5}

                     x++;

//  T(y) {x==4, y==6, y#init==5}

                     y++;

//  T() {x==4, y==6}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {x==4, y==6}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {0<=x, x<=5, x+y<=8, x+2y<=12, 0<=y, y<=5}

                     while (1)

//  T() {0<=x, x<=5, x+y<=8, x+2y<=12, 0<=y, y<=5}

                        ;
               else if (x>=0&&x<=5&&y>=0&&y<=5&&x==5&&y==3) {

//  T(x) {x==6, x#init==5, y==3}

                  x++;

//  T(y) {x==6, y==4, y#init==3}

                  y++;

//  T() {x==6, y==4}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {x==6, y==4}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=5, x+y<=8, 2x+y<=12, 0<=y, y<=5}

                  while (1)

//  T() {0<=x, x<=5, x+y<=8, 2x+y<=12, 0<=y, y<=5}

                     ;
            else {

//  T(x,y) {x==x#init+1, y==y#init+1, 4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (x>=0&&x<=5&&y>=0&&y<=5&&x>=3&&x<=4&&y>=3&&y<=4&&y<=7-x) {

//  T(x) {x==x#init+1, 4<=x, x<=5, x+y<=8, 3<=y, y<=4}

                  x++;

//  T(y) {y==y#init+1, 4<=x, x<=5, x+y<=9, 4<=y, y<=5}

                  y++;

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

                  while (1)

//  T() {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

                     ;

//  T() {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();

//  T(x,y) {x+y#init==x#init+y, 4<=x, x<=5, 4<=y, y<=5}

               while (flip())

//  T(x,y) {x+x#init==9, x==y, x+y#init==9, 4<=x, x<=5}

                  if (flip()) {

//  T(x,y) {x==5, x#init==4, y==5, y#init==4}

                     if (x>=0&&x<=5&&y>=0&&y<=5&&x==4&&y==4) {

//  T(x) {x==5, x#init==4, y==4}

                        x++;

//  T(y) {x==5, y==5, y#init==4}

                        y++;

//  T() {x==5, y==5}

                        if (x==2&&y==7)

//  T() {0==-1}

                           checking_error();

//  T() {x==5, y==5}

                        if (x==5&&y==7)

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {}

                        while (1)

//  T() {}

                           ;

//  T() {x==5, y==5}

                     if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                        assert_error();
                  }
                  else {

//  T(x,y) {x==4, x#init==5, y==4, y#init==5}

                     if (x>=4&&x<=9&&y>=4&&y<=9&&x==5&&y==5) {

//  T(x) {x==4, x#init==5, y==5}

                        x--;

//  T(y) {x==4, y==4, y#init==5}

                        y--;

//  T() {x==4, y==4}

                        if (x==2&&y==7)

//  T() {0==-1}

                           checking_error();

//  T() {x==4, y==4}

                        if (x==5&&y==7)

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {}

                        while (1)

//  T() {}

                           ;

//  T() {x==4, y==4}

                     if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                        assert_error();
                  }

//  T(x,y) {x==x#init+1, y==y#init+1, 1<=x, x<=6, 11<=x+y, y<=x+1,
//    x<=y+1, 1<=y, y<=6}

               if (x>=0&&x<=5&&y>=0&&y<=5&&y>=9-x) {

//  T(x) {x==x#init+1, 1<=x, x<=6, 10<=x+y, y<=x, x<=y+2, 0<=y, y<=5}

                  x++;

//  T(y) {y==y#init+1, 1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y,
//    y<=6}

                  y++;

//  T() {1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y, y<=6}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y, y<=6}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {y<=x+1, x<=y+1}

                  while (1)

//  T() {y<=x+1, x<=y+1}

                     ;

//  T() {4<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 4<=y, y<=6}

               if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();
            }

//  T(x,y) {x+y#init==x#init+y, x<=6, y<=x+2, x<=y+2, y<=6, y<=y#init}

            while (flip())

//  T(x,y) {x+y#init==x#init+y, y<=y#init}

               if (flip())

//  T(x,y) {x+y#init==x#init+y, x<=x#init}

                  while (flip())

//  T(x,y) {x==x#init-1, y==y#init-1, 4<=x, x<=8, 10<=x+y, 4<=y, y<=8}

                     if (flip()) {

//  T(x,y) {x==x#init-1, y==y#init-1, 4<=x, x<=8, 6<=y, y<=8}

                        if (x>=4&&x<=9&&y>=4&&y<=9&&x>=5&&y>=7) {

//  T(x) {x==x#init-1, 4<=x, x<=8, 7<=y, y<=9}

                           x--;

//  T(y) {y==y#init-1, 4<=x, x<=8, 6<=y, y<=8}

                           y--;

//  T() {4<=x, x<=8, 6<=y, y<=8}

                           if (x==2&&y==7)

//  T() {0==-1}

                              checking_error();

//  T() {4<=x, x<=8, 6<=y, y<=8}

                           if (x==5&&y==7)

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {}

                           while (1)

//  T() {}

                              ;

//  T() {4<=x, x<=8, 6<=y, y<=8}

                        if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                           assert_error();
                     }
                     else {

//  T(x,y) {x==x#init-1, y==y#init-1, 6<=x, x<=8, 4<=y, y<=8}

                        if (x>=4&&x<=9&&y>=4&&y<=9&&x>=7&&y>=5) {

//  T(x) {x==x#init-1, 6<=x, x<=8, 5<=y, y<=9}

                           x--;

//  T(y) {y==y#init-1, 6<=x, x<=8, 4<=y, y<=8}

                           y--;

//  T() {6<=x, x<=8, 4<=y, y<=8}

                           if (x==2&&y==7)

//  T() {0==-1}

                              checking_error();

//  T() {6<=x, x<=8, 4<=y, y<=8}

                           if (x==5&&y==7)

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {}

                           while (1)

//  T() {}

                              ;

//  T() {6<=x, x<=8, 4<=y, y<=8}

                        if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                           assert_error();
                     }
               else {

//  T(x,y) {x==x#init-1, y==y#init-1, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x>=5&&x<=6&&y>=5&&y<=6&&y>=11-x) {

//  T(x) {x==x#init-1, 4<=x, x<=5, 10<=x+y, 5<=y, y<=6}

                     x--;

//  T(y) {y==y#init-1, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     y--;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();

//  T(x,y) {x+y#init==x#init+y, 4<=x, x<=5, 4<=y, y<=5}

                  while (flip())

//  T(x,y) {x+x#init==9, x==y, x+y#init==9, 4<=x, x<=5}

                     if (flip()) {

//  T(x,y) {x==5, x#init==4, y==5, y#init==4}

                        if (x>=0&&x<=5&&y>=0&&y<=5&&x==4&&y==4) {

//  T(x) {x==5, x#init==4, y==4}

                           x++;

//  T(y) {x==5, y==5, y#init==4}

                           y++;

//  T() {x==5, y==5}

                           if (x==2&&y==7)

//  T() {0==-1}

                              checking_error();

//  T() {x==5, y==5}

                           if (x==5&&y==7)

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {}

                           while (1)

//  T() {}

                              ;

//  T() {x==5, y==5}

                        if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                           assert_error();
                     }
                     else {

//  T(x,y) {x==4, x#init==5, y==4, y#init==5}

                        if (x>=4&&x<=9&&y>=4&&y<=9&&x==5&&y==5) {

//  T(x) {x==4, x#init==5, y==5}

                           x--;

//  T(y) {x==4, y==4, y#init==5}

                           y--;

//  T() {x==4, y==4}

                           if (x==2&&y==7)

//  T() {0==-1}

                              checking_error();

//  T() {x==4, y==4}

                           if (x==5&&y==7)

//  T() {0==-1}

                              checking_error();
                        }
                        else

//  T() {}

                           while (1)

//  T() {}

                              ;

//  T() {x==4, y==4}

                        if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                           assert_error();
                     }

//  T(x,y) {x==x#init+1, y==y#init+1, 1<=x, x<=6, 11<=x+y, y<=x+1,
//    x<=y+1, 1<=y, y<=6}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&y>=9-x) {

//  T(x) {x==x#init+1, 1<=x, x<=6, 10<=x+y, y<=x, x<=y+2, 0<=y, y<=5}

                     x++;

//  T(y) {y==y#init+1, 1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y,
//    y<=6}

                     y++;

//  T() {1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y, y<=6}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {1<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 1<=y, y<=6}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {y<=x+1, x<=y+1}

                     while (1)

//  T() {y<=x+1, x<=y+1}

                        ;

//  T() {4<=x, x<=6, 11<=x+y, y<=x+1, x<=y+1, 4<=y, y<=6}

                  if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();
               }

//  T(x,y) {x+y#init==x#init+y, x<=x#init, x#init+y<=2x+3,
//    x#init+y<=x+6, y<=x+2, x<=y+2, x#init<=6, x#init<=y+3}

            if (flip())

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

               if (flip()) {

//  T() {0==-1}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x==4&&y>=7) {

//  T() {0==-1}

                     x--;

//  T() {0==-1}

                     y--;

//  T() {0==-1}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {0==-1}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                     while (1)

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                        ;

//  T() {0==-1}

                  "deadlock";

//  T() {0==-1}

                  if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
               }
               else if (flip()) {

//  T() {0==-1}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x>=7&&y==4) {

//  T() {0==-1}

                     x--;

//  T() {0==-1}

                     y--;

//  T() {0==-1}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {0==-1}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                     while (1)

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                        ;

//  T() {0==-1}

                  "deadlock";

//  T() {0==-1}

                  if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                     assert_error();

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;
               }
            else if (flip())

//  T(x,y) {x==x#init-1, x+y==8, x+y#init==9, 3<=x, x<=5}

               if (flip())

//  T(x,y) {x==3, x#init==4, y==5, y#init==6}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x==4&&y==6) {

//  T(x) {x==3, x#init==4, y==6}

                     x--;

//  T(y) {x==3, y==5, y#init==6}

                     y--;

//  T() {x==3, y==5}

                     if (x==2&&y==7)

//  T() {0==-1}

                        checking_error();

//  T() {x==3, y==5}

                     if (x==5&&y==7)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {x<=6, y<=x+2, 2y<=x+7, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                     while (1)

//  T() {x<=6, y<=x+2, 2y<=x+7, 3y<=x+14, x<=y+2, 3x<=y+14, y<=6}

                        ;
               else if (x>=4&&x<=9&&y>=4&&y<=9&&x==6&&y==4) {

//  T(x) {x==5, x#init==6, y==4}

                  x--;

//  T(y) {x==5, y==3, y#init==4}

                  y--;

//  T() {x==5, y==3}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {x==5, y==3}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 2x<=y+7, 3x<=y+14, y<=6}

                  while (1)

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, 2x<=y+7, 3x<=y+14, y<=6}

                     ;
            else {

//  T(x,y) {x==x#init-1, y==y#init-1, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (x>=4&&x<=9&&y>=4&&y<=9&&x>=5&&x<=6&&y>=5&&y<=6&&y>=11-x) {

//  T(x) {x==x#init-1, 4<=x, x<=5, 10<=x+y, 5<=y, y<=6}

                  x--;

//  T(y) {y==y#init-1, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  y--;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, x+y<=10, 3x<=y+14, y<=6}

                  while (1)

//  T() {x<=6, y<=x+2, 3y<=x+14, x<=y+2, x+y<=10, 3x<=y+14, y<=6}

                     ;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();

//  T(x,y) {x+y#init==x#init+y, 4<=x, x<=5, 4<=y, y<=5}

               while (flip())

//  T(x,y) {x+x#init==9, x==y, x+y#init==9, 4<=x, x<=5}

                  if (flip()) {

//  T(x,y) {x==5, x#init==4, y==5, y#init==4}

                     if (x>=0&&x<=5&&y>=0&&y<=5&&x==4&&y==4) {

//  T(x) {x==5, x#init==4, y==4}

                        x++;

//  T(y) {x==5, y==5, y#init==4}

                        y++;

//  T() {x==5, y==5}

                        if (x==2&&y==7)

//  T() {0==-1}

                           checking_error();

//  T() {x==5, y==5}

                        if (x==5&&y==7)

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {}

                        while (1)

//  T() {}

                           ;

//  T() {x==5, y==5}

                     if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                        assert_error();
                  }
                  else {

//  T(x,y) {x==4, x#init==5, y==4, y#init==5}

                     if (x>=4&&x<=9&&y>=4&&y<=9&&x==5&&y==5) {

//  T(x) {x==4, x#init==5, y==5}

                        x--;

//  T(y) {x==4, y==4, y#init==5}

                        y--;

//  T() {x==4, y==4}

                        if (x==2&&y==7)

//  T() {0==-1}

                           checking_error();

//  T() {x==4, y==4}

                        if (x==5&&y==7)

//  T() {0==-1}

                           checking_error();
                     }
                     else

//  T() {}

                        while (1)

//  T() {}

                           ;

//  T() {x==4, y==4}

                     if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                        assert_error();
                  }

//  T(x,y) {x==x#init-1, y==y#init-1, 3<=x, x<=8, y<=x+1, x<=y+1,
//    x+y<=7, 3<=y, y<=8}

               if (x>=4&&x<=9&&y>=4&&y<=9&&y<=9-x) {

//  T(x) {x==x#init-1, 3<=x, x<=8, y<=x+2, x<=y, x+y<=8, 4<=y, y<=9}

                  x--;

//  T(y) {y==y#init-1, 3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

                  y--;

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

                  if (x==2&&y==7)

//  T() {0==-1}

                     checking_error();

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

                  if (x==5&&y==7)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {y<=x+1, x<=y+1}

                  while (1)

//  T() {y<=x+1, x<=y+1}

                     ;

//  T() {3<=x, x<=8, y<=x+1, x<=y+1, x+y<=7, 3<=y, y<=8}

               if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  T() {0==-1}

                  assert_error();
            }
         }
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y;

//  P(x,y) {}

   x = rand();

//  P(x,y) {0<=x}

   y = rand();

//  P(x,y) {0<=x, 0<=y}

   if (x>=0&&x<=5&&y>=0&&y<=5&&y<=5-x)

//  P(x,y) {0<=x, x<=5, x+y<=5, 0<=y, y<=5}


      while (1) {

//  P(x,y) {y<=x+5, x<=y+5}

         if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P(x,y) {y<=x+5, x<=y+5}

            assert_error();

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

         if (flip())

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

            if (flip()) {

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

               if (x>=0&&x<=5&&y>=0&&y<=5&&x==5&&y<=2) {

//  P(x,y) {x==5, 0<=y, y<=2}

                  x++;

//  P(x,y) {x==6, 0<=y, y<=2}

                  y++;

//  P(x,y) {x==6, 1<=y, y<=3}

                  if (x==2&&y==7)

//  P() {0==-1}

                     checking_error();

//  P(x,y) {x==6, 1<=y, y<=3}

                  if (x==5&&y==7)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=5, x+y<=8, 3x<=y+12, 0<=y, y<=5}

                  while (1)

//  P(x,y) {0<=x, x<=5, x+y<=8, 3x<=y+12, 0<=y, y<=5}

                     ;

//  P(x,y) {x==6, 1<=y, y<=3}

               "deadlock";

//  P(x,y) {x==6, 1<=y, y<=3}

               if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                  assert_error();
            }
            else if (flip()) {

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

               if (x>=0&&x<=5&&y>=0&&y<=5&&x<=2&&y==5) {

//  P(x,y) {y==5, 0<=x, x<=2}

                  x++;

//  P(x,y) {y==5, 1<=x, x<=3}

                  y++;

//  P(x,y) {y==6, 1<=x, x<=3}

                  if (x==2&&y==7)

//  P() {0==-1}

                     checking_error();

//  P(x,y) {y==6, 1<=x, x<=3}

                  if (x==5&&y==7)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=5, 3y<=x+12, x+y<=8, 0<=y, y<=5}

                  while (1)

//  P(x,y) {0<=x, x<=5, 3y<=x+12, x+y<=8, 0<=y, y<=5}

                     ;

//  P(x,y) {y==6, 1<=x, x<=3}

               "deadlock";

//  P(x,y) {y==6, 1<=x, x<=3}

               if (!(!(x>=0&&x<=5&&y>=0&&y<=5)&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                  assert_error();
            }
         else if (flip())

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

            while (flip())

//  P(x,y) {0<=x, y<=x+5, x<=y+5, 0<=y}

               if (flip()) {

//  P(x,y) {0<=x, y<=x+5, x<=y+5, 0<=y}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x<=2&&y<=4) {

//  P(x,y) {0<=x, x<=2, 0<=y, y<=4}

                     x++;

//  P(x,y) {1<=x, x<=3, 0<=y, y<=4}

                     y++;

//  P(x,y) {1<=x, x<=3, 1<=y, y<=5}

                     if (x==2&&y==7)

//  P() {0==-1}

                        checking_error();

//  P(x,y) {1<=x, x<=3, 1<=y, y<=5}

                     if (x==5&&y==7)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {0<=x, 15<=5x+3y, y<=x+5, x<=y+5, 0<=y}

                     while (1)

//  P(x,y) {0<=x, 15<=5x+3y, y<=x+5, x<=y+5, 0<=y}

                        ;

//  P(x,y) {1<=x, x<=3, 1<=y, y<=5}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                     assert_error();
               }
               else if (flip()) {

//  P(x,y) {0<=x, y<=x+5, x<=y+5, 0<=y}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x<=4&&y<=2) {

//  P(x,y) {0<=x, x<=4, 0<=y, y<=2}

                     x++;

//  P(x,y) {1<=x, x<=5, 0<=y, y<=2}

                     y++;

//  P(x,y) {1<=x, x<=5, 1<=y, y<=3}

                     if (x==2&&y==7)

//  P() {0==-1}

                        checking_error();

//  P(x,y) {1<=x, x<=5, 1<=y, y<=3}

                     if (x==5&&y==7)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {0<=x, 15<=3x+5y, y<=x+5, x<=y+5, 0<=y}

                     while (1)

//  P(x,y) {0<=x, 15<=3x+5y, y<=x+5, x<=y+5, 0<=y}

                        ;

//  P(x,y) {1<=x, x<=5, 1<=y, y<=3}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                     assert_error();
               }
         else if (flip()) {

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

            if (x>=0&&x<=5&&y>=0&&y<=5&&x>=3&&x<=4&&y>=3&&y<=4&&y<=7-x) {

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               x++;

//  P(x,y) {4<=x, x<=5, x+y<=8, 3<=y, y<=4}

               y++;

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (x==2&&y==7)

//  P() {0==-1}

                  checking_error();

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               if (x==5&&y==7)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

               while (1)

//  P(x,y) {0<=x, x<=5, x+y<=8, 0<=y, y<=5}

                  ;

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

            if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

               assert_error();

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

            while (flip())

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

               if (flip()) {

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                  if (x>=0&&x<=5&&y>=0&&y<=5&&x==4&&y==4) {

//  P(x,y) {x==4, y==4}

                     x++;

//  P(x,y) {x==5, y==4}

                     y++;

//  P(x,y) {x==5, y==5}

                     if (x==2&&y==7)

//  P() {0==-1}

                        checking_error();

//  P(x,y) {x==5, y==5}

                     if (x==5&&y==7)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     while (1)

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                        ;

//  P(x,y) {x==5, y==5}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                     assert_error();
               }
               else {

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                  if (x>=4&&x<=9&&y>=4&&y<=9&&x==5&&y==5) {

//  P(x,y) {x==5, y==5}

                     x--;

//  P(x,y) {x==4, y==5}

                     y--;

//  P(x,y) {x==4, y==4}

                     if (x==2&&y==7)

//  P() {0==-1}

                        checking_error();

//  P(x,y) {x==4, y==4}

                     if (x==5&&y==7)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

                     while (1)

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

                        ;

//  P(x,y) {x==4, y==4}

                  if (!(x>=0&&x<=5&&y>=0&&y<=5&&(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                     assert_error();
               }

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

            if (x>=4&&x<=9&&y>=4&&y<=9&&y<=9-x) {

//  P(x,y) {4<=x, x<=5, x+y<=9, 4<=y, y<=5}

               x--;

//  P(x,y) {3<=x, x<=4, x+y<=8, 4<=y, y<=5}

               y--;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               if (x==2&&y==7)

//  P() {0==-1}

                  checking_error();

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               if (x==5&&y==7)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {4<=x, x<=5, 10<=x+y, 4<=y, y<=5}

               while (1)

//  P(x,y) {4<=x, x<=5, 10<=x+y, 4<=y, y<=5}

                  ;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

            if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

