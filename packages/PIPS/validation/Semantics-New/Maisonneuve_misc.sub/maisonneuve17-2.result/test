               }
               else

//  P(x,y) {4<=x, x<=5, 10<=x+y, 4<=y, y<=5}

                  while (1)

//  P(x,y) {4<=x, x<=5, 10<=x+y, 4<=y, y<=5}

                     ;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               if (!(x>=0&&x<=5&&y>=0&&y<=5&&!(x>=4&&x<=9&&y>=4&&y<=9)))

//  P() {0==-1}

                  assert_error();
            }
         }
      }
}
