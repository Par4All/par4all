
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(x,y) {}

   int x, y;

//  T(x) {0<=x}

   x = rand();

//  T(y) {0<=x, 0<=y}

   y = rand();

//  T() {0<=x, 10<=3x+2y, 10<=2x+3y, 5<=x+y, 0<=y}

   if (x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6))

//  T(x,y) {x+y#init==x#init+y, 0<=x, x<=4, x<=x#init, x+y<=5,
//    0<=x#init, x#init<=4, x#init+y#init<=5, 0<=y, y<=4, 0<=y#init,
//    y#init<=4}


      while (1) {

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

         if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

            assert_error();

//  T(x,y) {x+y#init==x#init+y, 3x#init<=7x, x<=x#init,
//    9x#init<=5x+16, 7x#init<=7x+4y#init, 5x#init+4y#init<=5x+16,
//    x#init+3<=3x+y#init, 2x#init+3<=3x+2y#init,
//    5x#init+4y#init<=x+20}

         if (flip()) {

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9,
//    2<=y, y<=6}

            if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T(x) {x==x#init+2, 2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

               x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               y += 2;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0<=x, x<=2, 0<=y, y<=2}

               while (1)

//  T() {0<=x, x<=2, 0<=y, y<=2}

                  ;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

            if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();

//  T(x,y) {x==x#init-2, y==y#init-2, 0<=x, x<=4, 3<=x+y, x+y<=5,
//    0<=y, y<=4}

            if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T(x) {x==x#init-2, 0<=x, x<=4, 5<=x+y, x+y<=7, 2<=y, y<=6}

               x -= 2;

//  T(y) {y==y#init-2, 0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               y -= 2;

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {4<=x, x<=6, x+y<=9, 4<=y, y<=6}

               while (1)

//  T() {4<=x, x<=6, x+y<=9, 4<=y, y<=6}

                  ;

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();
         }
         else {

//  T(x,y) {x+y#init==x#init+y, 2<=x, x#init<=x, x<=x#init+2,
//    x+x#init<=6, x#init+2<=x+y#init, x+2y#init<=x#init+6,
//    x+x#init+2y#init<=10}

            if (flip()) {

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9,
//    2<=y, y<=6}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T(x) {x==x#init+2, 2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

                  x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  y += 2;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=2, 0<=y, y<=2}

                  while (1)

//  T() {0<=x, x<=2, 0<=y, y<=2}

                     ;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=4, x+y<=5, 2<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=4, x+y<=7, 4<=y, y<=6}

                  x -= 2;

//  T(y) {y==y#init-2, 2<=x, x<=4, x+y<=5, 2<=y, y<=4}

                  y -= 2;

//  T() {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  while (1)

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                     ;

//  T() {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=4, 2<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  T(x) {x==x#init+2, 2<=x, x<=4, 0<=y, y<=2}

                  x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=4, 2<=y, y<=4}

                  y += 2;

//  T() {2<=x, x<=4, 2<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  while (1)

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                     ;

//  T() {2<=x, x<=4, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }

//  T(x,y) {x==x#init, y==y#init, 2<=x, x<=4, 2<=y, y<=4}

            while (flip())

//  T(x,y) {x==x#init, y==y#init, 2<=x, x<=3, 2<=y, y<=3}

               if (flip()) {

//  T(x,y) {x==6, x#init==4, y==6, y#init==4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  T(x) {x==6, x#init==4, y==4}

                     x += 2;

//  T(y) {x==6, y==6, y#init==4}

                     y += 2;

//  T() {x==6, y==6}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {0==-1}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }
               else if (flip()) {

//  T(x,y) {x==0, x#init==2, y==0, y#init==2}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  T(x) {x==0, x#init==2, y==2}

                     x -= 2;

//  T(y) {x==0, y==0, y#init==2}

                     y -= 2;

//  T() {x==0, y==0}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {0==-1}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }
               else {

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=5, 2<=y, y<=5}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  T(x) {x==x#init+2, 2<=x, x<=5, 0<=y, y<=3}

                     x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=5, 2<=y, y<=5}

                     y += 2;

//  T() {2<=x, x<=5, 2<=y, y<=5}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {2<=x, x<=5, 7<=x+y, 2<=y, y<=5}

                  if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=3, 2<=y, y<=3}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=3, 4<=y, y<=5}

                     x -= 2;

//  T(y) {y==y#init-2, 2<=x, x<=3, 2<=y, y<=3}

                     y -= 2;

//  T() {2<=x, x<=3, 2<=y, y<=3}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {2<=x, x<=5, 7<=x+y, x+y<=8, 2<=y, y<=5}

                     while (1)

//  T() {2<=x, x<=5, 7<=x+y, x+y<=8, 2<=y, y<=5}

                        ;

//  T() {2<=x, x<=3, 2<=y, y<=3}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=2, x+y<=3, 1<=y, y<=2}

            if (flip()) {

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  T(x) {x==x#init+2, 4<=x, x<=5, 2<=y, y<=3}

                  x += 2;

//  T(y) {y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

                  y += 2;

//  T() {4<=x, x<=5, 4<=y, y<=5}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T() {0==-1}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T() {0==-1}

                  x -= 2;

//  T() {0==-1}

                  y -= 2;

//  T() {0==-1}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  while (1)

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     ;

//  T() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  T(x) {x==x#init-2, 1<=x, x<=2, 3<=y, y<=4}

                  x -= 2;

//  T(y) {y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

                  y -= 2;

//  T() {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     ;

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
         }
      }
   else if (!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6))

//  T(x,y) {x+y#init==x#init+y, 2<=x, x<=6, x#init<=x, 5<=x+y,
//    2<=x#init, x#init<=6, 5<=x#init+y#init, 2<=y, y<=6, 2<=y#init,
//    y#init<=6}


      while (1) {

//  T() {2<=x, x<=6, 2<=y, y<=6}

         if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

            assert_error();

//  T(x,y) {x+y#init==x#init+y, x#init<=x, x<=x#init+2, 3x<=x#init+12,
//    3x+2y#init<=3x#init+12, 3x+2y#init<=2x#init+15,
//    3x+y#init<=x#init+15, 7<=x+y#init, 2<=x#init, 2<=y#init}

         if (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

            if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T(x) {x==x#init-2, 0<=x, x<=4, x+y<=7, 2<=y, y<=6}

               x -= 2;

//  T(y) {y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               y -= 2;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {4<=x, x<=6, 4<=y, y<=6}

               while (1)

//  T() {4<=x, x<=6, 4<=y, y<=6}

                  ;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9,
//    2<=y, y<=6}

            if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T(x) {x==x#init+2, 2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

               x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               y += 2;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0<=x, x<=2, 0<=y, y<=2}

               while (1)

//  T() {0<=x, x<=2, 0<=y, y<=2}

                  ;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

            if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();
         }
         else {

//  T(x,y) {x+y#init==x#init+y, 2<=x, x<=4, x<=x#init, x#init<=x+2,
//    x#init+2<=x+y#init, x+y#init<=x#init+4}

            if (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T(x) {x==x#init-2, 0<=x, x<=4, x+y<=7, 2<=y, y<=6}

                  x -= 2;

//  T(y) {y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

                  y -= 2;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {4<=x, x<=6, 4<=y, y<=6}

                  while (1)

//  T() {4<=x, x<=6, 4<=y, y<=6}

                     ;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=4, 2<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  T(x) {x==x#init+2, 2<=x, x<=4, 0<=y, y<=2}

                  x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=4, 2<=y, y<=4}

                  y += 2;

//  T() {2<=x, x<=4, 2<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  while (1)

//  T() {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                     ;

//  T() {2<=x, x<=4, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=4, 2<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=4, 4<=y, y<=6}

                  x -= 2;

//  T(y) {y==y#init-2, 2<=x, x<=4, 2<=y, y<=4}

                  y -= 2;

//  T() {2<=x, x<=4, 2<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=6, x+y<=9, 2<=y, y<=6}

                  while (1)

//  T() {2<=x, x<=6, x+y<=9, 2<=y, y<=6}

                     ;

//  T() {2<=x, x<=4, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }

//  T(x,y) {x==x#init, y==y#init, 2<=x, x<=4, 2<=y, y<=4}

            while (flip())

//  T(x,y) {x==x#init, y==y#init, 3<=x, x<=4, 3<=y, y<=4}

               if (flip()) {

//  T(x,y) {x==6, x#init==4, y==6, y#init==4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  T(x) {x==6, x#init==4, y==4}

                     x += 2;

//  T(y) {x==6, y==6, y#init==4}

                     y += 2;

//  T() {x==6, y==6}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {0==-1}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }
               else if (flip()) {

//  T(x,y) {x==0, x#init==2, y==0, y#init==2}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  T(x) {x==0, x#init==2, y==2}

                     x -= 2;

//  T(y) {x==0, y==0, y#init==2}

                     y -= 2;

//  T() {x==0, y==0}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {0==-1}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }
               else {

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=4, 1<=y, y<=4}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  T(x) {x==x#init-2, 1<=x, x<=4, 3<=y, y<=6}

                     x -= 2;

//  T(y) {y==y#init-2, 1<=x, x<=4, 1<=y, y<=4}

                     y -= 2;

//  T() {1<=x, x<=4, 1<=y, y<=4}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {}

                     while (1)

//  T() {}

                        ;

//  T() {1<=x, x<=4, x+y<=5, 1<=y, y<=4}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();

//  T(x,y) {x==x#init+2, y==y#init+2, 3<=x, x<=4, 3<=y, y<=4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  T(x) {x==x#init+2, 3<=x, x<=4, 1<=y, y<=2}

                     x += 2;

//  T(y) {y==y#init+2, 3<=x, x<=4, 3<=y, y<=4}

                     y += 2;

//  T() {3<=x, x<=4, 3<=y, y<=4}

                     if (x==1&&y==5)

//  T() {0==-1}

                        checking_error();
                  }
                  else

//  T() {1<=x, x<=4, 4<=x+y, x+y<=5, 1<=y, y<=4}

                     while (1)

//  T() {1<=x, x<=4, 4<=x+y, x+y<=5, 1<=y, y<=4}

                        ;

//  T() {3<=x, x<=4, 3<=y, y<=4}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                     assert_error();
               }

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

            if (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  T(x) {x==x#init-2, 1<=x, x<=2, 3<=y, y<=4}

                  x -= 2;

//  T(y) {y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

                  y -= 2;

//  T() {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     ;

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T() {0==-1}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T() {0==-1}

                  x += 2;

//  T() {0==-1}

                  y += 2;

//  T() {0==-1}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                  while (1)

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                     ;

//  T() {0==-1}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  T(x) {x==x#init+2, 4<=x, x<=5, 2<=y, y<=3}

                  x += 2;

//  T(y) {y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

                  y += 2;

//  T() {4<=x, x<=5, 4<=y, y<=5}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
         }
      }
   else if (x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6))

//  T(x,y) {x==x#init, y==y#init, 2<=x, x<=4, 2<=y, y<=4}


      while (1) {

//  T() {2<=x, x<=4, 2<=y, y<=4}

         if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

            assert_error();

//  T(x,y) {x==x#init, y==y#init, 2<=x, x<=4, 5<=x+y, y<=x+1, x<=y+1,
//    x+y<=7, 2<=y, y<=4}

         if (flip()) {

//  T(x,y) {x==6, x#init==4, y==6, y#init==4}

            if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  T(x) {x==6, x#init==4, y==4}

               x += 2;

//  T(y) {x==6, y==6, y#init==4}

               y += 2;

//  T() {x==6, y==6}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

               while (1)

//  T() {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                  ;

//  T() {0==-1}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();
         }
         else if (flip()) {

//  T(x,y) {x==0, x#init==2, y==0, y#init==2}

            if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  T(x) {x==0, x#init==2, y==2}

               x -= 2;

//  T(y) {x==0, y==0, y#init==2}

               y -= 2;

//  T() {x==0, y==0}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

               while (1)

//  T() {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                  ;

//  T() {0==-1}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();
         }
         else if (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

            if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  T(x) {x==x#init-2, 1<=x, x<=2, 3<=y, y<=4}

               x -= 2;

//  T(y) {y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

               y -= 2;

//  T() {1<=x, x<=2, 1<=y, y<=2}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

               while (1)

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  ;

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();

//  T(x,y) {x==x#init+2, y==y#init+2, 3<=x, x<=4, x+y<=7, 3<=y, y<=4}

            if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  T(x) {x==x#init+2, 3<=x, x<=4, x+y<=5, 1<=y, y<=2}

               x += 2;

//  T(y) {y==y#init+2, 3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               y += 2;

//  T() {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               if (x==1&&y==5)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {0==-1}

               while (1)

//  T() {0==-1}

                  ;

//  T() {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

               assert_error();
         }
         else {

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

            if (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  T(x) {x==x#init-2, 1<=x, x<=2, 3<=y, y<=4}

                  x -= 2;

//  T(y) {y==y#init-2, 1<=x, x<=2, 1<=y, y<=2}

                  y -= 2;

//  T() {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     ;

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T() {0==-1}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T() {0==-1}

                  x += 2;

//  T() {0==-1}

                  y += 2;

//  T() {0==-1}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                  while (1)

//  T() {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                     ;

//  T() {0==-1}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init+2, y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  T(x) {x==x#init+2, 4<=x, x<=5, 2<=y, y<=3}

                  x += 2;

//  T(y) {y==y#init+2, 4<=x, x<=5, 4<=y, y<=5}

                  y += 2;

//  T() {4<=x, x<=5, 4<=y, y<=5}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  T() {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }

//  T(x,y) {x==x#init, y==y#init, x<=5, 9<=x+y, y<=5}

            while (flip()) {

//  T(x,y) {x==x#init-2, y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T(x) {x==x#init-2, 0<=x, x<=4, x+y<=7, 2<=y, y<=6}

                  x -= 2;

//  T(y) {y==y#init-2, 0<=x, x<=4, x+y<=5, 0<=y, y<=4}

                  y -= 2;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {}

                  while (1)

//  T() {}

                     ;

//  T() {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T(x,y) {x==x#init+2, y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9,
//    2<=y, y<=6}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  T(x) {x==x#init+2, 2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

                  x += 2;

//  T(y) {y==y#init+2, 2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  y += 2;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0<=x, x<=2, 0<=y, y<=2}

                  while (1)

//  T() {0<=x, x<=2, 0<=y, y<=2}

                     ;

//  T() {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

            if (flip()) {

//  T() {0==-1}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  T() {0==-1}

                  x -= 2;

//  T() {0==-1}

                  y -= 2;

//  T() {0==-1}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  while (1)

//  T() {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     ;

//  T() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();

//  T() {0==-1}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  T() {0==-1}

                  x += 2;

//  T() {0==-1}

                  y += 2;

//  T() {0==-1}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
            else {

//  T(x,y) {x==x#init-2, y==y#init-2, 2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  T(x) {x==x#init-2, 2<=x, x<=3, 7<=x+y, 4<=y, y<=5}

                  x -= 2;

//  T(y) {y==y#init-2, 2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

                  y -= 2;

//  T() {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

                  if (x==1&&y==5)

//  T() {0==-1}

                     checking_error();
               }
               else

//  T() {0==-1}

                  while (1)

//  T() {0==-1}

                     ;

//  T() {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  T() {0==-1}

                  assert_error();
            }
         }
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y;

//  P(x,y) {}

   x = rand();

//  P(x,y) {0<=x}

   y = rand();

//  P(x,y) {0<=x, 0<=y}

   if (x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6))

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}


      while (1) {

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

         if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

            assert_error();

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

         if (flip()) {

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

            if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               x += 2;

//  P(x,y) {2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

               y += 2;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {0<=x, x<=2, 0<=y, y<=2}

               while (1)

//  P(x,y) {0<=x, x<=2, 0<=y, y<=2}

                  ;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

            if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, 7<=x+y, 2<=y, y<=4}

               assert_error();

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

            if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               x -= 2;

//  P(x,y) {0<=x, x<=4, 5<=x+y, x+y<=7, 2<=y, y<=6}

               y -= 2;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {4<=x, x<=6, x+y<=9, 4<=y, y<=6}

               while (1)

//  P(x,y) {4<=x, x<=6, x+y<=9, 4<=y, y<=6}

                  ;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

               assert_error();
         }
         else {

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

            if (flip()) {

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  x += 2;

//  P(x,y) {2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

                  y += 2;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=2, 0<=y, y<=2}

                  while (1)

//  P(x,y) {0<=x, x<=2, 0<=y, y<=2}

                     ;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, 7<=x+y, 2<=y, y<=4}

                  assert_error();

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  P(x,y) {4<=x, x<=6, x+y<=9, 4<=y, y<=6}

                  x -= 2;

//  P(x,y) {2<=x, x<=4, x+y<=7, 4<=y, y<=6}

                  y -= 2;

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  while (1)

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                     ;

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
            else {

//  P(x,y) {0<=x, x<=4, x+y<=5, 0<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  P(x,y) {0<=x, x<=2, 0<=y, y<=2}

                  x += 2;

//  P(x,y) {2<=x, x<=4, 0<=y, y<=2}

                  y += 2;

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  while (1)

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                     ;

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            while (flip())

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  P(x,y) {x==4, y==4}

                     x += 2;

//  P(x,y) {x==6, y==4}

                     y += 2;

//  P(x,y) {x==6, y==6}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                        ;

//  P(x,y) {x==6, y==6}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==6, y==6}

                     assert_error();
               }
               else if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  P(x,y) {x==2, y==2}

                     x -= 2;

//  P(x,y) {x==0, y==2}

                     y -= 2;

//  P(x,y) {x==0, y==0}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                        ;

//  P(x,y) {x==0, y==0}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==0, y==0}

                     assert_error();
               }
               else {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  P(x,y) {2<=x, x<=3, 2<=y, y<=3}

                     x += 2;

//  P(x,y) {4<=x, x<=5, 2<=y, y<=3}

                     y += 2;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                        ;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                  if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==4, y==4}

                     assert_error();

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     x -= 2;

//  P(x,y) {2<=x, x<=3, 7<=x+y, 4<=y, y<=5}

                     y -= 2;

//  P(x,y) {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P() {0==-1}

                     while (1)

//  P() {0==-1}

                        ;

//  P(x,y) {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                     assert_error();
               }

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  P(x,y) {2<=x, x<=3, 2<=y, y<=3}

                  x += 2;

//  P(x,y) {4<=x, x<=5, 2<=y, y<=3}

                  y += 2;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==4, y==4}

                  assert_error();

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P() {0==-1}

                  x -= 2;

//  P() {0==-1}

                  y -= 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  while (1)

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     ;

//  P() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
            else {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  P(x,y) {3<=x, x<=4, 3<=y, y<=4}

                  x -= 2;

//  P(x,y) {1<=x, x<=2, 3<=y, y<=4}

                  y -= 2;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  while (1)

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     ;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==2, y==2}

                  assert_error();
            }
         }
      }
   else if (!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6))

//  P(x,y) {2<=x, x<=6, 5<=x+y, 2<=y, y<=6}


      while (1) {

//  P(x,y) {2<=x, x<=6, 5<=x+y, 2<=y, y<=6}

         if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

            assert_error();

//  P(x,y) {2<=x, x<=6, 7<=x+y, 2<=y, y<=6}

         if (flip()) {

//  P(x,y) {2<=x, x<=6, 7<=x+y, 2<=y, y<=6}

            if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               x -= 2;

//  P(x,y) {0<=x, x<=4, 5<=x+y, x+y<=7, 2<=y, y<=6}

               y -= 2;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {4<=x, x<=6, 4<=y, y<=6}

               while (1)

//  P(x,y) {4<=x, x<=6, 4<=y, y<=6}

                  ;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

               assert_error();

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

            if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               x += 2;

//  P(x,y) {2<=x, x<=6, 5<=x+y, x+y<=7, 0<=y, y<=4}

               y += 2;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {0<=x, x<=2, 3<=x+y, 0<=y, y<=2}

               while (1)

//  P(x,y) {0<=x, x<=2, 3<=x+y, 0<=y, y<=2}

                  ;

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

            if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, 7<=x+y, 2<=y, y<=4}

               assert_error();
         }
         else {

//  P(x,y) {2<=x, x<=6, 7<=x+y, 2<=y, y<=6}

            if (flip()) {

//  P(x,y) {2<=x, x<=6, 7<=x+y, 2<=y, y<=6}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  x -= 2;

//  P(x,y) {0<=x, x<=4, 5<=x+y, x+y<=7, 2<=y, y<=6}

                  y -= 2;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {4<=x, x<=6, 4<=y, y<=6}

                  while (1)

//  P(x,y) {4<=x, x<=6, 4<=y, y<=6}

                     ;

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {2<=x, x<=4, x+y<=5, 2<=y, y<=4}

                  assert_error();

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  P(x,y) {0<=x, x<=2, 3<=x+y, 0<=y, y<=2}

                  x += 2;

//  P(x,y) {2<=x, x<=4, 5<=x+y, 0<=y, y<=2}

                  y += 2;

//  P(x,y) {2<=x, x<=4, 7<=x+y, 2<=y, y<=4}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                  while (1)

//  P(x,y) {0<=x, x<=4, 3<=x+y, x+y<=5, 0<=y, y<=4}

                     ;

//  P(x,y) {2<=x, x<=4, 7<=x+y, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
            else {

//  P(x,y) {2<=x, x<=6, 7<=x+y, 2<=y, y<=6}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  P(x,y) {4<=x, x<=6, 4<=y, y<=6}

                  x -= 2;

//  P(x,y) {2<=x, x<=4, 4<=y, y<=6}

                  y -= 2;

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                  while (1)

//  P(x,y) {2<=x, x<=6, 7<=x+y, x+y<=9, 2<=y, y<=6}

                     ;

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            while (flip())

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  P(x,y) {x==4, y==4}

                     x += 2;

//  P(x,y) {x==6, y==4}

                     y += 2;

//  P(x,y) {x==6, y==6}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                        ;

//  P(x,y) {x==6, y==6}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==6, y==6}

                     assert_error();
               }
               else if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  P(x,y) {x==2, y==2}

                     x -= 2;

//  P(x,y) {x==0, y==2}

                     y -= 2;

//  P(x,y) {x==0, y==0}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                        ;

//  P(x,y) {x==0, y==0}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==0, y==0}

                     assert_error();
               }
               else {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

                  if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  P(x,y) {3<=x, x<=4, 3<=y, y<=4}

                     x -= 2;

//  P(x,y) {1<=x, x<=2, 3<=y, y<=4}

                     y -= 2;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     while (1)

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                        ;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==2, y==2}

                     assert_error();

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                  if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                     x += 2;

//  P(x,y) {3<=x, x<=4, x+y<=5, 1<=y, y<=2}

                     y += 2;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

                     if (x==1&&y==5)

//  P() {0==-1}

                        checking_error();
                  }
                  else

//  P() {0==-1}

                     while (1)

//  P() {0==-1}

                        ;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

                  if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                     assert_error();
               }

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  P(x,y) {3<=x, x<=4, 3<=y, y<=4}

                  x -= 2;

//  P(x,y) {1<=x, x<=2, 3<=y, y<=4}

                  y -= 2;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

