                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==4, y==4}

                  assert_error();
            }
         }
      }
   else if (x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6))

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}


      while (1) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

         if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

            assert_error();

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

         if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (x>=0&&x<=4&&y>=0&&y<=4&&x==4&&y==4) {

//  P(x,y) {x==4, y==4}

               x += 2;

//  P(x,y) {x==6, y==4}

               y += 2;

//  P(x,y) {x==6, y==6}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

               while (1)

//  P(x,y) {2<=x, x<=4, x+y<=7, 2<=y, y<=4}

                  ;

//  P(x,y) {x==6, y==6}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==6, y==6}

               assert_error();
         }
         else if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (x>=2&&x<=6&&y>=2&&y<=6&&x==2&&y==2) {

//  P(x,y) {x==2, y==2}

               x -= 2;

//  P(x,y) {x==0, y==2}

               y -= 2;

//  P(x,y) {x==0, y==0}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

               while (1)

//  P(x,y) {2<=x, x<=4, 5<=x+y, 2<=y, y<=4}

                  ;

//  P(x,y) {x==0, y==0}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==0, y==0}

               assert_error();
         }
         else if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  P(x,y) {3<=x, x<=4, 3<=y, y<=4}

               x -= 2;

//  P(x,y) {1<=x, x<=2, 3<=y, y<=4}

               y -= 2;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

               while (1)

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  ;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==2, y==2}

               assert_error();

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

            if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

               x += 2;

//  P(x,y) {3<=x, x<=4, x+y<=5, 1<=y, y<=2}

               y += 2;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

               if (x==1&&y==5)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P() {0==-1}

               while (1)

//  P() {0==-1}

                  ;

//  P(x,y) {3<=x, x<=4, x+y<=7, 3<=y, y<=4}

            if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

               assert_error();
         }
         else {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

            if (flip()) {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>2&&y>2) {

//  P(x,y) {3<=x, x<=4, 3<=y, y<=4}

                  x -= 2;

//  P(x,y) {1<=x, x<=2, 3<=y, y<=4}

                  y -= 2;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                  while (1)

//  P(x,y) {2<=x, x<=4, x+y<=6, 2<=y, y<=4}

                     ;

//  P(x,y) {1<=x, x<=2, 1<=y, y<=2}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==2, y==2}

                  assert_error();

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  P() {0==-1}

                  x += 2;

//  P() {0==-1}

                  y += 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                  while (1)

//  P(x,y) {1<=x, x<=2, x+y<=3, 1<=y, y<=2}

                     ;

//  P() {0==-1}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
            else {

//  P(x,y) {2<=x, x<=4, 2<=y, y<=4}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<4&&y<4) {

//  P(x,y) {2<=x, x<=3, 2<=y, y<=3}

                  x += 2;

//  P(x,y) {4<=x, x<=5, 2<=y, y<=3}

                  y += 2;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                  while (1)

//  P(x,y) {2<=x, x<=4, 6<=x+y, 2<=y, y<=4}

                     ;

//  P(x,y) {4<=x, x<=5, 4<=y, y<=5}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P(x,y) {x==4, y==4}

                  assert_error();
            }

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

            while (flip()) {

//  P(x,y) {x<=5, 9<=x+y, y<=5}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P() {0==-1}

                  x -= 2;

//  P() {0==-1}

                  y -= 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  while (1)

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     ;

//  P() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();

//  P() {0==-1}

               if (x>=0&&x<=4&&y>=0&&y<=4&&(x>2||y>2)) {

//  P() {0==-1}

                  x += 2;

//  P() {0==-1}

                  y += 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P() {0==-1}

                  while (1)

//  P() {0==-1}

                     ;

//  P() {0==-1}

               if (!(!(x>=0&&x<=4&&y>=0&&y<=4)&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

            if (flip()) {

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (x>=2&&x<=6&&y>=2&&y<=6&&(x<4||y<4)) {

//  P() {0==-1}

                  x -= 2;

//  P() {0==-1}

                  y -= 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  while (1)

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                     ;

//  P() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&!(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();

//  P() {0==-1}

               if (x>=0&&x<=4&&y>=0&&y<=4&&x<=2&&y<=2) {

//  P() {0==-1}

                  x += 2;

//  P() {0==-1}

                  y += 2;

//  P() {0==-1}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P() {0==-1}

                  while (1)

//  P() {0==-1}

                     ;

//  P() {0==-1}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
            else {

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

               if (x>=2&&x<=6&&y>=2&&y<=6&&x>=4&&y>=4) {

//  P(x,y) {4<=x, x<=5, 9<=x+y, 4<=y, y<=5}

                  x -= 2;

//  P(x,y) {2<=x, x<=3, 7<=x+y, 4<=y, y<=5}

                  y -= 2;

//  P(x,y) {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

                  if (x==1&&y==5)

//  P() {0==-1}

                     checking_error();
               }
               else

//  P() {0==-1}

                  while (1)

//  P() {0==-1}

                     ;

//  P(x,y) {2<=x, x<=3, 5<=x+y, 2<=y, y<=3}

               if (!(x>=0&&x<=4&&y>=0&&y<=4&&(x>=2&&x<=6&&y>=2&&y<=6)))

//  P() {0==-1}

                  assert_error();
            }
         }
      }
}
