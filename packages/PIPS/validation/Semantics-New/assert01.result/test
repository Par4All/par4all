
Source code with transformers


//  T(main) {}

int main()
{

//  T(i) {}

   int i;                                                               /*0005*/

//  T(i) {2<=i, i<=3}

   i>1?(i = 2):(i = 3);                                                 /*0007*/

//  T(i) {i==2, 2<=i#init, i#init<=3}


   i = i>1?2:3;                                                         /*0009*/

//  T() {i==2}

   i>1?(void) 0:__assert_fail("i>1", "./assert01.c", 11, (const char *) 0);            /*0011*/

//  T(main) {i==2}


   return i>1;                                                          /*0013*/
}

Source code with preconditions


//  P() {}

int main()
{

//  P() {}

   int i;                                                               /*0005*/

//  P(i) {}

   i>1?(i = 2):(i = 3);                                                 /*0007*/

//  P(i) {2<=i, i<=3}


   i = i>1?2:3;                                                         /*0009*/

//  P(i) {i==2}

   i>1?(void) 0:__assert_fail("i>1", "./assert01.c", 11, (const char *) 0);            /*0011*/

//  P(i) {i==2}


   return i>1;                                                          /*0013*/
}
