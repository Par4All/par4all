//  T() {}

               if (1) {

//  T() {}

                  A[N-i-1][i+j-1] = 1.0;

//  T() {}

                  A[i-1][N-i-j-1] = 1.0;
               }

//  T() {}

            if (i==j)

//  T() {}

               A[i-1][j-1] = 1.0;
         }
   {

//  T(0`7`i) {}

      int i;

//  T(0`7`i) {0<=0`7`i, 0`7`i+1<=N}

