//  P() {}

int main()
{

//  P() {}

   int N = 100;

//  P(N) {N==100}

   double A[100][100];

//  P(N) {N==100}

   int i, j;

//  P(0`i,0`j,N) {N==100}


   for(i = 1; i <= N; i += 1)

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=100}

      for(j = 1; j <= N; j += 1)

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=100, 1<=0`j, 0`j<=100}

         //int x0 = (i*j);
         //double x1 = ((double)N / 2);
         if (1) {

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=100, 1<=0`j, 0`j<=100}

            if (2*i*j<N)

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=49, 1<=0`j, 0`j<=49}

               if (1) {

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=49, 1<=0`j, 0`j<=49}

                  A[N-i-1][i+j-1] = 1.0;

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=49, 1<=0`j, 0`j<=49}

                  A[i-1][N-i-j-1] = 1.0;
               }

//  P(0`i,0`j,N) {N==100, 1<=0`i, 0`i<=100, 1<=0`j, 0`j<=100}

            if (i==j)

//  P(0`i,0`j,N) {0`i==0`j, N==100, 1<=0`i, 0`i<=100}

               A[i-1][j-1] = 1.0;
         }
   {

//  P(0`i,0`j,N) {0`i==101, 0`j==101, N==100}

      int i;

//  P(0`7`i,0`i,0`j,N) {0`i==101, 0`j==101, N==100}

      for(i = 0; i <= N-1; i += 1) {

//  P(0`7`i,0`i,0`j,N) {0`i==101, 0`j==101, N==100, 0<=0`7`i,
//    0`7`i<=99}

         int j;

//  P(0`7`8`j,0`7`i,0`i,0`j,N) {0`i==101, 0`j==101, N==100, 0<=0`7`i,
//    0`7`i<=99}

         for(j = 0; j <= N-1; j += 1)

//  P(0`7`8`j,0`7`i,0`i,0`j,N) {0`i==101, 0`j==101, N==100,
//    0<=0`7`8`j, 0`7`8`j<=99, 0<=0`7`i, 0`7`i<=99}

            printf("%f\n", A[i][j]);
      }
   }

//  P(0`i,0`j,N) {0`i==101, 0`j==101, N==100}


   return 0;
}

