
TRANSFORMERS


//  T() {}

void run(void)
{


//  T(x,y) {}

   int x, y;

//  T(x) {0<=x}

   x = rand();

//  T(y) {0<=x, 0<=y}

   y = rand();

//  T() {0<=x, 6<=x+y, 0<=y}

   if (x>=0&&x<=5&&y>=0&&y<=5&&y<=5-x)

//  T(x,y) {x+y#init==x#init+y, 0<=x, x<=8, y<=x+5, x<=y+5, 0<=x#init,
//    x#init<=5, x#init+y#init<=5, 0<=y, y<=8, 0<=y#init, y#init<=5}


      while (1) {

//  T() {}

         if (x==2&&y==7)

//  T() {0==-1}

            checking_error();

//  T() {}

         if (x==5&&y==7)

//  T() {0==-1}

            checking_error();

//  T(x,y) {x+y#init==x#init+y, y#init+2<=x+y, x+y<=y#init+7,
//    y#init+2<=2y, y#init<=y+1, y<=y#init+1, 2y<=y#init+7}

         if (flip())

//  T(x,y) {x==x#init+1, y==y#init+1, 1<=x, x<=6, 1<=y, y<=6}

            if (x>=0&&x<=5&&y>=0&&y<=5) {

//  T(x) {x==x#init+1, 1<=x, x<=6, 0<=y, y<=5}

               x++;

//  T(y) {y==y#init+1, 1<=x, x<=6, 1<=y, y<=6}

               y++;

//  T() {1<=x, x<=6, 1<=y, y<=6}

               if (x==2&&y==7)

//  T() {0==-1}

                  checking_error();

//  T() {1<=x, x<=6, 1<=y, y<=6}

               if (x==5&&y==7)

//  T() {0==-1}

                  checking_error();
            }
            else

//  T() {}

               while (1)

//  T() {}

                  ;
         else if (x>=4&&x<=9&&y>=4&&y<=9) {

//  T(x) {x==x#init-1, 3<=x, x<=8, 4<=y, y<=9}

            x--;

//  T(y) {y==y#init-1, 3<=x, x<=8, 3<=y, y<=8}

            y--;

//  T() {3<=x, x<=8, 3<=y, y<=8}

            if (x==2&&y==7)

//  T() {0==-1}

               checking_error();

//  T() {3<=x, x<=8, 3<=y, y<=8}

            if (x==5&&y==7)

//  T() {0==-1}

               checking_error();
         }
         else

//  T() {}

            while (1)

//  T() {}

               ;
      }
}

PRECONDITIONS


//  P() {}

void run(void)
{


//  P() {}

   int x, y;

//  P(x,y) {}

   x = rand();

//  P(x,y) {0<=x}

   y = rand();

//  P(x,y) {0<=x, 0<=y}

   if (x>=0&&x<=5&&y>=0&&y<=5&&y<=5-x)

//  P(x,y) {0<=x, x<=5, x+y<=5, 0<=y, y<=5}


      while (1) {

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, 0<=y, y<=6}

         if (x==2&&y==7)

//  P() {0==-1}

            checking_error();

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, 0<=y, y<=6}

         if (x==5&&y==7)

//  P() {0==-1}

            checking_error();

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, 0<=y, y<=6}

         if (flip())

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, 0<=y, y<=6}

            if (x>=0&&x<=5&&y>=0&&y<=5) {

//  P(x,y) {0<=x, x<=5, y<=x+5, x<=y+5, 0<=y, y<=5}

               x++;

//  P(x,y) {1<=x, x<=6, y<=x+4, x<=y+6, 0<=y, y<=5}

               y++;

//  P(x,y) {1<=x, x<=6, y<=x+5, x<=y+5, 1<=y, y<=6}

               if (x==2&&y==7)

//  P() {0==-1}

                  checking_error();

//  P(x,y) {1<=x, x<=6, y<=x+5, x<=y+5, 1<=y, y<=6}

               if (x==5&&y==7)

//  P() {0==-1}

                  checking_error();
            }
            else

//  P(x,y) {0<=x, x<=6, 7<=x+y, y<=x+5, x<=y+5, 0<=y, y<=6}

               while (1)

//  P(x,y) {0<=x, x<=6, 7<=x+y, y<=x+5, x<=y+5, 0<=y, y<=6}

                  ;
         else if (x>=4&&x<=9&&y>=4&&y<=9) {

//  P(x,y) {4<=x, x<=6, y<=x+5, x<=y+5, 4<=y, y<=6}

            x--;

//  P(x,y) {3<=x, x<=5, y<=x+6, x<=y+4, 4<=y, y<=6}

            y--;

//  P(x,y) {3<=x, x<=5, y<=x+5, x<=y+5, 3<=y, y<=5}

            if (x==2&&y==7)

//  P() {0==-1}

               checking_error();

//  P(x,y) {3<=x, x<=5, y<=x+5, x<=y+5, 3<=y, y<=5}

            if (x==5&&y==7)

//  P() {0==-1}

               checking_error();
         }
         else

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, x+y<=9, 0<=y, y<=6}

            while (1)

//  P(x,y) {0<=x, x<=6, y<=x+5, x<=y+5, x+y<=9, 0<=y, y<=6}

               ;
      }
}
