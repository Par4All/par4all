C SET ALL  U(I) TO 0. TO AVOID TROUBLE IF NO MEAN FLOW IS DESIRED
C
      DO 10 I = 1, N2P                                                  0257

C  P(I,K1Q,K1QM,K2Q,K2QM,K3Q,K3QM,KDATA,KPF,KPP,KPRESS,KSM,KTA,KTAPE,
