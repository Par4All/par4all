C    3KZN+LZX==3KPN+1, 4KZN+MEMSIZ==4KPN+127, KPP==249, KPRESS==1,
C    KSM==1, KTA==50, KTAPE==0, KTF==1, KTP==50, KUF==1, KUP==249,
C    KVF==1, KVP==249, KZF==1, KZP==249, LE==1, LMAPP==1, LSKIP==2,
