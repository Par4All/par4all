# 1 "laguer.database/Tmp/laguer.cpp_processed.c"
# 1 "<command-line>"
# 1 "laguer.database/Tmp/laguer.cpp_processed.c"
//#include <math.h>
//#include "complex.h"
typedef struct {float r; float i;} fcomplex;
extern fcomplex Complex(float, float);
extern fcomplex Cdiv(fcomplex, fcomplex);
extern fcomplex Csub(fcomplex, fcomplex);
extern fcomplex Cadd(fcomplex, fcomplex);
extern fcomplex Cmul(fcomplex, fcomplex);
extern fcomplex RCmul(float, fcomplex);
extern fcomplex Conjg(fcomplex);
extern fcomplex Csqrt(fcomplex);
extern float Cabs(fcomplex);
void laguer(a,m,x,eps,polish)
fcomplex a[],*x;
int m,polish;
float eps;
{
 int j,iter;
 float err,dxold,cdx,abx;
 fcomplex sq,h,gp,gm,g2,g,b,d,dx,f,x1;
 void nrerror(char *);
 dxold=Cabs(*x);
 for (iter=1;iter<=100;iter++) {
  b=a[m];
  err=Cabs(b);
  d=f=Complex(0.0,0.0);
  abx=Cabs(*x);
  for (j=m-1;j>=0;j--) {
   f=Cadd(Cmul(*x,f),d);
   d=Cadd(Cmul(*x,d),b);
   b=Cadd(Cmul(*x,b),a[j]);
   err=Cabs(b)+abx*err;
  }
  err *= 6.e-8;
  if (Cabs(b) <= err) return;
  g=Cdiv(d,b);
  g2=Cmul(g,g);
  h=Csub(g2,RCmul(2.0,Cdiv(f,b)));
  sq=Csqrt(RCmul((float) (m-1),Csub(RCmul((float) m,h),g2)));
  gp=Cadd(g,sq);
  gm=Csub(g,sq);
  if (Cabs(gp) < Cabs(gm))gp=gm;
  dx=Cdiv(Complex((float) m,0.0),gp);
  x1=Csub(*x,dx);
  if (x->r == x1.r && x->i == x1.i) return;
  *x=x1;
  cdx=Cabs(dx);
  if (iter > 6 && cdx >= dxold) return;
  dxold=cdx;
  if (!polish)
   if (cdx <= eps*Cabs(*x)) return;
 }
 nrerror("Too many iterations in routine LAGUER");
}
# 13
void laguer(a,m,x,eps,polish)
fcomplex a[],*x;
int m,polish;
float eps;
{
 int j,iter;
 float err,dxold,cdx,abx;
 fcomplex sq,h,gp,gm,g2,g,b,d,dx,f,x1;
 void nrerror(char *);
 dxold=Cabs(*x);
 for (iter=1;iter<=100;iter++) {
  b=a[m];
  err=Cabs(b);
  d=f=Complex(0.0,0.0);
  abx=Cabs(*x);
  for (j=m-1;j>=0;j--) {
   f=Cadd(Cmul(*x,f),d);
   d=Cadd(Cmul(*x,d),b);
   b=Cadd(Cmul(*x,b),a[j]);
   err=Cabs(b)+abx*err;
  }
  err *= 6.e-8;
  if (Cabs(b) <= err) return;
  g=Cdiv(d,b);
  g2=Cmul(g,g);
  h=Csub(g2,RCmul(2.0,Cdiv(f,b)));
  sq=Csqrt(RCmul((float) (m-1),Csub(RCmul((float) m,h),g2)));
  gp=Cadd(g,sq);
  gm=Csub(g,sq);
  if (Cabs(gp) < Cabs(gm))gp=gm;
  dx=Cdiv(Complex((float) m,0.0),gp);
  x1=Csub(*x,dx);
  if (x->r == x1.r && x->i == x1.i) return;
  *x=x1;
  cdx=Cabs(dx);
  if (iter > 6 && cdx >= dxold) return;
  dxold=cdx;
  if (!polish)
   if (cdx <= eps*Cabs(*x)) return;
 }
 nrerror("Too many iterations in routine LAGUER");
}
//#include <math.h>
//#include "complex.h"
typedef struct {float r; float i;} fcomplex;
extern fcomplex Complex(float, float);
extern fcomplex Cdiv(fcomplex, fcomplex);
extern fcomplex Csub(fcomplex, fcomplex);
extern fcomplex Cadd(fcomplex, fcomplex);
extern fcomplex Cmul(fcomplex, fcomplex);
extern fcomplex RCmul(float, fcomplex);
extern fcomplex Conjg(fcomplex);
extern fcomplex Csqrt(fcomplex);
extern float Cabs(fcomplex);
extern void laguer();
void laguer(fcomplex a[], int m, fcomplex *x, float eps, int polish)
{
   int j, iter;
   float err, dxold, cdx, abx;
   fcomplex sq, h, gp, gm, g2, g, b, d, dx, f, x1;
   void nrerror(char *);
   dxold = Cabs(*x);
   for (iter = 1;iter<=100;iter++) {
      b = a[m];
      err = Cabs(b);
      d = f = Complex(0.0, 0.0);
      abx = Cabs(*x);
      for (j = m-1;j>=0;j--) {
         f = Cadd(Cmul(*x, f), d);
         d = Cadd(Cmul(*x, d), b);
         b = Cadd(Cmul(*x, b), a[j]);
         err = Cabs(b)+abx*err;
      }
      err *= 6.e-8;
      if (Cabs(b)<=err)
         return;
      g = Cdiv(d, b);
      g2 = Cmul(g, g);
      h = Csub(g2, RCmul(2.0, Cdiv(f, b)));
      sq = Csqrt(RCmul((float) (m-1), Csub(RCmul((float) m, h), g2)));
      gp = Cadd(g, sq);
      gm = Csub(g, sq);
      if (Cabs(gp)<Cabs(gm))
         gp = gm;
      dx = Cdiv(Complex((float) m, 0.0), gp);
      x1 = Csub(*x, dx);
      if (x->r==x1.r&&x->i==x1.i)
         return;
      *x = x1;
      cdx = Cabs(dx);
      if (iter>6&&cdx>=dxold)
         return;
      dxold = cdx;
      if (!polish)
         if (cdx<=eps*Cabs(*x))
            return;
   }
   nrerror("Too many iterations in routine LAGUER");
}
