
// IN Pointer values: none


// OUT Pointer values: none

int main()
{

// Pointer values: none

   int a[2], b, *p;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   a[0] = 0;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   a[1] = 1;

// Pointer values:
// p == *ANY_MODULE*:*UNDEFINED* (exact);

   p = a;

// Pointer values:
// p == a (exact);

   b = p[0];

// Pointer values:
// p == a (exact);

   b = p[1];

// Pointer values:
// p == a (exact);

   return b;
}
int main()
{
   int a[2], b, *p;
//             <    is written>: a[0]
   a[0] = 0;
//             <    is written>: a[1]
   a[1] = 1;
//             <    is written>: p
   p = a;
//             <    is read   >: a[0] p
//             <    is written>: b
   b = p[0];
//             <    is read   >: a[1] p
//             <    is written>: b
   b = p[1];
//             <    is read   >: b
   return b;
}
int main()
{
   int a[2], b, *p;

//  <a[PHI1]-W-EXACT-{PHI1==0}>

   a[0] = 0;

//  <a[PHI1]-W-EXACT-{PHI1==1}>

   a[1] = 1;

//  <p-W-EXACT-{}>

   p = a;

//  <a[PHI1]-R-EXACT-{PHI1==0}>
//  <b-W-EXACT-{}>
//  <p-R-EXACT-{}>

   b = p[0];

//  <a[PHI1]-R-EXACT-{PHI1==1}>
//  <b-W-EXACT-{}>
//  <p-R-EXACT-{}>

   b = p[1];

//  <b-R-EXACT-{}>

   return b;
}
int main()
{
   int a[2], b, *p;

//  <a[PHI1]-W-EXACT-{PHI1==0}>

   a[0] = 0;

//  <a[PHI1]-W-EXACT-{PHI1==1}>

   a[1] = 1;

//  <p-W-EXACT-{}>

   p = a;

//  <a[PHI1]-R-EXACT-{PHI1==0}>
//  <b-W-EXACT-{}>
//  <p-R-EXACT-{}>

   b = p[0];

//  <a[PHI1]-R-EXACT-{PHI1==1}>
//  <b-W-EXACT-{}>
//  <p-R-EXACT-{}>

   b = p[1];

//  <b-R-EXACT-{}>

   return b;
}
