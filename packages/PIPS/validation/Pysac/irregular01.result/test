void irregular01_l99999(int n, int a[n], int b[n])
{
   //PIPS generated variable
   int i;
l99999:
   for(i = 0; i <= 4*(n/4)-1; i += 4) {
      a[i] = a[1+b[i]];
      a[1+i] = a[1+b[1+i]];
      a[2+i] = a[1+b[2+i]];
      a[3+i] = a[1+b[3+i]];
   }
   for(i = 4*(n/4); i <= n-1; i += 1)
      a[i] = a[1+b[i]];
}
