# code
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

      I = 1                                                             0007

      DO WHILE (I.LE.10)                                                0009
         A(I) = I*1.23                                                  0010
         I = I+1                                                        0011
      ENDDO

      I = 2                                                             0014

      DO WHILE (I.LT.N)                                                 0016
         PRINT *, A(I-1)                                                0017
         I = I+1                                                        0018
      ENDDO

      PRINT *, 'bye'                                                    0021

      END
# proper effects
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)
C               <must be written>: I

      I = 1                                                             0007
C               <must be read   >: I

      DO WHILE (I.LE.10)                                                0009
C               <must be read   >: I
C               <must be written>: A(I)
         A(I) = I*1.23                                                  0010
C               <must be read   >: I
C               <must be written>: I
         I = I+1                                                        0011
      ENDDO
C               <must be written>: I

      I = 2                                                             0014
C               <must be read   >: I

      DO WHILE (I.LT.N)                                                 0016
C               <must be read   >: A(I-1) I __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)
         PRINT *, A(I-1)                                                0017
C               <must be read   >: I
C               <must be written>: I
         I = I+1                                                        0018
      ENDDO
C               <must be read   >: __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      PRINT *, 'bye'                                                    0021

      END
# cumulated effects
C               <must be read   >: __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)
C               <must be written>: I

      I = 1                                                             0007
C               <may be written >: A(*)
C               <must be read   >: I
C               <must be written>: I

      DO WHILE (I.LE.10)                                                0009
C               <may be written >: A(*)
C               <must be read   >: I
         A(I) = I*1.23                                                  0010
C               <must be read   >: I
C               <must be written>: I
         I = I+1                                                        0011
      ENDDO
C               <must be written>: I

      I = 2                                                             0014
C               <may be read    >: A(*)
C               <must be read   >: I __IO_EFFECTS:LUNS(6)
C               <must be written>: I __IO_EFFECTS:LUNS(6)

      DO WHILE (I.LT.N)                                                 0016
C               <may be read    >: A(*)
C               <must be read   >: I __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)
         PRINT *, A(I-1)                                                0017
C               <must be read   >: I
C               <must be written>: I
         I = I+1                                                        0018
      ENDDO
C               <must be read   >: __IO_EFFECTS:LUNS(6)
C               <must be written>: __IO_EFFECTS:LUNS(6)

      PRINT *, 'bye'                                                    0021

      END
# transformers

C  T() {}

      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

C  T(I) {I==1}


      I = 1                                                             0007

C  T(I) {I<=10, I#init<=I, I#init<=10}


      DO WHILE (I.LE.10)                                                0009

C  T() {}

         A(I) = I*1.23                                                  0010

C  T(I) {I==I#init+1}

         I = I+1                                                        0011
      ENDDO

C  T(I) {I==2}


      I = 2                                                             0014

C  T(I) {I<=9, I#init<=I, I#init<=9}


      DO WHILE (I.LT.N)                                                 0016

C  T() {}

         PRINT *, A(I-1)                                                0017

C  T(I) {I==I#init+1}

         I = I+1                                                        0018
      ENDDO

C  T() {}


      PRINT *, 'bye'                                                    0021

      END
# preconditions

C  P() {}

      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

C  P() {}


      I = 1                                                             0007

C  P(I) {I==1}


      DO WHILE (I.LE.10)                                                0009

C  P(I) {1<=I, I<=10}

         A(I) = I*1.23                                                  0010

C  P(I) {1<=I, I<=10}

         I = I+1                                                        0011
      ENDDO

C  P(I) {I==11}


      I = 2                                                             0014

C  P(I) {I==2}


      DO WHILE (I.LT.N)                                                 0016

C  P(I) {2<=I, I<=9}

         PRINT *, A(I-1)                                                0017

C  P(I) {2<=I, I<=9}

         I = I+1                                                        0018
      ENDDO

C  P(I) {I==10}


      PRINT *, 'bye'                                                    0021

      END
# must regions

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6}>

      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

      I = 1                                                             0007

C  <A(PHI1)-W-MAY-{I<=PHI1, 1<=PHI1, PHI1<=10}>


      DO WHILE (I.LE.10)                                                0009

C  <A(PHI1)-W-EXACT-{PHI1==I, 1<=I, I<=10}>

         A(I) = I*1.23                                                  0010
         I = I+1                                                        0011
      ENDDO

      I = 2                                                             0014

C  <A(PHI1)-R-MAY-{I<=PHI1+1, 1<=PHI1, PHI1<=8}>
C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, I<=9}>


      DO WHILE (I.LT.N)                                                 0016

C  <A(PHI1)-R-EXACT-{PHI1==I-1, 2<=I, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, 2<=I, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, 2<=I, I<=9}>

         PRINT *, A(I-1)                                                0017
         I = I+1                                                        0018
      ENDDO

C  <__IO_EFFECTS:LUNS(PHI1)-R-EXACT-{PHI1==6, I==10}>
C  <__IO_EFFECTS:LUNS(PHI1)-W-EXACT-{PHI1==6, I==10}>


      PRINT *, 'bye'                                                    0021

      END
# in regions

C  <__IO_EFFECTS:LUNS(PHI1)-IN-MAY-{PHI1==6}>

      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

      I = 1                                                             0007

      DO WHILE (I.LE.10)                                                0009
         A(I) = I*1.23                                                  0010
         I = I+1                                                        0011
      ENDDO

      I = 2                                                             0014

C  <A(PHI1)-IN-MAY-{I<=PHI1+1, 1<=PHI1, PHI1<=8}>
C  <__IO_EFFECTS:LUNS(PHI1)-IN-MAY-{PHI1==6, I<=9}>


      DO WHILE (I.LT.N)                                                 0016

C  <A(PHI1)-IN-EXACT-{PHI1==I-1, 2<=I, I<=9}>
C  <__IO_EFFECTS:LUNS(PHI1)-IN-EXACT-{PHI1==6, 2<=I, I<=9}>

         PRINT *, A(I-1)                                                0017
         I = I+1                                                        0018
      ENDDO

C  <__IO_EFFECTS:LUNS(PHI1)-IN-EXACT-{PHI1==6, I==10}>


      PRINT *, 'bye'                                                    0021

      END
# out regions
      PROGRAM W03

      INTEGER I, N
      PARAMETER (N=10)
      REAL A(N)

      I = 1                                                             0007

C  <A(PHI1)-OUT-MAY-{I<=PHI1, 1<=PHI1, PHI1<=8, I<=10}>


      DO WHILE (I.LE.10)                                                0009

C  <A(PHI1)-OUT-MAY-{PHI1==I, 1<=I, I<=10}>

         A(I) = I*1.23                                                  0010
         I = I+1                                                        0011
      ENDDO

      I = 2                                                             0014

      DO WHILE (I.LT.N)                                                 0016

C  <__IO_EFFECTS:LUNS(PHI1)-OUT-MAY-{PHI1==6, 2<=I, I<=9}>

         PRINT *, A(I-1)                                                0017
         I = I+1                                                        0018
      ENDDO

      PRINT *, 'bye'                                                    0021

      END

