
PROPER  EFFECTS

int main()
{
   int *a, b, c, d;
//             <    is written>: c
   c = 3;
//             <    is written>: a
   /* S1 */
   a = &c;
//             <    is read   >: d __IO_EFFECTS:LUNS[d]
//             <    is written>: __IO_EFFECTS:LUNS[d]
   /* S2 */
   read(d);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: b
   /* S3 */
   b = *a;
   /* S4 */
   
   
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a, b, c, d;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   c = 3;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /* S1 */
   a = &c;

// Points To:
// a -> c , EXACT

   /* S2 */
   read(d);

// Points To:
// a -> c , EXACT

   /* S3 */
   b = *a;

// Points To:
// a -> c , EXACT

   /* S4 */
   
   
   return 0;
}
