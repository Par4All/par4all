
PROPER  EFFECTS

int struct05()
{
   struct two {
      int first;
      int second;
   } x[10];
//             <    is written>: i
   int i = 0;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
      if (1)
//             <    is read   >: i x[i].second
//             <    is written>: x[i].first
         x[i].first = x[i].second;
      else
//             <    is read   >: i x[i].first
//             <    is written>: x[i].second
         x[i].second = x[i].first;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int struct05()
{

// Points To: none

   struct two {
      int first;
      int second;
   } x[10];

// Points To: none

   int i = 0;

// Points To: none


   for(i = 0; i <= 9; i += 1)

// Points To: none

      if (1)

// Points To: none

         x[i].first = x[i].second;
      else

// Points To: none

         x[i].second = x[i].first;
}
