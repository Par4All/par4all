
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
   int a[10];
//             <    is read   >: i
   do {
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   do {

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// p -> a[*] , MAY

      p = &a[i];

// Points To:
// p -> a[*] , EXACT

      i++;
   }
   while (i<10);

// Points To:
// p -> a[*] , EXACT

   return 0;
}
