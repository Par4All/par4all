
POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY


// Points To OUT: none

int store01(int *p, int i)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1[0] , MAY

   *p = i;

// Points To:
// p -> _p_1[0] , EXACT

   return;
}

PROPER EFFECTS

int store01(int *p, int i)
{
//             <    is read   >: i p
//             <    is written>: _p_1[0]
   *p = i;
   return;
}
