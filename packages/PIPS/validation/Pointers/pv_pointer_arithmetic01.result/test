
POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &a[0];

// Points To:
// p -> a[0] , EXACT

   p = p+1;

// Points To:
// p -> a[1] , EXACT

   p = p+3;

// Points To:
// p -> a[4] , EXACT

   p = p-1;

// Points To:
// p -> a[3] , EXACT

   p = p-2;

// Points To:
// p -> a[1] , EXACT

   return 0;
}
