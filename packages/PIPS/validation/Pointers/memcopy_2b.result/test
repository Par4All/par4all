
Point-to


// Points To IN:
// dst -> *NULL* , MAY
// dst -> _dst_3[0] , MAY
// src -> *NULL* , MAY
// src -> _src_2[0] , MAY


// Points To OUT:
// dst -> *NULL* , MAY
// dst -> _dst_3[0] , MAY
// src -> *NULL* , MAY
// src -> _src_2[0] , MAY

void memcopy_2b(int size, char *src, char *dst)
{

// Points To: none

   int i;

// Points To:
// dst -> *NULL* , MAY
// dst -> _dst_3[0] , MAY
// src -> *NULL* , MAY
// src -> _src_2[0] , MAY

   if (src!=(void *) 0&&dst!=(void *) 0) {

// Points To:
// dst -> _dst_3[0] , EXACT
// src -> _src_2[0] , EXACT

      char *s = (char *) src;

// Points To:
// dst -> _dst_3[0] , EXACT
// s -> _src_2[0] , EXACT
// src -> _src_2[0] , EXACT

      char *d = (char *) dst;

// Points To:
// d -> _dst_3[0] , EXACT
// dst -> _dst_3[0] , EXACT
// s -> _src_2[0] , EXACT
// src -> _src_2[0] , EXACT

      for(i = 0; i <= size-1; i += 1)

// Points To:
// d -> _dst_3[0] , EXACT
// dst -> _dst_3[0] , EXACT
// s -> _src_2[0] , EXACT
// src -> _src_2[0] , EXACT

         d[i] = s[i];
   }
}

Proper effects with point to

void memcopy_2b(int size, char *src, char *dst)
{
//             <    is declared>: i
   int i;
//             <    is read   >: dst src
//             <    is referenced>: dst src
   if (src!=(void *) 0&&dst!=(void *) 0) {
//             <    is read   >: src
//             <    is written>: s
//             <    is referenced>: s src
//             <    is declared>: s
      char *s = (char *) src;
//             <    is read   >: dst
//             <    is written>: d
//             <    is referenced>: d dst
//             <    is declared>: d
      char *d = (char *) dst;
//             <    is read   >: size
//             <    is written>: i
//             <    is referenced>: i size
      for(i = 0; i <= size-1; i += 1)
//             <    is read   >: _src_2[i] d i s size
//             <    is written>: _dst_3[i]
//             <    is referenced>: d i s size
         d[i] = s[i];
   }
}

Regions with point to


//  <_dst_3[PHI1]-W-MAY-{0<=PHI1, PHI1+1<=size}>
//  <_src_2[PHI1]-R-MAY-{0<=PHI1, PHI1+1<=size}>
//  <dst-R-EXACT-{}>
//  <src-R-EXACT-{}>

void memcopy_2b(int size, char *src, char *dst)
{
   int i;

//  <_dst_3[PHI1]-W-MAY-{0<=PHI1, PHI1+1<=size}>
//  <_src_2[PHI1]-R-MAY-{0<=PHI1, PHI1+1<=size}>
//  <dst-R-EXACT-{}>
//  <dst-RE-MAY-{}>
//  <src-RE-MAY-{}>
//  <src-R-EXACT-{}>

   if (src!=(void *) 0&&dst!=(void *) 0) {

//  <s-RE-EXACT-{}>
//  <s-WE-EXACT-{}>
//  <s-W-EXACT-{}>
//  <src-R-EXACT-{}>
//  <src-RE-EXACT-{}>

      char *s = (char *) src;

//  <d-RE-EXACT-{}>
//  <d-WE-EXACT-{}>
//  <d-W-EXACT-{}>
//  <dst-R-EXACT-{}>
//  <dst-RE-EXACT-{}>

      char *d = (char *) dst;

//  <_dst_3[PHI1]-W-EXACT-{0<=PHI1, PHI1+1<=size}>
//  <_src_2[PHI1]-R-EXACT-{0<=PHI1, PHI1+1<=size}>
//  <d-R-EXACT-{}>
//  <d-RE-EXACT-{}>
//  <s-R-EXACT-{}>
//  <s-RE-EXACT-{}>

      for(i = 0; i <= size-1; i += 1)

//  <_dst_3[PHI1]-W-EXACT-{PHI1==i, 0<=i, i+1<=size}>
//  <_src_2[PHI1]-R-EXACT-{PHI1==i, 0<=i, i+1<=size}>
//  <d-R-EXACT-{}>
//  <d-RE-EXACT-{}>
//  <s-R-EXACT-{}>
//  <s-RE-EXACT-{}>

         d[i] = s[i];
   }
}

Data dependence graph


 *********************** Use-Def Chains *********************

	13 --> 19 with conflicts
		from <i-WE-EXACT> to <i-RE-EXACT>
	13 --> 20 with conflicts
		from <i-WE-EXACT> to <i-RE-EXACT>
	17 --> 20 with conflicts
		from <s-W-EXACT> to <s-R-EXACT>
		from <s-WE-EXACT> to <s-RE-EXACT>
	18 --> 20 with conflicts
		from <d-W-EXACT> to <d-R-EXACT>
		from <d-WE-EXACT> to <d-RE-EXACT>
	20 --> 20 with conflicts
		from <_dst_3[i]-W-EXACT> to <_dst_3[i]-W-EXACT>

****************** End of Dependence Graph ******************

Parallel code

void memcopy_2b(int size, char *src, char *dst)
{
   int i;
   if (src!=(void *) 0&&dst!=(void *) 0) {
      char *s = (char *) src;
      char *d = (char *) dst;
      forall(i = 0; i <= size-1; i += 1)
         d[i] = s[i];
   }
}
