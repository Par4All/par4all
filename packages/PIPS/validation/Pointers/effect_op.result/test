
PROPER  EFFECTS

int main()
{
//             <    is written>: b
   int b = 1;
//             <    is written>: i
   int i = 10;
//             <    is read   >: b i
//             <    is written>: b

   b = b+i;
//             <    is read   >: b i
//             <    is written>: b
   b = b*i;
//             <    is read   >: b i
//             <    is written>: b
   b = b/i;
//             <    is read   >: b i
//             <    is written>: b
   b = b&i;
//             <    is read   >: b i
//             <    is written>: b
   b = b^i;
//             <    is read   >: b i
//             <    is written>: b
   b = b|i;
//             <    is read   >: b i
//             <    is written>: b
   b += i;
//             <    is read   >: b i
//             <    is written>: b
   b *= i;
//             <    is read   >: b i
//             <    is written>: b
   b /= i;
//             <    is read   >: b i
//             <    is written>: b
   b -= i;
//             <    is read   >: b i
//             <    is written>: b
   b &= i;
//             <    is read   >: b i
//             <    is written>: b
   b |= i;
//             <    is read   >: b i
//             <    is written>: b
   b ^= i;
//             <    is read   >: b
//             <    is written>: b
   b++;
//             <    is read   >: b
//             <    is written>: b
   b--;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int b = 1;

// Points To: none

   int i = 10;

// Points To: none


   b = b+i;

// Points To: none

   b = b*i;

// Points To: none

   b = b/i;

// Points To: none

   b = b&i;

// Points To: none

   b = b^i;

// Points To: none

   b = b|i;

// Points To: none

   b += i;

// Points To: none

   b *= i;

// Points To: none

   b /= i;

// Points To: none

   b -= i;

// Points To: none

   b &= i;

// Points To: none

   b |= i;

// Points To: none

   b ^= i;

// Points To: none

   b++;

// Points To: none

   b--;

// Points To: none


   return 0;
}
