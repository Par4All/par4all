
PROPER  EFFECTS

int main()
{
//             <    is written>: a y y1
   int a = 2, *y1 = &a, *x, **y = &y1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: x
   x = *y;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a = 2, *y1 = &a, *x, **y = &y1;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> y1 , EXACT
// y1 -> a , EXACT

   x = *y;

// Points To:
// x -> a , EXACT
// y -> y1 , EXACT
// y1 -> a , EXACT


   return 0;
}
