
PROPER  EFFECTS

void arithmetic05(int a[10])
{
//             <    is written>: q
   int *q = a;
   int *p;
//             <    is read   >: q
//             <    is written>: p q

   p = q++;
   return;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

void arithmetic05(int a[10])
{

// Points To: none

   int *q = a;

// Points To:
// q -> a[0] , EXACT

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> a[0] , EXACT


   p = q++;

// Points To:
// p -> q , EXACT
// q -> a[2] , EXACT

   return;
}
