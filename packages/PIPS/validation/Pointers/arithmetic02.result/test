
PROPER  EFFECTS

int main()
{
   int *pdeb, *pfin, *p;
//             <    is written>: pdeb

   pdeb = &t[0];
//             <    is written>: pfin
   /*   repère le premier élément de t   */
   pfin = &t[10-1];
//             <    is read   >: pdeb
//             <    is written>: p
   /*   repère le dernier élément de t   */
   
   p = pdeb;
//             <    is read   >: p pfin
   while (p<=pfin) {
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
      if (*p==0)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: p pdeb stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
         printf("%d ", p-pdeb);
//             <    is read   >: p
//             <    is written>: p
      p++;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf("\n");
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *pdeb, *pfin, *p;

// Points To:
// p -> undefined , EXACT
// pdeb -> undefined , EXACT
// pfin -> undefined , EXACT


   pdeb = &t[0];

// Points To:
// p -> undefined , EXACT
// pdeb -> t[0] , EXACT
// pfin -> undefined , EXACT

   /*   repère le premier élément de t   */
   pfin = &t[10-1];

// Points To:
// p -> undefined , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

   /*   repère le dernier élément de t   */
   
   p = pdeb;

// Points To:
// p -> t[0] , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

   while (p<=pfin) {

// Points To:
// p -> t[0] , MAY
// p -> t[1] , MAY
// p -> t[2] , MAY
// p -> t[3] , MAY
// p -> t[4] , MAY
// p -> t[5] , MAY
// p -> t[6] , MAY
// p -> t[7] , MAY
// p -> t[8] , MAY
// p -> t[9] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

      if (*p==0)

// Points To:
// p -> t[0] , MAY
// p -> t[1] , MAY
// p -> t[2] , MAY
// p -> t[3] , MAY
// p -> t[4] , MAY
// p -> t[5] , MAY
// p -> t[6] , MAY
// p -> t[7] , MAY
// p -> t[8] , MAY
// p -> t[9] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

         printf("%d ", p-pdeb);

// Points To:
// p -> t[0] , MAY
// p -> t[1] , MAY
// p -> t[2] , MAY
// p -> t[3] , MAY
// p -> t[4] , MAY
// p -> t[5] , MAY
// p -> t[6] , MAY
// p -> t[7] , MAY
// p -> t[8] , MAY
// p -> t[9] , MAY
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

      p++;
   }

// Points To:
// p -> t[10] , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

   printf("\n");

// Points To:
// p -> t[10] , EXACT
// pdeb -> t[0] , EXACT
// pfin -> t[9] , EXACT

   return 0;
}
