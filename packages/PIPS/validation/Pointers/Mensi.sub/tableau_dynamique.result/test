void init_array(parray_t pa)
{
   int i;
//             <    is read   >: _pa_1.dim pa
//             <    is written>: i
   for(i = 0; i <= pa->dim-1; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: _pa_1.data _pa_1.dim i pa
      (pa->data)[i] = (float) i;

   return;
}

