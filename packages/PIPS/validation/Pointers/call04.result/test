
PROPER  EFFECTS

int main()
{
//             <    is written>: d1
   int d1 = 10;
//             <    is written>: d2
   int d2 = 10;
   int y[d1][d2];
//             <may be written>: y[*][*]
//             <    is read   >: d1 d2

   call04(d1, d2, y);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int d1 = 10;

// Points To: none

   int d2 = 10;

// Points To: none

   int y[d1][d2];

// Points To: none


   call04(d1, d2, y);

// Points To: none

   return 0;
}
