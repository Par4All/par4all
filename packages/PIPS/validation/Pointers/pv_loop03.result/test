
PROPER  EFFECTS

int main()
{
   int i;
   int *p;
   int a[10];
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: p
      p = &a[i];
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   for(i = 0; i <= 9; i += 1)

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// p -> a[*] , MAY

      p = &a[i];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// p -> a[*] , MAY

   return 0;
}
