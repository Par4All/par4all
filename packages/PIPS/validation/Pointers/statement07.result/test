
PROPER  EFFECTS

int main()
{
   int *x, *a, i;
//             <    is written>: i
   i = 1;
//             <    is written>: a
   a = &i;
//             <    is read   >: a
//             <    is written>: x
   x = a;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *x, *a, i;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   i = 1;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a = &i;

// Points To:
// a -> i , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   x = a;

// Points To:
// a -> i , EXACT
// x -> i , EXACT


   return 0;
}
