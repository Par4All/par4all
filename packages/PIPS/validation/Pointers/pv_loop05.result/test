
PROPER  EFFECTS

int main()
{
   int i;
   int *p, *q;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is written>: i

   for(i = 0; i <= 4; i += 1) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i q
      *q = i;
//             <    is read   >: q
//             <    is written>: q
      q++;
   }
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = (int *) malloc(10*sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = p;

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_8[0] , EXACT


   for(i = 0; i <= 4; i += 1) {

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_8[0] , MAY
// q -> *HEAP*_l_8[*] , MAY

      *q = i;

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_8[0] , MAY
// q -> *HEAP*_l_8[*] , MAY

      q++;
   }

// Points To:
// p -> *HEAP*_l_8[0] , EXACT
// q -> *HEAP*_l_8[*] , MAY
// q -> *HEAP*_l_8[0] , MAY


   free(p);

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *HEAP*_l_8[*] , MAY
// q -> *HEAP*_l_8[0] , MAY

   return 0;
}
