
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: i
   do {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i p
      p[i] = i;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   while (i<10);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p;

// Points To:
// p -> undefined , EXACT


   p = (int *) malloc(10*sizeof(int));

// Points To:
// p -> *HEAP*_l_8[0] , MAY

   do {

// Points To:
// p -> *HEAP*_l_8[0] , MAY

      p[i] = i;

// Points To:
// p -> *HEAP*_l_8[0] , MAY

      i++;
   }
   while (i<10);

// Points To:
// p -> *HEAP*_l_8[0] , MAY

   return 0;
}
