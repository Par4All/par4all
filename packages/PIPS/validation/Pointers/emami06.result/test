
PROPER  EFFECTS

int main()
{
//             <    is written>: a b x1 y1
   int a = 2, b = 1, **x, **y, *x1 = &a, *y1 = &b;
//             <    is written>: x
   x = &x1;
//             <    is written>: y
   y = &y1;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   *x = *y;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a = 2, b = 1, **x, **y, *x1 = &a, *y1 = &b;

// Points To:
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// x1 -> a , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y1 -> b , EXACT

   x = &x1;

// Points To:
// x -> x1 , EXACT
// x1 -> a , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y1 -> b , EXACT

   y = &y1;

// Points To:
// x -> x1 , EXACT
// x1 -> a , EXACT
// y -> y1 , EXACT
// y1 -> b , EXACT

   *x = *y;

// Points To:
// x -> x1 , EXACT
// x1 -> b , EXACT
// y -> y1 , EXACT
// y1 -> b , EXACT


   return 0;
}
