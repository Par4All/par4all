
POINTS TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int i;

// Points To: none

   int **pp;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   srand(time((void *) 0));

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   if (alea()) {

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int *pi = &i;

// Points To:
// pi -> i , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi;

// Points To:
// pi -> i , EXACT
// pp -> pi , EXACT


      **pp = 1;
   }

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b1 , MAY

   
   // pp points to a dead value in the stack
   printf("%p\n", pp);

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// pp -> *ANY_MODULE*:*NOWHERE*_b1 , MAY

   
   // possible segfault.
   // should generate an error while analyzing?
   **pp = 0;

// Points To: unreachable

   return 0;
}

// Points To IN: none


// Points To OUT: none

static int alea(void)
{

// Points To: none

   int r = rand();

// Points To: none

   printf("%d\n", r);

// Points To: none

   return rand()%2==1;
}

