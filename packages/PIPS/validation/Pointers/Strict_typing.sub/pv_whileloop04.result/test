
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   while (i<10) {

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// p[*] -> a[*] , MAY

      p[i] = &a[i];

// Points To:
// p[*] -> a[*] , EXACT

      i++;
   }

// Points To:
// p[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// p[*] -> a[*] , MAY

   return 0;
}
