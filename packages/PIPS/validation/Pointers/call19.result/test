
PROPER  EFFECTS

int bla(int *p)
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: p
   *p = 13;
   return 17;
}
int call19(int *q)
{
   int i, k;
   something st;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   // bla(int *)
   i = bla(q);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: i
   i += *q;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i
   i += bla(&k);
//             <    is read   >: i k
//             <    is written>: i
   i += k;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i
   i += bla(&st.data);
//             <    is read   >: i st.data
//             <    is written>: i
   i += st.data;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: i
   // foo(int)
   i += foo(*q);
//             <    is read   >: i k
//             <    is written>: i
   i += foo(k);
//             <    is read   >: i st.data
//             <    is written>: i
   i += foo(st.data);
//             <    is read   >: i
   return i;
}
int foo(int a)
{
//             <    is read   >: a
   return a*3;
}

POINTS-TO


// Points To IN:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY


// Points To OUT: none

int bla(int *p)
{

// Points To:
// p -> *NULL_POINTER* , MAY
// p -> _p_1 , MAY

   *p = 13;

// Points To:
// p -> _p_1 , EXACT

   return 17;
}

// Points To IN:
// q -> *NULL_POINTER* , MAY
// q -> _q_1 , MAY


// Points To OUT: none

int call19(int *q)
{

// Points To:
// q -> *NULL_POINTER* , MAY
// q -> _q_1 , MAY

   int i, k;

// Points To:
// q -> *NULL_POINTER* , MAY
// q -> _q_1 , MAY

   something st;

// Points To:
// q -> *NULL_POINTER* , MAY
// q -> _q_1 , MAY

   // bla(int *)
   i = bla(q);

// Points To:
// q -> *NULL_POINTER* , MAY
// q -> _q_1 , MAY

   i += *q;

// Points To:
// q -> _q_1 , EXACT

   i += bla(&k);

// Points To:
// q -> _q_1 , EXACT

   i += k;

// Points To:
// q -> _q_1 , EXACT

   i += bla(&st.data);

// Points To:
// q -> _q_1 , EXACT

   i += st.data;

// Points To:
// q -> _q_1 , EXACT

   // foo(int)
   i += foo(*q);

// Points To:
// q -> _q_1 , EXACT

   i += foo(k);

// Points To:
// q -> _q_1 , EXACT

   i += foo(st.data);

// Points To:
// q -> _q_1 , EXACT

   return i;
}

// Points To IN: none


// Points To OUT: none

int foo(int a)
{

// Points To: none

   return a*3;
}
