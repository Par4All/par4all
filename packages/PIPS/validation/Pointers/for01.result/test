
PROPER  EFFECTS

int main()
{
   int i, j, k;
   int a[500];
//             <    is written>: i

   i = 0;
//             <    is written>: j
   j = 1;
//             <    is written>: i
   for(i = 0; i <= 499; i += 1) {
//             <    is read   >: j
//             <    is written>: j
      j++;
//             <    is read   >: i
//             <    is written>: a[i]
      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;
   }
//             <    is written>: k
   k = 2;
//             <    is read   >: k
   return k;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i, j, k;

// Points To: none

   int a[500];

// Points To: none


   i = 0;

// Points To: none

   j = 1;

// Points To: none

   for(i = 0; i <= 499; i += 1) {

// Points To: none

      j++;

// Points To: none

      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;
   }

// Points To: none

   k = 2;

// Points To: none

   return k;
}
