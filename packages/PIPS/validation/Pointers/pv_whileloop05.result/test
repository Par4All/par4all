
PROPER  EFFECTS

int main()
{
//             <    is written>: i
   int i = 0;
   int *p, *q;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is read   >: i

   while (i<5) {
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: i q
      *q = i;
//             <    is read   >: q
//             <    is written>: q
      q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i = 0;

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = (int *) malloc(10*sizeof(int));

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = p;

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *HEAP*_l_7[0] , EXACT


   while (i<5) {

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *HEAP*_l_7[0] , MAY
// q -> *HEAP*_l_7[*] , MAY

      *q = i;

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *HEAP*_l_7[0] , MAY
// q -> *HEAP*_l_7[*] , MAY

      q++;

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *HEAP*_l_7[0] , MAY
// q -> *HEAP*_l_7[*] , MAY

      i++;
   }

// Points To:
// p -> *HEAP*_l_7[0] , EXACT
// q -> *HEAP*_l_7[0] , MAY
// q -> *HEAP*_l_7[*] , MAY


   free(p);

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *HEAP*_l_7[0] , MAY
// q -> *HEAP*_l_7[*] , MAY

   return 0;
}
