
PROPER EFFECTS

int main()
{
   int *i, j, k;
//             <    is written>: k
   k = 0;
//             <    is read   >: k
//             <    is written>: j
   j = k;
//             <    is written>: i
   i = &j;

   return 0;
}
POINTS TO
//  {}
int main()
{
//  points to = {}
   int *i, j, k;
//  {(i,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   k = 0;
//  {(i,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   j = k;
//  {(i,*ANY_MODULE*:*NOWHERE*,-Exact-)}
   i = &j;
//  {(i,j,-Exact-)}

   return 0;
}
