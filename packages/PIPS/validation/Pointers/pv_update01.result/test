
PROPER  EFFECTS

int main()
{
   int a[10];
   int *p;
//             <    is written>: p
   p = &a[0];
//             <    is read   >: p
//             <    is written>: p
   p++;
//             <    is read   >: p
//             <    is written>: p
   ++p;
//             <    is read   >: p
//             <    is written>: p
   p--;
//             <    is read   >: p
//             <    is written>: p
   --p;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *p;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &a[0];

// Points To:
// p -> a[0] , EXACT

   p++;

// Points To:
// p -> a[*] , EXACT

   ++p;

// Points To:
// p -> a[*] , EXACT

   p--;

// Points To:
// p -> a[*] , EXACT

   --p;

// Points To:
// p -> a[*] , EXACT

   return 0;
}
