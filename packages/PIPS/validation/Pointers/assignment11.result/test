
PROPER  EFFECTS

int assignment11()
{
   struct {
      int a[10][10];
   } s;
//             <    is written>: q
   int *q = (s.a)[0];
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*

   return *q;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int assignment11()
{

// Points To: none

   struct {
      int a[10][10];
   } s;

// Points To: none

   int *q = (s.a)[0];

// Points To:
// q -> s.a[0][0] , EXACT


   return *q;
}
