
PROPER  EFFECTS

int main()
{
   char *end;
   long int li;
//             <    is written>: end li

   li = strtol("32445667 is a number", &end, 0);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   char *end;

// Points To:
// end -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   long int li;

// Points To:
// end -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   li = strtol("32445667 is a number", &end, 0);

// Points To:
// end -> *ANY_MODULE*:*ANYWHERE*_b0 , EXACT

   return 0;
}
