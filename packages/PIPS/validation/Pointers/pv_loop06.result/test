
PROPER  EFFECTS

int main()
{
   int i;
   int *p, *q;
   int a[10];
   int b[10];
//             <    is written>: p

   p = &a[0];
//             <    is written>: q
   q = &b[0];
//             <    is written>: i
   for(i = 0; i <= 9; i += 1)
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
      *p++ = 2**q++;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   int *p, *q;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int a[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b[10];

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   p = &a[0];

// Points To:
// p -> a[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &b[0];

// Points To:
// p -> a[0] , EXACT
// q -> b[0] , EXACT

   for(i = 0; i <= 9; i += 1)

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// p -> a[0] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> b[0] , MAY

      *p++ = 2**q++;

// Points To:
// p -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// p -> a[0] , MAY
// q -> *ANY_MODULE*:*ANYWHERE*_b0 , MAY
// q -> b[0] , MAY

   return 0;
}
