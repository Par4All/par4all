
PROPER  EFFECTS

int main(void)
{
   int i;
   int **pp;
//             <    is read   >: __TIME_EFFECTS:KALI
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __TIME_EFFECTS:KALI
   srand(time((void *) 0));
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: _RANDOM_EFFECTS:GEN_SEED 
//                               __IO_EFFECTS:LUNS[1]

   if (alea()) {
//             <    is written>: pi
      int *pi = &i;
//             <    is written>: pp
      pp = &pi;
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

      **pp = 1;
   }
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: pp stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   
   // pp points to a dead value in the stack
   printf("%p\n", pp);
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
   
   // possible segfault.
   // should generate an error while analyzing?
   **pp = 0;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main(void)
{

// Points To: none

   int i;

// Points To: none

   int **pp;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   srand(time((void *) 0));

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   if (alea()) {

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      int *pi = &i;

// Points To:
// pi -> i , EXACT
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      pp = &pi;

// Points To:
// pi -> i , EXACT
// pp -> pi , EXACT


      **pp = 1;
   }

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   
   // pp points to a dead value in the stack
   printf("%p\n", pp);

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   
   // possible segfault.
   // should generate an error while analyzing?
   **pp = 0;

// Points To:
// pp -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   return 0;
}
