
PROPER  EFFECTS

int main()
{
   int **x, **y, *z, *t;
   int i, j;
//             <    is written>: i
   i = 1;
//             <    is written>: j
   j = 2;
//             <    is written>: z

   z = &i;
//             <    is written>: t
   t = &j;
//             <    is read   >: t
//             <    is written>: z

   z = t;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: x z
   *x = z;
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: stdout x __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
   printf(" x = %p\n", x);
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int **x, **y, *z, *t;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   int i, j;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   i = 1;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT

   j = 2;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT


   z = &i;

// Points To:
// t -> *ANY_MODULE*:*NOWHERE*_b1 , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> i , EXACT

   t = &j;

// Points To:
// t -> j , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> i , EXACT


   z = t;

// Points To:
// t -> j , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> j , EXACT

   *x = z;

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> j , EXACT
// t -> j , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> j , EXACT

   printf(" x = %p\n", x);

// Points To:
// *ANY_MODULE*:*ANYWHERE*_b0 -> j , EXACT
// t -> j , EXACT
// x -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// y -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// z -> j , EXACT

   return 0;
}
