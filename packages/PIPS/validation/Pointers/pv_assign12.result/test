
PROPER  EFFECTS

int main()
{
   int *a[2];
   int b[2];
//             <    is written>: c
   int c = 0;

   if (1)
//             <    is written>: a[0]
      a[0] = &b[0];
   else
//             <    is written>: a[0]
      a[0] = &b[1];
//             <    is written>: a[1]

   a[1] = &c;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a[2];

// Points To: none

   int b[2];

// Points To: none

   int c = 0;

// Points To: none


   if (1)

// Points To: none

      a[0] = &b[0];
   else

// Points To: none

      a[0] = &b[1];

// Points To:
// a[0] -> b[1] , MAY
// a[0] -> b[0] , MAY


   a[1] = &c;

// Points To:
// a[0] -> b[1] , MAY
// a[0] -> b[0] , MAY
// a[1] -> c , EXACT

   return 0;
}
