   int j = 1;                                                           /*0007*/
//  points to = {}
   
   // effects on useless expression
   0;                                                                   /*0010*/
//  points to = {}
   // effects on empty references...
   j/i;                                                                 /*0012*/
//  points to = {}
   (void) j/i;                                                          /*0013*/
//  points to = {}
   return i/j;                                                          /*0014*/
}
