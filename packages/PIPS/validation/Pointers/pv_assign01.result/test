
PROPER  EFFECTS

int main()
{
   int *a;
//             <    is written>: b
   int b = 0;
//             <    is written>: a
   a = &b;
   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int *a;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int b = 0;

// Points To:
// a -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   a = &b;

// Points To:
// a -> b , EXACT

   return 0;
}
