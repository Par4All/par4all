
PROPER  EFFECTS

int main()
{
   char buf[20];
   size_t nbytes;
   ssize_t bytes_read;
   int fd;
//             <    is written>: nbytes
   nbytes = sizeof(buf);
//             <may be written>: buf[*]
//             <    is read   >: fd nbytes __IO_EFFECTS:LUNS[fd]
//             <    is written>: bytes_read __IO_EFFECTS:LUNS[fd]
   bytes_read = read(fd, buf, nbytes);
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   char buf[20];

// Points To: none

   size_t nbytes;

// Points To: none

   ssize_t bytes_read;

// Points To: none

   int fd;

// Points To: none

   nbytes = sizeof(buf);

// Points To: none

   bytes_read = read(fd, buf, nbytes);
}
