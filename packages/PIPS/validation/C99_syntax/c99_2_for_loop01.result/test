int main()
{
// (0,1)
//:   BEGIN UNSTRUCTURED
   {
// (1,1)
      // BEGIN BLOCK
   {{
// (1,2)
      // BEGIN BLOCK
   {{
// (1,3)
      // BEGIN BLOCK
   {{
// (1,4)
      double a, b;
// (1,5)

      b = 1;
// (1,6)
      a = (b*b);
   }}
      // END BLOCK
      {
// (1,7)
         // BEGIN BLOCK
      {{
// (1,8)
         int i;
// (1,9)
         
         /* First loop */
         for (i = 0;i<67;i++)
// (1,10)
            a += b;
      }}
         // END BLOCK
      }
   }}
      // END BLOCK
      {
// (1,11)
         // BEGIN BLOCK
      {{
// (1,12)
         int j;
// (1,13)
         
         // Second loop
         for (j = 32;j>(-34);j--)
// (1,14)
            b -= a;
      }}
         // END BLOCK
      }
// (1,15)
      
      /* after the loop */
      return 0;
   }}
      // END BLOCK
   }
//:   END UNSTRUCTURED
}

//  P() {}

int main()
{

//  P() {}

// (0,1)
//:   BEGIN UNSTRUCTURED
   {

//  P() {}

// (1,1)
      // BEGIN BLOCK
   {{

//  P() {}

// (1,2)
      // BEGIN BLOCK
   {{

//  P() {}

// (1,3)
      // BEGIN BLOCK
   {{

//  P() {}

// (1,4)
      double a, b;

//  P() {}

// (1,5)

      b = 1;

//  P() {}

// (1,6)
      a = (b*b);
   }}
      // END BLOCK
      {

//  P() {}

// (1,7)
         // BEGIN BLOCK
      {{

//  P() {}

// (1,8)
         int i;

//  P(i) {}

// (1,9)
         
         /* First loop */
         for (i = 0;i<67;i++)

//  P(i) {0<=i, i<=66}

// (1,10)
            a += b;
      }}
         // END BLOCK
      }
   }}
      // END BLOCK
      {

//  P() {}

// (1,11)
         // BEGIN BLOCK
      {{

//  P() {}

// (1,12)
         int j;

//  P(j) {}

// (1,13)
         
         // Second loop
         for (j = 32;j>(-34);j--)

//  P(j) {0<=j+33, j<=32}

// (1,14)
            b -= a;
      }}
         // END BLOCK
      }

//  P() {}

// (1,15)
      
      /* after the loop */
      return 0;
   }}
      // END BLOCK
   }
//:   END UNSTRUCTURED
}
