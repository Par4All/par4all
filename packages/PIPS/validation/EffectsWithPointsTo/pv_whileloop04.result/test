
int main()
{
//             <    is written>: i
   int i = 0;
   int *p[10];
   int a[10];
//             <    is read   >: i

   while (i<10) {
//             <    is read   >: i
//             <    is written>: p[i]
      p[i] = &a[i];
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
   return 0;
}

