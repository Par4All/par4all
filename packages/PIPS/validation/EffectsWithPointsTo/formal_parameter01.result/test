
int formal_parameter01(int **pi)
{
   int **q;
   int *i;
//             <    is written>: i

   i = 0;
//             <    is read   >: pi
//             <    is written>: q
   q = pi;
//             <    is written>: pi
   pi = &i;
   return 0;
}

