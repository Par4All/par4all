
int main()
{
   mystruct s1;
   mystruct s2;
//             <may be read   >: s1.b[*]
//             <may be written>: s2.b[*]
//             <    is read   >: s1.a s1.c
//             <    is written>: s2.a s2.c
   s2 = s1;
   return 0;
}

