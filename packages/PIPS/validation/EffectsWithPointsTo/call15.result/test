
Points-to for "call15"


// Points To IN:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY
// q -> *NULL_POINTER* , MAY
// q -> _q_2[0] , MAY


// Points To OUT: none

void call15(int *pi, tf_t *q)
{

// Points To:
// pi -> *NULL_POINTER* , MAY
// pi -> _pi_1[0] , MAY

   *pi = 1;

// Points To:
// pi -> _pi_1[0] , EXACT
// q -> *NULL_POINTER* , MAY
// q -> _q_2[0] , MAY

   q->one = 1;
}

Proper effects with points-to for "call15"

void call15(int *pi, tf_t *q)
{
//             <    is read   >: pi
//             <    is written>: _pi_1[0]
   *pi = 1;
//             <    is read   >: q
//             <    is written>: _q_2[0].one
   q->one = 1;
}

Cumulated effects with points-to for "call15"

//             <    is read   >: pi q
//             <    is written>: _pi_1[0] _q_2[0].one
void call15(int *pi, tf_t *q)
{
//             <    is read   >: pi
//             <    is written>: _pi_1[0]
   *pi = 1;
//             <    is read   >: q
//             <    is written>: _q_2[0].one
   q->one = 1;
}

Points-to for "main"


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   tf_t s;

// Points To: none


   call15(&i, &s);
}

Proper effects with points-to for "main"

int main()
{
   int i;
   tf_t s;
