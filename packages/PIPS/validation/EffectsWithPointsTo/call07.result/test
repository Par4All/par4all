void call07(int r, int m, int *x)
{
   int j;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: r x

   x[4] = 10*r+4;
}
int main()
{
//             <    is written>: d1
   int d1 = 4;
//             <    is written>: d2
   int d2 = 4;
   int y[d1][d2];
   int i, j;
//             <    is read   >: d1
//             <    is written>: i

   for(i = 0; i <= d1-1; i += 1)
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: d1 d2 i
      call07(i, d2, &y[i][0]);
}

