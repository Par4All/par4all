
PROPER  EFFECTS 

void call01(int *pi, tf_t *q)
{
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: pi
   *pi = 1;
//             <    is read   >: pi
//             <    is written>: pi
   /* pi is passed by value: pi++ does not generate a summary effect
        for call01 */
   pi++;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   q->one = 1;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*
//             <    is read   >: q
   (q->two)[4] = 2;
}
int main()
{
   int i;
   tf_t s;
//             <may be written>: *ANY_MODULE*:*ANYWHERE*

   call01(&i, &s);
}

POINTS-TO


// Points To IN:
// pi -> _pi_1[0] , EXACT
// q -> _q_2[0] , EXACT


// Points To OUT: none

void call01(int *pi, tf_t *q)
{

// Points To:
// pi -> _pi_1[0] , EXACT
// q -> _q_2[0] , EXACT

   *pi = 1;

// Points To:
// pi -> _pi_1[0] , EXACT
// q -> _q_2[0] , EXACT

   /* pi is passed by value: pi++ does not generate a summary effect
        for call01 */
   pi++;

// Points To:
// pi -> _pi_1[*] , EXACT
// q -> _q_2[0] , EXACT

   q->one = 1;

// Points To:
// pi -> _pi_1[*] , EXACT
// q -> _q_2[0] , EXACT

   (q->two)[4] = 2;
}

// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i;

// Points To: none

   tf_t s;

// Points To: none


   call01(&i, &s);
}

PROPER  EFFECTS WITH POINTS TO

void call01(int *pi, tf_t *q)
{
//             <    is read   >: pi
//             <    is written>: _pi_1[0]
   *pi = 1;
//             <    is read   >: pi
//             <    is written>: pi
   /* pi is passed by value: pi++ does not generate a summary effect
        for call01 */
   pi++;
//             <    is read   >: q
//             <    is written>: _q_2[0].one
   q->one = 1;
//             <    is read   >: q
//             <    is written>: _q_2[0].two[4]
   (q->two)[4] = 2;
}
int main()
{
   int i;
   tf_t s;

   call01(&i, &s);
}

