
Initial code

float dotprod(int n, float b[n], float c[n])
{
   float a;
   int i;
   for(i = 0; i <= n-1; i += 1)
      a += inner_loop(i, b, c);
   
   
   return a;
}

After Unfolding

float dotprod(int n, float b[n], float c[n])
{
   float a;
   int i;
   //PIPS generated variable
   float _return0;
   for(i = 0; i <= n-1; i += 1) {
      _return0 = b[i]+c[i];
      a += _return0;
   }
   
   
   return a;
}
