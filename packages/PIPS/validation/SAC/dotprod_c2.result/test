simdized code

float dotprod(int n, float b[n], float c[n])
{
   float a;
   int i;
   //PIPS generated variable
   int LU_NUB0, LU_IB0, LU_IND0;
   //PIPS generated variable
   float RED0[0+1], RED1[3+1], _return00, _return01, _return02, _return03, a0, _return04;
   //PIPS:SAC generated float vector(s)
   float v4sf_vec0[3+1], v4sf_vec1[3+1], v4sf_vec2[3+1], v4sf_vec3[3+1];
   LU_NUB0 = n;
   LU_IB0 = MOD(LU_NUB0, 4);
   RED0[0] = 0.000000;
   for(LU_IND0 = 0; LU_IND0 <= LU_IB0-1; LU_IND0 += 1) {
      _return04 = b[LU_IND0]+c[LU_IND0];
      RED0[0] = RED0[0]+_return04;
   }
   a0 = RED0[0]+a0;
   RED1[0] = 0.000000;
   RED1[1] = 0.000000;
   RED1[2] = 0.000000;
   RED1[3] = 0.000000;
   SIMD_LOAD_V4SF(v4sf_vec3, &RED1[0]);
   for(LU_IND0 = LU_IB0; LU_IND0 <= LU_NUB0-1; LU_IND0 += 4) {
      SIMD_LOAD_V4SF(v4sf_vec2, &c[LU_IND0]);
      SIMD_LOAD_V4SF(v4sf_vec1, &b[LU_IND0]);
      SIMD_ADDPS(v4sf_vec0, v4sf_vec1, v4sf_vec2);
      SIMD_ADDPS(v4sf_vec3, v4sf_vec3, v4sf_vec0);
   }
   SIMD_SAVE_GENERIC_V4SF(v4sf_vec0, &_return03, &_return02, &_return01, &_return00);
   SIMD_SAVE_V4SF(v4sf_vec3, &RED1[0]);
   a = RED1[3]+RED1[2]+RED1[1]+RED1[0]+a0;
   i = MAX0(LU_NUB0, 0);
   
   
   return a;
