      SUBROUTINE DOTPROD(A, B, C)

! include "DOTPROD_INC.f"
        INTEGER*2 SIZE
        PARAMETER (SIZE=10000)
! end include "DOTPROD_INC.f"
!       INCLUDE 'DOTPROD_INC.f'

      INTEGER I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
      DO 10 I = 1, SIZE                                                 0013
         A(1) = A(1)+B(I)*C(I)                                          0014
10       CONTINUE                                                       0015

      END
