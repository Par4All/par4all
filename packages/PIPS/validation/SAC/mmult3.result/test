void Matrix_Mult(float a[3][3], float b[3][3], float c[3][3])
{
   int i, j, k;
loop0:
   for(i = 0; i <= 2; i += 1)
loop1:
      for(j = 0; j <= 2; j += 1) {
         c[i][j] = 0;
loop2:
         for(k = 0; k <= 2; k += 1)
            c[i][j] = c[i][j]+a[i][k]*b[k][j];
      }
}
