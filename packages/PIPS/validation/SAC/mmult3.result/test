void Matrix_Mult(float a[3][3], float b[3][3], float c[3][3])
{
   int i, j, k;
   //PIPS generated variable
   float F_0, F_1, F_2, F_3, F_4, F_5, F_6, F_7, F_8;
loop0:
   for(i = 0; i <= 2; i += 1) {
loop1:      ;
      c[i][0] = 0;
      F_0 = a[i][0]*b[0][0];
      c[i][0] = c[i][0]+F_0;
      F_1 = a[i][1]*b[1][0];
      c[i][0] = c[i][0]+F_1;
      F_2 = a[i][2]*b[2][0];
      c[i][0] = c[i][0]+F_2;
      k = 3;
      c[i][1] = 0;
      F_3 = a[i][0]*b[0][1];
      c[i][1] = c[i][1]+F_3;
      F_4 = a[i][1]*b[1][1];
      c[i][1] = c[i][1]+F_4;
      F_5 = a[i][2]*b[2][1];
      c[i][1] = c[i][1]+F_5;
      k = 3;
      c[i][2] = 0;
      F_6 = a[i][0]*b[0][2];
      c[i][2] = c[i][2]+F_6;
      F_7 = a[i][1]*b[1][2];
      c[i][2] = c[i][2]+F_7;
      F_8 = a[i][2]*b[2][2];
      c[i][2] = c[i][2]+F_8;
      k = 3;
      j = 3;
   }
}
