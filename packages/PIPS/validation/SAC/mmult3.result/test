simdized code

void Matrix_Mult(float a[3][3], float b[3][3], float c[3][3])
{
   int j;
   //PIPS generated variable
   float F_0, F_1, F_2, F_3, F_4, F_5, F_6, F_7, F_8;
   //PIPS generated variable
   int i0, k0, k1, k2;
   //SAC generated temporary array
   a4sf pdata0 = {0, 0, 0, 0}, pdata1 = {0, 0, 0, 0};
   //PIPS:SAC generated variable
   overloaded PADDING_VALUE;
loop0:
   for(i0 = 0; i0 <= 2; i0 += 1) {
      //PIPS:SAC generated v4sf vector(s)
      v4sf vec00, vec10, vec20, vec30, vec40, vec50, vec60, vec70, vec80, vec90, vec100, vec110;
loop1:      ;
      c[i0][0] = 0;
      SIMD_LOAD_V4SF(vec20, &b[0][0]);
      SIMD_LOAD_GENERIC_V4SF(vec10, a[i0][0], a[i0][0], a[i0][0], a[i0][1]);
      SIMD_MULPS(vec00, vec10, vec20);
      SIMD_STORE_V4SF(vec00, &pdata0[0]);
      SIMD_LOAD_V4SF(vec50, &b[1][1]);
      SIMD_LOAD_GENERIC_V4SF(vec40, a[i0][1], a[i0][1], a[i0][2], a[i0][2]);
      SIMD_MULPS(vec30, vec40, vec50);
      SIMD_STORE_V4SF(vec30, &pdata1[0]);
      pdata0[0] = a[i0][0]*b[0][0];
      k0 = 3;
      c[i0][1] = 0;
      k1 = 3;
      c[i0][2] = 0;
      F_8 = a[i0][2]*b[2][2];
      k2 = 3;
      j = 3;
      SIMD_LOAD_V4SF(vec70, &pdata0[0]);
      SIMD_LOAD_V4SF(vec60, &c[i0][0]);
      SIMD_ADDPS(vec60, vec60, vec70);
      SIMD_STORE_MASKED_V4SF(vec60, &c[i0][0]);
      SIMD_LOAD_GENERIC_V4SF(vec90, pdata0[3], pdata1[0], pdata1[1], PADDING_VALUE);
      SIMD_LOAD_V4SF(vec80, &c[i0][0]);
      SIMD_ADDPS(vec80, vec80, vec90);
      SIMD_STORE_MASKED_V4SF(vec80, &c[i0][0]);
      SIMD_LOAD_GENERIC_V4SF(vec110, pdata1[2], pdata1[3], F_8, PADDING_VALUE);
      SIMD_LOAD_V4SF(vec100, &c[i0][0]);
      SIMD_ADDPS(vec100, vec100, vec110);
      SIMD_STORE_MASKED_V4SF(vec100, &c[i0][0]);
   }
}
void Matrix_Mult(float a[3][3], float b[3][3], float c[3][3])
{
   int j;
   //PIPS generated variable
   float F_0, F_1, F_2, F_3, F_4, F_5, F_6, F_7, F_8;
   //PIPS generated variable
   int i0, k0, k1, k2;
   //SAC generated temporary array
   a4sf pdata0 = {0, 0, 0, 0}, pdata1 = {0, 0, 0, 0};
   //PIPS:SAC generated variable
   overloaded PADDING_VALUE;
   //PIPS generated variable
   v4sf vec00_0, vec10_0, vec20_0, vec30_0, vec40_0, vec50_0, vec60_0, vec70_0, vec80_0, vec90_0, vec100_0, vec110_0;
   SIMD_LOAD_V4SF(vec50_0, &b[1][1]);
   SIMD_LOAD_V4SF(vec20_0, &b[0][0]);
loop0:
   for(i0 = 0; i0 <= 2; i0 += 1) {
      //PIPS:SAC generated v4sf vector(s)
      SIMD_LOAD_GENERIC_V4SF(vec40_0, a[i0][1], a[i0][1], a[i0][2], a[i0][2]);
      SIMD_LOAD_GENERIC_V4SF(vec10_0, a[i0][0], a[i0][0], a[i0][0], a[i0][1]);
loop1:      ;
      c[i0][0] = 0;
      SIMD_MULPS(vec00_0, vec10_0, vec20_0);
      SIMD_STORE_V4SF(vec00_0, &pdata0[0]);
      SIMD_MULPS(vec30_0, vec40_0, vec50_0);
      SIMD_STORE_V4SF(vec30_0, &pdata1[0]);
      SIMD_LOAD_GENERIC_V4SF(vec90_0, pdata0[3], pdata1[0], pdata1[1], PADDING_VALUE);
      pdata0[0] = a[i0][0]*b[0][0];
      SIMD_LOAD_V4SF(vec70_0, &pdata0[0]);

      c[i0][1] = 0;

      c[i0][2] = 0;
      SIMD_LOAD_V4SF(vec60_0, &c[i0][0]);
      F_8 = a[i0][2]*b[2][2];
      SIMD_LOAD_GENERIC_V4SF(vec110_0, pdata1[2], pdata1[3], F_8, PADDING_VALUE);


      SIMD_ADDPS(vec60_0, vec60_0, vec70_0);
      SIMD_STORE_MASKED_V4SF(vec60_0, &c[i0][0]);
      SIMD_LOAD_V4SF(vec80_0, &c[i0][0]);
      SIMD_ADDPS(vec80_0, vec80_0, vec90_0);
      SIMD_STORE_MASKED_V4SF(vec80_0, &c[i0][0]);
      SIMD_LOAD_V4SF(vec100_0, &c[i0][0]);
      SIMD_ADDPS(vec100_0, vec100_0, vec110_0);
      SIMD_STORE_MASKED_V4SF(vec100_0, &c[i0][0]);
   }
}
