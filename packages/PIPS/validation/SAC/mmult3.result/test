{
   int i, j, k;
loop0:
   for(i = 0; i <= 2; i += 1) {
loop1:      ;
      c[i][0] = 0;
      c[i][0] = c[i][0]+a[i][0]*b[0][0];
      c[i][0] = c[i][0]+a[i][1]*b[1][0];
      c[i][0] = c[i][0]+a[i][2]*b[2][0];
      k = 3;
      c[i][1] = 0;
      c[i][1] = c[i][1]+a[i][0]*b[0][1];
      c[i][1] = c[i][1]+a[i][1]*b[1][1];
      c[i][1] = c[i][1]+a[i][2]*b[2][1];
