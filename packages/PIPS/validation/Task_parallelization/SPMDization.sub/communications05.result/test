         for(_rtl_0_0_7 = bi; _rtl_0_0_7 <= size-1; _rtl_0_0_7 += 1)
            for(_rtl_0_1_7 = bj; _rtl_0_1_7 <= size-1; _rtl_0_1_7 += 1)
