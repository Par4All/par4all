            for(_rtl_0_1 = 0; _rtl_0_1 <= bj-1; _rtl_0_1 += 1)
               MPI_Recv(&b[_rtl_0_0][_rtl_0_1], 1, MPI_INT, 1, MPI_ANY_TAG, MPI_COMM_WORLD, &status0);

         for(i = 0; i <= size-1; i += 1)
            for(j = 0; j <= size-1; j += 1)
               printf("%d\n", b[i][j]);
      }
   }
   MPI_Finalize();

