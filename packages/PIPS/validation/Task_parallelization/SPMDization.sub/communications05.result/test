         MPI_Recv(&size, 1, MPI_INT, 0, MPI_ANY_TAG, MPI_COMM_WORLD, &status0);
         for(i = 0; i <= bi-1; i += 1)
            for(j = bj; j <= size-1; j += 1)
               b[i][j] = 2*a[i][j];
         for(_rtl_0_0_6 = 0; _rtl_0_0_6 <= bi-1; _rtl_0_0_6 += 1)
            for(_rtl_0_1_6 = bj; _rtl_0_1_6 <= size-1; _rtl_0_1_6 += 1)
