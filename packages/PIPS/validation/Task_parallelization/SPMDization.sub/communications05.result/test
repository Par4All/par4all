               b[i][j] = 2*a[i][j];
         for(_rtl_0_0_4 = bi; _rtl_0_0_4 <= size-1; _rtl_0_0_4 += 1)
            for(_rtl_0_1_4 = 0; _rtl_0_1_4 <= bj-1; _rtl_0_1_4 += 1)
               MPI_Isend(&b[_rtl_0_0_4][_rtl_0_1_4], 1, MPI_INT, 0, 0, MPI_COMM_WORLD, &request0);
