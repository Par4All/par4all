! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <terapix.h>
#include <terapix_ucode.h>

/* FREIA terapix helper function for module freia_42
 * 3 input images, 3 output images
 * 11 image operations in dag
 * dag length is 6, dag width is 3
 * cost is 65 cycles per imagelet row
 */
freia_status freia_42_helper_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  freia_data2d * o2,
  freia_data2d * i0,
  freia_data2d * i1,
  freia_data2d * i2,
  const int32_t * pi6)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // operation 0 parameters
  int16_t p_0[9];
  const int32_t x_0 = 0;
  const int32_t y_0 = 0;
  terapix_mcu_instr mcu_instr[2][8];

  // imagelets definitions:
  // - 5 computation imagelets
  // - 3 double buffer imagelets
  int imagelet_size = 128;
  int imagelet_1 = 0;
  int imagelet_2 = 128;
  int imagelet_3 = 256;
  int imagelet_4 = 384;
  int imagelet_5 = 512;
  int imagelet_6 = 640;
  int imagelet_7 = 768;
  int imagelet_8 = 896;

  // double buffer assignment
  int imagelet_io_1_0 = imagelet_1;
  int imagelet_io_1_1 = imagelet_6;
  int imagelet_io_2_0 = imagelet_2;
  int imagelet_io_2_1 = imagelet_7;
  int imagelet_io_3_0 = imagelet_3;
  int imagelet_io_3_1 = imagelet_8;

  // body:
  // mcode param
  mcode.raw = (void*) terapix_ucode_array;
  mcode.size = TERAPIX_UCODE_SIZE_T;
  freia_mg_write_microcode(&mcode);

  // subimage operation
  param.size = sizeof(mcu_instr);
  param.raw = (void*) mcu_instr;

  // dyn_param contents  dyn_param.raw = &gram;
  dyn_param.size = sizeof(terapix_gram);
  // 0: E8(1) -> 4
  mcu_instr[0][0].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][0].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][0].macrocode.ymin1 = 0;
  mcu_instr[1][0].macrocode.ymin1 = 0;
  mcu_instr[0][0].macrocode.xmin2 = imagelet_4;
  mcu_instr[1][0].macrocode.xmin2 = imagelet_4;
  mcu_instr[0][0].macrocode.ymin2 = 0;
  mcu_instr[1][0].macrocode.ymin2 = 0;
  // copy of operation 0 parameters
  for(i=0; i<9; i++)
    p_0[i] = pi6[i];
  gram.xoffset = x_0;
  gram.yoffset = y_0;
  gram.width = 3;
  gram.height = 3;
  gram.params = p_0;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][0].macrocode.xmin3 = x_0;
  mcu_instr[1][0].macrocode.xmin3 = x_0;
  mcu_instr[0][0].macrocode.ymin3 = y_0;
  mcu_instr[1][0].macrocode.ymin3 = y_0;
  mcu_instr[0][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][0].macrocode.iter2 = imagelet_size;
  mcu_instr[1][0].macrocode.iter2 = imagelet_size;
  mcu_instr[0][0].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  mcu_instr[1][0].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 1: E8(3) -> 5
  mcu_instr[0][1].macrocode.xmin1 = imagelet_io_3_0;
  mcu_instr[1][1].macrocode.xmin1 = imagelet_io_3_1;
  mcu_instr[0][1].macrocode.ymin1 = 0;
  mcu_instr[1][1].macrocode.ymin1 = 0;
  mcu_instr[0][1].macrocode.xmin2 = imagelet_5;
  mcu_instr[1][1].macrocode.xmin2 = imagelet_5;
  mcu_instr[0][1].macrocode.ymin2 = 0;
  mcu_instr[1][1].macrocode.ymin2 = 0;
  mcu_instr[0][1].macrocode.xmin3 = x_0;
  mcu_instr[1][1].macrocode.xmin3 = x_0;
  mcu_instr[0][1].macrocode.ymin3 = y_0;
  mcu_instr[1][1].macrocode.ymin3 = y_0;
  mcu_instr[0][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][1].macrocode.iter2 = imagelet_size;
  mcu_instr[1][1].macrocode.iter2 = imagelet_size;
  mcu_instr[0][1].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  mcu_instr[1][1].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 2: D8(1) -> 3
  mcu_instr[0][2].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][2].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][2].macrocode.ymin1 = 0;
  mcu_instr[1][2].macrocode.ymin1 = 0;
  mcu_instr[0][2].macrocode.xmin2 = imagelet_3;
  mcu_instr[1][2].macrocode.xmin2 = imagelet_3;
  mcu_instr[0][2].macrocode.ymin2 = 0;
  mcu_instr[1][2].macrocode.ymin2 = 0;
  mcu_instr[0][2].macrocode.xmin3 = x_0;
  mcu_instr[1][2].macrocode.xmin3 = x_0;
  mcu_instr[0][2].macrocode.ymin3 = y_0;
  mcu_instr[1][2].macrocode.ymin3 = y_0;
  mcu_instr[0][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][2].macrocode.iter2 = imagelet_size;
  mcu_instr[1][2].macrocode.iter2 = imagelet_size;
  mcu_instr[0][2].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  mcu_instr[1][2].macrocode.addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 3: -(3,4) -> 1
  mcu_instr[0][3].macrocode.xmin1 = imagelet_3;
  mcu_instr[1][3].macrocode.xmin1 = imagelet_3;
  mcu_instr[0][3].macrocode.ymin1 = 0;
  mcu_instr[1][3].macrocode.ymin1 = 0;
  mcu_instr[0][3].macrocode.xmin2 = imagelet_4;
  mcu_instr[1][3].macrocode.xmin2 = imagelet_4;
  mcu_instr[0][3].macrocode.ymin2 = 0;
  mcu_instr[1][3].macrocode.ymin2 = 0;
  mcu_instr[0][3].macrocode.xmin3 = imagelet_io_1_0;
  mcu_instr[1][3].macrocode.xmin3 = imagelet_io_1_1;
  mcu_instr[0][3].macrocode.ymin3 = 0;
  mcu_instr[1][3].macrocode.ymin3 = 0;
  mcu_instr[0][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][3].macrocode.iter2 = imagelet_size;
  mcu_instr[1][3].macrocode.iter2 = imagelet_size;
  mcu_instr[0][3].macrocode.addrStart = TERAPIX_UCODE_SUB3;
  mcu_instr[1][3].macrocode.addrStart = TERAPIX_UCODE_SUB3;
  // 4: -|(2,1) -> 1
  mcu_instr[0][4].macrocode.xmin1 = imagelet_io_2_0;
  mcu_instr[1][4].macrocode.xmin1 = imagelet_io_2_1;
  mcu_instr[0][4].macrocode.ymin1 = 0;
  mcu_instr[1][4].macrocode.ymin1 = 0;
  mcu_instr[0][4].macrocode.xmin2 = imagelet_io_1_0;
  mcu_instr[1][4].macrocode.xmin2 = imagelet_io_1_1;
  mcu_instr[0][4].macrocode.ymin2 = 0;
  mcu_instr[1][4].macrocode.ymin2 = 0;
  mcu_instr[0][4].macrocode.xmin3 = imagelet_io_1_0;
  mcu_instr[1][4].macrocode.xmin3 = imagelet_io_1_1;
  mcu_instr[0][4].macrocode.ymin3 = 0;
  mcu_instr[1][4].macrocode.ymin3 = 0;
  mcu_instr[0][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][4].macrocode.iter2 = imagelet_size;
  mcu_instr[1][4].macrocode.iter2 = imagelet_size;
  mcu_instr[0][4].macrocode.addrStart = TERAPIX_UCODE_ABS_DIFF3;
  mcu_instr[1][4].macrocode.addrStart = TERAPIX_UCODE_ABS_DIFF3;
  // 5: -(3,5) -> 5
  mcu_instr[0][5].macrocode.xmin1 = imagelet_3;
  mcu_instr[1][5].macrocode.xmin1 = imagelet_3;
  mcu_instr[0][5].macrocode.ymin1 = 0;
  mcu_instr[1][5].macrocode.ymin1 = 0;
  mcu_instr[0][5].macrocode.xmin2 = imagelet_5;
  mcu_instr[1][5].macrocode.xmin2 = imagelet_5;
  mcu_instr[0][5].macrocode.ymin2 = 0;
  mcu_instr[1][5].macrocode.ymin2 = 0;
  mcu_instr[0][5].macrocode.xmin3 = imagelet_5;
  mcu_instr[1][5].macrocode.xmin3 = imagelet_5;
  mcu_instr[0][5].macrocode.ymin3 = 0;
  mcu_instr[1][5].macrocode.ymin3 = 0;
  mcu_instr[0][5].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][5].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][5].macrocode.iter2 = imagelet_size;
  mcu_instr[1][5].macrocode.iter2 = imagelet_size;
  mcu_instr[0][5].macrocode.addrStart = TERAPIX_UCODE_SUB3;
  mcu_instr[1][5].macrocode.addrStart = TERAPIX_UCODE_SUB3;
  // 6: -|(1,5) -> 5
  mcu_instr[0][6].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][6].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][6].macrocode.ymin1 = 0;
  mcu_instr[1][6].macrocode.ymin1 = 0;
  mcu_instr[0][6].macrocode.xmin2 = imagelet_5;
  mcu_instr[1][6].macrocode.xmin2 = imagelet_5;
  mcu_instr[0][6].macrocode.ymin2 = 0;
  mcu_instr[1][6].macrocode.ymin2 = 0;
  mcu_instr[0][6].macrocode.xmin3 = imagelet_5;
  mcu_instr[1][6].macrocode.xmin3 = imagelet_5;
  mcu_instr[0][6].macrocode.ymin3 = 0;
  mcu_instr[1][6].macrocode.ymin3 = 0;
  mcu_instr[0][6].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][6].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][6].macrocode.iter2 = imagelet_size;
  mcu_instr[1][6].macrocode.iter2 = imagelet_size;
  mcu_instr[0][6].macrocode.addrStart = TERAPIX_UCODE_ABS_DIFF3;
  mcu_instr[1][6].macrocode.addrStart = TERAPIX_UCODE_ABS_DIFF3;
  // 7: <(5,1) -> 1
  mcu_instr[0][7].macrocode.xmin1 = imagelet_5;
  mcu_instr[1][7].macrocode.xmin1 = imagelet_5;
  mcu_instr[0][7].macrocode.ymin1 = 0;
  mcu_instr[1][7].macrocode.ymin1 = 0;
  mcu_instr[0][7].macrocode.xmin2 = imagelet_io_1_0;
  mcu_instr[1][7].macrocode.xmin2 = imagelet_io_1_1;
  mcu_instr[0][7].macrocode.ymin2 = 0;
  mcu_instr[1][7].macrocode.ymin2 = 0;
  mcu_instr[0][7].macrocode.xmin3 = imagelet_io_1_0;
  mcu_instr[1][7].macrocode.xmin3 = imagelet_io_1_1;
  mcu_instr[0][7].macrocode.ymin3 = 0;
  mcu_instr[1][7].macrocode.ymin3 = 0;
  mcu_instr[0][7].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][7].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][7].macrocode.iter2 = imagelet_size;
  mcu_instr[1][7].macrocode.iter2 = imagelet_size;
  mcu_instr[0][7].macrocode.addrStart = TERAPIX_UCODE_INF3;
  mcu_instr[1][7].macrocode.addrStart = TERAPIX_UCODE_INF3;

  // inputs:
  // - imagelet 1 is i0 for i0
  // - imagelet 2 is i1 for i2
  // - imagelet 3 is i2 for i4

  // outputs:
  // - imagelet 1 is o0 for i1
  // - imagelet 1 is o1 for i1
  // - imagelet 1 is o2 for i7

  // double buffer management:
  // - buffer 1/6
  // - buffer 2/7
  // - buffer 3/8

  // output imagelet erosion:
  //   north: 1
  //   south: 1
  //    west: 1
  //    east: 1

  ret = freia_cg_template_process_3i_3o(&param, o0, o1, o2, i0, i1, i2);

  // extract measures

  return ret;
}


! updated function

int freia_42(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdout;                                                  /*0007*/

   freia_data2d *i0;                                                    /*0009*/
   freia_data2d *i1;                                                    /*0010*/
   freia_data2d *i2;                                                    /*0011*/
   freia_data2d *i4;                                                    /*0013*/
   freia_data2d *i7;                                                    /*0015*/

   freia_common_open_input(&fdin, 0);                                   /*0018*/

   i0 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0020*/
   i1 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0021*/
   i2 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0022*/
   i4 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0024*/
   i7 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0027*/

   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, 8);          /*0029*/

   freia_common_rx_image(i0, &fdin);                                    /*0031*/
   freia_common_rx_image(i2, &fdin);                                    /*0032*/
   freia_common_rx_image(i4, &fdin);                                    /*0033*/

   freia_42_helper_0_0(i1, i1, i7, i0, i2, i4, freia_morpho_kernel_8c);                /*0035*/

   freia_common_tx_image(i7, &fdout);                                   /*0047*/

   freia_common_destruct_data(i0);                                      /*0049*/
   freia_common_destruct_data(i1);                                      /*0050*/
   freia_common_destruct_data(i2);                                      /*0051*/
   freia_common_destruct_data(i4);                                      /*0053*/
   freia_common_destruct_data(i7);                                      /*0056*/

   freia_common_close_input(&fdin);                                     /*0058*/
   freia_common_close_output(&fdout);                                   /*0059*/
   return 0;                                                            /*0060*/
}

! cleaned up function

int freia_42(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdout;                                                  /*0007*/

   freia_data2d *i0;                                                    /*0009*/
   freia_data2d *i1;                                                    /*0010*/
   freia_data2d *i2;                                                    /*0011*/
   freia_data2d *i4;                                                    /*0013*/
   freia_data2d *i7;                                                    /*0015*/

   freia_common_open_input(&fdin, 0);                                   /*0018*/

   i0 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0020*/
   i1 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0021*/
   i2 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0022*/
   i4 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0024*/
   i7 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0027*/

   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, 8);          /*0029*/

   freia_common_rx_image(i0, &fdin);                                    /*0031*/
   freia_common_rx_image(i2, &fdin);                                    /*0032*/
   freia_common_rx_image(i4, &fdin);                                    /*0033*/

   freia_42_helper_0_0(i1, i1, i7, i0, i2, i4, freia_morpho_kernel_8c);                /*0035*/

   freia_common_tx_image(i7, &fdout);                                   /*0047*/

   freia_common_destruct_data(i0);                                      /*0049*/
   freia_common_destruct_data(i1);                                      /*0050*/
   freia_common_destruct_data(i2);                                      /*0051*/
   freia_common_destruct_data(i4);                                      /*0053*/
   freia_common_destruct_data(i7);                                      /*0056*/

   freia_common_close_input(&fdin);                                     /*0058*/
   freia_common_close_output(&fdout);                                   /*0059*/
   return 0;                                                            /*0060*/
}
