! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 5 ops and 0 copies, optimized to 5 ops and 0+0+0 copies

// hold kernels for freia_53_opencl_helper_D8_511
static cl_kernel freia_53_opencl_helper_D8_511_kernel[2];

// compile kernels for freia_53_opencl_helper_D8_511
static freia_status freia_53_opencl_helper_D8_511_compile(void)
{
  // OpenCL source for freia_53_opencl_helper_D8_511
  const char * freia_53_opencl_helper_D8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_53_opencl_helper_D8_511\n"
    "KERNEL void freia_53_opencl_helper_D8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t9 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "\n"
    "    // pixel computations\n"
    "    t9 = PIXEL_SUP(t9, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t9 = PIXEL_SUP(t9, (is_N)? PIXEL_MIN: in0N);\n"
    "    t9 = PIXEL_SUP(t9, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t9 = PIXEL_SUP(t9, (is_W)? PIXEL_MIN: in0W);\n"
    "    t9 = PIXEL_SUP(t9, in0);\n"
    "    t9 = PIXEL_SUP(t9, (is_E)? PIXEL_MIN: in0E);\n"
    "    t9 = PIXEL_SUP(t9, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t9 = PIXEL_SUP(t9, (is_S)? PIXEL_MIN: in0S);\n"
    "    t9 = PIXEL_SUP(t9, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t9;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_53_opencl_helper_D8_511_source, "freia_53_opencl_helper_D8_511", "-DPIXEL8", &freia_53_opencl_helper_D8_511_kernel[0]);
  err |= freia_op_compile_kernel(freia_53_opencl_helper_D8_511_source, "freia_53_opencl_helper_D8_511", "-DPIXEL16", &freia_53_opencl_helper_D8_511_kernel[1]);
  return err;
}

// helper function freia_53_opencl_helper_D8_511
freia_status freia_53_opencl_helper_D8_511(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_53_opencl_helper_D8_511_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_53_opencl_helper_D8_511_kernel[bpp];

  // call kernel freia_53_opencl_helper_D8_511
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}

// hold kernels for freia_53_opencl_helper_0_0_1
static cl_kernel freia_53_opencl_helper_0_0_1_kernel[2];

// compile kernels for freia_53_opencl_helper_0_0_1
static freia_status freia_53_opencl_helper_0_0_1_compile(void)
{
  // OpenCL source for freia_53_opencl_helper_0_0_1
  const char * freia_53_opencl_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_53_opencl_helper_0_0_1\n"
    "KERNEL void freia_53_opencl_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t11 = PIXEL_MAX;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "    PIXEL in1 = j1[i];\n"
    "\n"
    "    // pixel computations\n"
    "    t11 = PIXEL_INF(t11, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t11 = PIXEL_INF(t11, (is_N)? PIXEL_MAX: in0N);\n"
    "    t11 = PIXEL_INF(t11, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t11 = PIXEL_INF(t11, (is_W)? PIXEL_MAX: in0W);\n"
    "    t11 = PIXEL_INF(t11, in0);\n"
    "    t11 = PIXEL_INF(t11, (is_E)? PIXEL_MAX: in0E);\n"
    "    t11 = PIXEL_INF(t11, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t11 = PIXEL_INF(t11, (is_S)? PIXEL_MAX: in0S);\n"
    "    t11 = PIXEL_INF(t11, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "    PIXEL t10 = PIXEL_INF(in0, c0);\n"
    "    PIXEL t12 = PIXEL_INF(t10, t11);\n"
    "    PIXEL t13 = PIXEL_SUB(in1, t12);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t13;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_53_opencl_helper_0_0_1_source, "freia_53_opencl_helper_0_0_1", "-DPIXEL8", &freia_53_opencl_helper_0_0_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_53_opencl_helper_0_0_1_source, "freia_53_opencl_helper_0_0_1", "-DPIXEL16", &freia_53_opencl_helper_0_0_1_kernel[1]);
  return err;
}

// helper function freia_53_opencl_helper_0_0_1
freia_status freia_53_opencl_helper_0_0_1(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_53_opencl_helper_0_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_53_opencl_helper_0_0_1_kernel[bpp];

  // call kernel freia_53_opencl_helper_0_0_1
  err |= freia_op_call_kernel(kernel, 1, 2, 1, 0, o0, i0, i1, c0);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_53

// opencl for dag 0

// opencl function freia_53_opencl_helper_D8_511
KERNEL void freia_53_opencl_helper_D8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t9 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t9 = PIXEL_SUP(t9, (is_N|is_W)? PIXEL_MIN: in0NW);
    t9 = PIXEL_SUP(t9, (is_N)? PIXEL_MIN: in0N);
    t9 = PIXEL_SUP(t9, (is_N|is_E)? PIXEL_MIN: in0NE);
    t9 = PIXEL_SUP(t9, (is_W)? PIXEL_MIN: in0W);
    t9 = PIXEL_SUP(t9, in0);
    t9 = PIXEL_SUP(t9, (is_E)? PIXEL_MIN: in0E);
    t9 = PIXEL_SUP(t9, (is_S|is_W)? PIXEL_MIN: in0SW);
    t9 = PIXEL_SUP(t9, (is_S)? PIXEL_MIN: in0S);
    t9 = PIXEL_SUP(t9, (is_S|is_E)? PIXEL_MIN: in0SE);

    // set output pixels
    p0[i] = t9;
  }
}

// opencl function freia_53_opencl_helper_0_0_1
KERNEL void freia_53_opencl_helper_0_0_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t11 = PIXEL_MAX;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];
    PIXEL in1 = j1[i];

    // pixel computations
    t11 = PIXEL_INF(t11, (is_N|is_W)? PIXEL_MAX: in0NW);
    t11 = PIXEL_INF(t11, (is_N)? PIXEL_MAX: in0N);
    t11 = PIXEL_INF(t11, (is_N|is_E)? PIXEL_MAX: in0NE);
    t11 = PIXEL_INF(t11, (is_W)? PIXEL_MAX: in0W);
    t11 = PIXEL_INF(t11, in0);
    t11 = PIXEL_INF(t11, (is_E)? PIXEL_MAX: in0E);
    t11 = PIXEL_INF(t11, (is_S|is_W)? PIXEL_MAX: in0SW);
    t11 = PIXEL_INF(t11, (is_S)? PIXEL_MAX: in0S);
    t11 = PIXEL_INF(t11, (is_S|is_E)? PIXEL_MAX: in0SE);
    PIXEL t10 = PIXEL_INF(in0, c0);
    PIXEL t12 = PIXEL_INF(t10, t11);
    PIXEL t13 = PIXEL_SUB(in1, t12);

    // set output pixels
    p0[i] = t13;
  }
}

