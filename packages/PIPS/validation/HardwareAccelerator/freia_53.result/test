   freia_data2d *td = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0007*/

   freia_aipo_dilate_8c(tb, in, freia_morpho_kernel_8c);                /*0009*/
   freia_aipo_inf_const(tc, tb, 255);                                   /*0010*/
