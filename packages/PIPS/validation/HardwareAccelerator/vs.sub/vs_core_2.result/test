// opencl function vs_core_2_opencl_helper_0_1_2
KERNEL void vs_core_2_opencl_helper_0_1_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t39 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];
    PIXEL t40 = PIXEL_MAX;
    PIXEL in1 = j1[i];

    // pixel computations
    t39 = PIXEL_SUP(t39, (is_N|is_W)? PIXEL_MIN: in0NW);
    t39 = PIXEL_SUP(t39, (is_N)? PIXEL_MIN: in0N);
    t39 = PIXEL_SUP(t39, (is_N|is_E)? PIXEL_MIN: in0NE);
    t39 = PIXEL_SUP(t39, (is_W)? PIXEL_MIN: in0W);
    t39 = PIXEL_SUP(t39, in0);
    t39 = PIXEL_SUP(t39, (is_E)? PIXEL_MIN: in0E);
    t39 = PIXEL_SUP(t39, (is_S|is_W)? PIXEL_MIN: in0SW);
    t39 = PIXEL_SUP(t39, (is_S)? PIXEL_MIN: in0S);
    t39 = PIXEL_SUP(t39, (is_S|is_E)? PIXEL_MIN: in0SE);
    t40 = PIXEL_INF(t40, (is_N|is_W)? PIXEL_MAX: in0NW);
    t40 = PIXEL_INF(t40, (is_N)? PIXEL_MAX: in0N);
    t40 = PIXEL_INF(t40, (is_N|is_E)? PIXEL_MAX: in0NE);
    t40 = PIXEL_INF(t40, (is_W)? PIXEL_MAX: in0W);
    t40 = PIXEL_INF(t40, in0);
    t40 = PIXEL_INF(t40, (is_E)? PIXEL_MAX: in0E);
    t40 = PIXEL_INF(t40, (is_S|is_W)? PIXEL_MAX: in0SW);
    t40 = PIXEL_INF(t40, (is_S)? PIXEL_MAX: in0S);
    t40 = PIXEL_INF(t40, (is_S|is_E)? PIXEL_MAX: in0SE);
    PIXEL t41 = PIXEL_SUB(t39, t40);
    PIXEL t43 = PIXEL_ADD(t41, c0);
    PIXEL t44 = PIXEL_AND(t43, c1);
    PIXEL t45 = PIXEL_MUL(in1, t44);

    // set output pixels
    p0[i] = t45;
  }
}

! updated function

freia_status vs_core_2(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   
   
   
   
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);           /*0012*/
   int binvalue, maxmotion, minmotion;                                  /*0013*/

   vs_core_2_opencl_helper_0_0_0(mv, bg, bg, in, motion_a, motion_b, &maxmotion, &minmotion, motion_a+motion_b);     /*0015*/
   // was cast
   // was cast
   
   // measure
   
   // bg update
   if (maxmotion-minmotion>motion_trig)                                 /*0029*/
      binvalue = maxmotion*motion_th/100;                               /*0030*/

   freia_aipo_threshold(mv, mv, binvalue, 255, true);                   /*0032*/
   
   // open
   vs_core_2_opencl_helper_E8_511(t1, mv);                              /*0035*/
   vs_core_2_opencl_helper_D8_511(t1, t1);                              /*0036*/
   
   // gradient
   vs_core_2_opencl_helper_0_1_2(mv, t1, stab, -1, 1);                  /*0039*/
   
   
   freia_common_destruct_data(t1);                                      /*0048*/
   return FREIA_OK;                                                     /*0050*/
}

! cleaned up function

freia_status vs_core_2(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   
   
   
   
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);           /*0012*/
   int binvalue, maxmotion, minmotion;                                  /*0013*/

   vs_core_2_opencl_helper_0_0_0(mv, bg, bg, in, motion_a, motion_b, &maxmotion, &minmotion, motion_a+motion_b);     /*0015*/
   // was cast
   // was cast
   
   // measure
   
   // bg update
   if (maxmotion-minmotion>motion_trig)                                 /*0029*/
      binvalue = maxmotion*motion_th/100;                               /*0030*/

   freia_aipo_threshold(mv, mv, binvalue, 255, 1);                      /*0032*/
   
   // open
   vs_core_2_opencl_helper_E8_511(t1, mv);                              /*0035*/
   vs_core_2_opencl_helper_D8_511(t1, t1);                              /*0036*/
   
   // gradient
   vs_core_2_opencl_helper_0_1_2(mv, t1, stab, -1, 1);                  /*0039*/
   
   
   freia_common_destruct_data(t1);                                      /*0048*/
   return 0;                                                            /*0050*/
}
