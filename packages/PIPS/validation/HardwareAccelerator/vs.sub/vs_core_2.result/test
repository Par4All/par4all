#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function vs_core_2

// opencl for dag 0

// opencl function vs_core_2_opencl_helper_0_0_0
KERNEL void vs_core_2_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  GLOBAL TMeasure * redX)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // get input pixels
    PIXEL in1 = j1[i];
    PIXEL in0 = j0[i];

    // pixel computations
    PIXEL t15 = PIXEL_ABSDIFF(in1, in0);
    PIXEL t19 = PIXEL_MUL(in1, c0);
    PIXEL t20 = PIXEL_MUL(in0, c1);
    PIXEL t21 = PIXEL_ADD(t20, t19);
    PIXEL_MAXIMUM(red26, t15);
    PIXEL_MINIMUM(red27, t15);
    PIXEL t22 = PIXEL_DIV(t21, c2);

    // set output pixels
    p0[i] = t15;
    p1[i] = t22;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].max = maxv;
  redX[thrid].min = minv;
}

// opencl function vs_core_2_opencl_helper_D8_511
KERNEL void vs_core_2_opencl_helper_D8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
