! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 16 ops and 2 copies, optimized to 16 ops and 0+0+0 copies

// hold kernels for vs_core_2_opencl_helper_0_0_0
static cl_kernel vs_core_2_opencl_helper_0_0_0_kernel[2];

// compile kernels for vs_core_2_opencl_helper_0_0_0
static freia_status vs_core_2_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for vs_core_2_opencl_helper_0_0_0
  const char * vs_core_2_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_2_opencl_helper_0_0_0\n"
    "KERNEL void vs_core_2_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in0 = j0[i];\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t15 = PIXEL_ABSDIFF(in1, in0);\n"
    "    PIXEL t19 = PIXEL_MUL(in1, c0);\n"
    "    PIXEL t20 = PIXEL_MUL(in0, c1);\n"
    "    PIXEL t21 = PIXEL_ADD(t20, t19);\n"
    "    PIXEL_MAXIMUM(red26, t15);\n"
    "    PIXEL_MINIMUM(red27, t15);\n"
    "    PIXEL t22 = PIXEL_DIV(t21, c2);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t15;\n"
    "    p1[i] = t22;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].max = maxv;\n"
    "  redX[thrid].min = minv;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_0_0_source, "vs_core_2_opencl_helper_0_0_0", "-DPIXEL8", &vs_core_2_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_0_0_source, "vs_core_2_opencl_helper_0_0_0", "-DPIXEL16", &vs_core_2_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function vs_core_2_opencl_helper_0_0_0
freia_status vs_core_2_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t * po4,
  int32_t * po5,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_2_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_2_opencl_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel vs_core_2_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 2, 3, 1, o0, o1, i0, i1, c0, c1, c2, &redres);

  // return reduction results
  *po4 = redres.maximum;
  *po5 = redres.minimum;

  return err;
}

// hold kernels for vs_core_2_opencl_helper_D8_511
static cl_kernel vs_core_2_opencl_helper_D8_511_kernel[2];

// compile kernels for vs_core_2_opencl_helper_D8_511
static freia_status vs_core_2_opencl_helper_D8_511_compile(void)
{
  // OpenCL source for vs_core_2_opencl_helper_D8_511
  const char * vs_core_2_opencl_helper_D8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_2_opencl_helper_D8_511\n"
    "KERNEL void vs_core_2_opencl_helper_D8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t36 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "\n"
    "    // pixel computations\n"
    "    t36 = PIXEL_SUP(t36, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t36 = PIXEL_SUP(t36, (is_N)? PIXEL_MIN: in0N);\n"
    "    t36 = PIXEL_SUP(t36, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t36 = PIXEL_SUP(t36, (is_W)? PIXEL_MIN: in0W);\n"
    "    t36 = PIXEL_SUP(t36, in0);\n"
    "    t36 = PIXEL_SUP(t36, (is_E)? PIXEL_MIN: in0E);\n"
    "    t36 = PIXEL_SUP(t36, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t36 = PIXEL_SUP(t36, (is_S)? PIXEL_MIN: in0S);\n"
    "    t36 = PIXEL_SUP(t36, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t36;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_D8_511_source, "vs_core_2_opencl_helper_D8_511", "-DPIXEL8", &vs_core_2_opencl_helper_D8_511_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_D8_511_source, "vs_core_2_opencl_helper_D8_511", "-DPIXEL16", &vs_core_2_opencl_helper_D8_511_kernel[1]);
  return err;
}

// helper function vs_core_2_opencl_helper_D8_511
freia_status vs_core_2_opencl_helper_D8_511(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_2_opencl_helper_D8_511_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_2_opencl_helper_D8_511_kernel[bpp];

  // call kernel vs_core_2_opencl_helper_D8_511
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}

// hold kernels for vs_core_2_opencl_helper_E8_511
static cl_kernel vs_core_2_opencl_helper_E8_511_kernel[2];

// compile kernels for vs_core_2_opencl_helper_E8_511
static freia_status vs_core_2_opencl_helper_E8_511_compile(void)
{
  // OpenCL source for vs_core_2_opencl_helper_E8_511
  const char * vs_core_2_opencl_helper_E8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_2_opencl_helper_E8_511\n"
    "KERNEL void vs_core_2_opencl_helper_E8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t35 = PIXEL_MAX;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
