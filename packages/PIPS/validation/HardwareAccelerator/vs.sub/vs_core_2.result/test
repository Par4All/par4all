    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t36 = PIXEL_SUP(t36, (is_N|is_W)? PIXEL_MIN: in0NW);
    t36 = PIXEL_SUP(t36, (is_N)? PIXEL_MIN: in0N);
    t36 = PIXEL_SUP(t36, (is_N|is_E)? PIXEL_MIN: in0NE);
