    "    t35 = PIXEL_INF(t35, (is_N)? PIXEL_MAX: in0N);\n"
    "    t35 = PIXEL_INF(t35, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t35 = PIXEL_INF(t35, (is_W)? PIXEL_MAX: in0W);\n"
    "    t35 = PIXEL_INF(t35, in0);\n"
    "    t35 = PIXEL_INF(t35, (is_E)? PIXEL_MAX: in0E);\n"
    "    t35 = PIXEL_INF(t35, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t35 = PIXEL_INF(t35, (is_S)? PIXEL_MAX: in0S);\n"
    "    t35 = PIXEL_INF(t35, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t35;\n"
    "  }\n"
    "}\n";
