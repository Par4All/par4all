  si.mux[3][3].op = SPOC_MUX_IN1;
  // 0`out_102 [4 input 1] -> [4 poc 1] 326 erode_8c
  // nope
  // poc 326 erode_8c stage 4 side 1
  si.poc[4][1].op = SPOC_POC_ERODE;
  si.poc[4][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][1].kernel[i] = pi3[i];


