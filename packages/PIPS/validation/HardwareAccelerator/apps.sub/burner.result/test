  mcu_macro[1][16].addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 17: E8(3) -> 2
  mcu_macro[0][17].xmin1 = imagelet_3;
  mcu_macro[1][17].xmin1 = imagelet_3;
