  si.poc[7][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[7][0].kernel[i] = pi0[i];


  // STAGE 0
  // 0`out_178 [0 input 1] -> [0 poc 1] 250 dilate_8c
  // nope
  // poc 250 dilate_8c stage 0 side 1
