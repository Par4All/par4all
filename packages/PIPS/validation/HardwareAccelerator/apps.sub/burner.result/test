  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi2[i];


  // STAGE 2
  // 0`out_104 [1 poc 1] -> [2 poc 1] 324 dilate_8c
