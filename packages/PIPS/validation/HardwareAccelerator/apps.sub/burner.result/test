  mcu_macro[1][12].addrStart = TERAPIX_UCODE_DILATE_3_3;
  // 13: D8(2) -> 1
  mcu_macro[0][13].xmin1 = imagelet_io_2_0;
  mcu_macro[1][13].xmin1 = imagelet_io_2_1;
  mcu_macro[0][13].ymin1 = 0;
  mcu_macro[1][13].ymin1 = 0;
