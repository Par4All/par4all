  mcu_macro[1][18].xmin1 = imagelet_3;
  mcu_macro[0][18].ymin1 = 0;
  mcu_macro[1][18].ymin1 = 0;
  mcu_macro[0][18].xmin2 = imagelet_io_2_0;
