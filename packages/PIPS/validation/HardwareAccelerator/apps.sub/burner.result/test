  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi1[i];


  // STAGE 1
  // 0`out_177 [0 poc 1] -> [1 poc 1] 251 dilate_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`out_177 [1 input 1] -> [1 poc 1] 251 dilate_8c
