  si.mux[1][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`imWork1 [1 output 0] -> [7 output 0] 0 null
  // 0`imWork1 [2 input 0] -> [7 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;
