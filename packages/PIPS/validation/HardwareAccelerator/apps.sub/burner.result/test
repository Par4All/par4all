  si.poc[4][0].op = SPOC_POC_ERODE;
  si.poc[4][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][0].kernel[i] = pi0[i];


  // STAGE 5
  // 0`imWork1_6 [4 poc 0] -> [5 poc 0] 222 erode_8c
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`imWork1_6 [5 input 0] -> [5 poc 0] 222 erode_8c
