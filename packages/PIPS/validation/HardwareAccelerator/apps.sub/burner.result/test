  mcu_macro[1][15].addrStart = TERAPIX_UCODE_ERODE_3_3;
  // 16: E8(2) -> 1
  mcu_macro[0][16].xmin1 = imagelet_io_2_0;
  mcu_macro[1][16].xmin1 = imagelet_io_2_1;
