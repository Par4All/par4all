  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];


  // STAGE 3
  // 0`imWork1_8 [2 poc 0] -> [3 poc 0] 220 erode_8c
  si.mux[2][0].op = SPOC_MUX_IN0;
  // 0`imWork1_8 [3 input 0] -> [3 poc 0] 220 erode_8c
  // nope
