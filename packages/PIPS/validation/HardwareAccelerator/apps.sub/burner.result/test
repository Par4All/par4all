  si.poc[6][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[6][0].kernel[i] = pi0[i];


  // STAGE 7
  // 0`imWork1_116 [6 poc 0] -> [7 poc 0] 112 dilate_8c
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`imWork1_116 [7 input 0] -> [7 poc 0] 112 dilate_8c
  // nope
