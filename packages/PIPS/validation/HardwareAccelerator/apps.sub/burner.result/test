    sp.poc[3][0].kernel[i] = pi0[i];


  // STAGE 4
  // 0`imWork1_175 [3 poc 0] -> [4 poc 0] 53 dilate_8c
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`imWork1_175 [4 input 0] -> [4 poc 0] 53 dilate_8c
  // nope
  // poc 53 dilate_8c stage 4 side 0
