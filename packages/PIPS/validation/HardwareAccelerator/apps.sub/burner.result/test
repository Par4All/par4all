  // 0`out_173 [5 input 1] -> [5 poc 1] 255 dilate_8c
  // nope
  // poc 255 dilate_8c stage 5 side 1
  si.poc[5][1].op = SPOC_POC_DILATE;
  si.poc[5][1].grid = SPOC_POC_8_CONNEX;
