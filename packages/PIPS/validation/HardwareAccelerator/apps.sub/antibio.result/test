  si.poc[3][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[3][0].kernel[i] = pi0[i];


  // STAGE 4
  // 0`imOut_9 [3 poc 0] -> [4 poc 0] 218 dilate_8c
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`imOut_9 [4 input 0] -> [4 poc 0] 218 dilate_8c
  // nope
  // poc 218 dilate_8c stage 4 side 0
