   imIn = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);  /*0073*/
   imOut = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);                /*0074*/
   
