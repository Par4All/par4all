// opencl function main_opencl_helper_4_0_0
KERNEL void main_opencl_helper_4_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4,
  int c5)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * p1 = o1 + ofs_o1;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  GLOBAL PIXEL * j1 = i1 + ofs_i1;
  GLOBAL PIXEL * j2 = i2 + ofs_i2;

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    // pixel computations
    PIXEL t162 = PIXEL_SUB(in1, in2);
    PIXEL t175 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    PIXEL t164 = PIXEL_THRESHOLD(t162, c3, c4, c5);
    PIXEL t176 = PIXEL_NOT(t175);
    // set output pixels
    p0[i] = t164;
    p1[i] = t176;
  }
}

