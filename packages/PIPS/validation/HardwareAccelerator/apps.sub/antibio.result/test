  // 0`imIn [2 input 1] -> [2 alu] 79 inf
  // nope
  // 0`w1_0_17 [2 poc 0] -> [2 alu] 79 inf
  // nope
  // alu 79 inf stage 2
  si.alu[2][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 3
  // 0`w1_0_16 [2 alu] -> [3 poc 0] 82 dilate_8c
