  return err;
}

// helper function freia_skip_06_opencl_helper_0_0_0
freia_status freia_skip_06_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  int32_t c0,
  int32_t c1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_skip_06_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_skip_06_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_skip_06_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 1, 2, 0, o0, o1, i0, c0, c1);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_skip_06

// opencl for dag 0

// opencl function freia_skip_06_opencl_helper_0_0_0
KERNEL void freia_skip_06_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0,
  int c1)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * p1 = o1 + ofs_o1;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t7 = PIXEL_MUL(in0, in0);
    PIXEL t10 = PIXEL_AND(t7, c0);
    PIXEL t11 = PIXEL_XOR(t7, c1);
    PIXEL t12 = PIXEL_ADDSAT(t10, t11);
    // set output pixels
    p0[i] = t7;
    p1[i] = t12;
  }
}

