// compile kernels for freia_skip_06_opencl_helper_0_0_0
static freia_status freia_skip_06_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_skip_06_opencl_helper_0_0_0
  const char * freia_skip_06_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_skip_06_opencl_helper_0_0_0\n"
    "KERNEL void freia_skip_06_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t7 = PIXEL_MUL(in0, in0);\n"
    "    PIXEL t10 = PIXEL_AND(t7, c0);\n"
    "    PIXEL t11 = PIXEL_XOR(t7, c1);\n"
    "    PIXEL t12 = PIXEL_ADDSAT(t10, t11);\n"
    "    // set output pixels\n"
    "    p0[i] = t7;\n"
    "    p1[i] = t12;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_skip_06_opencl_helper_0_0_0_source, "freia_skip_06_opencl_helper_0_0_0", "-DPIXEL8", &freia_skip_06_opencl_helper_0_0_0_kernel[0]);
