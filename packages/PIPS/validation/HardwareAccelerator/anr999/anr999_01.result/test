   freia_aipo_global_vol(in, &measure_vol);                             /*0039*/

   freia_aipo_dilate_8c(od, in, freia_morpho_kernel_8c);                /*0041*/
   freia_aipo_dilate_8c(od, od, freia_morpho_kernel_8c);                /*0042*/
   i_0 = 10;                                                            /*0043*/

   I_6 = 0;                                                             /*0045*/
   I_6;                                                                 /*0046*/

