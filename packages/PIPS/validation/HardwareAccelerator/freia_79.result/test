! initial function

void freia_79(freia_data2d *imOut, const freia_data2d *imIn)
{
   freia_data2d *w1, *w2;                                               /*0006*/
   w1 = freia_common_create_data(imIn->bpp, imIn->widthWa, imIn->heightWa);            /*0007*/
   w2 = freia_common_create_data(imIn->bpp, imIn->widthWa, imIn->heightWa);            /*0008*/

   freia_aipo_dilate_8c(w2, imIn, freia_morpho_kernel_8c);              /*0010*/
   freia_aipo_sub(w1, imIn, w2);                                        /*0011*/
   freia_aipo_mul_const(w2, w1, 2);                                     /*0012*/
   freia_aipo_copy(imOut, w2);                                          /*0013*/

   if (true)                                                            /*0015*/
      freia_aipo_add(imOut, imOut, w2);                                 /*0016*/

   freia_common_destruct_data(w1);                                      /*0018*/
   freia_common_destruct_data(w2);                                      /*0019*/
}

