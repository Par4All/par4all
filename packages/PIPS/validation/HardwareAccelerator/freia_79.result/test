   freia_aipo_dilate_8c(w2, imIn, freia_morpho_kernel_8c);              /*0010*/
   freia_79_opencl_helper_0_0_1(w2, w2, imIn, 2);                       /*0011*/
   freia_aipo_copy(imOut, w2);

   if (true)                                                            /*0015*/
      freia_aipo_add(imOut, imOut, w2);                                 /*0016*/

   freia_common_destruct_data(w2);                                      /*0019*/
}

