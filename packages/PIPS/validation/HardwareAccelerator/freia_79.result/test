// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 3 ops and 1 copies, optimized to 3 ops and 0+0+1 copies

// hold kernels for freia_79_opencl_helper_0_0_1
static cl_kernel freia_79_opencl_helper_0_0_1_kernel[2];

// compile kernels for freia_79_opencl_helper_0_0_1
static freia_status freia_79_opencl_helper_0_0_1_compile(void)
{
  // OpenCL source for freia_79_opencl_helper_0_0_1
  const char * freia_79_opencl_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_79_opencl_helper_0_0_1\n"
    "KERNEL void freia_79_opencl_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t11 = PIXEL_SUB(in1, in0);\n"
    "    PIXEL t12 = PIXEL_MUL(t11, c0);\n"
    "    // set output pixels\n"
    "    p0[i] = t12;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_79_opencl_helper_0_0_1_source, "freia_79_opencl_helper_0_0_1", "-DPIXEL8", &freia_79_opencl_helper_0_0_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_79_opencl_helper_0_0_1_source, "freia_79_opencl_helper_0_0_1", "-DPIXEL16", &freia_79_opencl_helper_0_0_1_kernel[1]);
  return err;
}

// helper function freia_79_opencl_helper_0_0_1
freia_status freia_79_opencl_helper_0_0_1(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_79_opencl_helper_0_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_79_opencl_helper_0_0_1_kernel[bpp];

  // call kernel freia_79_opencl_helper_0_0_1
  err |= freia_op_call_kernel(kernel, 1, 2, 1, 0, o0, i0, i1, c0);

  return err;
}

// dag 1: 1 ops and 0 copies, optimized to 1 ops and 0+0+0 copies
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_79

// opencl for dag 0

// opencl function freia_79_opencl_helper_0_0_1
KERNEL void freia_79_opencl_helper_0_0_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width,
  int pitch,
  int c0)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;
  GLOBAL PIXEL * j1 = i1 + ofs_i1;

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t11 = PIXEL_SUB(in1, in0);
    PIXEL t12 = PIXEL_MUL(t11, c0);
    // set output pixels
    p0[i] = t12;
  }
}

