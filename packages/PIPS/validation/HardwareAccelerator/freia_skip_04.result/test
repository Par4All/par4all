#
# AIPO
#

! initial function

void freia_skip_04(freia_data2d *o, freia_data2d *i)
{
   int32_t max, seuil;                                                  /*0005*/
   freia_data2d *t = freia_common_create_data(16, 128, 128);            /*0006*/
   // use some control in an intermediate statement
   freia_aipo_global_max(i, &max);                                      /*0008*/
   freia_aipo_xor_const(t, i, 111);                                     /*0009*/
   if (max>128)                                                         /*0010*/
      seuil = max-10;                                                   /*0011*/
   else
      seuil = max+15;                                                   /*0013*/
   freia_aipo_threshold(o, t, 8, seuil-4, true);                        /*0014*/
   freia_common_destruct_data(t);                                       /*0015*/
}

