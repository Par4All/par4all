! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 14 ops and 1 copies, optimized to 7 ops and 0+0+0 copies

// hold kernels for freia_51_opencl_helper_0_0_4
static cl_kernel freia_51_opencl_helper_0_0_4_kernel[2];

// compile kernels for freia_51_opencl_helper_0_0_4
static freia_status freia_51_opencl_helper_0_0_4_compile(void)
{
  // OpenCL source for freia_51_opencl_helper_0_0_4
  const char * freia_51_opencl_helper_0_0_4_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_51_opencl_helper_0_0_4\n"
    "KERNEL void freia_51_opencl_helper_0_0_4(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  int2 mmin = { PIXEL_MAX, 0 };\n"
    "  int2 mmax = { PIXEL_MIN, 0 };\n"
    "  int idy = get_global_id(0);\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t34 = PIXEL_INF(in0, c0);\n"
    "    PIXEL t36 = PIXEL_INF(t34, in0);\n"
    "    PIXEL_VOLUME(red38, t36);\n"
    "    // set output pixels\n"
    "    p0[i] = t36;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  redX[idy].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_51_opencl_helper_0_0_4_source, "freia_51_opencl_helper_0_0_4", "-DPIXEL8", &freia_51_opencl_helper_0_0_4_kernel[0]);
  err |= freia_op_compile_kernel(freia_51_opencl_helper_0_0_4_source, "freia_51_opencl_helper_0_0_4", "-DPIXEL16", &freia_51_opencl_helper_0_0_4_kernel[1]);
  return err;
}

// helper function freia_51_opencl_helper_0_0_4
freia_status freia_51_opencl_helper_0_0_4(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t c0,
  int32_t * po2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_51_opencl_helper_0_0_4_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_51_opencl_helper_0_0_4_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_51_opencl_helper_0_0_4
  err |= freia_op_call_kernel(kernel, 1, 1, 1, 1, o0, i0, c0, &redres);

  // return reduction results
  *po2 = redres.volume;

  return err;
}

// dag 1: 3 ops and 0 copies, optimized to 3 ops and 0+0+0 copies

// hold kernels for freia_51_opencl_helper_1_0_1
static cl_kernel freia_51_opencl_helper_1_0_1_kernel[2];

// compile kernels for freia_51_opencl_helper_1_0_1
static freia_status freia_51_opencl_helper_1_0_1_compile(void)
{
  // OpenCL source for freia_51_opencl_helper_1_0_1
  const char * freia_51_opencl_helper_1_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_51_opencl_helper_1_0_1\n"
    "KERNEL void freia_51_opencl_helper_1_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  int2 mmin = { PIXEL_MAX, 0 };\n"
    "  int2 mmax = { PIXEL_MIN, 0 };\n"
    "  int idy = get_global_id(0);\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t42 = PIXEL_INF(in1, in0);\n"
    "    PIXEL_VOLUME(red43, t42);\n"
    "    // set output pixels\n"
    "    p0[i] = t42;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  redX[idy].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_51_opencl_helper_1_0_1_source, "freia_51_opencl_helper_1_0_1", "-DPIXEL8", &freia_51_opencl_helper_1_0_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_51_opencl_helper_1_0_1_source, "freia_51_opencl_helper_1_0_1", "-DPIXEL16", &freia_51_opencl_helper_1_0_1_kernel[1]);
  return err;
}

// helper function freia_51_opencl_helper_1_0_1
freia_status freia_51_opencl_helper_1_0_1(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t * po3)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_51_opencl_helper_1_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_51_opencl_helper_1_0_1_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_51_opencl_helper_1_0_1
  err |= freia_op_call_kernel(kernel, 1, 2, 0, 1, o0, i0, i1, &redres);

  // return reduction results
  *po3 = redres.volume;

  return err;
}

