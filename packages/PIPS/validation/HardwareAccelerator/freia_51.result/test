// FREIA-SPoC helper function for module freia_51
freia_status freia_51_spoc_helper_1_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0,
  int32_t * po1,
  int32_t * po2)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  spoc_reduction reduc;
  freia_reduction_results redres;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`out [0 input 1] -> [0 poc 1] 29 dilate_8c
  // nope
  // poc 29 dilate_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_DILATE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];

  // 0`in [0 input 0] -> [0 alu] 30 inf
  // nope
  // 0`out_16 [0 poc 1] -> [0 alu] 30 inf
  // nope
  // alu 30 inf stage 0
  si.alu[0][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 1
  // 0`out_15 [0 alu] -> [1 poc 0] 31 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;
  // 0`out_15 [1 input 0] -> [1 poc 0] 31 dilate_8c
  // nope
  // poc 31 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // 0`in [0 input 0] -> [1 alu] 32 inf
  si.mux[0][1].op = SPOC_MUX_IN0;
  si.mux[0][3].op = SPOC_MUX_IN0;
  // 0`in [1 input 1] -> [1 alu] 32 inf
  // nope
  // 0`out_14 [1 poc 0] -> [1 alu] 32 inf
  // nope
  // alu 32 inf stage 1
  si.alu[1][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 2
  // 0`out_13 [1 alu] -> [2 poc 0] 33 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;
  // 0`out_13 [2 input 0] -> [2 poc 0] 33 dilate_8c
  // nope
  // poc 33 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];

  // 0`in [1 poc 1] -> [2 alu] 34 inf
  si.mux[1][3].op = SPOC_MUX_IN1;
  // 0`in [2 input 1] -> [2 alu] 34 inf
  // nope
  // 0`out_12 [2 poc 0] -> [2 alu] 34 inf
  // nope
  // alu 34 inf stage 2
  si.alu[2][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 3
  // 0`out_11 [2 alu] -> [3 poc 0] 35 dilate_8c
  si.mux[2][0].op = SPOC_MUX_IN1;
  si.mux[2][2].op = SPOC_MUX_IN0;
  // 0`out_11 [3 input 0] -> [3 poc 0] 35 dilate_8c
  // nope
  // poc 35 dilate_8c stage 3 side 0
  si.poc[3][0].op = SPOC_POC_DILATE;
  si.poc[3][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[3][0].kernel[i] = pi0[i];

  // 0`in [2 poc 1] -> [3 alu] 36 inf
  si.mux[2][3].op = SPOC_MUX_IN1;
  // 0`in [3 input 1] -> [3 alu] 36 inf
  // nope
  // 0`out_10 [3 poc 0] -> [3 alu] 36 inf
  // nope
  // alu 36 inf stage 3
  si.alu[3][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 4
  // 0`out_9 [3 alu] -> [4 poc 0] 37 dilate_8c
  si.mux[3][0].op = SPOC_MUX_IN1;
  si.mux[3][2].op = SPOC_MUX_IN0;
  // 0`out_9 [4 input 0] -> [4 poc 0] 37 dilate_8c
  // nope
  // poc 37 dilate_8c stage 4 side 0
  si.poc[4][0].op = SPOC_POC_DILATE;
  si.poc[4][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][0].kernel[i] = pi0[i];

  // 0`in [3 poc 1] -> [4 alu] 38 inf
  si.mux[3][3].op = SPOC_MUX_IN1;
  // 0`in [4 input 1] -> [4 alu] 38 inf
  // nope
  // 0`out_8 [4 poc 0] -> [4 alu] 38 inf
  // nope
  // alu 38 inf stage 4
  si.alu[4][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 5
  // 0`out_7 [4 alu] -> [5 poc 0] 39 dilate_8c
  si.mux[4][0].op = SPOC_MUX_IN1;
  si.mux[4][2].op = SPOC_MUX_IN0;
  // 0`out_7 [5 input 0] -> [5 poc 0] 39 dilate_8c
  // nope
  // poc 39 dilate_8c stage 5 side 0
  si.poc[5][0].op = SPOC_POC_DILATE;
  si.poc[5][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[5][0].kernel[i] = pi0[i];

  // 0`in [4 poc 1] -> [5 alu] 40 inf
  si.mux[4][3].op = SPOC_MUX_IN1;
  // 0`in [5 input 1] -> [5 alu] 40 inf
  // nope
  // 0`out_6 [5 poc 0] -> [5 alu] 40 inf
  // nope
  // alu 40 inf stage 5
  si.alu[5][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 6
  // 0`out_5 [5 alu] -> [6 poc 0] 41 dilate_8c
  si.mux[5][0].op = SPOC_MUX_IN1;
  si.mux[5][2].op = SPOC_MUX_IN0;
  // 0`out_5 [6 input 0] -> [6 poc 0] 41 dilate_8c
  // nope
  // poc 41 dilate_8c stage 6 side 0
  si.poc[6][0].op = SPOC_POC_DILATE;
  si.poc[6][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[6][0].kernel[i] = pi0[i];

  // 0`in [5 poc 1] -> [6 alu] 42 inf
  si.mux[5][3].op = SPOC_MUX_IN1;
  // 0`in [6 input 1] -> [6 alu] 42 inf
  // nope
  // 0`out_4 [6 poc 0] -> [6 alu] 42 inf
  // nope
  // alu 42 inf stage 6
  si.alu[6][0].op = SPOC_ALU_INF_IN0_IN1;

  // 0`out_3 [6 alu] -> [6 measure 0] 42 inf
  si.mux[6][0].op = SPOC_MUX_IN1;
  si.mux[6][2].op = SPOC_MUX_IN0;


  // STAGE 7
  // 0`out_3 [6 measure 0] -> [7 poc 0] 44 dilate_8c
  // 0`out_3 [7 input 0] -> [7 poc 0] 44 dilate_8c
  // nope
  // poc 44 dilate_8c stage 7 side 0
  si.poc[7][0].op = SPOC_POC_DILATE;
  si.poc[7][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[7][0].kernel[i] = pi0[i];

  // 0`in [6 poc 1] -> [7 alu] 45 inf
