// generated OpenCL kernels for function freia_51

// opencl for dag 0

// opencl function freia_51_opencl_helper_0_0_4
KERNEL void freia_51_opencl_helper_0_0_4(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0,
  GLOBAL TMeasure * redX)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;

  // reduction stuff is currently hardcoded...
  int vol = 0;
  int2 mmin = { PIXEL_MAX, 0 };
  int2 mmax = { PIXEL_MIN, 0 };
  int idy = get_global_id(0);

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t34 = PIXEL_INF(in0, c0);
    PIXEL t36 = PIXEL_INF(t34, in0);
    PIXEL_VOLUME(red38, t36);
    // set output pixels
    p0[i] = t36;
  }

  // reduction copy out
  redX[idy].vol = vol;
}

