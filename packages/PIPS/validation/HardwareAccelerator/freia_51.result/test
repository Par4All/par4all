// hold kernels for freia_51_opencl_helper_2_0_0
static cl_kernel freia_51_opencl_helper_2_0_0_kernel[2];

// compile kernels for freia_51_opencl_helper_2_0_0
static freia_status freia_51_opencl_helper_2_0_0_compile(void)
{
  // OpenCL source for freia_51_opencl_helper_2_0_0
  const char * freia_51_opencl_helper_2_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_51_opencl_helper_2_0_0\n"
    "KERNEL void freia_51_opencl_helper_2_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0)\n"
    "{\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int gid = pitch*get_global_id(0);\n"
    "  int i;\n"
    "  for (i=gid; i < (gid+width); i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t47 = PIXEL_SUB(in0, in1);\n"
    "    PIXEL t48 = PIXEL_MUL(t47, c0);\n"
    "    // set output pixels\n"
    "    p0[i] = t48;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_51_opencl_helper_2_0_0_source, "freia_51_opencl_helper_2_0_0", "-DPIXEL8", &freia_51_opencl_helper_2_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_51_opencl_helper_2_0_0_source, "freia_51_opencl_helper_2_0_0", "-DPIXEL16", &freia_51_opencl_helper_2_0_0_kernel[1]);
  return err;
}

// helper function freia_51_opencl_helper_2_0_0
freia_status freia_51_opencl_helper_2_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_51_opencl_helper_2_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_51_opencl_helper_2_0_0_kernel[bpp];

  // call kernel freia_51_opencl_helper_2_0_0
  err |= freia_op_call_kernel(kernel, 1, 2, 1, 0, o0, i0, i1, c0);

  return err;
}
