   int vol, pmin, pmax, x, y;                                           /*0006*/
   freia_76_terapix_helper_0_0_0(in, &pmax, &vol, &pmin, &x, &y);       /*0007*/
   return vol/(pmin+pmax)*2;                                            /*0010*/
}

