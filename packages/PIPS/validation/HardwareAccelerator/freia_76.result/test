! initial function

int freia_76(const freia_data2d *in)
{
   int vol, pmin, pmax, x, y;                                           /*0006*/
   freia_aipo_global_max(in, &pmax);                                    /*0007*/
   freia_aipo_global_vol(in, &vol);                                     /*0008*/
   freia_aipo_global_min_coord(in, &pmin, &x, &y);                      /*0009*/
   return vol/(pmin+pmax)*2;                                            /*0010*/
}

