   oop_core_spoc_helper_0_2_0(img1, img1_3, imgtmp, freia_morpho_kernel_6c);           /*0081*/

   oop_core_spoc_helper_0_3_0(img2, imgtt4, freia_morpho_kernel_8c);    /*0095*/

   oop_core_spoc_helper_0_4_0(img2, img1_3, img2, freia_morpho_kernel_6c);             /*0100*/

   oop_core_spoc_helper_0_5_0(img3, img1, img2, 15, 255, 1);            /*0104*/
   
   
