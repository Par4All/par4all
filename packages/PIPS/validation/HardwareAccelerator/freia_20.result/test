freia_status freia_20(freia_data2d *o, freia_data2d *i, int32_t *k)
{
   
   
   
   
   
   
   
   
   
   
   
   
   freia_data2d *t3 = freia_common_create_data(16, 128, 128), *t7 = freia_common_create_data(16, 128, 128);          /*0018*/
   
   // long pipeline that must be cut
   freia_20_terapix_helper_0_0_0(t3, i, k);                             /*0021*/
   freia_20_terapix_helper_0_0_1(t7, t3, k);                            /*0025*/
   // should cut here
   freia_20_terapix_helper_0_0_2(o, t7, k);                             /*0030*/

   freia_common_destruct_data(t3);                                      /*0039*/
   freia_common_destruct_data(t7);                                      /*0043*/

   return 0;                                                            /*0049*/
}
