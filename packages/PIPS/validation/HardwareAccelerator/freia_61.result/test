! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 2 ops and 0 copies, optimized to 2 ops and 0+0+0 copies

// hold kernels for freia_61_opencl_helper_0_0_0
static cl_kernel freia_61_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_61_opencl_helper_0_0_0
static freia_status freia_61_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_61_opencl_helper_0_0_0
  const char * freia_61_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_61_opencl_helper_0_0_0\n"
    "KERNEL void freia_61_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int height = get_global_size(0);\n"
    "  int thrid = get_global_id(0) + height*get_global_id(1);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t7 = PIXEL_ADD(in0, in1);\n"
    "    PIXEL t8 = PIXEL_THRESHOLD(t7, c0, c1, c2);\n"
    "    // set output pixels\n"
    "    p0[i] = t7;\n"
    "    p1[i] = t8;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_61_opencl_helper_0_0_0_source, "freia_61_opencl_helper_0_0_0", "-DPIXEL8", &freia_61_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_61_opencl_helper_0_0_0_source, "freia_61_opencl_helper_0_0_0", "-DPIXEL16", &freia_61_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_61_opencl_helper_0_0_0
freia_status freia_61_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_61_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_61_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_61_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 2, 3, 0, o0, o1, i0, i1, c0, c1, c2);

  return err;
}

// dag 1: 1 ops and 0 copies, optimized to 1 ops and 0+0+0 copies
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_61

// opencl for dag 0

// opencl function freia_61_opencl_helper_0_0_0
KERNEL void freia_61_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image below
  int pitch,
  int c0,
  int c1,
  int c2)
{
  // get id & compute global image shift
  int height = get_global_size(0);
  int thrid = get_global_id(0) + height*get_global_id(1);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;
  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t7 = PIXEL_ADD(in0, in1);
    PIXEL t8 = PIXEL_THRESHOLD(t7, c0, c1, c2);
    // set output pixels
    p0[i] = t7;
    p1[i] = t8;
  }
}

