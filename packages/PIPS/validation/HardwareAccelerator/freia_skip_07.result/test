// opencl for dag 0

// opencl function freia_skip_07_opencl_helper_0_0_0
KERNEL void freia_skip_07_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0,
  int c1)
{
  GLOBAL PIXEL * p0 = o0 + ofs_o0;
  GLOBAL PIXEL * j0 = i0 + ofs_i0;

  // thread's pixel loop
  int gid = pitch*get_global_id(0);
  int i;
  for (i=gid; i < (gid+width); i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t10 = PIXEL_MUL(in0, in0);
    PIXEL t11 = PIXEL_AND(t10, c0);
    PIXEL t12 = PIXEL_XOR(t10, c1);
    PIXEL t13 = PIXEL_ADDSAT(t11, t12);
    // set output pixels
    p0[i] = t13;
  }
}

