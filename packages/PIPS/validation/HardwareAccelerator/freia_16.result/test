! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <terapix.h>
#include <terapix_ucode.h>

/* FREIA terapix helper function for module freia_16
 * 1 input image, 2 output images
 * 6 image operations in dag
 * dag length is 2, dag width is 2
 * cost is 22 cycles per imagelet row
 */
freia_status freia_16_helper_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  freia_data2d * i0,
  int32_t * po3,
  int32_t * po4,
  int32_t * po5,
  const int32_t * pi6,
  int32_t pi7)
{
  // declarations:
  freia_microcode mcode;
  freia_op_param param;
  freia_dynamic_param dyn_param;
  terapix_gram gram;
  int i;
  freia_status ret;
  // operation 3 parameters
  int16_t p_3[9];
  const int32_t x_3 = 0;
  const int32_t y_3 = 0;
  // operation 4 parameters
  int16_t p_4[1];
  const int32_t x_4 = 3;
  const int32_t y_4 = 0;
  terapix_mcu_instr mcu_instr[2][5];

  // imagelets definitions:
  // - 2 computation imagelets
  // - 2 double buffer imagelets
  int imagelet_size = 255;
  int imagelet_1 = 0;
  int imagelet_2 = 255;
  int imagelet_3 = 510;
  int imagelet_4 = 765;

  // double buffer assignment
  int imagelet_io_1_0 = imagelet_1;
  int imagelet_io_1_1 = imagelet_3;
  int imagelet_io_2_0 = imagelet_2;
  int imagelet_io_2_1 = imagelet_4;

  // body:
  // mcode param
  mcode.raw = (void*) terapix_ucode_array;
  mcode.size = TERAPIX_UCODE_SIZE_T;
  freia_mg_write_microcode(&mcode);

  // subimage operation
  param.size = sizeof(mcu_instr);
  param.raw = (void*) mcu_instr;

  // dyn_param contents  dyn_param.raw = &gram;
  dyn_param.size = sizeof(terapix_gram);
  // set measure min at 1023
  // 0: min(1)
  mcu_instr[0][0].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][0].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][0].macrocode.ymin1 = 0;
  mcu_instr[1][0].macrocode.ymin1 = 0;
  mcu_instr[0][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][0].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][0].macrocode.iter2 = imagelet_size;
  mcu_instr[1][0].macrocode.iter2 = imagelet_size;
  mcu_instr[0][0].macrocode.addrStart = TERAPIX_UCODE_MIN?;
  mcu_instr[1][0].macrocode.addrStart = TERAPIX_UCODE_MIN?;
  // set measure max at 1022
  // 1: max(1)
  mcu_instr[0][1].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][1].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][1].macrocode.ymin1 = 0;
  mcu_instr[1][1].macrocode.ymin1 = 0;
  mcu_instr[0][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][1].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][1].macrocode.iter2 = imagelet_size;
  mcu_instr[1][1].macrocode.iter2 = imagelet_size;
  mcu_instr[0][1].macrocode.addrStart = TERAPIX_UCODE_MAX?;
  mcu_instr[1][1].macrocode.addrStart = TERAPIX_UCODE_MAX?;
  // set measure vol at 1020
  // 2: vol(1)
  mcu_instr[0][2].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][2].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][2].macrocode.ymin1 = 0;
  mcu_instr[1][2].macrocode.ymin1 = 0;
  mcu_instr[0][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][2].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][2].macrocode.iter2 = imagelet_size;
  mcu_instr[1][2].macrocode.iter2 = imagelet_size;
  mcu_instr[0][2].macrocode.addrStart = TERAPIX_UCODE_VOL?;
  mcu_instr[1][2].macrocode.addrStart = TERAPIX_UCODE_VOL?;
  // 3: E6(1) -> 2
  mcu_instr[0][3].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][3].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][3].macrocode.ymin1 = 0;
  mcu_instr[1][3].macrocode.ymin1 = 0;
  mcu_instr[0][3].macrocode.xmin2 = imagelet_io_2_0;
  mcu_instr[1][3].macrocode.xmin2 = imagelet_io_2_1;
  mcu_instr[0][3].macrocode.ymin2 = 0;
  mcu_instr[1][3].macrocode.ymin2 = 0;
  // copy of operation 3 parameters
  for(i=0; i<9; i++)
    p_3[i] = pi6[i];
  gram.xoffset = x_3;
  gram.yoffset = y_3;
  gram.width = 3;
  gram.height = 3;
  gram.params = p_3;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][3].macrocode.xmin3 = x_3;
  mcu_instr[1][3].macrocode.xmin3 = x_3;
  mcu_instr[0][3].macrocode.ymin3 = y_3;
  mcu_instr[1][3].macrocode.ymin3 = y_3;
  mcu_instr[0][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][3].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][3].macrocode.iter2 = imagelet_size;
  mcu_instr[1][3].macrocode.iter2 = imagelet_size;
  mcu_instr[0][3].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3?;
  mcu_instr[1][3].macrocode.addrStart = TERAPIX_UCODE_ERODE_3_3?;
  // 4: +.(1) -> 1
  mcu_instr[0][4].macrocode.xmin1 = imagelet_io_1_0;
  mcu_instr[1][4].macrocode.xmin1 = imagelet_io_1_1;
  mcu_instr[0][4].macrocode.ymin1 = 0;
  mcu_instr[1][4].macrocode.ymin1 = 0;
  mcu_instr[0][4].macrocode.xmin2 = imagelet_io_1_0;
  mcu_instr[1][4].macrocode.xmin2 = imagelet_io_1_1;
  mcu_instr[0][4].macrocode.ymin2 = 0;
  mcu_instr[1][4].macrocode.ymin2 = 0;
  // copy of operation 4 parameters
  p_4[0] = pi7;
  gram.xoffset = x_4;
  gram.yoffset = y_4;
  gram.width = 1;
  gram.height = 1;
  gram.params = p_4;
  freia_mg_write_dynamic_param(&dyn_param);
  mcu_instr[0][4].macrocode.xmin3 = x_4;
  mcu_instr[1][4].macrocode.xmin3 = x_4;
  mcu_instr[0][4].macrocode.ymin3 = y_4;
  mcu_instr[1][4].macrocode.ymin3 = y_4;
  mcu_instr[0][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[1][4].macrocode.iter1 = TERAPIX_PE_NUMBER;
  mcu_instr[0][4].macrocode.iter2 = imagelet_size;
  mcu_instr[1][4].macrocode.iter2 = imagelet_size;
  mcu_instr[0][4].macrocode.addrStart = TERAPIX_UCODE_ADD_CONST;
  mcu_instr[1][4].macrocode.addrStart = TERAPIX_UCODE_ADD_CONST;

  // inputs:
  // - imagelet 1 is i0 for i

  // outputs:
  // - imagelet 1 is o0 for o0
  // - imagelet 2 is o1 for o1

  // double buffer management:
  // - buffer 1/3
  // - buffer 2/4

  // output imagelet erosion:
  //   north: 1
  //   south: 1
  //    west: 1
  //    east: 1

  ret = freia_cg_template_process_1i_2o(&param, o0, o1, i0);

  // extract measures
  // get measure min result from 1023
  // get measure max result from 1022
  // get measure vol result from 1020

  return ret;
}


