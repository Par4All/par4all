  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int height = get_global_size(0);
  int thrid = get_global_id(0) + height*get_global_id(1);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;
  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL t23 = PIXEL_MIN;
    PIXEL in1NW = (is_N|is_W)? 0: j1[i-pitch-1];
    PIXEL in1N = (is_N)? 0: j1[i-pitch];
    PIXEL in1NE = (is_N|is_E)? 0: j1[i-pitch+1];
    PIXEL in1W = (is_W)? 0: j1[i-1];
    PIXEL in1 = j1[i];
    PIXEL in1E = (is_E)? 0: j1[i+1];
    PIXEL in1SW = (is_S|is_W)? 0: j1[i+pitch-1];
    PIXEL in1S = (is_S)? 0: j1[i+pitch];
    PIXEL in1SE = (is_S|is_E)? 0: j1[i+pitch+1];
    PIXEL t24 = PIXEL_MAX;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];
    // pixel computations
    t23 = PIXEL_SUP(t23, (is_N|is_W)? PIXEL_MIN: in1NW);
    t23 = PIXEL_SUP(t23, (is_N)? PIXEL_MIN: in1N);
    t23 = PIXEL_SUP(t23, (is_N|is_E)? PIXEL_MIN: in1NE);
    t23 = PIXEL_SUP(t23, (is_W)? PIXEL_MIN: in1W);
    t23 = PIXEL_SUP(t23, in1);
    t23 = PIXEL_SUP(t23, (is_E)? PIXEL_MIN: in1E);
    t23 = PIXEL_SUP(t23, (is_S|is_W)? PIXEL_MIN: in1SW);
    t23 = PIXEL_SUP(t23, (is_S)? PIXEL_MIN: in1S);
    t23 = PIXEL_SUP(t23, (is_S|is_E)? PIXEL_MIN: in1SE);
    t24 = PIXEL_INF(t24, (is_N|is_W)? PIXEL_MAX: in0NW);
    t24 = PIXEL_INF(t24, (is_N)? PIXEL_MAX: in0N);
    t24 = PIXEL_INF(t24, (is_N|is_E)? PIXEL_MAX: in0NE);
    t24 = PIXEL_INF(t24, (is_W)? PIXEL_MAX: in0W);
    t24 = PIXEL_INF(t24, in0);
    t24 = PIXEL_INF(t24, (is_E)? PIXEL_MAX: in0E);
    t24 = PIXEL_INF(t24, (is_S|is_W)? PIXEL_MAX: in0SW);
    t24 = PIXEL_INF(t24, (is_S)? PIXEL_MAX: in0S);
    t24 = PIXEL_INF(t24, (is_S|is_E)? PIXEL_MAX: in0SE);
    PIXEL t27 = PIXEL_SUB(t23, t24);
    // set output pixels
    p0[i] = t27;
  }
}

// opencl function copy_00_opencl_helper_0_0_4
KERNEL void copy_00_opencl_helper_0_0_4(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int height = get_global_size(0);
  int thrid = get_global_id(0) + height*get_global_id(1);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;
  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL t18 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];
    PIXEL t19 = PIXEL_MAX;
    // pixel computations
    t18 = PIXEL_SUP(t18, (is_N|is_W)? PIXEL_MIN: in0NW);
    t18 = PIXEL_SUP(t18, (is_N)? PIXEL_MIN: in0N);
    t18 = PIXEL_SUP(t18, (is_N|is_E)? PIXEL_MIN: in0NE);
    t18 = PIXEL_SUP(t18, (is_W)? PIXEL_MIN: in0W);
    t18 = PIXEL_SUP(t18, in0);
    t18 = PIXEL_SUP(t18, (is_E)? PIXEL_MIN: in0E);
    t18 = PIXEL_SUP(t18, (is_S|is_W)? PIXEL_MIN: in0SW);
    t18 = PIXEL_SUP(t18, (is_S)? PIXEL_MIN: in0S);
    t18 = PIXEL_SUP(t18, (is_S|is_E)? PIXEL_MIN: in0SE);
    t19 = PIXEL_INF(t19, (is_N|is_W)? PIXEL_MAX: in0NW);
    t19 = PIXEL_INF(t19, (is_N)? PIXEL_MAX: in0N);
    t19 = PIXEL_INF(t19, (is_N|is_E)? PIXEL_MAX: in0NE);
    t19 = PIXEL_INF(t19, (is_W)? PIXEL_MAX: in0W);
    t19 = PIXEL_INF(t19, in0);
    t19 = PIXEL_INF(t19, (is_E)? PIXEL_MAX: in0E);
    t19 = PIXEL_INF(t19, (is_S|is_W)? PIXEL_MAX: in0SW);
    t19 = PIXEL_INF(t19, (is_S)? PIXEL_MAX: in0S);
    t19 = PIXEL_INF(t19, (is_S|is_E)? PIXEL_MAX: in0SE);
    PIXEL t22 = PIXEL_SUB(t18, t19);
    // set output pixels
    p0[i] = t22;
  }
}

! updated function

int copy_00(freia_data2d *o0, freia_data2d *o1, freia_data2d *in)
{
   freia_data2d *a, *c, *d;                                             /*0005*/
   a = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0006*/
   c = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0008*/
   d = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0009*/
   
   // 2
   copy_00_opencl_helper_0_0_0(d, c, c);                                /*0016*/
   
   // 1
   copy_00_opencl_helper_0_0_1(a, c, in);                               /*0012*/
   copy_00_opencl_helper_0_0_3(o1, c, d);                               /*0023*/
   
   // 4
   copy_00_opencl_helper_0_0_4(o0, a);                                  /*0018*/
   
   // 3
   
   return 0;                                                            /*0029*/
}

! cleaned up function

int copy_00(freia_data2d *o0, freia_data2d *o1, freia_data2d *in)
{
   freia_data2d *a, *c, *d;                                             /*0005*/
   a = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0006*/
   c = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0008*/
   d = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0009*/
   
   // 2
   copy_00_opencl_helper_0_0_0(d, c, c);                                /*0016*/
   
   // 1
   copy_00_opencl_helper_0_0_1(a, c, in);                               /*0012*/
   copy_00_opencl_helper_0_0_3(o1, c, d);                               /*0023*/
   
   // 4
   copy_00_opencl_helper_0_0_4(o0, a);                                  /*0018*/
   
   // 3
   
   return 0;                                                            /*0029*/
}
