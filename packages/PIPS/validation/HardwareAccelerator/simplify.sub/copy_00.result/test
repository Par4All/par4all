! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 10 ops and 0 copies, optimized to 10 ops and 0+0+0 copies

// hold kernels for copy_00_opencl_helper_0_0_0
static cl_kernel copy_00_opencl_helper_0_0_0_kernel[2];

// compile kernels for copy_00_opencl_helper_0_0_0
static freia_status copy_00_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for copy_00_opencl_helper_0_0_0
  const char * copy_00_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function copy_00_opencl_helper_0_0_0\n"
    "KERNEL void copy_00_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int height = get_global_size(0);\n"
    "  int thrid = get_global_id(0) + height*get_global_id(1);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL t16 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "    PIXEL t17 = PIXEL_MAX;\n"
    "    // pixel computations\n"
    "    t16 = PIXEL_SUP(t16, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t16 = PIXEL_SUP(t16, (is_N)? PIXEL_MIN: in0N);\n"
    "    t16 = PIXEL_SUP(t16, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t16 = PIXEL_SUP(t16, (is_W)? PIXEL_MIN: in0W);\n"
    "    t16 = PIXEL_SUP(t16, in0);\n"
    "    t16 = PIXEL_SUP(t16, (is_E)? PIXEL_MIN: in0E);\n"
    "    t16 = PIXEL_SUP(t16, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t16 = PIXEL_SUP(t16, (is_S)? PIXEL_MIN: in0S);\n"
    "    t16 = PIXEL_SUP(t16, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "    t17 = PIXEL_INF(t17, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t17 = PIXEL_INF(t17, (is_N)? PIXEL_MAX: in0N);\n"
    "    t17 = PIXEL_INF(t17, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t17 = PIXEL_INF(t17, (is_W)? PIXEL_MAX: in0W);\n"
    "    t17 = PIXEL_INF(t17, in0);\n"
    "    t17 = PIXEL_INF(t17, (is_E)? PIXEL_MAX: in0E);\n"
    "    t17 = PIXEL_INF(t17, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t17 = PIXEL_INF(t17, (is_S)? PIXEL_MAX: in0S);\n"
    "    t17 = PIXEL_INF(t17, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "    // set output pixels\n"
    "    p0[i] = t16;\n"
    "    p1[i] = t17;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(copy_00_opencl_helper_0_0_0_source, "copy_00_opencl_helper_0_0_0", "-DPIXEL8", &copy_00_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(copy_00_opencl_helper_0_0_0_source, "copy_00_opencl_helper_0_0_0", "-DPIXEL16", &copy_00_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function copy_00_opencl_helper_0_0_0
freia_status copy_00_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= copy_00_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = copy_00_opencl_helper_0_0_0_kernel[bpp];

  // call kernel copy_00_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 1, 0, 0, o0, o1, i0);

  return err;
}

// hold kernels for copy_00_opencl_helper_0_0_1
static cl_kernel copy_00_opencl_helper_0_0_1_kernel[2];

// compile kernels for copy_00_opencl_helper_0_0_1
static freia_status copy_00_opencl_helper_0_0_1_compile(void)
{
  // OpenCL source for copy_00_opencl_helper_0_0_1
  const char * copy_00_opencl_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function copy_00_opencl_helper_0_0_1\n"
    "KERNEL void copy_00_opencl_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int height = get_global_size(0);\n"
    "  int thrid = get_global_id(0) + height*get_global_id(1);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL t12 = PIXEL_MIN;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "    PIXEL t13 = PIXEL_MAX;\n"
    "    // pixel computations\n"
    "    t12 = PIXEL_SUP(t12, (is_N|is_W)? PIXEL_MIN: in0NW);\n"
    "    t12 = PIXEL_SUP(t12, (is_N)? PIXEL_MIN: in0N);\n"
    "    t12 = PIXEL_SUP(t12, (is_N|is_E)? PIXEL_MIN: in0NE);\n"
    "    t12 = PIXEL_SUP(t12, (is_W)? PIXEL_MIN: in0W);\n"
    "    t12 = PIXEL_SUP(t12, in0);\n"
    "    t12 = PIXEL_SUP(t12, (is_E)? PIXEL_MIN: in0E);\n"
    "    t12 = PIXEL_SUP(t12, (is_S|is_W)? PIXEL_MIN: in0SW);\n"
    "    t12 = PIXEL_SUP(t12, (is_S)? PIXEL_MIN: in0S);\n"
    "    t12 = PIXEL_SUP(t12, (is_S|is_E)? PIXEL_MIN: in0SE);\n"
    "    t13 = PIXEL_INF(t13, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t13 = PIXEL_INF(t13, (is_N)? PIXEL_MAX: in0N);\n"
    "    t13 = PIXEL_INF(t13, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t13 = PIXEL_INF(t13, (is_W)? PIXEL_MAX: in0W);\n"
    "    t13 = PIXEL_INF(t13, in0);\n"
    "    t13 = PIXEL_INF(t13, (is_E)? PIXEL_MAX: in0E);\n"
    "    t13 = PIXEL_INF(t13, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t13 = PIXEL_INF(t13, (is_S)? PIXEL_MAX: in0S);\n"
    "    t13 = PIXEL_INF(t13, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "    // set output pixels\n"
