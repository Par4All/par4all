! initial function

int freia_dup_15(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdout;                                                  /*0007*/
   freia_data2d *in;                                                    /*0008*/
   freia_data2d *og;                                                    /*0009*/
   freia_data2d *od;                                                    /*0010*/
   freia_data2d *t;                                                     /*0011*/

   freia_common_open_input(&fdin, 0);                                   /*0013*/
   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, fdin.framebpp);             /*0014*/

   in = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0016*/
   od = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0017*/
   og = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0018*/
   t = freia_common_create_data(og->bpp, og->widthWa, og->heightWa);    /*0019*/

   freia_common_rx_image(in, &fdin);                                    /*0021*/

   freia_aipo_dilate_8c(od, in, freia_morpho_kernel_8c);                /*0023*/
   freia_aipo_dilate_8c(t, in, freia_morpho_kernel_8c);                 /*0024*/
   freia_aipo_erode_8c(og, in, freia_morpho_kernel_8c);                 /*0025*/
   freia_aipo_sub(og, t, og);                                           /*0026*/

   freia_common_tx_image(od, &fdout);                                   /*0028*/
   freia_common_tx_image(og, &fdout);                                   /*0029*/

   freia_common_destruct_data(in);                                      /*0031*/
   freia_common_destruct_data(od);                                      /*0032*/
   freia_common_destruct_data(og);                                      /*0033*/
   freia_common_destruct_data(t);                                       /*0034*/

   freia_common_close_input(&fdin);                                     /*0036*/
   freia_common_close_output(&fdout);                                   /*0037*/

   return 0;                                                            /*0039*/
}

