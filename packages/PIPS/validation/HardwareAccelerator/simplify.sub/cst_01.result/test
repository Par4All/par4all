! initial function

freia_status cst_01(freia_data2d *o, uint32_t k, int32_t v)
{
   // this means o := 0
   freia_aipo_set_constant(o, v);                                       /*0006*/
   freia_aipo_add(o, o, o);                                             /*0007*/
   freia_aipo_dilate_6c(o, o, k);                                       /*0008*/
   freia_aipo_add_const(o, o, v);                                       /*0009*/
   freia_aipo_sub_const(o, o, v);                                       /*0010*/
   freia_aipo_mul_const(o, o, v);                                       /*0011*/
   freia_aipo_div_const(o, o, v);                                       /*0012*/
   return FREIA_OK;                                                     /*0013*/
}

