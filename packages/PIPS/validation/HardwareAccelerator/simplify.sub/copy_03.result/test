! initial function

int copy_03(freia_data2d *out, const freia_data2d *in)
{
   freia_data2d *a, *b;                                                 /*0005*/
   a = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0006*/
   b = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0007*/
   
   // check for CSE-enabled constant images
   freia_aipo_add_const(a, in, 1);                                      /*0010*/
   freia_aipo_add_const(b, in, 1);                                      /*0011*/
   freia_aipo_sub(out, a, b);                                           /*0012*/

   freia_common_destruct_data(a);                                       /*0014*/
   freia_common_destruct_data(b);                                       /*0015*/

   return 0;                                                            /*0017*/
}

