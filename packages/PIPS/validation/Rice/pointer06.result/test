
POINTS TO


// Points To IN: none


// Points To OUT:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// *HEAP*_l_10.right -> *HEAP*_l_12 , MAY

void pointer06()
{

// Points To: none

   struct tree {
      int val[10];
      struct tree *left;
      struct tree *right;
   };

// Points To: none

   int i;

// Points To: none

   struct tree *t = (struct tree *) malloc(sizeof(struct tree ));

// Points To:
// t -> *HEAP*_l_10 , MAY

   t->left = (struct tree *) malloc(sizeof(struct tree ));

// Points To:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// t -> *HEAP*_l_10 , MAY

   t->right = (struct tree *) malloc(sizeof(struct tree ));

// Points To:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// *HEAP*_l_10.right -> *HEAP*_l_12 , MAY
// t -> *HEAP*_l_10 , MAY

   struct tree *tl = t->left;

// Points To:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// *HEAP*_l_10.right -> *HEAP*_l_12 , MAY
// t -> *HEAP*_l_10 , MAY
// tl -> *HEAP*_l_11 , MAY

   struct tree *tr = t->right;

// Points To:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// *HEAP*_l_10.right -> *HEAP*_l_12 , MAY
// t -> *HEAP*_l_10 , MAY
// tl -> *HEAP*_l_11 , MAY
// tr -> *HEAP*_l_12 , MAY

   for(i = 0; i <= 9; i += 1)

// Points To:
// *HEAP*_l_10.left -> *HEAP*_l_11 , MAY
// *HEAP*_l_10.right -> *HEAP*_l_12 , MAY
// t -> *HEAP*_l_10 , MAY
// tl -> *HEAP*_l_11 , MAY
// tr -> *HEAP*_l_12 , MAY

      (t->val)[i] = (tl->val)[i]+(tr->val)[i];
}

Proper effects with points-to

void pointer06()
{
//             <    is declared>: tree
   struct tree {
      int val[10];
      struct tree *left;
      struct tree *right;
   };
//             <    is declared>: i
   int i;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: t _MALLOC_EFFECTS:_MALLOC_
//             <    is referenced>: t
//             <    is declared>: t
   struct tree *t = (struct tree *) malloc(sizeof(struct tree ));
//             <may be written>: *HEAP*_l_10.left
//             <    is read   >: t _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
//             <    is referenced>: t
   t->left = (struct tree *) malloc(sizeof(struct tree ));
//             <may be written>: *HEAP*_l_10.right
//             <    is read   >: t _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
//             <    is referenced>: t
   t->right = (struct tree *) malloc(sizeof(struct tree ));
//             <may be read   >: *HEAP*_l_10.left
//             <    is read   >: t
//             <    is written>: tl
//             <    is referenced>: t tl
//             <    is declared>: tl
   struct tree *tl = t->left;
//             <may be read   >: *HEAP*_l_10.right
//             <    is read   >: t
//             <    is written>: tr
//             <    is referenced>: t tr
//             <    is declared>: tr
   struct tree *tr = t->right;
//             <    is written>: i
//             <    is referenced>: i
   for(i = 0; i <= 9; i += 1)
//             <may be read   >: *HEAP*_l_11.val[i] *HEAP*_l_12.val[i]
//             <may be written>: *HEAP*_l_10.val[i]
//             <    is read   >: i t tl tr
//             <    is referenced>: i t tl tr
      (t->val)[i] = (tl->val)[i]+(tr->val)[i];
}

Parallel code

void pointer06()
{
   struct tree {
      int val[10];
      struct tree *left;
      struct tree *right;
   };
   int i;
   struct tree *t = (struct tree *) malloc(sizeof(struct tree ));
   t->left = (struct tree *) malloc(sizeof(struct tree ));
   t->right = (struct tree *) malloc(sizeof(struct tree ));
   struct tree *tl = t->left;
   struct tree *tr = t->right;
   forall(i = 0; i <= 9; i += 1)
      (t->val)[i] = (tl->val)[i]+(tr->val)[i];
}
