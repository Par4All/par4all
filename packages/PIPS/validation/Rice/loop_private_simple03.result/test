
Proper effects for "main" after privatization

int main()
{
   float a[10];
   int i;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1) {
      float z;
//             <    is written>: z
      z = 0.0;
//             <    is read   >: i z
//             <    is written>: a[i]
      a[i] = z;
   }
   return 0;
}

Parallel code for "main"

int main()
{
   float a[10];
   int i;

   for(i = 0; i <= 9; i += 1) {
      float z;
      z = 0.0;
      a[i] = z;
   }
   return 0;
}
