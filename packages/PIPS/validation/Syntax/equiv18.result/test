      PROGRAM EQUIV18

C     Check that offsets in a common area are correct regardless of 
C     equivalences. Offsets:
C     x=0
C     y=96
C     u=200
C     w=4

      REAL X(50), W(100)

      EQUIVALENCE (X(25),Y)
      EQUIVALENCE (U, W(50))

      COMMON /FOO/ X, U

      X(1) = Y+W(10)                                                    0017

      PRINT *, U, Y                                                     0019

