
      SUBROUTINE DIGITALIZE(X, LX, MX, NX)
      REAL X(MX,NX,LX)

      COMMON /C2/ M,MM1,MP1

      MS = MM1/2                                                        0007
      MM = MP1/2                                                        0008

      DO K = 2, N-1                                                     0010
         DO J = 2, MM                                                   0011
            JJ = J+MS                                                   0012
            X(J,K,1) = X(JJ,K,2)                                        0013
            X(JJ,K,1) = X(J,K,2)                                        0014
         ENDDO
      ENDDO

      END

      SUBROUTINE FLIPFLOP(X,N,C)
      INTEGER N
      REAL X(N,N,2), C

      INTEGER I, J, NEW, OLD, T

      NEW = 1                                                           0008

10    CONTINUE                                                          0010
      OLD = NEW                                                         0011
      NEW = 3-OLD                                                       0012

      DO J = 2, N-1                                                     0014
         DO I = 2, N-1                                                  0015
            X(I,J,NEW) = C*X(I,J,OLD)+(1-C)*(X(I+1,J,OLD)+X(I-1,J,OLD   0016
     &      )+X(I,J-1,OLD)+X(I,J+1,OLD))                                0016
         ENDDO
      ENDDO

      IF (ABS(X(5,5,NEW)-X(5,5,OLD)).GT.0.01) GOTO 10                   0021

      END

      SUBROUTINE INIT

      COMMON /C1/ PX,DFI

      READ (5, *) PX, DFI                                               0006

      END

      SUBROUTINE MATMUL(X, Y, Z, L, M, N)
c     compute x := y * z
      REAL X(L,N), Y(L,M), Z(M,N)

      DO I = 1, L                                                       0006
         DO J = 1, N                                                    0007
            X(I,J) = 0.                                                 0008
            DO K = 1, M                                                 0009
               X(I,J) = X(I,J)+Y(I,K)*Z(K,J)                            0010
            ENDDO
         ENDDO
      ENDDO

      END
      PROGRAM SEQ

c     Goal: show different dependence tests and privatization

      PARAMETER (N=10)
      PARAMETER (LX=32, MX=92, NX=42)

      REAL A(N,N), B(N,N), C(N,N,2)

      REAL D(LX, MX, NX)

      READ *, A                                                         0012

      CALL MATMUL(B, A, A, N, N, N)                                     0014

      CALL SMATMUL(B, A, A, N, N, N)                                    0016

      CALL TRANSPOSE(B, N)                                              0018

      CALL FLIPFLOP(C, N, 0.25)                                         0020

      CALL INIT                                                         0022
      CALL TESTINIT                                                     0023
      CALL DIGITALIZE(D, LX, MX, NX)                                    0024

      END

      SUBROUTINE SMATMUL(X, Y, Z, L, M, N)
c     compute x := y * z
      REAL X(L,N), Y(L,M), Z(M,N)

      DO I = 1, L                                                       0006
         DO J = 1, N                                                    0007
            S = 0.                                                      0008
            DO K = 1, M                                                 0009
               S = S+Y(I,K)*Z(K,J)                                      0010
            ENDDO
            X(I,J) = S                                                  0012
         ENDDO
      ENDDO

      END

      SUBROUTINE TESTINIT

      PARAMETER (LX=32, MX=92, NX=42)

      COMMON /C1/ PX,DFI

      COMMON /C2/ M,MM1,MP1
c
c input data consistency checking
c
      M = NINT(PX/DFI+1.)                                               0012
      MM1 = M-1                                                         0013
      MP1 = M+1                                                         0014
      IF (.NOT.(M.GT.1.AND.M.LE.MX)) THEN                               0015
         WRITE (6, *) L, M, N, LCRY                                     0016
         STOP 3                                                         0017
      ENDIF
      END

      SUBROUTINE TRANSPOSE(X,N)
      REAL X(N,N)

      DO I = 1, N-1                                                     0005
         DO J = I+1, N                                                  0006
            T = X(I,J)                                                  0007
            X(I,J) = X(J,I)                                             0008
            X(J,I) = T                                                  0009
         ENDDO
      ENDDO

      END

      SUBROUTINE DIGITALIZE(X, LX, MX, NX)
      REAL X(MX,NX,LX)

      COMMON /C2/ M,MM1,MP1
C             <    is read   >: MM1
C             <    is written>: MS
C             <    is referenced>: MM1 MS

      MS = MM1/2                                                        0007
C             <    is read   >: MP1
C             <    is written>: MM
C             <    is referenced>: MM MP1
      MM = MP1/2                                                        0008
C             <    is read   >: N
C             <    is written>: K
C             <    is referenced>: K N

      DO K = 2, N-1                                                     0010
C             <    is read   >: MM N
C             <    is written>: J
C             <    is referenced>: J MM N
         DO J = 2, MM                                                   0011
C             <    is read   >: J MM MS N
C             <    is written>: JJ
C             <    is referenced>: J JJ MM MS N
            JJ = J+MS                                                   0012
C             <    is read   >: J JJ K MM N X(JJ,K,2)
C             <    is written>: X(J,K,1)
C             <    is referenced>: J JJ K MM N X X
            X(J,K,1) = X(JJ,K,2)                                        0013
C             <    is read   >: J JJ K MM N X(J,K,2)
C             <    is written>: X(JJ,K,1)
C             <    is referenced>: J JJ K MM N X X
            X(JJ,K,1) = X(J,K,2)                                        0014
         ENDDO
      ENDDO

      END

      SUBROUTINE FLIPFLOP(X,N,C)
      INTEGER N
      REAL X(N,N,2), C

      INTEGER I, J, NEW, OLD, T
C             <    is written>: NEW
C             <    is referenced>: NEW

      NEW = 1                                                           0008

10    CONTINUE                                                          0010
C             <    is read   >: NEW
C             <    is written>: OLD
C             <    is referenced>: NEW OLD
      OLD = NEW                                                         0011
C             <    is read   >: OLD
C             <    is written>: NEW
C             <    is referenced>: NEW OLD
      NEW = 3-OLD                                                       0012
C             <    is read   >: N
C             <    is written>: J
C             <    is referenced>: J N

      DO J = 2, N-1                                                     0014
C             <    is read   >: N
C             <    is written>: I
C             <    is referenced>: I N
         DO I = 2, N-1                                                  0015
C             <    is read   >: C I J N NEW OLD X(I,J,OLD)
C                               X(I,J+1,OLD) X(I,J-1,OLD) X(I+1,J,OLD)
C                               X(I-1,J,OLD)
C             <    is written>: X(I,J,NEW)
C             <    is referenced>: C I J N NEW OLD X X X X X X
            X(I,J,NEW) = C*X(I,J,OLD)+(1-C)*(X(I+1,J,OLD)+X(I-1,J,OLD   0016
     &      )+X(I,J-1,OLD)+X(I,J+1,OLD))                                0016
         ENDDO
      ENDDO
C             <    is read   >: NEW OLD X(5,5,NEW) X(5,5,OLD)
C             <    is referenced>: NEW OLD X X

      IF (ABS(X(5,5,NEW)-X(5,5,OLD)).GT.0.01) GOTO 10                   0021

      END

      SUBROUTINE INIT

      COMMON /C1/ PX,DFI
C             <    is read   >: __IO_EFFECTS:LUNS(5)
C             <    is written>: DFI PX __IO_EFFECTS:LUNS(5)
C             <    is referenced>: DFI PX __IO_EFFECTS:LUNS 
C                               __IO_EFFECTS:LUNS

      READ (5, *) PX, DFI                                               0006

      END

      SUBROUTINE MATMUL(X, Y, Z, L, M, N)
c     compute x := y * z
      REAL X(L,N), Y(L,M), Z(M,N)
C             <    is read   >: L
C             <    is written>: I
C             <    is referenced>: I L

      DO I = 1, L                                                       0006
C             <    is read   >: L N
C             <    is written>: J
C             <    is referenced>: J L N
         DO J = 1, N                                                    0007
C             <    is read   >: I J L N
C             <    is written>: X(I,J)
C             <    is referenced>: I J L N X
            X(I,J) = 0.                                                 0008
C             <    is read   >: L M N
C             <    is written>: K
C             <    is referenced>: K L M N
            DO K = 1, M                                                 0009
C             <    is read   >: I J K L M N X(I,J) Y(I,K) Z(K,J)
C             <    is written>: X(I,J)
C             <    is referenced>: I J K L M N X X Y Z
               X(I,J) = X(I,J)+Y(I,K)*Z(K,J)                            0010
            ENDDO
         ENDDO
      ENDDO

      END
      PROGRAM SEQ

c     Goal: show different dependence tests and privatization

      PARAMETER (N=10)
      PARAMETER (LX=32, MX=92, NX=42)

      REAL A(N,N), B(N,N), C(N,N,2)

      REAL D(LX, MX, NX)
C             <may be written>: A(*,*)
C             <    is read   >: __IO_EFFECTS:LUNS(5)
C             <    is written>: __IO_EFFECTS:LUNS(5)
C             <    is referenced>: A __IO_EFFECTS:LUNS 
C                               __IO_EFFECTS:LUNS

      READ *, A                                                         0012
C             <may be read   >: A(*,*) A(*,*) B(*,*)
C             <may be written>: B(*,*)
C             <may be referenced>: A(*,*) A(*,*) B(*,*)

      CALL MATMUL(B, A, A, N, N, N)                                     0014
C             <may be read   >: A(*,*) A(*,*)
C             <may be written>: B(*,*)
C             <may be referenced>: A(*,*) A(*,*) B(*,*)

      CALL SMATMUL(B, A, A, N, N, N)                                    0016
C             <may be read   >: B(*,*)
C             <may be written>: B(*,*)
C             <may be referenced>: B(*,*)

      CALL TRANSPOSE(B, N)                                              0018
C             <may be read   >: C(*,*,*)
C             <may be written>: C(*,*,*)
C             <may be referenced>: C(*,*,*)

      CALL FLIPFLOP(C, N, 0.25)                                         0020
C             <    is read   >: __IO_EFFECTS:LUNS(5)
C             <    is written>: INIT:DFI INIT:PX __IO_EFFECTS:LUNS(5)
C             <    is referenced>: INIT:DFI INIT:PX __IO_EFFECTS:LUNS

      CALL INIT                                                         0022
C             <may be read   >: __IO_EFFECTS:LUNS(*)
C             <may be written>: __IO_EFFECTS:LUNS(*)
C             <may be referenced>: __IO_EFFECTS:LUNS
C             <    is read   >: DIGITALIZE:M INIT:DFI INIT:PX
C             <    is written>: DIGITALIZE:M DIGITALIZE:MM1 
C                               DIGITALIZE:MP1
C             <    is referenced>: DIGITALIZE:M DIGITALIZE:MM1 
C                               DIGITALIZE:MP1 INIT:DFI INIT:PX
      CALL TESTINIT                                                     0023
C             <may be read   >: D(*,*,*)
C             <may be written>: D(*,*,*)
C             <may be referenced>: D(*,*,*)
C             <    is read   >: DIGITALIZE:MM1 DIGITALIZE:MP1
C             <    is referenced>: DIGITALIZE:MM1 DIGITALIZE:MP1
      CALL DIGITALIZE(D, LX, MX, NX)                                    0024

      END

      SUBROUTINE SMATMUL(X, Y, Z, L, M, N)
c     compute x := y * z
      REAL X(L,N), Y(L,M), Z(M,N)
C             <    is read   >: L
C             <    is written>: I
C             <    is referenced>: I L

      DO I = 1, L                                                       0006
C             <    is read   >: L N
C             <    is written>: J
C             <    is referenced>: J L N
         DO J = 1, N                                                    0007
C             <    is read   >: L N
C             <    is written>: S
C             <    is referenced>: L N S
            S = 0.                                                      0008
C             <    is read   >: L M N
C             <    is written>: K
C             <    is referenced>: K L M N
            DO K = 1, M                                                 0009
C             <    is read   >: I J K L M N S Y(I,K) Z(K,J)
C             <    is written>: S
C             <    is referenced>: I J K L M N S Y Z
               S = S+Y(I,K)*Z(K,J)                                      0010
            ENDDO
C             <    is read   >: I J L N S
C             <    is written>: X(I,J)
C             <    is referenced>: I J L N S X
            X(I,J) = S                                                  0012
         ENDDO
      ENDDO

      END

      SUBROUTINE TESTINIT

      PARAMETER (LX=32, MX=92, NX=42)

      COMMON /C1/ PX,DFI

      COMMON /C2/ M,MM1,MP1
C             <    is read   >: DFI PX
C             <    is written>: M
C             <    is referenced>: DFI M PX
c
c input data consistency checking
c
      M = NINT(PX/DFI+1.)                                               0012
C             <    is read   >: M
C             <    is written>: MM1
C             <    is referenced>: M MM1
      MM1 = M-1                                                         0013
C             <    is read   >: M
C             <    is written>: MP1
C             <    is referenced>: M MP1
      MP1 = M+1                                                         0014
C             <    is read   >: M
C             <    is referenced>: M
      IF (.NOT.(M.GT.1.AND.M.LE.MX)) THEN                               0015
C             <    is read   >: L LCRY M N __IO_EFFECTS:LUNS(6)
C             <    is written>: __IO_EFFECTS:LUNS(6)
C             <    is referenced>: L LCRY M N __IO_EFFECTS:LUNS 
C                               __IO_EFFECTS:LUNS
         WRITE (6, *) L, M, N, LCRY                                     0016
C             <    is read   >: __IO_EFFECTS:LUNS(0)
C             <    is written>: __IO_EFFECTS:LUNS(0)
C             <    is referenced>: __IO_EFFECTS:LUNS __IO_EFFECTS:LUNS
         STOP 3                                                         0017
      ENDIF
      END

      SUBROUTINE TRANSPOSE(X,N)
      REAL X(N,N)
C             <    is read   >: N
C             <    is written>: I
C             <    is referenced>: I N

      DO I = 1, N-1                                                     0005
C             <    is read   >: I N
C             <    is written>: J
C             <    is referenced>: I J N
         DO J = I+1, N                                                  0006
C             <    is read   >: I J N X(I,J)
C             <    is written>: T
C             <    is referenced>: I J N T X
            T = X(I,J)                                                  0007
C             <    is read   >: I J N X(J,I)
C             <    is written>: X(I,J)
C             <    is referenced>: I J N X X
            X(I,J) = X(J,I)                                             0008
C             <    is read   >: I J N T
C             <    is written>: X(J,I)
C             <    is referenced>: I J N T X
            X(J,I) = T                                                  0009
         ENDDO
      ENDDO

      END
// chains for DIGITALIZE

