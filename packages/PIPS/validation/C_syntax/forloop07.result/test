int main()
{
   int i, j;                                                            /*0004*/
   i = 0;                                                               /*0005*/
   /* This is an overcommented diluted for loop to test line numbering
        :-) */
   
   /* before init */
   
   /* after init */
   /* before cond */
   /* after cond */
   /* before inc */
   
   /* after inc */
   for (j = 0;j<5;j++)                                                  /*0008*/
      /* loop body */
      i++;                                                              /*0022*/
   /* after the loop */
   return 0;                                                            /*0026*/
}
