//             <may be read   >: stdout stdout[0] __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
int main()
{
//             <    is written>: s
   int t, i, a[100], b[100/2], c[100/2], s = 0;
//             <may be read   >: a[*] b[*] c[*] stdout stdout[0] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: a[*] b[*] c[*] stdout[0] 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: t
   for(t = 0; t <= 9; t += 1) {
//             <may be read   >: t
//             <may be written>: a[*]
//             <    is written>: i
scmp_task_0:
      for(i = 0; i <= 99; i += 1)
//             <may be written>: a[*]
//             <    is read   >: i t
         a[i] = i*t;
//             <may be read   >: a[*]
//             <may be written>: b[*]
//             <    is written>: i
scmp_task_1:
      for(i = 0; i <= 49; i += 1)
//             <may be read   >: a[*]
//             <may be written>: b[*]
//             <    is read   >: i
         b[i] = a[i];
//             <may be read   >: a[*]
//             <may be written>: c[*]
//             <    is written>: i
scmp_task_2:
      for(i = 0; i <= 49; i += 1)
//             <may be read   >: a[*]
//             <may be written>: c[*]
//             <    is read   >: i
         c[i] = a[i+100/2];
//             <may be read   >: b[*] stdout stdout[0] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is written>: i
scmp_task_3:
      for(i = 0; i <= 49; i += 1)
//             <may be read   >: b[*] stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
         printf("%d\n", b[i]);
//             <may be read   >: c[*] stdout stdout[0] 
//                               __IO_EFFECTS:LUNS[1]
//             <may be written>: stdout[0] __IO_EFFECTS:LUNS[1]
//             <    is written>: i
      //s = s + a[i];
scmp_task_4:
      for(i = 0; i <= 49; i += 1)
//             <may be read   >: c[*] stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: i stdout __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
         printf("%d\n", c[i]);
   }
   return 0;
}
