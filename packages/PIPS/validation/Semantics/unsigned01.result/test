int main()
{
   unsigned int i = 2;
   int j = 4;
   int k = 6;

   i++;
   i += j;
   i = k;
   return;
}

//  P() {}

int main()
{

//  P() {}

   unsigned int i = 2;

//  P(i) {i==2}

   int j = 4;

//  P(i,j) {i==2, j==4}

   int k = 6;

//  P(i,j,k) {i==2, j==4, k==6}


   i++;

//  P(i,j,k) {i==3, j==4, k==6}

   i += j;

//  P(i,j,k) {i==7, j==4, k==6}

   i = k;

//  P(i,j,k) {i==6, j==4, k==6}

   return;
}
