
Source code with proper effects

void complete03()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   {
      // BEGIN BLOCK
//             <    is written>: j
      int j = 0;                                                        /*0014*/
//             <    is read   >: i
//             <    is written>: i
      for (i = 0;i>10;i++) {                                            /*0015*/
         // BEGIN BLOCK
//             <    is read   >: j
//             <    is written>: j
         j++;                                                           /*0016*/
//             <    is read   >: i
//             <    is written>: i
         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }
//             <    is read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with cumulated effects

void complete03()
{
   // BEGIN BLOCK
   int i;                                                               /*0010*/
   {
//             <    is read   >: i
//             <    is written>: i
      // BEGIN BLOCK
//             <    is written>: j
      int j = 0;                                                        /*0014*/
//             <may be read   >: j
//             <may be written>: j
//             <    is read   >: i
//             <    is written>: i
      for (i = 0;i>10;i++) {                                            /*0015*/
//             <    is read   >: i j
//             <    is written>: i j
         // BEGIN BLOCK
//             <    is read   >: j
//             <    is written>: j
         j++;                                                           /*0016*/
//             <    is read   >: i
//             <    is written>: i
         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }
//             <    is read   >: i

   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with transformers


//  T() {}

void complete03()
{

//  T() {}

   // BEGIN BLOCK

//  T(i) {}

   int i;                                                               /*0010*/
   {

//  T(i) {i==0}

      // BEGIN BLOCK

//  T(j) {j==0}

      int j = 0;                                                        /*0014*/

//  T() {0==-1}

      for (i = 0;i>10;i++) {                                            /*0015*/

//  T(i,j) {i==i#init-1, j==j#init+1}

         // BEGIN BLOCK

//  T(j) {j==j#init+1}

         j++;                                                           /*0016*/

//  T(i) {i==i#init-1}

         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }

//  T() {}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

Source code with preconditions


//  P() {}

void complete03()
{

//  P() {}

   // BEGIN BLOCK

//  P() {}

   int i;                                                               /*0010*/
   {

//  P(i) {}

      // BEGIN BLOCK

//  P(i) {}

      int j = 0;                                                        /*0014*/

//  P(i,j) {j==0}

      for (i = 0;i>10;i++) {                                            /*0015*/

//  P() {0==-1}

         // BEGIN BLOCK

//  P() {0==-1}

         j++;                                                           /*0016*/

//  P() {0==-1}

         i--;                                                           /*0017*/
         // END BLOCK
      }
      // END BLOCK
   }

//  P(i) {i==0}


   foo(i);                                                              /*0021*/
   // END BLOCK
}

