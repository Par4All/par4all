
Source code with proper effects

int main()
{
   int i, j, k;                                                         /*0003*/
   int a[500];                                                          /*0004*/
//             <    is written>: j
   j = 1;
//             <    is written>: i

   for(i = 0; i <= 499; i += 1) {
//             <    is read   >: i
//             <    is written>: a[i]
      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;                                                         /*0008*/
//             <    is read   >: j
//             <    is written>: j
      j++;
   }
//             <    is written>: k
   // We have j==501 in the preconditions here... since we use
   // the proper activate and properties
   k = 2;                                                               /*0012*/
//             <    is read   >: k
   return k;                                                            /*0013*/
}

Source code with cumulated effects

int main()
{
   int i, j, k;                                                         /*0003*/
   int a[500];                                                          /*0004*/
//             <    is written>: j
   j = 1;
//             <may be read   >: i j
//             <may be written>: a[*] j
//             <    is written>: i

   for(i = 0; i <= 499; i += 1) {
//             <may be written>: a[*]
//             <    is read   >: i
      // Cumulated effects should be a[i] here and not a[*]
      a[i] = i;                                                         /*0008*/
//             <    is read   >: j
//             <    is written>: j
      j++;
   }
//             <    is written>: k
   // We have j==501 in the preconditions here... since we use
   // the proper activate and properties
   k = 2;                                                               /*0012*/
//             <    is read   >: k
   return k;                                                            /*0013*/
}

