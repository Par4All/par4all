
//  T(main) {main==0}

int main(void)
{

//  T(b) {}

   _Bool b;

//  T(i,j,k) {}

   int i, j, k;

//  T(i) {i<=j, j<=i+1}


   if (k)

//  T(i) {i==j}

      i = j;
   else

//  T(i) {i==j-1, k==0}

      i = j-1;

//  T(b) {b==b#init, 0<=b, b<=1, i<=j, j<=i+1}


   b = b&&i<=j;

//  T(main) {main==0, 0<=b, b<=1, i<=j, j<=i+1}


   return 0;
}
