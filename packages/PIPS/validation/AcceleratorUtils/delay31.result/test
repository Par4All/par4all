int a(int i)
{

//  <j[PHI1]-W-MAY-{}>

   int j[2] = {1, 2}, k[2];

//  <k[PHI1]-W-EXACT-{PHI1==0}>

   k[0] = 0;

//  <j[PHI1]-R-EXACT-{0<=PHI1, PHI1<=1}>
//  <k[PHI1]-W-EXACT-{0<=PHI1, PHI1<=1}>

   load(k, j);

//  <j[PHI1]-W-EXACT-{0<=PHI1, PHI1<=1}>
//  <k[PHI1]-R-EXACT-{0<=PHI1, PHI1<=1}>

   store(j, k);

//  <j[PHI1]-R-EXACT-{PHI1==1}>

   return j[1];
}

