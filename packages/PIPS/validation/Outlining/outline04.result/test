Select the most precise analysis:
int main(int argc, char *argv[])
{
   int i, j;

init_kernel:   for (i = 0;i<64;i++)
      for (j = 0;j<64;j++)
         imagein_re[i][j] = 2*i;

   for (i = 0;i<64;i++) {
      printf("Line %d:\n");
      for (j = 0;j<64;j++)
         printf("%d ", imagein_re[i][j]);
      puts("\n");
   }

   return 0;
}

