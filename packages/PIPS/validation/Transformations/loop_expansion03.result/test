after loop expansion
int pop(int pop)
{
   int i, j;
   //PIPS generated variable
   int I_0;
   I_0 = i;
#pragma omp for private(i,j)
l99999:
   for(i = i; i <= I_0+128*((127-i+op)/128)-1; i += 1) {
      j = i*2;
      if (i>=I_0&&i<=op-1) 
         printf("%d", j);
   }
}
