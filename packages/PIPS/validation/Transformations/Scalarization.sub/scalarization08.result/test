
OUT regions

int scalarization08(int x[10], int y[10][10])
{
   int i, j, k;

//  <x[PHI1]-OUT-EXACT-{PHI1==1}>

   for(i = 0; i <= 9; i += 1)

//  <x[PHI1]-OUT-EXACT-{PHI1==1, PHI1==i}>

      for(j = 0; j <= 9; j += 1)

//  <x[PHI1]-OUT-EXACT-{PHI1==1, PHI1==i, j==9, 0<=j}>

         x[i] = y[i][j];
   //printf("%d", x[1]);
   k = x[1];
   return k;
}

Code after scalarization

int scalarization08(int x[10], int y[10][10])
{
   int i, j, k;
   for(i = 0; i <= 9; i += 1) {
      //PIPS generated variable
      register int x_0;

      for(j = 0; j <= 9; j += 1)
         x_0 = y[i][j];
      x[i] = x_0;
   }
   //printf("%d", x[1]);
   k = x[1];
   return k;
}
