      ret = 0;
      goto break_1;
switch_1_case_6:      ;
      freia_aipo_dilate_6c(imout, imin, kernel_6c);
      for(i = 1; i <= size-1; i += 1)
         freia_aipo_dilate_6c(imout, imout, kernel_6c);
      ret = 0;
