
"main" file

int main()
{
   int i;
   int k = 2;
   int j;
   int m;
   int a[10];
   i = 1, j = 2*(m-m), k = 3*(m-m);
   i = k += j-j;
   a[i-i] = 1, a[j-j] = 2, a[k-k] = 3;

   k -= j-j;
   k *= j-j;
   k /= j+1-j;
   k <<= j-j;
   k >>= j-j;
   k |= j*m-j*m;
   i+++j-j;
   return i;
}

"main" file with preconditions


//  P() {}

int main()
{

//  P() {}

   int i;

//  P() {}

   int k = 2;

//  P() {k==2}

   int j;

//  P() {k==2}

   int m;

//  P() {k==2}

   int a[10];

//  P() {k==2}

   i = 1, j = 2*(m-m), k = 3*(m-m);

//  P(i,j,k) {i==1, j==0, k==0}

   i = k += j-j;

//  P(i,j,k) {i==0, j==0, k==0}

   a[i-i] = 1, a[j-j] = 2, a[k-k] = 3;

//  P(i,j,k) {i==0, j==0, k==0}


   k -= j-j;

//  P(i,j,k) {i==0, j==0, k==0}

   k *= j-j;

//  P(i,j,k) {i==0, j==0, k==0}

   k /= j+1-j;

//  P(i,j,k) {i==0, j==0}

   k <<= j-j;

//  P(i,j,k) {i==0, j==0}

   k >>= j-j;

//  P(i,j,k) {i==0, j==0}

   k |= j*m-j*m;

//  P(i,j,k) {i==0, j==0}

   i+++j-j;

//  P(i,j,k) {j==0}

   return i;
}

"main" file after partial evaluation

int main()
{
   int i;
   int k = 2;
   int j;
   int m;
   int a[10];
   i = 1, j = 0, k = 0;
   i = k += 0;
   a[0] = 1, a[0] = 2, a[0] = 3;

   k -= 0;
   k *= 0;
   k /= 1;
   k <<= 0;
   k >>= 0;
   k |= 0;
   i++;
   return i;
}
