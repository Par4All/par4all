
"main" file

int main()
{
   int i;
   int j;
   i+++j-j;
   j;
   return i;
}

"main" file with preconditions


//  P() {}

int main()
{

//  P() {}

   int i;

//  P() {}

   int j;

//  P() {}

   i+++j-j;

//  P(i) {}

   j;

//  P(i) {}

   return i;
}

"main" file after partial evaluation

int main()
{
   int i;
   int j;
   i++;
   j;
   return i;
}
