int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;
   
   // first loop nest : 
   // do par <- annotate
   //   do seq
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         if (i==0)
            a[i] = (float) i;
         else
            a[i] = a[i]+1.0;
   
   // second loop nest : 
   // do par <- annotate whole loop nest
   //   do par
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         b[i][j] = (float) i*j;
   
   // third loop nest : 
   // do par <- annotate whole loop nest
   //   do par
   //     do par
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         for(k = 0; k <= 498; k += 1)
            c[i][j][k] = (float) i*j;
   
   // fourth loop nest : 
   // do par <- annotate whole loop nest
   //   do par
   for(i = 10; i <= 508; i += 1)
      for(j = 20; j <= 518; j += 1)
         b[i-10][j-20] = (float) i*j;
   
   
   // fifth loop nest : 
   // do seq
   //   do par <- do not annotate
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         b[i+1][j] = b[i][j]*2.0;
   
   
   return 0;
}
int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;
   
   // first loop nest : 
   // do par <- annotate
   //   do seq
#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
      for(j = 0; j <= 498; j += 1)
         if (i==0)
            a[i] = (float) i;
         else
            a[i] = a[i]+1.0;
   
   // second loop nest : 
   // do par <- annotate whole loop nest
   //   do par
#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)
         b[i][j] = (float) i*j;
   
   // third loop nest : 
   // do par <- annotate whole loop nest
   //   do par
   //     do par
#pragma omp parallel for private(j,k)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for private(k)
      for(j = 0; j <= 498; j += 1)
#pragma omp parallel for 
         for(k = 0; k <= 498; k += 1)
            c[i][j][k] = (float) i*j;
   
   // fourth loop nest : 
   // do par <- annotate whole loop nest
   //   do par
#pragma omp parallel for private(j)
   for(i = 10; i <= 508; i += 1)
#pragma omp parallel for 
      for(j = 20; j <= 518; j += 1)
         b[i-10][j-20] = (float) i*j;
   
   
   // fifth loop nest : 
   // do seq
   //   do par <- do not annotate
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)
         b[i+1][j] = b[i][j]*2.0;
   
   
   return 0;
}
int main()
{
   float a[501];
   float b[501][501];
   float c[501][501][501];
   int i, j, k;
   // Loop nest P4A begin,1D(499)
#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
      // Loop nest P4A end
      if (i<=498)
         for(j = 0; j <= 498; j += 1)
            if (i==0)
               a[i] = (float) i;
            else
               a[i] = a[i]+1.0;
   // Loop nest P4A begin,2D(499,499)
#pragma omp parallel for private(j)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)
         // Loop nest P4A end
         if (i<=498&&j<=498) 
            b[i][j] = (float) i*j;
   // Loop nest P4A begin,3D(499,499,499)
#pragma omp parallel for private(j,k)
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for private(k)
      for(j = 0; j <= 498; j += 1)
#pragma omp parallel for 
         for(k = 0; k <= 498; k += 1)
            // Loop nest P4A end
            if (i<=498&&j<=498&&k<=498) 
               c[i][j][k] = (float) i*j;
   // Loop nest P4A begin,2D(499,499)
#pragma omp parallel for private(j)
   for(i = 10; i <= 508; i += 1)
#pragma omp parallel for 
      for(j = 20; j <= 518; j += 1)
         // Loop nest P4A end
         if (i<=508&&j<=518) 
            b[i-10][j-20] = (float) i*j;
   
   
   // fifth loop nest : 
   // do seq
   //   do par <- do not annotate
   for(i = 0; i <= 498; i += 1)
#pragma omp parallel for 
      for(j = 0; j <= 498; j += 1)
         b[i+1][j] = b[i][j]*2.0;
   
   
   return 0;
}
