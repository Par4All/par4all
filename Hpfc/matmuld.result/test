!
! File MATMUL_A_5_2_node.h
! This file has been automatically generated by the HPF compiler
!
      IF (MSTATUS(5).EQ.5) THEN
         IF (.NOT.LIVE MAPPING(2)) THEN
! send part
! remapping A[5]: A[5] -> A_1[2]
            PSI1 = MY POS(1,1)
            PSI2 = MY POS(2,1)
            IF (MY LID.LE.4) THEN
               PSIp1 = PSI1
               T LID = 2*PSIp1-1
! - lazy broadcasting
               BUF INDEX = 0
               SEND NOT INITIALIZED = .TRUE.
               DO LALPHA2 = 1, 5
                  DO LALPHA1 = 1, 5
                     LALPHAp2 = LALPHA2+5*PSI2-5
                     LALPHAp1 = LALPHA1
                     BUF INDEX = BUF INDEX+1
                     HPFC REAL8 BUFF(BUF INDEX) = A(LALPHA1,LALPHA2)
                     IF (BUF INDEX.EQ.HPFC REAL8 BUFF SIZE) THEN
                        CALL HPFC REAL8 BUFPCK
                        CALL HPFC BROADCAST 1(T LID, 2, 1)
                        BUF INDEX = 0
                        SEND NOT INITIALIZED = .TRUE.
                     ENDIF
                  ENDDO
               ENDDO
               IF (BUF INDEX.NE.0) THEN
                  CALL HPFC REAL8 BUFPCK
                  CALL HPFC BROADCAST 1(T LID, 2, 1)
               ENDIF
            ENDIF
! receive part
            PSIp1 = MY POS(1,1)
            PSIp2 = MY POS(2,1)
            IF (MY LID.LE.4) THEN
               PSI1 = PSIp1
               DO PSI2 = 1, 2
                  T LID = 2*PSI1+PSI2-2
                  IF (MY LID.NE.T LID) THEN
! - lazy receiving
                     BUF INDEX = 0
                     RECEIVED NOT PERFORMED = .TRUE.
                     SIZE OF RECEIVED BUFFER = 0
                     DO LALPHA2 = 1, 5
                        DO LALPHA1 = 1, 5
                           LALPHAp2 = LALPHA2+5*PSI2-5
                           LALPHAp1 = LALPHA1
                           IF (BUF INDEX.EQ.SIZE OF RECEIVED BUFFER
     &                     ) THEN
                              RECEIVED NOT PERFORMED = .TRUE.
                              CALL HPFC REAL8 BUFUPK(T LID)
                              BUF INDEX = 0
                           ENDIF
                           BUF INDEX = BUF INDEX+1
                           A_1(LALPHAp1,LALPHAp2) = HPFC REAL8 BUFF(
     &                     BUF INDEX)
                        ENDDO
                     ENDDO
                  ELSE
! - lazy copying
                     DO LALPHA2 = 1, 5
                        DO LALPHA1 = 1, 5
                           LALPHAp2 = LALPHA2+5*PSI2-5
                           LALPHAp1 = LALPHA1
                           A_1(LALPHAp1,LALPHAp2) = A(LALPHA1,LALPHA2
     &                     )
                        ENDDO
                     ENDDO
                  ENDIF
               ENDDO
            ENDIF
! end of remapping
         ENDIF
         LIVE MAPPING(2) = .TRUE.
         MSTATUS(5) = 2
      ENDIF
!
! That is all for MATMUL_A_5_2_node.h
!
!
! File MATMUL_B_4_1_node.h
! This file has been automatically generated by the HPF compiler
!
      IF (MSTATUS(4).EQ.4) THEN
         IF (.NOT.LIVE MAPPING(1)) THEN
! send part
! remapping B[4]: B[4] -> B_1[1]
            PSI1 = MY POS(1,1)
            PSI2 = MY POS(2,1)
            IF (MY LID.LE.4) THEN
               PSIp2 = PSI2
               T LID = PSIp2
! - lazy broadcasting
               BUF INDEX = 0
               SEND NOT INITIALIZED = .TRUE.
               DO LALPHA2 = 1, 5
                  DO LALPHA1 = 1, 5
                     LALPHAp2 = LALPHA2
                     LALPHAp1 = LALPHA1+5*PSI1-5
                     BUF INDEX = BUF INDEX+1
                     HPFC REAL8 BUFF(BUF INDEX) = B(LALPHA1,LALPHA2)
                     IF (BUF INDEX.EQ.HPFC REAL8 BUFF SIZE) THEN
                        CALL HPFC REAL8 BUFPCK
                        CALL HPFC BROADCAST 1(T LID, 2, 2)
                        BUF INDEX = 0
                        SEND NOT INITIALIZED = .TRUE.
                     ENDIF
                  ENDDO
               ENDDO
               IF (BUF INDEX.NE.0) THEN
                  CALL HPFC REAL8 BUFPCK
                  CALL HPFC BROADCAST 1(T LID, 2, 2)
               ENDIF
            ENDIF
! receive part
            PSIp1 = MY POS(1,1)
            PSIp2 = MY POS(2,1)
            IF (MY LID.LE.4) THEN
               DO PSI1 = 1, 2
                  PSI2 = PSIp2
                  T LID = 2*PSI1+PSI2-2
                  IF (MY LID.NE.T LID) THEN
! - lazy receiving
                     BUF INDEX = 0
                     RECEIVED NOT PERFORMED = .TRUE.
                     SIZE OF RECEIVED BUFFER = 0
                     DO LALPHA2 = 1, 5
                        DO LALPHA1 = 1, 5
                           LALPHAp2 = LALPHA2
                           LALPHAp1 = LALPHA1+5*PSI1-5
                           IF (BUF INDEX.EQ.SIZE OF RECEIVED BUFFER
     &                     ) THEN
                              RECEIVED NOT PERFORMED = .TRUE.
                              CALL HPFC REAL8 BUFUPK(T LID)
                              BUF INDEX = 0
                           ENDIF
                           BUF INDEX = BUF INDEX+1
                           B_1(LALPHAp1,LALPHAp2) = HPFC REAL8 BUFF(
     &                     BUF INDEX)
                        ENDDO
                     ENDDO
                  ELSE
! - lazy copying
                     DO LALPHA2 = 1, 5
                        DO LALPHA1 = 1, 5
                           LALPHAp2 = LALPHA2
                           LALPHAp1 = LALPHA1+5*PSI1-5
                           B_1(LALPHAp1,LALPHAp2) = B(LALPHA1,LALPHA2
     &                     )
                        ENDDO
                     ENDDO
                  ENDIF
               ENDDO
            ENDIF
! end of remapping
         ENDIF
         LIVE MAPPING(1) = .TRUE.
         MSTATUS(4) = 1
      ENDIF
