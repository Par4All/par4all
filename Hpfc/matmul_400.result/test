         IF (.NOT.LIVE MAPPING(5)) THEN
! send part
! remapping A[5]: A_1[2] -> A[5]
            PSI1 = MY POS(1,1)
            PSI2 = MY POS(2,1)
            IF (MY LID.LE.4.AND.PSI2.EQ.PSI1) THEN
               DO TMP3 = 0, 1
                  PSIp1 = PSI1
                  PSIp2 = TMP3+1
                  T LID = 2*PSIp1+PSIp2-2
                  IF (MY LID.NE.T LID.AND..NOT.HPFC TWIN P(2, T LID)
     &            ) THEN
! - lazy sending
                     BUF INDEX = 0
                     SEND NOT INITIALIZED = .TRUE.
                     DO LALPHA2 = 200*PSIp2-199, 200*PSIp2
                        DO LALPHA1 = 1, 200
                           LALPHAp2 = LALPHA2-200*PSIp2+200
                           LALPHAp1 = LALPHA1
                           BUF INDEX = BUF INDEX+1
                           HPFC REAL8 BUFF(BUF INDEX) = A_1(LALPHA1,
     &                     LALPHA2)
                           IF (BUF INDEX.EQ.HPFC REAL8 BUFF SIZE
     &                     ) THEN
                              CALL HPFC REAL8 BUFPCK
                              CALL HPFC REMAPBRD 0(2, T LID)
                              BUF INDEX = 0
                              SEND NOT INITIALIZED = .TRUE.
                           ENDIF
                        ENDDO
                     ENDDO
                     IF (BUF INDEX.NE.0) THEN
                        CALL HPFC REAL8 BUFPCK
                        CALL HPFC REMAPBRD 0(2, T LID)
                     ENDIF
                  ENDIF
               ENDDO
            ENDIF
! receive part
            PSIp1 = MY POS(1,1)
            PSIp2 = MY POS(2,1)
            IF (MY LID.LE.4) THEN
               PSI1 = PSIp1
               PSI2 = PSIp1
               TMP3 = PSIp2-1
               T LID = 2*PSI1+PSI2-2
               IF (MY LID.NE.T LID.AND..NOT.HPFC TWIN P(2, T LID)
     &         ) THEN
