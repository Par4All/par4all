     &                  ) THEN
                           RECEIVED NOT PERFORMED = .TRUE.
                           CALL HPFC REAL8 BUFUPK(T LID)
                           BUF INDEX = 0
                        ENDIF
                        BUF INDEX = BUF INDEX+1
                        A(LALPHAp1,LALPHAp2) = HPFC REAL8 BUFF(
     &                  BUF INDEX)
                     ENDDO
                  ENDDO
               ELSE
! - lazy copying
                  DO LALPHA2 = 200*PSIp2-199, 200*PSIp2
                     DO LALPHA1 = 1, 200
