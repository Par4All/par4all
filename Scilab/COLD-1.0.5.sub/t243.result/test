   scilab_rt_tic__();
   double _u_x[1][1000];
   trid__d2(1, 1000, _u_x);
   double _u_elapsedTime = scilab_rt_toc__();
   /* disp("Elapsed Time: "); */
   /* disp(elapsedTime); */
   scilab_rt_disp_s0_("Mean value of matrix x: ");
