      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_07, I_06, I_05, I_04, I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 RED0(0:7), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      DO 99998 LU_IND0 = 0, 9999, 8
c SIMD_COMMENT_0
         I_07 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         RED0(0) = RED0(0)+I_07                                         0014
c SIMD_COMMENT_0
         I_06 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         RED0(1) = RED0(1)+I_06                                         0014
c SIMD_COMMENT_0
         I_05 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         RED0(2) = RED0(2)+I_05                                         0014
c SIMD_COMMENT_0
         I_04 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         RED0(3) = RED0(3)+I_04                                         0014
c SIMD_COMMENT_0
         I_03 = B(LU_IND0+5)*C(LU_IND0+5)
c SIMD_COMMENT_1
         RED0(4) = RED0(4)+I_03                                         0014
c SIMD_COMMENT_0
         I_02 = B(LU_IND0+6)*C(LU_IND0+6)
c SIMD_COMMENT_1
         RED0(5) = RED0(5)+I_02                                         0014
c SIMD_COMMENT_0
         I_01 = B(LU_IND0+7)*C(LU_IND0+7)
c SIMD_COMMENT_1
         RED0(6) = RED0(6)+I_01                                         0014
c SIMD_COMMENT_0
         I_00 = B(LU_IND0+8)*C(LU_IND0+8)
c SIMD_COMMENT_1
         RED0(7) = RED0(7)+I_00                                         0014
99998    CONTINUE
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_07, I_06, I_05, I_04, I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v8hi_vec9(0:7), v8hi_vec8(0:7), v8hi_vec7(0:7), 
     &v8hi_vec6(0:7), v8hi_vec5(0:7), v8hi_vec4(0:7), v8hi_vec3(0:7)
     &, v8hi_vec2(0:7), v8hi_vec1(0:7), v8hi_vec0(0:7), RED0(0:7), A(
     &1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      DO 99998 LU_IND0 = 0, 9999, 8
         CALL SIMD_LOAD_V8HI(v8hi_vec2, C(LU_IND0+1))
         CALL SIMD_LOAD_V8HI(v8hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v8hi_vec0, v8hi_vec1, v8hi_vec2)
         CALL SIMD_SAVE_GENERIC_V8HI(v8hi_vec0, I_07, I_06, I_05, 
     &   I_04, I_03, I_02, I_01, I_00)
         CALL SIMD_LOAD_V8HI(v8hi_vec3, RED0(0))
         CALL SIMD_ADDW(v8hi_vec3, v8hi_vec3, v8hi_vec0)
         CALL SIMD_SAVE_V8HI(v8hi_vec3, RED0(0))
99998    CONTINUE
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_07, I_06, I_05, I_04, I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v8hi_vec9(0:7), v8hi_vec8(0:7), v8hi_vec7(0:7), 
     &v8hi_vec6(0:7), v8hi_vec5(0:7), v8hi_vec4(0:7), v8hi_vec3(0:7)
     &, v8hi_vec2(0:7), v8hi_vec1(0:7), v8hi_vec0(0:7), RED0(0:7), A(
     &1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      DO 99998 LU_IND0 = 0, 9999, 8
         CALL SIMD_LOAD_V8HI(v8hi_vec2, C(LU_IND0+1))
         CALL SIMD_LOAD_V8HI(v8hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v8hi_vec0, v8hi_vec1, v8hi_vec2)
         CALL SIMD_LOAD_V8HI(v8hi_vec3, RED0(0))
         CALL SIMD_ADDW(v8hi_vec3, v8hi_vec3, v8hi_vec0)
         CALL SIMD_SAVE_V8HI(v8hi_vec3, RED0(0))
99998    CONTINUE
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_07, I_06, I_05, I_04, I_03, I_02, I_01, I_00, I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 v8hi_vec9(0:7), v8hi_vec8(0:7), v8hi_vec7(0:7), 
     &v8hi_vec6(0:7), v8hi_vec5(0:7), v8hi_vec4(0:7), v8hi_vec3(0:7)
     &, v8hi_vec2(0:7), v8hi_vec1(0:7), v8hi_vec0(0:7), RED0(0:7), A(
     &1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      CALL SIMD_LOAD_V8HI(v8hi_vec3, RED0(0))
      DO 99998 LU_IND0 = 0, 9999, 8
         CALL SIMD_LOAD_V8HI(v8hi_vec2, C(LU_IND0+1))
         CALL SIMD_LOAD_V8HI(v8hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v8hi_vec0, v8hi_vec1, v8hi_vec2)
         CALL SIMD_ADDW(v8hi_vec3, v8hi_vec3, v8hi_vec0)
99998    CONTINUE
      CALL SIMD_SAVE_V8HI(v8hi_vec3, RED0(0))
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER LU_IND0
      INTEGER*2 v8hi_vec3(0:7), v8hi_vec2(0:7), v8hi_vec1(0:7), 
     &v8hi_vec0(0:7), RED0(0:7), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      CALL SIMD_LOAD_V8HI(v8hi_vec3, RED0(0))
      DO 99998 LU_IND0 = 0, 9999, 8
         CALL SIMD_LOAD_V8HI(v8hi_vec2, C(LU_IND0+1))
         CALL SIMD_LOAD_V8HI(v8hi_vec1, B(LU_IND0+1))
         CALL SIMD_MULW(v8hi_vec0, v8hi_vec1, v8hi_vec2)
         CALL SIMD_ADDW(v8hi_vec3, v8hi_vec3, v8hi_vec0)
99998    CONTINUE
      CALL SIMD_SAVE_V8HI(v8hi_vec3, RED0(0))
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)

      END

