         I_0 = B(I)*C(I)
         A(1) = A(1)+I_0                                                0014
10       CONTINUE                                                       0015

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
      LU_NUB0 = 10000
      LU_IB0 = 0
      LU_IND0 = 0
99999 CONTINUE
      DO 99998 LU_IND0 = 0, 9999, 8
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+5)*C(LU_IND0+5)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+6)*C(LU_IND0+6)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+7)*C(LU_IND0+7)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+8)*C(LU_IND0+8)
c SIMD_COMMENT_1
         A(1) = A(1)+I_0                                                0014
99998    CONTINUE
      I = 10001

      END
      SUBROUTINE DOTPROD(A, B, C)
      INTEGER*2 SIZE
      PARAMETER (SIZE = 10000)
      INTEGER*2 I_0
      INTEGER LU_IND0, LU_IB0, LU_NUB0, I
      INTEGER*2 RED0(0:7), A(1:1), B(1:SIZE), C(1:SIZE)

      A(1) = 0                                                          0012
      LU_NUB0 = 10000
      LU_IB0 = 0
      LU_IND0 = 0
99999 CONTINUE
      RED0(0) = 0
      RED0(1) = 0
      RED0(2) = 0
      RED0(3) = 0
      RED0(4) = 0
      RED0(5) = 0
      RED0(6) = 0
      RED0(7) = 0
      DO 99998 LU_IND0 = 0, 9999, 8
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+1)*C(LU_IND0+1)
c SIMD_COMMENT_1
         RED0(0) = RED0(0)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+2)*C(LU_IND0+2)
c SIMD_COMMENT_1
         RED0(1) = RED0(1)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+3)*C(LU_IND0+3)
c SIMD_COMMENT_1
         RED0(2) = RED0(2)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+4)*C(LU_IND0+4)
c SIMD_COMMENT_1
         RED0(3) = RED0(3)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+5)*C(LU_IND0+5)
c SIMD_COMMENT_1
         RED0(4) = RED0(4)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+6)*C(LU_IND0+6)
c SIMD_COMMENT_1
         RED0(5) = RED0(5)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+7)*C(LU_IND0+7)
c SIMD_COMMENT_1
         RED0(6) = RED0(6)+I_0                                          0014
c SIMD_COMMENT_0
         I_0 = B(LU_IND0+8)*C(LU_IND0+8)
c SIMD_COMMENT_1
         RED0(7) = RED0(7)+I_0                                          0014
99998    CONTINUE
      A(1) = RED0(7)+RED0(6)+RED0(5)+RED0(4)+RED0(3)+RED0(2)+RED0(1)+
     &RED0(0)+A(1)
      I = 10001

      END
      SUBROUTINE DOTPROD(A, B, C)
