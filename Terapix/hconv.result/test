void runner(int n, int img_out[n][n-2], int img[n][n], int mask[3])
{
   int x, y, z;
#pragma terapix
   for(y = 0; y <= n-1; y += 1)
      for(x = 1; x <= n-1-1; x += 1) {
         int val = 0;
         for(z = -1; z <= 1; z += 1)
            val += img[y][x+z]*mask[1+z];
         img_out[y][x-1] = val;
      }
}
