void dscal_ur(int n, float da, float dx[n])
{
   int i, m;
   m = n%5;
   if (m!=0) {
      for(i = 0; i <= m-1; i += 1)
         dx[i] = da*dx[i];
      if (n<5)
         return;
   }
   for(i = m; i <= n-1; i += 5) {
      dx[i] = da*dx[i];
      dx[i+1] = da*dx[i+1];
      dx[i+2] = da*dx[i+2];
      dx[i+3] = da*dx[i+3];
      dx[i+4] = da*dx[i+4];
   }
}
