! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 3 ops and 2 copies, optimized to 3 ops and 0+0+0 copies

// hold kernels for freia_dup_14_opencl_helper_0_0_0
static cl_kernel freia_dup_14_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_dup_14_opencl_helper_0_0_0
static freia_status freia_dup_14_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_dup_14_opencl_helper_0_0_0
  const char * freia_dup_14_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_dup_14_opencl_helper_0_0_0\n"
    "KERNEL void freia_dup_14_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  // N not needed\n"
    "  // S not needed\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t38 = PIXEL_MAX;\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL t45 = PIXEL_MIN;\n"
    "\n"
    "    // pixel computations\n"
    "    t38 = PIXEL_INF(t38, (is_W)? PIXEL_MAX: in0W);\n"
    "    t38 = PIXEL_INF(t38, in0);\n"
    "    t38 = PIXEL_INF(t38, (is_E)? PIXEL_MAX: in0E);\n"
    "    t45 = PIXEL_SUP(t45, (is_W)? PIXEL_MIN: in0W);\n"
    "    t45 = PIXEL_SUP(t45, in0);\n"
    "    t45 = PIXEL_SUP(t45, (is_E)? PIXEL_MIN: in0E);\n"
    "    PIXEL t49 = PIXEL_AND(t38, t45);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t49;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_dup_14_opencl_helper_0_0_0_source, "freia_dup_14_opencl_helper_0_0_0", "-DPIXEL8", &freia_dup_14_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_dup_14_opencl_helper_0_0_0_source, "freia_dup_14_opencl_helper_0_0_0", "-DPIXEL16", &freia_dup_14_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_dup_14_opencl_helper_0_0_0
freia_status freia_dup_14_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_dup_14_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_dup_14_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_dup_14_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_dup_14

// opencl for dag 0

// opencl function freia_dup_14_opencl_helper_0_0_0
KERNEL void freia_dup_14_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  // N not needed
  // S not needed

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t38 = PIXEL_MAX;
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL t45 = PIXEL_MIN;

    // pixel computations
    t38 = PIXEL_INF(t38, (is_W)? PIXEL_MAX: in0W);
    t38 = PIXEL_INF(t38, in0);
    t38 = PIXEL_INF(t38, (is_E)? PIXEL_MAX: in0E);
    t45 = PIXEL_SUP(t45, (is_W)? PIXEL_MIN: in0W);
    t45 = PIXEL_SUP(t45, in0);
    t45 = PIXEL_SUP(t45, (is_E)? PIXEL_MIN: in0E);
    PIXEL t49 = PIXEL_AND(t38, t45);

    // set output pixels
    p0[i] = t49;
  }
}

! updated function

int freia_dup_14(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdout;                                                  /*0007*/

   freia_data2d *imin;                                                  /*0009*/
   freia_data2d *imout;                                                 /*0012*/
   
   
   freia_status ret_0 = 0;                                              /*0019*/
   freia_status ret_1;                                                  /*0021*/
   
   /* open videos flow */
   freia_common_open_input(&fdin, 0);                                   /*0024*/
   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, 8);          /*0025*/
   
   /* images creation */
   imin = freia_common_create_data(16, fdin.framewidth, fdin.frameheight);             /*0028*/
   imout = freia_common_create_data(16, fdin.framewidth, fdin.frameheight);            /*0031*/

   freia_common_rx_image(imin, &fdin);                                  /*0033*/
   
   
   
   ret_0 = 0;                                                           /*0037*/
   freia_dup_14_opencl_helper_0_0_0(imout, imin);                       /*0038*/
   
   
   ret_1 = 0;                                                           /*0044*/
   
   
   
   freia_common_tx_image(imout, &fdout);                                /*0051*/
   
   /* images destruction */
   freia_common_destruct_data(imin);                                    /*0054*/
   freia_common_destruct_data(imout);                                   /*0057*/
   
   /* close videos flow */
   freia_common_close_input(&fdin);                                     /*0060*/
   freia_common_close_output(&fdout);                                   /*0061*/

   return 0;                                                            /*0063*/
}

