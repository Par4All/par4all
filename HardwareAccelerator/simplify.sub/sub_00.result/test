// compile kernels for sub_00_opencl_helper_0_0_0
static freia_status sub_00_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for sub_00_opencl_helper_0_0_0
  const char * sub_00_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function sub_00_opencl_helper_0_0_0\n"
    "KERNEL void sub_00_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  int c3,\n"
    "  int c4)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // pixel computations\n"
    "    PIXEL t7 = PIXEL_SET(c0);\n"
    "    PIXEL t9 = PIXEL_SET(c1);\n"
    "    PIXEL t10 = PIXEL_THRESHOLD(t9, c2, c3, c4);\n"
    "    // set output pixels\n"
    "    p0[i] = t7;\n"
    "    p1[i] = t10;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(sub_00_opencl_helper_0_0_0_source, "sub_00_opencl_helper_0_0_0", "-DPIXEL8", &sub_00_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(sub_00_opencl_helper_0_0_0_source, "sub_00_opencl_helper_0_0_0", "-DPIXEL16", &sub_00_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function sub_00_opencl_helper_0_0_0
freia_status sub_00_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  int32_t c0,
  int32_t c1,
  int32_t c2,
  int32_t c3,
  int32_t c4)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= sub_00_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = o0->bpp>>4;
  cl_kernel kernel = sub_00_opencl_helper_0_0_0_kernel[bpp];

  // call kernel sub_00_opencl_helper_0_0_0
