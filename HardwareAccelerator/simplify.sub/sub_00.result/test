// generated OpenCL kernels for function sub_00

// opencl for dag 0

// opencl function sub_00_opencl_helper_0_0_0
KERNEL void sub_00_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // pixel computations
    PIXEL t7 = PIXEL_SET(c0);
    PIXEL t9 = PIXEL_SET(c1);
    PIXEL t10 = PIXEL_THRESHOLD(t9, c2, c3, c4);
    // set output pixels
    p0[i] = t7;
    p1[i] = t10;
  }
}

