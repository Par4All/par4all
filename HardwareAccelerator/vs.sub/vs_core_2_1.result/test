! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 16 ops and 2 copies, optimized to 16 ops and 0+0+0 copies

// hold kernels for vs_core_2_opencl_helper_0_0_0
static cl_kernel vs_core_2_opencl_helper_0_0_0_kernel[2];

// compile kernels for vs_core_2_opencl_helper_0_0_0
static freia_status vs_core_2_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for vs_core_2_opencl_helper_0_0_0
  const char * vs_core_2_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_2_opencl_helper_0_0_0\n"
    "KERNEL void vs_core_2_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  int2 mmin = { PIXEL_MAX, 0 };\n"
    "  int2 mmax = { PIXEL_MIN, 0 };\n"
    "  int idy = get_global_id(0);\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t15 = PIXEL_ABSDIFF(in1, in0);\n"
    "    PIXEL t19 = PIXEL_MUL(in1, c0);\n"
    "    PIXEL t20 = PIXEL_MUL(in0, c1);\n"
    "    PIXEL t21 = PIXEL_ADD(t20, t19);\n"
    "    PIXEL_MAXIMUM(red26, t15);\n"
    "    PIXEL_MINIMUM(red27, t15);\n"
    "    PIXEL t22 = PIXEL_DIV(t21, c2);\n"
    "    // set output pixels\n"
    "    p0[i] = t15;\n"
    "    p1[i] = t22;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  redX[idy].max = mmax.x;\n"
    "  redX[idy].min = mmin.x;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_0_0_source, "vs_core_2_opencl_helper_0_0_0", "-DPIXEL8", &vs_core_2_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_0_0_source, "vs_core_2_opencl_helper_0_0_0", "-DPIXEL16", &vs_core_2_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function vs_core_2_opencl_helper_0_0_0
freia_status vs_core_2_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t * po4,
  int32_t * po5,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_2_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_2_opencl_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel vs_core_2_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 2, 3, 1, o0, o1, i0, i1, c0, c1, c2, &redres);

  // return reduction results
  *po4 = redres.maximum;
  *po5 = redres.minimum;

  return err;
}

// hold kernels for vs_core_2_opencl_helper_0_1_2
static cl_kernel vs_core_2_opencl_helper_0_1_2_kernel[2];

// compile kernels for vs_core_2_opencl_helper_0_1_2
static freia_status vs_core_2_opencl_helper_0_1_2_compile(void)
{
  // OpenCL source for vs_core_2_opencl_helper_0_1_2
  const char * vs_core_2_opencl_helper_0_1_2_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function vs_core_2_opencl_helper_0_1_2\n"
    "KERNEL void vs_core_2_opencl_helper_0_1_2(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in2 = j2[i];\n"
    "    // pixel computations\n"
    "    PIXEL t41 = PIXEL_SUB(in0, in1);\n"
    "    PIXEL t43 = PIXEL_ADD(t41, c0);\n"
    "    PIXEL t44 = PIXEL_AND(t43, c1);\n"
    "    PIXEL t45 = PIXEL_MUL(in2, t44);\n"
    "    // set output pixels\n"
    "    p0[i] = t45;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_1_2_source, "vs_core_2_opencl_helper_0_1_2", "-DPIXEL8", &vs_core_2_opencl_helper_0_1_2_kernel[0]);
  err |= freia_op_compile_kernel(vs_core_2_opencl_helper_0_1_2_source, "vs_core_2_opencl_helper_0_1_2", "-DPIXEL16", &vs_core_2_opencl_helper_0_1_2_kernel[1]);
  return err;
}

// helper function vs_core_2_opencl_helper_0_1_2
freia_status vs_core_2_opencl_helper_0_1_2(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const freia_data2d * i2,
  int32_t c0,
  int32_t c1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= vs_core_2_opencl_helper_0_1_2_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = vs_core_2_opencl_helper_0_1_2_kernel[bpp];

  // call kernel vs_core_2_opencl_helper_0_1_2
  err |= freia_op_call_kernel(kernel, 1, 3, 2, 0, o0, i0, i1, i2, c0, c1);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function vs_core_2

// opencl for dag 0

// opencl function vs_core_2_opencl_helper_0_0_0
KERNEL void vs_core_2_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2,
  GLOBAL TMeasure * redX)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // reduction stuff is currently hardcoded...
  int vol = 0;
  int2 mmin = { PIXEL_MAX, 0 };
  int2 mmax = { PIXEL_MIN, 0 };
  int idy = get_global_id(0);

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t15 = PIXEL_ABSDIFF(in1, in0);
    PIXEL t19 = PIXEL_MUL(in1, c0);
    PIXEL t20 = PIXEL_MUL(in0, c1);
    PIXEL t21 = PIXEL_ADD(t20, t19);
    PIXEL_MAXIMUM(red26, t15);
    PIXEL_MINIMUM(red27, t15);
    PIXEL t22 = PIXEL_DIV(t21, c2);
    // set output pixels
    p0[i] = t15;
    p1[i] = t22;
  }

  // reduction copy out
  redX[idy].max = mmax.x;
  redX[idy].min = mmin.x;
}

// opencl function vs_core_2_opencl_helper_0_1_2
KERNEL void vs_core_2_opencl_helper_0_1_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width,
  int pitch,
  int c0,
  int c1)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    // pixel computations
    PIXEL t41 = PIXEL_SUB(in0, in1);
    PIXEL t43 = PIXEL_ADD(t41, c0);
    PIXEL t44 = PIXEL_AND(t43, c1);
    PIXEL t45 = PIXEL_MUL(in2, t44);
    // set output pixels
    p0[i] = t45;
  }
}

! updated function

freia_status vs_core_2(freia_data2d *bg, freia_data2d *mv, freia_data2d *in, freia_data2d *stab, int motion_a, int motion_b, int motion_trig, int motion_th)
{
   
   
   
   
   freia_data2d *t1 = freia_common_create_data(16, 128, 128), *t2 = freia_common_create_data(16, 128, 128);          /*0012*/
   int binvalue, maxmotion, minmotion;                                  /*0013*/

   vs_core_2_opencl_helper_0_0_0(mv, bg, bg, in, motion_a, motion_b, &maxmotion, &minmotion, motion_a+motion_b);     /*0015*/
   // was cast
   // was cast
   
   // measure
   
   // bg update
   if (maxmotion-minmotion>motion_trig)                                 /*0029*/
      binvalue = maxmotion*motion_th/100;                               /*0030*/

   freia_aipo_threshold(mv, mv, binvalue, 255, true);                   /*0032*/
   
   // open
   freia_aipo_erode_8c(t1, mv, freia_morpho_kernel_8c);                 /*0035*/
   freia_aipo_dilate_8c(t1, t1, freia_morpho_kernel_8c);                /*0036*/
   
   // gradient
   freia_aipo_dilate_8c(t2, t1, freia_morpho_kernel_8c);                /*0039*/
   freia_aipo_erode_8c(mv, t1, freia_morpho_kernel_8c);                 /*0040*/
   vs_core_2_opencl_helper_0_1_2(mv, t2, mv, stab, -1, 1);              /*0041*/
   
   
   freia_common_destruct_data(t1);                                      /*0048*/
   freia_common_destruct_data(t2);                                      /*0049*/
   return FREIA_OK;                                                     /*0050*/
}

