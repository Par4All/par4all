  // call kernel vs_core_loop_opencl_helper_3_1_2
  err |= freia_op_call_kernel(kernel, 1, 2, 2, 0, o0, i0, i1, c0, c1);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function vs_core_loop

// opencl for dag 0

// opencl for dag 1

// opencl for dag 2

// opencl for dag 3

// opencl function vs_core_loop_opencl_helper_3_0_0
KERNEL void vs_core_loop_opencl_helper_3_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  GLOBAL TMeasure * redX)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // get input pixels
    PIXEL in2 = j2[i];
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];

    // pixel computations
    PIXEL t87 = PIXEL_ABSDIFF(in2, in0);
    PIXEL t91 = PIXEL_MUL(in2, c0);
    PIXEL t92 = PIXEL_MUL(in1, c1);
    PIXEL t93 = PIXEL_ADD(t92, t91);
    PIXEL_MAXIMUM(red98, t87);
    PIXEL_MINIMUM(red99, t87);
    PIXEL t94 = PIXEL_DIV(t93, c2);

    // set output pixels
    p0[i] = t87;
    p1[i] = t94;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].max = maxv;
  redX[thrid].min = minv;
}

// opencl function vs_core_loop_opencl_helper_D8_511
KERNEL void vs_core_loop_opencl_helper_D8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t107 = PIXEL_MIN;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t107 = PIXEL_SUP(t107, (is_N|is_W)? PIXEL_MIN: in0NW);
    t107 = PIXEL_SUP(t107, (is_N)? PIXEL_MIN: in0N);
    t107 = PIXEL_SUP(t107, (is_N|is_E)? PIXEL_MIN: in0NE);
    t107 = PIXEL_SUP(t107, (is_W)? PIXEL_MIN: in0W);
    t107 = PIXEL_SUP(t107, in0);
    t107 = PIXEL_SUP(t107, (is_E)? PIXEL_MIN: in0E);
    t107 = PIXEL_SUP(t107, (is_S|is_W)? PIXEL_MIN: in0SW);
    t107 = PIXEL_SUP(t107, (is_S)? PIXEL_MIN: in0S);
    t107 = PIXEL_SUP(t107, (is_S|is_E)? PIXEL_MIN: in0SE);

    // set output pixels
    p0[i] = t107;
  }
}

// opencl function vs_core_loop_opencl_helper_E8_511
KERNEL void vs_core_loop_opencl_helper_E8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t106 = PIXEL_MAX;
    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];
    PIXEL in0N = (is_N)? 0: j0[i-pitch];
    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];
    PIXEL in0W = (is_W)? 0: j0[i-1];
    PIXEL in0 = j0[i];
    PIXEL in0E = (is_E)? 0: j0[i+1];
    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];
    PIXEL in0S = (is_S)? 0: j0[i+pitch];
    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];

    // pixel computations
    t106 = PIXEL_INF(t106, (is_N|is_W)? PIXEL_MAX: in0NW);
    t106 = PIXEL_INF(t106, (is_N)? PIXEL_MAX: in0N);
    t106 = PIXEL_INF(t106, (is_N|is_E)? PIXEL_MAX: in0NE);
    t106 = PIXEL_INF(t106, (is_W)? PIXEL_MAX: in0W);
    t106 = PIXEL_INF(t106, in0);
    t106 = PIXEL_INF(t106, (is_E)? PIXEL_MAX: in0E);
    t106 = PIXEL_INF(t106, (is_S|is_W)? PIXEL_MAX: in0SW);
    t106 = PIXEL_INF(t106, (is_S)? PIXEL_MAX: in0S);
    t106 = PIXEL_INF(t106, (is_S|is_E)? PIXEL_MAX: in0SE);

    // set output pixels
    p0[i] = t106;
  }
}

// opencl function vs_core_loop_opencl_helper_3_1_2
KERNEL void vs_core_loop_opencl_helper_3_1_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // N & S boundaries, one thread on first dimension per row
  int is_N = (get_global_id(0)==0);
  int is_S = (get_global_id(0)==(height-1));

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));

    // get input pixels
    PIXEL t111 = PIXEL_MIN;
    PIXEL in1NW = (is_N|is_W)? 0: j1[i-pitch-1];
    PIXEL in1N = (is_N)? 0: j1[i-pitch];
    PIXEL in1NE = (is_N|is_E)? 0: j1[i-pitch+1];
    PIXEL in1W = (is_W)? 0: j1[i-1];
    PIXEL in1 = j1[i];
    PIXEL in1E = (is_E)? 0: j1[i+1];
    PIXEL in1SW = (is_S|is_W)? 0: j1[i+pitch-1];
    PIXEL in1S = (is_S)? 0: j1[i+pitch];
    PIXEL in1SE = (is_S|is_E)? 0: j1[i+pitch+1];
    PIXEL t112 = PIXEL_MAX;
    PIXEL in0 = j0[i];

    // pixel computations
    t111 = PIXEL_SUP(t111, (is_N|is_W)? PIXEL_MIN: in1NW);
    t111 = PIXEL_SUP(t111, (is_N)? PIXEL_MIN: in1N);
    t111 = PIXEL_SUP(t111, (is_N|is_E)? PIXEL_MIN: in1NE);
    t111 = PIXEL_SUP(t111, (is_W)? PIXEL_MIN: in1W);
    t111 = PIXEL_SUP(t111, in1);
    t111 = PIXEL_SUP(t111, (is_E)? PIXEL_MIN: in1E);
    t111 = PIXEL_SUP(t111, (is_S|is_W)? PIXEL_MIN: in1SW);
    t111 = PIXEL_SUP(t111, (is_S)? PIXEL_MIN: in1S);
    t111 = PIXEL_SUP(t111, (is_S|is_E)? PIXEL_MIN: in1SE);
    t112 = PIXEL_INF(t112, (is_N|is_W)? PIXEL_MAX: in1NW);
    t112 = PIXEL_INF(t112, (is_N)? PIXEL_MAX: in1N);
    t112 = PIXEL_INF(t112, (is_N|is_E)? PIXEL_MAX: in1NE);
    t112 = PIXEL_INF(t112, (is_W)? PIXEL_MAX: in1W);
    t112 = PIXEL_INF(t112, in1);
    t112 = PIXEL_INF(t112, (is_E)? PIXEL_MAX: in1E);
    t112 = PIXEL_INF(t112, (is_S|is_W)? PIXEL_MAX: in1SW);
    t112 = PIXEL_INF(t112, (is_S)? PIXEL_MAX: in1S);
    t112 = PIXEL_INF(t112, (is_S|is_E)? PIXEL_MAX: in1SE);
    PIXEL t113 = PIXEL_SUB(t111, t112);
    PIXEL t118 = PIXEL_ADD(t113, c0);
    PIXEL t119 = PIXEL_AND(t118, c1);
    PIXEL t120 = PIXEL_MUL(in0, t119);

    // set output pixels
    p0[i] = t120;
  }
}

! updated function

int vs_core_loop(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdmotion;                                               /*0007*/

   freia_data2d *imcurrent;                                             /*0009*/
   freia_data2d *imstab;                                                /*0011*/
   freia_data2d *imtmp2;                                                /*0013*/
   freia_data2d *imbg16;                                                /*0016*/
   freia_data2d *imbg;                                                  /*0017*/
   freia_data2d *immotion;                                              /*0018*/

   int32_t maxmotion, minmotion;                                        /*0021*/
   register int32_t binvalue = 128;                                     /*0022*/

   register int32_t xshiftacc, yshiftacc;                               /*0024*/
   register freia_status end = 0;                                       /*0025*/

   freia_initialize(0, (void *) 0);                                     /*0035*/

   freia_common_open_input(&fdin, 0);                                   /*0037*/

   imcurrent = freia_common_create_data(16, fdin.framewidth, fdin.frameheight);        /*0039*/

   imstab = freia_common_create_data(16, fdin.framewidth-20, fdin.frameheight-20);     /*0043*/
   imtmp2 = freia_common_create_data(16, imstab->width, imstab->height);               /*0044*/

   imbg16 = freia_common_create_data(16, imstab->width, imstab->height);               /*0048*/
   imbg = freia_common_create_data(16, imstab->width, imstab->height);  /*0049*/
   immotion = freia_common_create_data(16, imstab->width, imstab->height);             /*0050*/

   freia_common_open_output(&fdmotion, 2, imstab->width, imstab->height, imstab->bpp);                /*0052*/
   
   // application start
   xshiftacc = 0;                                                       /*0059*/
   yshiftacc = 0;                                                       /*0060*/

   freia_aipo_set_constant(imbg16, 0);                                  /*0054*/
   freia_aipo_copy(imbg, imbg16);

   end = freia_common_rx_image(imcurrent, &fdin);                       /*0062*/
   
   // video loop
l99979:   ;                                                                    /*0065*/

   if (end==0) {                                                        /*0067*/
   }
   else {
      goto l99980;
   }
   
   // stabilization
   end = freia_common_rx_image(imcurrent, &fdin);                       /*0075*/

   if (end!=0) goto l99980;                                             /*0077*/

   freia_common_set_wa(imcurrent, xshiftacc+10, yshiftacc+10, fdin.framewidth-20, fdin.frameheight-20);              /*0079*/
   freia_aipo_copy(imstab, imcurrent);
   freia_common_reset_wa(imcurrent);                                    /*0081*/
   
   
   // constrast enhance
   
   // absolute difference background and current frame
   vs_core_loop_opencl_helper_3_0_0(immotion, imbg16, imbg, imbg16, imstab, 10, 90, &maxmotion, &minmotion, 100);    /*0087*/
   
   // measures
   
   // background update
   
   if (maxmotion-minmotion>75)                                          /*0101*/
      binvalue = 30*maxmotion/100;                                      /*0102*/
   
   // threshold
   freia_aipo_threshold(immotion, immotion, binvalue, 255, 1);          /*0105*/
   vs_core_loop_opencl_helper_E8_511(imtmp2, immotion);                 /*0106*/
   vs_core_loop_opencl_helper_D8_511(imtmp2, imtmp2);                   /*0107*/

   vs_core_loop_opencl_helper_3_1_2(immotion, imstab, imtmp2, -1, 1);   /*0111*/
   
   // save contrast motion detection
   freia_aipo_copy(imbg, imbg16);
   
   
   // output image
   freia_common_tx_image(immotion, &fdmotion);                          /*0123*/

   fprintf(stdout, "INFO: %s: ""frame %d processed\n", __FUNCTION__, fdin.frameindex);                /*0125*/
   goto l99979;
l99980:   ;
   
   // cleanup at end of application
   freia_common_destruct_data(imcurrent);                               /*0130*/
   freia_common_destruct_data(imstab);                                  /*0133*/
   freia_common_destruct_data(imtmp2);                                  /*0134*/
   freia_common_destruct_data(imbg16);                                  /*0137*/
   freia_common_destruct_data(imbg);                                    /*0138*/
   freia_common_destruct_data(immotion);                                /*0139*/

   freia_common_close_input(&fdin);                                     /*0141*/
   freia_common_close_output(&fdmotion);                                /*0142*/

   freia_shutdown();                                                    /*0144*/
   return 0;                                                            /*0145*/
}

! cleaned up function

int vs_core_loop(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdmotion;                                               /*0007*/

   freia_data2d *imcurrent;                                             /*0009*/
   freia_data2d *imstab;                                                /*0011*/
   freia_data2d *imtmp2;                                                /*0013*/
   freia_data2d *imbg16;                                                /*0016*/
   freia_data2d *imbg;                                                  /*0017*/
   freia_data2d *immotion;                                              /*0018*/

   int32_t maxmotion, minmotion;                                        /*0021*/
   register int32_t binvalue = 128;                                     /*0022*/

   register int32_t xshiftacc, yshiftacc;                               /*0024*/
   register freia_status end = 0;                                       /*0025*/

   freia_initialize(0, (void *) 0);                                     /*0035*/

   freia_common_open_input(&fdin, 0);                                   /*0037*/

   imcurrent = freia_common_create_data(16, fdin.framewidth, fdin.frameheight);        /*0039*/

   imstab = freia_common_create_data(16, fdin.framewidth-20, fdin.frameheight-20);     /*0043*/
   imtmp2 = freia_common_create_data(16, imstab->width, imstab->height);               /*0044*/

   imbg16 = freia_common_create_data(16, imstab->width, imstab->height);               /*0048*/
   imbg = freia_common_create_data(16, imstab->width, imstab->height);  /*0049*/
   immotion = freia_common_create_data(16, imstab->width, imstab->height);             /*0050*/

   freia_common_open_output(&fdmotion, 2, imstab->width, imstab->height, imstab->bpp);                /*0052*/
   
   // application start
   xshiftacc = 0;                                                       /*0059*/
   yshiftacc = 0;                                                       /*0060*/

   freia_aipo_set_constant(imbg16, 0);                                  /*0054*/
   freia_aipo_copy(imbg, imbg16);

   end = freia_common_rx_image(imcurrent, &fdin);                       /*0062*/
   
   // video loop
l99979:   ;                                                                    /*0065*/

   if (end==0) {                                                        /*0067*/
   }
   else {
      goto l99980;
   }
   
   // stabilization
   end = freia_common_rx_image(imcurrent, &fdin);                       /*0075*/

   if (end!=0) goto l99980;                                             /*0077*/

   freia_common_set_wa(imcurrent, 10, 10, fdin.framewidth-20, fdin.frameheight-20);    /*0079*/
   freia_aipo_copy(imstab, imcurrent);
   freia_common_reset_wa(imcurrent);                                    /*0081*/
   
   
   // constrast enhance
   
   // absolute difference background and current frame
   vs_core_loop_opencl_helper_3_0_0(immotion, imbg16, imbg, imbg16, imstab, 10, 90, &maxmotion, &minmotion, 100);    /*0087*/
   
   // measures
   
   // background update
   
   if (maxmotion-minmotion>75)                                          /*0101*/
      binvalue = 30*maxmotion/100;                                      /*0102*/
   
   // threshold
   freia_aipo_threshold(immotion, immotion, binvalue, 255, 1);          /*0105*/
   vs_core_loop_opencl_helper_E8_511(imtmp2, immotion);                 /*0106*/
   vs_core_loop_opencl_helper_D8_511(imtmp2, imtmp2);                   /*0107*/

   vs_core_loop_opencl_helper_3_1_2(immotion, imstab, imtmp2, -1, 1);   /*0111*/
   
   // save contrast motion detection
   freia_aipo_copy(imbg, imbg16);
   
   
   // output image
   freia_common_tx_image(immotion, &fdmotion);                          /*0123*/

   fprintf(stdout, "INFO: %s: ""frame %d processed\n", __FUNCTION__, fdin.frameindex);                /*0125*/
   goto l99979;
l99980:   ;
   
   // cleanup at end of application
   freia_common_destruct_data(imcurrent);                               /*0130*/
   freia_common_destruct_data(imstab);                                  /*0133*/
   freia_common_destruct_data(imtmp2);                                  /*0134*/
   freia_common_destruct_data(imbg16);                                  /*0137*/
   freia_common_destruct_data(imbg);                                    /*0138*/
   freia_common_destruct_data(immotion);                                /*0139*/

   freia_common_close_input(&fdin);                                     /*0141*/
   freia_common_close_output(&fdmotion);                                /*0142*/

   freia_shutdown();                                                    /*0144*/
   return 0;                                                            /*0145*/
}
