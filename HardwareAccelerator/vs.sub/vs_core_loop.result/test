  err |= freia_op_compile_kernel(vs_core_loop_opencl_helper_3_1_2_source, "vs_core_loop_opencl_helper_3_1_2", "-DPIXEL16", &vs_core_loop_opencl_helper_3_1_2_kernel[1]);
  return err;
}

// helper function vs_core_loop_opencl_helper_3_1_2
freia_status vs_core_loop_opencl_helper_3_1_2(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
