   int i_0;                                                             /*0026*/
   freia_status ret_1;                                                  /*0027*/
   int i_1;                                                             /*0028*/
   freia_status ret_2;                                                  /*0029*/
   int i_2;                                                             /*0030*/
   freia_status ret_3;                                                  /*0031*/
   int i_3, i_4;                                                        /*0032*/
   
