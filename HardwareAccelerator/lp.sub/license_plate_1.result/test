   freia_aipo_dilate_8c(out, out, freia_morpho_kernel_8c);              /*0146*/
   freia_aipo_dilate_8c(out, out, freia_morpho_kernel_8c);              /*0147*/
   i_4 = 3;                                                             /*0148*/

   _return4 = 0;                                                        /*0150*/

