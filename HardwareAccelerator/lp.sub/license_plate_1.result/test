   freia_data2d *imcloseth;                                             /*0014*/
   freia_data2d *imand;                                                 /*0015*/
   freia_data2d *imfilt;                                                /*0016*/
   freia_data2d *imout;                                                 /*0017*/
   freia_data2d *out;                                                   /*0018*/

