void freia_77(freia_data2d *out, const freia_data2d *i0, const freia_data2d *i1)
{
   freia_data2d *t0, *t1, *t2, *t3;                                     /*0009*/
   t0 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0010*/
   t1 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0011*/
   t2 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0012*/
   t3 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0013*/
   freia_aipo_dilate_8c(t2, i0, freia_morpho_kernel_8c);                /*0017*/

   freia_77_opencl_helper_0_0_1(t0, t3, t2, i0, i1, 2);                 /*0018*/
   freia_aipo_erode_8c(t1, t0, freia_morpho_kernel_8c);                 /*0016*/
   freia_aipo_add(out, t3, t1);                                         /*0019*/

   freia_common_destruct_data(t0);                                      /*0021*/
   freia_common_destruct_data(t1);                                      /*0022*/
   freia_common_destruct_data(t2);                                      /*0023*/
   freia_common_destruct_data(t3);                                      /*0024*/
}
#
# FREIA COMPILATION DONE
#

