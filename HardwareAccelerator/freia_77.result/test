! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 5 ops and 0 copies, optimized to 5 ops and 0+0+0 copies

// hold kernels for freia_77_opencl_helper_0_0_1
static cl_kernel freia_77_opencl_helper_0_0_1_kernel[2];

// compile kernels for freia_77_opencl_helper_0_0_1
static freia_status freia_77_opencl_helper_0_0_1_compile(void)
{
  // OpenCL source for freia_77_opencl_helper_0_0_1
  const char * freia_77_opencl_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_77_opencl_helper_0_0_1\n"
    "KERNEL void freia_77_opencl_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch,\n"
    "  int c0)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in2 = j2[i];\n"
    "    // pixel computations\n"
    "    PIXEL t15 = PIXEL_ADD(in1, c0);\n"
    "    PIXEL t18 = PIXEL_SUB(in0, in2);\n"
    "    // set output pixels\n"
    "    p0[i] = t15;\n"
    "    p1[i] = t18;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_77_opencl_helper_0_0_1_source, "freia_77_opencl_helper_0_0_1", "-DPIXEL8", &freia_77_opencl_helper_0_0_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_77_opencl_helper_0_0_1_source, "freia_77_opencl_helper_0_0_1", "-DPIXEL16", &freia_77_opencl_helper_0_0_1_kernel[1]);
  return err;
}

// helper function freia_77_opencl_helper_0_0_1
freia_status freia_77_opencl_helper_0_0_1(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const freia_data2d * i2,
  int32_t c0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_77_opencl_helper_0_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_77_opencl_helper_0_0_1_kernel[bpp];

  // call kernel freia_77_opencl_helper_0_0_1
  err |= freia_op_call_kernel(kernel, 2, 3, 1, 0, o0, o1, i0, i1, i2, c0);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_77

// opencl for dag 0

// opencl function freia_77_opencl_helper_0_0_1
KERNEL void freia_77_opencl_helper_0_0_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width, // of the working area, vs image below
  int pitch,
  int c0)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    // pixel computations
    PIXEL t15 = PIXEL_ADD(in1, c0);
    PIXEL t18 = PIXEL_SUB(in0, in2);
    // set output pixels
    p0[i] = t15;
    p1[i] = t18;
  }
}

! updated function

void freia_77(freia_data2d *out, const freia_data2d *i0, const freia_data2d *i1)
{
   freia_data2d *t0, *t1, *t2, *t3;                                     /*0009*/
   t0 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0010*/
   t1 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0011*/
   t2 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0012*/
   t3 = freia_common_create_data(i0->bpp, i0->widthWa, i0->heightWa);   /*0013*/
   freia_aipo_dilate_8c(t2, i0, freia_morpho_kernel_8c);                /*0017*/

   freia_77_opencl_helper_0_0_1(t0, t3, t2, i0, i1, 2);                 /*0018*/
   freia_aipo_erode_8c(t1, t0, freia_morpho_kernel_8c);                 /*0016*/
   freia_aipo_add(out, t3, t1);                                         /*0019*/

   freia_common_destruct_data(t0);                                      /*0021*/
   freia_common_destruct_data(t1);                                      /*0022*/
   freia_common_destruct_data(t2);                                      /*0023*/
   freia_common_destruct_data(t3);                                      /*0024*/
}

