! updated function

freia_status freia_37(freia_data2d *o, freia_data2d *i, int32_t *kern)
{
   
   
   freia_data2d *e = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);    /*0006*/
   // hardware accelerator generated variable
   freia_data2d *d_1 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_1 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d_2 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_2 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d_3 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_3 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);
   
   // preschedule example for terapix tests
   
   freia_aipo_erode_8c(e_3, i, kern);                                   /*0010*/
   freia_aipo_dilate_8c(d_3, i, kern);                                  /*0011*/

   freia_aipo_erode_8c(e_2, e_3, kern);                                 /*0013*/
   freia_aipo_dilate_8c(d_2, d_3, kern);                                /*0014*/

   freia_aipo_erode_8c(e_1, e_2, kern);                                 /*0016*/
   freia_aipo_dilate_8c(d_1, d_2, kern);                                /*0017*/

   freia_aipo_erode_8c(e, e_1, kern);                                   /*0019*/
   freia_aipo_dilate_8c(d, d_1, kern);                                  /*0020*/

   freia_aipo_sub(o, d, e);                                             /*0022*/

   freia_common_destruct_data(e);                                       /*0024*/
   freia_common_destruct_data(e_1);
   freia_common_destruct_data(e_2);
   freia_common_destruct_data(e_3);
   freia_common_destruct_data(d);                                       /*0025*/
   freia_common_destruct_data(d_1);
   freia_common_destruct_data(d_2);
   freia_common_destruct_data(d_3);

   return FREIA_OK;                                                     /*0027*/
}

! cleaned up function

freia_status freia_37(freia_data2d *o, freia_data2d *i, int32_t *kern)
{
   
   
   freia_data2d *e = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);    /*0006*/
   // hardware accelerator generated variable
   freia_data2d *d_1 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_1 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d_2 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_2 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *d_3 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa), *e_3 = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);
   
   // preschedule example for terapix tests
   
   freia_aipo_erode_8c(e_3, i, kern);                                   /*0010*/
   freia_aipo_dilate_8c(d_3, i, kern);                                  /*0011*/

   freia_aipo_erode_8c(e_2, e_3, kern);                                 /*0013*/
   freia_aipo_dilate_8c(d_2, d_3, kern);                                /*0014*/

   freia_aipo_erode_8c(e_1, e_2, kern);                                 /*0016*/
   freia_aipo_dilate_8c(d_1, d_2, kern);                                /*0017*/

   freia_aipo_erode_8c(e, e_1, kern);                                   /*0019*/
   freia_aipo_dilate_8c(d, d_1, kern);                                  /*0020*/

   freia_aipo_sub(o, d, e);                                             /*0022*/

   freia_common_destruct_data(e);                                       /*0024*/
   freia_common_destruct_data(e_1);
   freia_common_destruct_data(e_2);
   freia_common_destruct_data(e_3);
   freia_common_destruct_data(d);                                       /*0025*/
   freia_common_destruct_data(d_1);
   freia_common_destruct_data(d_2);
   freia_common_destruct_data(d_3);

   return 0;                                                            /*0027*/
}
#
# FREIA COMPILATION DONE
#

