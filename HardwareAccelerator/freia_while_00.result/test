! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 6 ops and 0 copies, optimized to 6 ops and 0+0+0 copies

// hold kernels for freia_while_00_opencl_helper_E8_511
static cl_kernel freia_while_00_opencl_helper_E8_511_kernel[2];

// compile kernels for freia_while_00_opencl_helper_E8_511
static freia_status freia_while_00_opencl_helper_E8_511_compile(void)
{
  // OpenCL source for freia_while_00_opencl_helper_E8_511
  const char * freia_while_00_opencl_helper_E8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_while_00_opencl_helper_E8_511\n"
    "KERNEL void freia_while_00_opencl_helper_E8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // N & S boundaries, one thread on first dimension per row\n"
    "  int is_N = (get_global_id(0)==0);\n"
    "  int is_S = (get_global_id(0)==(height-1));\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // W & E boundaries, assuming i global index\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "\n"
    "    // get input pixels\n"
    "    PIXEL t7 = PIXEL_MAX;\n"
    "    PIXEL in0NW = (is_N|is_W)? 0: j0[i-pitch-1];\n"
    "    PIXEL in0N = (is_N)? 0: j0[i-pitch];\n"
    "    PIXEL in0NE = (is_N|is_E)? 0: j0[i-pitch+1];\n"
    "    PIXEL in0W = (is_W)? 0: j0[i-1];\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in0E = (is_E)? 0: j0[i+1];\n"
    "    PIXEL in0SW = (is_S|is_W)? 0: j0[i+pitch-1];\n"
    "    PIXEL in0S = (is_S)? 0: j0[i+pitch];\n"
    "    PIXEL in0SE = (is_S|is_E)? 0: j0[i+pitch+1];\n"
    "\n"
    "    // pixel computations\n"
    "    t7 = PIXEL_INF(t7, (is_N|is_W)? PIXEL_MAX: in0NW);\n"
    "    t7 = PIXEL_INF(t7, (is_N)? PIXEL_MAX: in0N);\n"
    "    t7 = PIXEL_INF(t7, (is_N|is_E)? PIXEL_MAX: in0NE);\n"
    "    t7 = PIXEL_INF(t7, (is_W)? PIXEL_MAX: in0W);\n"
    "    t7 = PIXEL_INF(t7, in0);\n"
    "    t7 = PIXEL_INF(t7, (is_E)? PIXEL_MAX: in0E);\n"
    "    t7 = PIXEL_INF(t7, (is_S|is_W)? PIXEL_MAX: in0SW);\n"
    "    t7 = PIXEL_INF(t7, (is_S)? PIXEL_MAX: in0S);\n"
    "    t7 = PIXEL_INF(t7, (is_S|is_E)? PIXEL_MAX: in0SE);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t7;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_while_00_opencl_helper_E8_511_source, "freia_while_00_opencl_helper_E8_511", "-DPIXEL8", &freia_while_00_opencl_helper_E8_511_kernel[0]);
  err |= freia_op_compile_kernel(freia_while_00_opencl_helper_E8_511_source, "freia_while_00_opencl_helper_E8_511", "-DPIXEL16", &freia_while_00_opencl_helper_E8_511_kernel[1]);
  return err;
}

// helper function freia_while_00_opencl_helper_E8_511
freia_status freia_while_00_opencl_helper_E8_511(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_while_00_opencl_helper_E8_511_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_while_00_opencl_helper_E8_511_kernel[bpp];

  // call kernel freia_while_00_opencl_helper_E8_511
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}

