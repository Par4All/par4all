// DAG "dag_0": #i=1 #o=1 #c=6 #s=0 #I=0 #B=0 #A=0
digraph "dag_0" {
  // inputs
  "in" [shape=circle];

  // outputs
  "out" [shape=circle];

  // computation vertices
  "12 vol" [shape=diamond];
  "11 <" [shape=trapezium,orientation=270];
  "11 <" -> "12 vol" [label="out"];
  "11 <" -> "out";
  "10 D8" [shape=box];
  "10 D8" -> "11 <" [label="out"];
  "8 vol" [shape=diamond];
  "7 E8" [shape=box];
  "7 E8" -> "10 D8" [label="out"];
  "7 E8" -> "8 vol" [label="out"];
  "6 D8" [shape=box];
  "6 D8" -> "7 E8" [label="out"];
  "in" -> "11 <";
  "in" -> "6 D8";
}
