! updated function

int freia_while_00(freia_data2d *out, const freia_data2d *in)
{
   int volprevious, volcurrent;                                         /*0005*/
   freia_while_00_opencl_helper_D8_511(out, in);                        /*0006*/
   freia_while_00_opencl_helper_E8_511(out, out);                       /*0007*/
   freia_while_00_opencl_helper_0_0_1(out, in, out, &volprevious);      /*0008*/
   freia_aipo_global_vol(out, &volcurrent);                             /*0012*/
   while (volcurrent!=volprevious) {                                    /*0013*/
      volprevious = volcurrent;                                         /*0014*/
      freia_while_00_opencl_helper_1_0_0(out, in, out, &volcurrent);    /*0015*/
   }
   return 0;                                                            /*0019*/
}

! cleaned up function

