}
// graph for dag "dag_cleaned_0" of module "freia_while_00" in dot format
// DAG "dag_cleaned_0": #i=1 #o=1 #c=6 #s=0 #I=0 #B=0 #A=0
digraph "dag_cleaned_0" {
  // inputs
  "in" [shape=circle];

  // outputs
  "out" [shape=circle];

