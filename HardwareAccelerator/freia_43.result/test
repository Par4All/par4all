! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 11 ops and 0 copies, optimized to 8 ops and 0+0+0 copies

// hold kernels for freia_43_opencl_helper_0_0_0
static cl_kernel freia_43_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_43_opencl_helper_0_0_0
static freia_status freia_43_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_43_opencl_helper_0_0_0
  const char * freia_43_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_43_opencl_helper_0_0_0\n"
    "KERNEL void freia_43_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  GLOBAL PIXEL * i2, // const?\n"
    "  int ofs_i2,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in2 = j2[i];\n"
    "    // pixel computations\n"
    "    PIXEL t54 = in2;\n"
    "    t54 = PIXEL_SUP(t54, (is_N|is_W)? PIXEL_MIN: j2[i-pitch-1]);\n"
    "    t54 = PIXEL_SUP(t54, (is_N)? PIXEL_MIN: j2[i-pitch]);\n"
    "    t54 = PIXEL_SUP(t54, (is_N|is_E)? PIXEL_MIN: j2[i-pitch+1]);\n"
    "    t54 = PIXEL_SUP(t54, (is_W)? PIXEL_MIN: j2[i-1]);\n"
    "    t54 = PIXEL_SUP(t54, (is_E)? PIXEL_MIN: j2[i+1]);\n"
    "    t54 = PIXEL_SUP(t54, (is_W|is_S)? PIXEL_MIN: j2[i+pitch-1]);\n"
    "    t54 = PIXEL_SUP(t54, (is_S)? PIXEL_MIN: j2[i+pitch]);\n"
    "    t54 = PIXEL_SUP(t54, (is_E|is_S)? PIXEL_MIN: j2[i+pitch+1]);\n"
    "    PIXEL t47 = in0;\n"
    "    t47 = PIXEL_INF(t47, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);\n"
    "    t47 = PIXEL_INF(t47, (is_N)? PIXEL_MAX: j0[i-pitch]);\n"
    "    t47 = PIXEL_INF(t47, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);\n"
    "    t47 = PIXEL_INF(t47, (is_W)? PIXEL_MAX: j0[i-1]);\n"
    "    t47 = PIXEL_INF(t47, (is_E)? PIXEL_MAX: j0[i+1]);\n"
    "    t47 = PIXEL_INF(t47, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);\n"
    "    t47 = PIXEL_INF(t47, (is_S)? PIXEL_MAX: j0[i+pitch]);\n"
    "    t47 = PIXEL_INF(t47, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);\n"
    "    PIXEL t48 = in0;\n"
    "    t48 = PIXEL_SUP(t48, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);\n"
    "    t48 = PIXEL_SUP(t48, (is_N)? PIXEL_MIN: j0[i-pitch]);\n"
    "    t48 = PIXEL_SUP(t48, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);\n"
    "    t48 = PIXEL_SUP(t48, (is_W)? PIXEL_MIN: j0[i-1]);\n"
    "    t48 = PIXEL_SUP(t48, (is_E)? PIXEL_MIN: j0[i+1]);\n"
    "    t48 = PIXEL_SUP(t48, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);\n"
    "    t48 = PIXEL_SUP(t48, (is_S)? PIXEL_MIN: j0[i+pitch]);\n"
    "    t48 = PIXEL_SUP(t48, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);\n"
    "    PIXEL t49 = PIXEL_SUB(t47, t48);\n"
    "    PIXEL t55 = PIXEL_SUB(t47, t54);\n"
    "    PIXEL t50 = PIXEL_ABSDIFF(in1, t49);\n"
    "    PIXEL t56 = PIXEL_ABSDIFF(t49, t55);\n"
    "    PIXEL t57 = PIXEL_INF(t56, t50);\n"
    "    // set output pixels\n"
    "    p0[i] = t57;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_43_opencl_helper_0_0_0_source, "freia_43_opencl_helper_0_0_0", "-DPIXEL8", &freia_43_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_43_opencl_helper_0_0_0_source, "freia_43_opencl_helper_0_0_0", "-DPIXEL16", &freia_43_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_43_opencl_helper_0_0_0
freia_status freia_43_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const freia_data2d * i2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_43_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_43_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_43_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 3, 0, 0, o0, i0, i1, i2);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_43

// opencl for dag 0

// opencl function freia_43_opencl_helper_0_0_0
KERNEL void freia_43_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    // pixel computations
    PIXEL t54 = in2;
    t54 = PIXEL_SUP(t54, (is_N|is_W)? PIXEL_MIN: j2[i-pitch-1]);
    t54 = PIXEL_SUP(t54, (is_N)? PIXEL_MIN: j2[i-pitch]);
    t54 = PIXEL_SUP(t54, (is_N|is_E)? PIXEL_MIN: j2[i-pitch+1]);
    t54 = PIXEL_SUP(t54, (is_W)? PIXEL_MIN: j2[i-1]);
    t54 = PIXEL_SUP(t54, (is_E)? PIXEL_MIN: j2[i+1]);
    t54 = PIXEL_SUP(t54, (is_W|is_S)? PIXEL_MIN: j2[i+pitch-1]);
    t54 = PIXEL_SUP(t54, (is_S)? PIXEL_MIN: j2[i+pitch]);
    t54 = PIXEL_SUP(t54, (is_E|is_S)? PIXEL_MIN: j2[i+pitch+1]);
    PIXEL t47 = in0;
    t47 = PIXEL_INF(t47, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);
    t47 = PIXEL_INF(t47, (is_N)? PIXEL_MAX: j0[i-pitch]);
    t47 = PIXEL_INF(t47, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);
    t47 = PIXEL_INF(t47, (is_W)? PIXEL_MAX: j0[i-1]);
    t47 = PIXEL_INF(t47, (is_E)? PIXEL_MAX: j0[i+1]);
    t47 = PIXEL_INF(t47, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);
    t47 = PIXEL_INF(t47, (is_S)? PIXEL_MAX: j0[i+pitch]);
    t47 = PIXEL_INF(t47, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);
    PIXEL t48 = in0;
    t48 = PIXEL_SUP(t48, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t48 = PIXEL_SUP(t48, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t48 = PIXEL_SUP(t48, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t48 = PIXEL_SUP(t48, (is_W)? PIXEL_MIN: j0[i-1]);
    t48 = PIXEL_SUP(t48, (is_E)? PIXEL_MIN: j0[i+1]);
    t48 = PIXEL_SUP(t48, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t48 = PIXEL_SUP(t48, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t48 = PIXEL_SUP(t48, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    PIXEL t49 = PIXEL_SUB(t47, t48);
    PIXEL t55 = PIXEL_SUB(t47, t54);
    PIXEL t50 = PIXEL_ABSDIFF(in1, t49);
    PIXEL t56 = PIXEL_ABSDIFF(t49, t55);
    PIXEL t57 = PIXEL_INF(t56, t50);
    // set output pixels
    p0[i] = t57;
  }
}

! updated function

int freia_43(void)
{
   freia_dataio fdin;                                                   /*0006*/
   freia_dataio fdout;                                                  /*0007*/

   freia_data2d *i0;                                                    /*0009*/
   freia_data2d *i2;                                                    /*0011*/
   freia_data2d *i4;                                                    /*0013*/
   freia_data2d *i7;                                                    /*0016*/

   freia_common_open_input(&fdin, 0);                                   /*0024*/

   i0 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0026*/
   i2 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0028*/
   i4 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0030*/
   i7 = freia_common_create_data(fdin.framebpp, fdin.framewidth, fdin.frameheight);    /*0033*/

   freia_common_open_output(&fdout, 0, fdin.framewidth, fdin.frameheight, 8);          /*0041*/

   freia_common_rx_image(i0, &fdin);                                    /*0043*/
   freia_common_rx_image(i2, &fdin);                                    /*0044*/
   freia_common_rx_image(i4, &fdin);                                    /*0045*/

   freia_43_opencl_helper_0_0_0(i7, i0, i2, i4);                        /*0047*/

   freia_common_tx_image(i7, &fdout);                                   /*0059*/

   freia_common_destruct_data(i0);                                      /*0061*/
   freia_common_destruct_data(i2);                                      /*0063*/
   freia_common_destruct_data(i4);                                      /*0065*/
   freia_common_destruct_data(i7);                                      /*0068*/

   freia_common_close_input(&fdin);                                     /*0076*/
   freia_common_close_output(&fdout);                                   /*0077*/
   return 0;                                                            /*0078*/
}

