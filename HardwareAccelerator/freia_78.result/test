   if (1)                                                               /*0007*/
      freia_78_spoc_helper_1_0_0(t, in, 2);                             /*0008*/
   freia_78_spoc_helper_2_0_0(out, t, 2);                               /*0009*/
   freia_common_destruct_data(t);                                       /*0010*/
}
