! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 14 ops and 0 copies, optimized to 14 ops and 0+0+0 copies
// split 0: 6 cuts
// # SPOC calls: 6

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // i [0 input 0] -> [0 poc 0] 10 dilate_8c
  // nope
  // poc 10 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_11 [0 poc 0] -> [1 poc 0] 11 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_11 [1 input 0] -> [1 poc 0] 11 erode_8c
  // nope
  // poc 11 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // output image 0`t_10 on 0
  // 0`t_10 [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t_10 [1 output 0] -> [1 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i0);

  return ret;
}

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_1(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t_10 [0 input 0] -> [0 poc 0] 14 erode_8c
  // nope
  // poc 14 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_9 [0 poc 0] -> [1 poc 0] 15 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_9 [1 input 0] -> [1 poc 0] 15 dilate_8c
  // nope
  // poc 15 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // output image 0`t_8 on 0
  // 0`t_8 [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t_8 [1 output 0] -> [1 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_2(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t_8 [0 input 0] -> [0 poc 0] 18 dilate_8c
  // nope
  // poc 18 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_7 [0 poc 0] -> [1 poc 0] 19 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_7 [1 input 0] -> [1 poc 0] 19 erode_8c
  // nope
  // poc 19 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // 0`t_6 [1 poc 0] -> [1 alu] 21 inf
  // nope
  // i [0 input 1] -> [1 alu] 21 inf
  si.mux[0][3].op = SPOC_MUX_IN1;
  // i [1 input 1] -> [1 alu] 21 inf
  // nope
  // alu 21 inf stage 1
  si.alu[1][0].op = SPOC_ALU_INF_IN0_IN1;


  // output image 0`t_6 on 0 and image o_1 on 1
  // 0`t_6 [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t_6 [1 output 0] -> [1 output 0] 0 null
  // nope

  // o_1 [1 alu] -> [1 output 1] 0 null
  si.mux[1][1].op = SPOC_MUX_IN1;
  si.mux[1][3].op = SPOC_MUX_IN0;

  // fill in to the end...
  // o_1 [1 output 1] -> [1 output 1] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_3(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t_6 [0 input 0] -> [0 poc 0] 24 erode_8c
  // nope
  // poc 24 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_5 [0 poc 0] -> [1 poc 0] 25 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_5 [1 input 0] -> [1 poc 0] 25 dilate_8c
  // nope
  // poc 25 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // output image 0`t_4 on 0
  // 0`t_4 [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t_4 [1 output 0] -> [1 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_4(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t_4 [0 input 0] -> [0 poc 0] 28 dilate_8c
  // nope
  // poc 28 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_3 [0 poc 0] -> [1 poc 0] 29 erode_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_3 [1 input 0] -> [1 poc 0] 29 erode_8c
  // nope
  // poc 29 erode_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_ERODE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // output image 0`t_2 on 0
  // 0`t_2 [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t_2 [1 output 0] -> [1 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module median_1
freia_status median_1_spoc_helper_0_0_5(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`t_2 [0 input 0] -> [0 poc 0] 32 erode_8c
  // nope
  // poc 32 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`t_1 [0 poc 0] -> [1 poc 0] 33 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`t_1 [1 input 0] -> [1 poc 0] 33 dilate_8c
  // nope
  // poc 33 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // 0`t [1 poc 0] -> [1 alu] 35 sup
  // nope
  // o_1 [0 input 1] -> [1 alu] 35 sup
  si.mux[0][3].op = SPOC_MUX_IN1;
  // o_1 [1 input 1] -> [1 alu] 35 sup
  // nope
  // alu 35 sup stage 1
  si.alu[1][0].op = SPOC_ALU_SUP_IN0_IN1;


  // output image o on 0
  // o [1 alu] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;

  // fill in to the end...
  // o [1 output 0] -> [1 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  return ret;
}

! updated function

freia_status median_1(freia_data2d *o, freia_data2d *i)
{
   freia_status ret = 0;                                                /*0005*/
   freia_data2d *t = freia_common_create_data(i->bpp, i->widthWa, i->heightWa);        /*0006*/
   
   // int32_t c = 8;
   // ret =  freia_cipo_close(t, i, c, 1);
   median_1_spoc_helper_0_0_0(t, i, freia_morpho_kernel_8c);            /*0010*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   median_1_spoc_helper_0_0_1(t, t, i, freia_morpho_kernel_8c);         /*0014*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   median_1_spoc_helper_0_0_2(t, o, t, i, freia_morpho_kernel_8c);      /*0018*/
   
   
   // ret |= freia_cipo_open(t, i, c, 1);
   median_1_spoc_helper_0_0_3(t, t, o, freia_morpho_kernel_8c);         /*0024*/
   
   // ret |= freia_cipo_close(t, t, c, 1);
   median_1_spoc_helper_0_0_4(t, t, o, freia_morpho_kernel_8c);         /*0028*/
   
   // ret |= freia_cipo_open(t, t, c, 1);
   median_1_spoc_helper_0_0_5(o, t, o, freia_morpho_kernel_8c);         /*0032*/
   
   
   // cleanup
   ret |= freia_common_destruct_data(t);                                /*0038*/
   return ret;                                                          /*0039*/
}

