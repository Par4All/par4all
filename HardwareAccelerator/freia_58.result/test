      k = 0;                                                            /*0012*/
      j = 0;                                                            /*0013*/
      freia_58_terapix_helper_1_0_0(out, in, out, freia_morpho_kernel_8c, &volcurrent);               /*0010*/
      l = 0;                                                            /*0015*/

   }
   while (volcurrent!=volprevious);                                     /*0019*/
   n = m;                                                               /*0020*/
   return n;                                                            /*0021*/
}
