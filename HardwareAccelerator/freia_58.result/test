      k = 0;                                                            /*0056*/
      freia_58_spoc_helper_1_0_0(out, in, out, freia_morpho_kernel_8c, &volprevious, &volcurrent);    /*0004*/
      l = 0;                                                            /*0009*/

      j = 0;                                                            /*0014*/
      l = 0;                                                            /*0016*/

      j = 0;                                                            /*0021*/
