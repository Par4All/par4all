! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 1 ops and 0 copies, optimized to 1 ops and 0+0+0 copies
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_58
freia_status freia_58_spoc_helper_0_0_0(
  const freia_data2d * i0,
  int32_t * po0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  spoc_reduction reduc;
  freia_reduction_results redres;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // out [0 input 0] -> [0 measure 0] 0 undefined
  si.mux[0][0].op = SPOC_MUX_IN0;


  // no output image

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, NULL, NULL, i0, NULL);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 2 global_vol stage 0 side 0
  *po0 = (int32_t) reduc.measure[0][0].volume;

  return ret;
}

// dag 1: 17 ops and 0 copies, optimized to 17 ops and 0+0+0 copies
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_58
freia_status freia_58_spoc_helper_1_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0,
  int32_t * po1)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  spoc_reduction reduc;
  freia_reduction_results redres;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // out [0 input 1] -> [0 poc 1] 5 dilate_8c
  // nope
  // poc 5 dilate_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_DILATE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];

  // in [0 input 0] -> [0 alu] 9 inf
  // nope
  // out_15 [0 poc 1] -> [0 alu] 9 inf
  // nope
  // alu 9 inf stage 0
  si.alu[0][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 1
  // out_14 [0 alu] -> [1 poc 0] 12 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;
  // out_14 [1 input 0] -> [1 poc 0] 12 dilate_8c
  // nope
  // poc 12 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];

  // in [0 input 0] -> [1 alu] 16 inf
  si.mux[0][1].op = SPOC_MUX_IN0;
  si.mux[0][3].op = SPOC_MUX_IN0;
  // in [1 input 1] -> [1 alu] 16 inf
  // nope
  // out_13 [1 poc 0] -> [1 alu] 16 inf
  // nope
  // alu 16 inf stage 1
  si.alu[1][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 2
  // out_12 [1 alu] -> [2 poc 0] 19 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN1;
  si.mux[1][2].op = SPOC_MUX_IN0;
  // out_12 [2 input 0] -> [2 poc 0] 19 dilate_8c
  // nope
  // poc 19 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];

  // in [1 poc 1] -> [2 alu] 23 inf
  si.mux[1][3].op = SPOC_MUX_IN1;
  // in [2 input 1] -> [2 alu] 23 inf
  // nope
  // out_11 [2 poc 0] -> [2 alu] 23 inf
  // nope
  // alu 23 inf stage 2
  si.alu[2][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 3
  // out_10 [2 alu] -> [3 poc 0] 26 dilate_8c
  si.mux[2][0].op = SPOC_MUX_IN1;
  si.mux[2][2].op = SPOC_MUX_IN0;
  // out_10 [3 input 0] -> [3 poc 0] 26 dilate_8c
  // nope
  // poc 26 dilate_8c stage 3 side 0
  si.poc[3][0].op = SPOC_POC_DILATE;
  si.poc[3][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[3][0].kernel[i] = pi0[i];

  // in [2 poc 1] -> [3 alu] 30 inf
  si.mux[2][3].op = SPOC_MUX_IN1;
  // in [3 input 1] -> [3 alu] 30 inf
  // nope
  // out_9 [3 poc 0] -> [3 alu] 30 inf
  // nope
  // alu 30 inf stage 3
  si.alu[3][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 4
  // out_8 [3 alu] -> [4 poc 0] 33 dilate_8c
  si.mux[3][0].op = SPOC_MUX_IN1;
  si.mux[3][2].op = SPOC_MUX_IN0;
  // out_8 [4 input 0] -> [4 poc 0] 33 dilate_8c
  // nope
  // poc 33 dilate_8c stage 4 side 0
  si.poc[4][0].op = SPOC_POC_DILATE;
  si.poc[4][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[4][0].kernel[i] = pi0[i];

  // in [3 poc 1] -> [4 alu] 37 inf
  si.mux[3][3].op = SPOC_MUX_IN1;
  // in [4 input 1] -> [4 alu] 37 inf
  // nope
  // out_7 [4 poc 0] -> [4 alu] 37 inf
  // nope
  // alu 37 inf stage 4
  si.alu[4][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 5
  // out_6 [4 alu] -> [5 poc 0] 40 dilate_8c
  si.mux[4][0].op = SPOC_MUX_IN1;
  si.mux[4][2].op = SPOC_MUX_IN0;
  // out_6 [5 input 0] -> [5 poc 0] 40 dilate_8c
  // nope
  // poc 40 dilate_8c stage 5 side 0
  si.poc[5][0].op = SPOC_POC_DILATE;
  si.poc[5][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[5][0].kernel[i] = pi0[i];

  // in [4 poc 1] -> [5 alu] 44 inf
  si.mux[4][3].op = SPOC_MUX_IN1;
  // in [5 input 1] -> [5 alu] 44 inf
  // nope
  // out_5 [5 poc 0] -> [5 alu] 44 inf
  // nope
  // alu 44 inf stage 5
  si.alu[5][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 6
  // out_4 [5 alu] -> [6 poc 0] 47 dilate_8c
  si.mux[5][0].op = SPOC_MUX_IN1;
  si.mux[5][2].op = SPOC_MUX_IN0;
  // out_4 [6 input 0] -> [6 poc 0] 47 dilate_8c
  // nope
  // poc 47 dilate_8c stage 6 side 0
  si.poc[6][0].op = SPOC_POC_DILATE;
  si.poc[6][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[6][0].kernel[i] = pi0[i];

  // in [5 poc 1] -> [6 alu] 51 inf
  si.mux[5][3].op = SPOC_MUX_IN1;
  // in [6 input 1] -> [6 alu] 51 inf
  // nope
  // out_3 [6 poc 0] -> [6 alu] 51 inf
  // nope
  // alu 51 inf stage 6
  si.alu[6][0].op = SPOC_ALU_INF_IN0_IN1;


  // STAGE 7
  // out_2 [6 alu] -> [7 poc 0] 54 dilate_8c
  si.mux[6][0].op = SPOC_MUX_IN1;
  si.mux[6][2].op = SPOC_MUX_IN0;
  // out_2 [7 input 0] -> [7 poc 0] 54 dilate_8c
  // nope
  // poc 54 dilate_8c stage 7 side 0
  si.poc[7][0].op = SPOC_POC_DILATE;
  si.poc[7][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[7][0].kernel[i] = pi0[i];

  // in [6 poc 1] -> [7 alu] 58 inf
  si.mux[6][3].op = SPOC_MUX_IN1;
  // in [7 input 1] -> [7 alu] 58 inf
  // nope
  // out_1 [7 poc 0] -> [7 alu] 58 inf
  // nope
  // alu 58 inf stage 7
  si.alu[7][0].op = SPOC_ALU_INF_IN0_IN1;

  // out [7 alu] -> [7 measure 0] 58 inf
  si.mux[7][0].op = SPOC_MUX_IN1;
  si.mux[7][2].op = SPOC_MUX_IN0;


  // output image out on 0
  // out [7 measure 0] -> [7 output 0] 0 null
  // nope

  // fill in to the end...
  // out [7 output 0] -> [7 output 0] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, NULL, i0, i1);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 61 global_vol stage 7 side 0
  *po1 = (int32_t) reduc.measure[7][0].volume;

  return ret;
}

! updated function

int freia_58(freia_data2d *out, const freia_data2d *in)
{
   int volprevious, volcurrent, i, j, k, l, m, n;                       /*0001*/
   freia_58_spoc_helper_0_0_0(out, &volcurrent);                        /*0002*/
   do {                                                                 /*0003*/
      volprevious = volcurrent;                                         /*0004*/
      i = 1;                                                            /*0006*/
      k = 0;                                                            /*0007*/
      j = 0;                                                            /*0008*/
      i = 1;                                                            /*0013*/
      k = 0;                                                            /*0014*/
      i = 1;                                                            /*0020*/
      k = 0;                                                            /*0021*/
      i = 1;                                                            /*0027*/
      k = 0;                                                            /*0028*/
      i = 1;                                                            /*0034*/
      k = 0;                                                            /*0035*/
      i = 1;                                                            /*0041*/
      k = 0;                                                            /*0042*/
      i = 1;                                                            /*0048*/
      k = 0;                                                            /*0049*/
      i = 1;                                                            /*0055*/
      k = 0;                                                            /*0056*/
      freia_58_spoc_helper_1_0_0(out, in, out, freia_morpho_kernel_8c, &volcurrent);   /*0005*/
      l = j;                                                            /*0010*/
      m |= l;                                                           /*0011*/
      j = 0;                                                            /*0015*/
      l = j;                                                            /*0017*/
      m |= l;                                                           /*0018*/
      j = 0;                                                            /*0022*/
      l = j;                                                            /*0024*/
      m |= l;                                                           /*0025*/
      j = 0;                                                            /*0029*/
      l = j;                                                            /*0031*/
      m |= l;                                                           /*0032*/
      j = 0;                                                            /*0036*/
      l = j;                                                            /*0038*/
      m |= l;                                                           /*0039*/
      j = 0;                                                            /*0043*/
      l = j;                                                            /*0045*/
      m |= l;                                                           /*0046*/
      j = 0;                                                            /*0050*/
      l = j;                                                            /*0052*/
      m |= l;                                                           /*0053*/
      j = 0;                                                            /*0057*/
      l = j;                                                            /*0059*/
      m |= l;                                                           /*0060*/
   }
   while (volcurrent!=volprevious);                                     /*0003*/
   n = m;                                                               /*0062*/
   return n;                                                            /*0063*/
}

! cleaned up function

int freia_58(freia_data2d *out, const freia_data2d *in)
{
   int volprevious, volcurrent, i, j, k, l, m, n;                       /*0001*/
   freia_58_spoc_helper_0_0_0(out, &volcurrent);                        /*0002*/
   do {                                                                 /*0003*/
      volprevious = volcurrent;                                         /*0004*/
      i = 1;                                                            /*0006*/
      k = 0;                                                            /*0007*/
      j = 0;                                                            /*0008*/
      i = 1;                                                            /*0013*/
      k = 0;                                                            /*0014*/
      i = 1;                                                            /*0020*/
      k = 0;                                                            /*0021*/
      i = 1;                                                            /*0027*/
      k = 0;                                                            /*0028*/
      i = 1;                                                            /*0034*/
      k = 0;                                                            /*0035*/
      i = 1;                                                            /*0041*/
      k = 0;                                                            /*0042*/
      i = 1;                                                            /*0048*/
      k = 0;                                                            /*0049*/
      i = 1;                                                            /*0055*/
      k = 0;                                                            /*0056*/
      freia_58_spoc_helper_1_0_0(out, in, out, freia_morpho_kernel_8c, &volcurrent);   /*0005*/
      l = 0;                                                            /*0010*/

      j = 0;                                                            /*0015*/
      l = 0;                                                            /*0017*/

      j = 0;                                                            /*0022*/
      l = 0;                                                            /*0024*/

      j = 0;                                                            /*0029*/
      l = 0;                                                            /*0031*/

      j = 0;                                                            /*0036*/
      l = 0;                                                            /*0038*/

      j = 0;                                                            /*0043*/
      l = 0;                                                            /*0045*/

      j = 0;                                                            /*0050*/
      l = 0;                                                            /*0052*/

      j = 0;                                                            /*0057*/
      l = 0;                                                            /*0059*/

   }
   while (volcurrent!=volprevious);                                     /*0003*/
   n = m;                                                               /*0062*/
   return n;                                                            /*0063*/
}
