  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_ret_01_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_ret_01_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_ret_01_opencl_helper_0_0_0
