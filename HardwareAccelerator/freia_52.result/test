  si.alu[7][0].op = SPOC_ALU_INF_IN0_IN1;


  // output image 0`imWork1 on 0
  // 0`imWork1 [7 alu] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN1;
  si.mux[7][2].op = SPOC_MUX_IN0;

