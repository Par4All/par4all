   freia_aipo_dilate_8c(w1_1, w1_1, freia_morpho_k6_2);                 /*0832*/
   freia_aipo_dilate_8c(w1_1, w1_1, freia_morpho_k6_2);                 /*0833*/
   
   
   _return40 = 0;                                                       /*0836*/
   ret_18 = 0;                                                          /*0837*/
   
   
   freia_aipo_erode_8c(w1_1, w1_1, neighborTransposed_15);              /*0840*/
