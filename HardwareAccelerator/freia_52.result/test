  // 0`imWork1_2 [7 input 0] -> [7 alu] 600 inf
  // nope
  // 0`imin [6 poc 1] -> [7 alu] 600 inf
  si.mux[6][3].op = SPOC_MUX_IN1;
  // 0`imin [7 input 1] -> [7 alu] 600 inf
  // nope
