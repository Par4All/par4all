  return ret;
}


! updated function

int freia_69(freia_data2d *o0, freia_data2d *o1, const freia_data2d *in)
{

   freia_69_terapix_helper_0_0_0(o0, o1, in, 1, 1, 1, 1, 1, 1, 1, freia_morpho_kernel_8c, 255);       /*0009*/

   return 0;                                                            /*0026*/
}

