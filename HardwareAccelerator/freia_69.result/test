// hold kernels for freia_69_opencl_helper_E8_511
static cl_kernel freia_69_opencl_helper_E8_511_kernel[2];

// compile kernels for freia_69_opencl_helper_E8_511
static freia_status freia_69_opencl_helper_E8_511_compile(void)
{
  // OpenCL source for freia_69_opencl_helper_E8_511
  const char * freia_69_opencl_helper_E8_511_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_69_opencl_helper_E8_511\n"
    "KERNEL void freia_69_opencl_helper_E8_511(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t16 = PIXEL_MIN;\n"
    "    t16 = PIXEL_SUP(t16, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);\n"
    "    t16 = PIXEL_SUP(t16, (is_N)? PIXEL_MIN: j0[i-pitch]);\n"
    "    t16 = PIXEL_SUP(t16, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);\n"
    "    t16 = PIXEL_SUP(t16, (is_W)? PIXEL_MIN: j0[i-1]);\n"
    "    t16 = PIXEL_SUP(t16, in0);\n"
    "    t16 = PIXEL_SUP(t16, (is_E)? PIXEL_MIN: j0[i+1]);\n"
    "    t16 = PIXEL_SUP(t16, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);\n"
    "    t16 = PIXEL_SUP(t16, (is_S)? PIXEL_MIN: j0[i+pitch]);\n"
    "    t16 = PIXEL_SUP(t16, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);\n"
    "    // set output pixels\n"
    "    p0[i] = t16;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_69_opencl_helper_E8_511_source, "freia_69_opencl_helper_E8_511", "-DPIXEL8", &freia_69_opencl_helper_E8_511_kernel[0]);
  err |= freia_op_compile_kernel(freia_69_opencl_helper_E8_511_source, "freia_69_opencl_helper_E8_511", "-DPIXEL16", &freia_69_opencl_helper_E8_511_kernel[1]);
  return err;
}

