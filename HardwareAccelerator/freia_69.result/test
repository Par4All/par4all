  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * o2,
  int ofs_o2,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image below
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4,
  int c5,
  int c6,
  int c7)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * p2 = o2 + ofs_o2 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t9 = PIXEL_ADD(in0, c0);
    PIXEL t19 = PIXEL_NOT(in0);
    PIXEL t10 = PIXEL_ADD(t9, c1);
    PIXEL t20 = PIXEL_INF(t19, c2);
    PIXEL t11 = PIXEL_ADD(t10, c3);
    PIXEL t12 = PIXEL_ADD(t11, c4);
    PIXEL t13 = PIXEL_ADD(t12, c5);
    PIXEL t14 = PIXEL_ADD(t13, c6);
    PIXEL t15 = PIXEL_ADD(t14, c7);
    // set output pixels
    p0[i] = t19;
    p1[i] = t15;
    p2[i] = t20;
  }
}

// opencl function freia_69_opencl_helper_E8_511
KERNEL void freia_69_opencl_helper_E8_511(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t16 = PIXEL_MIN;
    t16 = PIXEL_SUP(t16, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t16 = PIXEL_SUP(t16, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t16 = PIXEL_SUP(t16, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t16 = PIXEL_SUP(t16, (is_W)? PIXEL_MIN: j0[i-1]);
    t16 = PIXEL_SUP(t16, in0);
    t16 = PIXEL_SUP(t16, (is_E)? PIXEL_MIN: j0[i+1]);
    t16 = PIXEL_SUP(t16, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t16 = PIXEL_SUP(t16, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t16 = PIXEL_SUP(t16, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    // set output pixels
    p0[i] = t16;
  }
}

// opencl function freia_69_opencl_helper_0_0_2
KERNEL void freia_69_opencl_helper_0_0_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  GLOBAL PIXEL * i3, // const?
  int ofs_i3,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;
  GLOBAL PIXEL * j3 = i3 + ofs_i3 + shift;

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    PIXEL in3 = j3[i];
    // pixel computations
    PIXEL t21 = PIXEL_MIN;
    t21 = PIXEL_SUP(t21, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t21 = PIXEL_SUP(t21, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t21 = PIXEL_SUP(t21, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t21 = PIXEL_SUP(t21, (is_W)? PIXEL_MIN: j0[i-1]);
    t21 = PIXEL_SUP(t21, in0);
    t21 = PIXEL_SUP(t21, (is_E)? PIXEL_MIN: j0[i+1]);
    t21 = PIXEL_SUP(t21, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t21 = PIXEL_SUP(t21, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t21 = PIXEL_SUP(t21, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    PIXEL t17 = PIXEL_MIN;
    t17 = PIXEL_SUP(t17, (is_N|is_W)? PIXEL_MIN: j2[i-pitch-1]);
    t17 = PIXEL_SUP(t17, (is_N)? PIXEL_MIN: j2[i-pitch]);
    t17 = PIXEL_SUP(t17, (is_N|is_E)? PIXEL_MIN: j2[i-pitch+1]);
    t17 = PIXEL_SUP(t17, (is_W)? PIXEL_MIN: j2[i-1]);
    t17 = PIXEL_SUP(t17, in2);
    t17 = PIXEL_SUP(t17, (is_E)? PIXEL_MIN: j2[i+1]);
    t17 = PIXEL_SUP(t17, (is_W|is_S)? PIXEL_MIN: j2[i+pitch-1]);
    t17 = PIXEL_SUP(t17, (is_S)? PIXEL_MIN: j2[i+pitch]);
    t17 = PIXEL_SUP(t17, (is_E|is_S)? PIXEL_MIN: j2[i+pitch+1]);
    PIXEL t18 = PIXEL_SUB(in1, t17);
    PIXEL t22 = PIXEL_INF(in3, t21);
    // set output pixels
    p0[i] = t18;
    p1[i] = t22;
  }
}

! updated function

int freia_69(freia_data2d *o0, freia_data2d *o1, const freia_data2d *in)
{
   freia_data2d *t1;                                                    /*0005*/
   t1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0006*/

   freia_69_opencl_helper_0_0_0(t1, o0, o1, in, 1, 1, 255, 1, 1, 1, 1, 1);             /*0009*/
   freia_69_opencl_helper_E8_511(o0, o0);                               /*0016*/
   freia_69_opencl_helper_0_0_2(o0, o1, t1, in, o0, o1);                /*0017*/

   freia_common_destruct_data(t1);                                      /*0024*/
   return 0;                                                            /*0026*/
}

! cleaned up function

int freia_69(freia_data2d *o0, freia_data2d *o1, const freia_data2d *in)
{
   freia_data2d *t1;                                                    /*0005*/
   t1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0006*/

   freia_69_opencl_helper_0_0_0(t1, o0, o1, in, 1, 1, 255, 1, 1, 1, 1, 1);             /*0009*/
   freia_69_opencl_helper_E8_511(o0, o0);                               /*0016*/
   freia_69_opencl_helper_0_0_2(o0, o1, t1, in, o0, o1);                /*0017*/

   freia_common_destruct_data(t1);                                      /*0024*/
   return 0;                                                            /*0026*/
}
