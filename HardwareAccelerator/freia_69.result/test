static cl_kernel freia_69_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_69_opencl_helper_0_0_0
static freia_status freia_69_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_69_opencl_helper_0_0_0
  const char * freia_69_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_69_opencl_helper_0_0_0\n"
    "KERNEL void freia_69_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * o2,\n"
    "  int ofs_o2,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  int c3,\n"
    "  int c4,\n"
    "  int c5,\n"
    "  int c6,\n"
    "  int c7)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * p2 = o2 + ofs_o2 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t9 = PIXEL_ADD(in0, c0);\n"
    "    PIXEL t19 = PIXEL_NOT(in0);\n"
    "    PIXEL t10 = PIXEL_ADD(t9, c1);\n"
    "    PIXEL t20 = PIXEL_INF(t19, c2);\n"
    "    PIXEL t11 = PIXEL_ADD(t10, c3);\n"
    "    PIXEL t12 = PIXEL_ADD(t11, c4);\n"
    "    PIXEL t13 = PIXEL_ADD(t12, c5);\n"
    "    PIXEL t14 = PIXEL_ADD(t13, c6);\n"
    "    PIXEL t15 = PIXEL_ADD(t14, c7);\n"
    "    // set output pixels\n"
    "    p0[i] = t19;\n"
    "    p1[i] = t15;\n"
    "    p2[i] = t20;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_69_opencl_helper_0_0_0_source, "freia_69_opencl_helper_0_0_0", "-DPIXEL8", &freia_69_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_69_opencl_helper_0_0_0_source, "freia_69_opencl_helper_0_0_0", "-DPIXEL16", &freia_69_opencl_helper_0_0_0_kernel[1]);
  return err;
}

