  return err;
}

// helper function freia_74_opencl_helper_0_0_0
freia_status freia_74_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t c0,
  int32_t c1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_74_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_74_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_74_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 1, 2, 0, o0, i0, c0, c1);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_74

// opencl for dag 0

// opencl function freia_74_opencl_helper_0_0_0
KERNEL void freia_74_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image below
  int pitch,
  int c0,
  int c1)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t17 = PIXEL_MAX;
    t17 = PIXEL_INF(t17, (is_W)? PIXEL_MAX: j0[i-1]);
    t17 = PIXEL_INF(t17, in0);
    t17 = PIXEL_INF(t17, (is_E)? PIXEL_MAX: j0[i+1]);
    PIXEL t15 = PIXEL_MIN;
    t15 = PIXEL_SUP(t15, (is_W)? PIXEL_MIN: j0[i-1]);
    t15 = PIXEL_SUP(t15, in0);
    t15 = PIXEL_SUP(t15, (is_E)? PIXEL_MIN: j0[i+1]);
    PIXEL t18 = PIXEL_ADD(t15, c0);
    PIXEL t19 = PIXEL_MUL(t17, c1);
    PIXEL t20 = PIXEL_AND(t18, t19);
    // set output pixels
    p0[i] = t20;
  }
}

