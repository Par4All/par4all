// hold kernels for freia_74_opencl_helper_0_0_0
static cl_kernel freia_74_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_74_opencl_helper_0_0_0
static freia_status freia_74_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_74_opencl_helper_0_0_0
  const char * freia_74_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_74_opencl_helper_0_0_0\n"
    "KERNEL void freia_74_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t17 = PIXEL_MAX;\n"
    "    t17 = PIXEL_INF(t17, (is_W)? PIXEL_MAX: j0[i-1]);\n"
    "    t17 = PIXEL_INF(t17, in0);\n"
    "    t17 = PIXEL_INF(t17, (is_E)? PIXEL_MAX: j0[i+1]);\n"
    "    PIXEL t15 = PIXEL_MIN;\n"
    "    t15 = PIXEL_SUP(t15, (is_W)? PIXEL_MIN: j0[i-1]);\n"
    "    t15 = PIXEL_SUP(t15, in0);\n"
    "    t15 = PIXEL_SUP(t15, (is_E)? PIXEL_MIN: j0[i+1]);\n"
    "    PIXEL t18 = PIXEL_ADD(t15, c0);\n"
    "    PIXEL t19 = PIXEL_MUL(t17, c1);\n"
    "    PIXEL t20 = PIXEL_AND(t18, t19);\n"
    "    // set output pixels\n"
    "    p0[i] = t20;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_74_opencl_helper_0_0_0_source, "freia_74_opencl_helper_0_0_0", "-DPIXEL8", &freia_74_opencl_helper_0_0_0_kernel[0]);
