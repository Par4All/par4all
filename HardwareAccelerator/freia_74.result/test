   t0 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0010*/
   t1 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0011*/
   t2 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0012*/
   t3 = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);   /*0013*/

   ret_0 = freia_aipo_erode_8c(t0, in, kernel1x3);                      /*0015*/
   ret_1 = ret_0;                                                       /*0016*/
   freia_aipo_dilate_8c(t1, in, kernel1x3);                             /*0017*/
