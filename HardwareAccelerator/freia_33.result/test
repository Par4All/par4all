  "X1" [shape=circle];
  "tC2" [shape=circle];

  // outputs
  "o" [shape=circle];

  // computation vertices
  "34 +" [shape=trapezium,orientation=270];
  "34 +" -> "o";
  "tC2" -> "34 +";
  "33 thr" [shape=parallelogram];
  "33 thr" -> "34 +" [label="tD"];
  "X1" -> "33 thr";
}

! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 11 ops and 0 copies, optimized to 11 ops and 0+0+0 copies

// hold kernels for freia_33_opencl_helper_0_1_1
static cl_kernel freia_33_opencl_helper_0_1_1_kernel[2];

// compile kernels for freia_33_opencl_helper_0_1_1
static freia_status freia_33_opencl_helper_0_1_1_compile(void)
{
  // OpenCL source for freia_33_opencl_helper_0_1_1
  const char * freia_33_opencl_helper_0_1_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_33_opencl_helper_0_1_1\n"
    "KERNEL void freia_33_opencl_helper_0_1_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  // no tiling on first dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // loop i upper bound\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t26 = PIXEL_THRESHOLD(in0, c0, c1, c2);\n"
    "    PIXEL t27 = PIXEL_MUL(t26, t26);\n"
    "    // set output pixels\n"
    "    p0[i] = t27;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_33_opencl_helper_0_1_1_source, "freia_33_opencl_helper_0_1_1", "-DPIXEL8", &freia_33_opencl_helper_0_1_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_33_opencl_helper_0_1_1_source, "freia_33_opencl_helper_0_1_1", "-DPIXEL16", &freia_33_opencl_helper_0_1_1_kernel[1]);
  return err;
}

// helper function freia_33_opencl_helper_0_1_1
freia_status freia_33_opencl_helper_0_1_1(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t c0,
  int32_t c1,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_33_opencl_helper_0_1_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_33_opencl_helper_0_1_1_kernel[bpp];

  // call kernel freia_33_opencl_helper_0_1_1
  err |= freia_op_call_kernel(kernel, 1, 1, 3, 0, o0, i0, c0, c1, c2);

  return err;
}

// hold kernels for freia_33_opencl_helper_0_1_3
static cl_kernel freia_33_opencl_helper_0_1_3_kernel[2];

// compile kernels for freia_33_opencl_helper_0_1_3
static freia_status freia_33_opencl_helper_0_1_3_compile(void)
{
  // OpenCL source for freia_33_opencl_helper_0_1_3
  const char * freia_33_opencl_helper_0_1_3_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_33_opencl_helper_0_1_3\n"
    "KERNEL void freia_33_opencl_helper_0_1_3(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  // no tiling on first dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t30 = PIXEL_DIV(in1, in0);\n"
    "    PIXEL_VOLUME(red31, t30);\n"
    "    // set output pixels\n"
    "    p0[i] = t30;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_33_opencl_helper_0_1_3_source, "freia_33_opencl_helper_0_1_3", "-DPIXEL8", &freia_33_opencl_helper_0_1_3_kernel[0]);
  err |= freia_op_compile_kernel(freia_33_opencl_helper_0_1_3_source, "freia_33_opencl_helper_0_1_3", "-DPIXEL16", &freia_33_opencl_helper_0_1_3_kernel[1]);
  return err;
}

// helper function freia_33_opencl_helper_0_1_3
freia_status freia_33_opencl_helper_0_1_3(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t * po3)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_33_opencl_helper_0_1_3_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_33_opencl_helper_0_1_3_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_33_opencl_helper_0_1_3
  err |= freia_op_call_kernel(kernel, 1, 2, 0, 1, o0, i0, i1, &redres);

  // return reduction results
  *po3 = redres.volume;

  return err;
}

