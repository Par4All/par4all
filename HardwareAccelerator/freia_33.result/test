// generated OpenCL kernels for function freia_33

// opencl for dag 0

// opencl function freia_33_opencl_helper_0_1_1
KERNEL void freia_33_opencl_helper_0_1_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2)
{
  // get id & compute global image shift
  // no tiling on first dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // loop i upper bound
  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;

  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t26 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    PIXEL t27 = PIXEL_MUL(t26, t26);
    // set output pixels
    p0[i] = t27;
  }
}

// opencl function freia_33_opencl_helper_0_1_3
KERNEL void freia_33_opencl_helper_0_1_3(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  GLOBAL TMeasure * redX)
{
  // get id & compute global image shift
  // no tiling on first dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;

  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // get input pixels
    PIXEL in1 = j1[i];
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t30 = PIXEL_DIV(in1, in0);
    PIXEL_VOLUME(red31, t30);
    // set output pixels
    p0[i] = t30;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].vol = vol;
}

// opencl function freia_33_opencl_helper_0_2_0
KERNEL void freia_33_opencl_helper_0_2_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2)
{
  // get id & compute global image shift
  // no tiling on first dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // loop i upper bound
  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;

  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t33 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    PIXEL t34 = PIXEL_ADD(in1, t33);
    // set output pixels
    p0[i] = t34;
  }
}

! updated function

freia_status freia_33(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tB2 = freia_common_create_data(16, 128, 128), *tC2 = freia_common_create_data(16, 128, 128);     /*0013*/
   int sA, sB, sC;                                                      /*0014*/
   // A
   freia_aipo_erode_8c(X0, i, k);                                       /*0022*/
   
   // complex dag for split
   // A
   freia_aipo_global_max(i, &sA);                                       /*0018*/
   // A
   freia_aipo_global_min(X0, &sB);                                      /*0024*/
   // B
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0020*/
   // C
   freia_33_opencl_helper_0_1_1(tC2, X0, sB-10, sB-1, false);           /*0026*/
   // B
   freia_aipo_dilate_8c(tB2, tB, k);                                    /*0029*/
   freia_33_opencl_helper_0_1_3(X1, tB, tB2, &sC);                      /*0030*/
   // D
   freia_33_opencl_helper_0_2_0(o, X1, tC2, sC-10, sC-1, false);        /*0033*/

   freia_common_destruct_data(X0);                                      /*0036*/
   freia_common_destruct_data(X1);                                      /*0037*/
   freia_common_destruct_data(tB);                                      /*0038*/
   freia_common_destruct_data(tB2);                                     /*0039*/
   freia_common_destruct_data(tC2);                                     /*0041*/
   return FREIA_OK;                                                     /*0043*/
}

