    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_33_opencl_helper_0_2_0_source, "freia_33_opencl_helper_0_2_0", "-DPIXEL8", &freia_33_opencl_helper_0_2_0_kernel[0]);
