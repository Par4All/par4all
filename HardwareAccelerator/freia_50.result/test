! initial function

freia_status freia_50(freia_data2d *out, freia_data2d *i0, freia_data2d *i1)
{
   freia_data2d *t1 = freia_common_create_data(16, 128, 128);           /*0005*/
   freia_data2d *t2 = freia_common_create_data(16, 128, 128);           /*0006*/

   freia_aipo_erode_8c(t1, i0, freia_morpho_kernel_8c);                 /*0008*/
   freia_aipo_inf(t2, i0, i1);                                          /*0009*/
   freia_aipo_sub(out, t1, t2);                                         /*0010*/

   freia_common_destruct_data(t1);                                      /*0012*/
   freia_common_destruct_data(t2);                                      /*0013*/
   return FREIA_OK;                                                     /*0014*/
}

