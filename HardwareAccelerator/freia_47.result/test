! initial function

freia_status freia_47(freia_data2d *in, freia_data2d *o1, freia_data2d *o2)
{
   freia_data2d *t;                                                     /*0006*/
   t = freia_common_create_data(in->bpp, in->widthWa, in->heightWa);    /*0007*/
   // useless copy that requires backwards and back-forwards propagation
   freia_aipo_add(t, in, o1);                                           /*0009*/
   freia_aipo_mul(o2, t, in);                                           /*0010*/
   freia_aipo_copy(o1, t);                                              /*0011*/
   freia_common_destruct_data(t);                                       /*0012*/
   return FREIA_OK;                                                     /*0013*/
}

