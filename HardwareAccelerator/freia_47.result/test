// graph for dag "dag_cleaned_0" of module "freia_47" in dot format
// DAG "dag_cleaned_0": #i=2 #o=2 #c=2 #s=0 #I=0 #B=0 #A=0
digraph "dag_cleaned_0" {
  // inputs
  "in" [shape=circle];
  "o1" [shape=circle];

  // outputs
