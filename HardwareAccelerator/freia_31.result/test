  "o1" [shape=circle];

  // computation vertices
  "10 thr" [shape=parallelogram];
  "10 thr" -> "o1";
  "9 ^" [shape=trapezium,orientation=270];
  "9 ^" -> "o0";
  "i" -> "10 thr";
  "i" -> "9 ^";
  "i" -> "9 ^";
}

! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 2 ops and 0 copies, optimized to 2 ops and 0+0+0 copies

// hold kernels for freia_31_opencl_helper_0_0_0
static cl_kernel freia_31_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_31_opencl_helper_0_0_0
static freia_status freia_31_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_31_opencl_helper_0_0_0
  const char * freia_31_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_31_opencl_helper_0_0_0\n"
    "KERNEL void freia_31_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width,\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t9 = PIXEL_XOR(in0, in0);\n"
    "    PIXEL t10 = PIXEL_THRESHOLD(in0, c0, c1, c2);\n"
    "    // set output pixels\n"
    "    p0[i] = t9;\n"
    "    p1[i] = t10;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_31_opencl_helper_0_0_0_source, "freia_31_opencl_helper_0_0_0", "-DPIXEL8", &freia_31_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_31_opencl_helper_0_0_0_source, "freia_31_opencl_helper_0_0_0", "-DPIXEL16", &freia_31_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_31_opencl_helper_0_0_0
freia_status freia_31_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  int32_t c0,
  int32_t c1,
  int32_t c2)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_31_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_31_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_31_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 1, 3, 0, o0, o1, i0, c0, c1, c2);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_31

// opencl for dag 0

// opencl function freia_31_opencl_helper_0_0_0
KERNEL void freia_31_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width,
  int pitch,
  int c0,
  int c1,
  int c2)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL t9 = PIXEL_XOR(in0, in0);
    PIXEL t10 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    // set output pixels
    p0[i] = t9;
    p1[i] = t10;
  }
}

