{
   // parallel reuses (similar to freia_27)
   // o0 = i ^ i
   // o1 = f(i)
   freia_31_opencl_helper_0_0_0(o0, o1, i, 10, 20, true);               /*0009*/

   return FREIA_OK;                                                     /*0012*/
}

