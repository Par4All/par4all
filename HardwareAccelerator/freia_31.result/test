! initial function

freia_status freia_31(freia_data2d *o0, freia_data2d *o1, freia_data2d *i)
{
   // parallel reuses (similar to freia_27)
   // o0 = i ^ i
   // o1 = f(i)
   freia_aipo_xor(o0, i, i);                                            /*0009*/
   freia_aipo_threshold(o1, i, 10, 20, true);                           /*0010*/

   return FREIA_OK;                                                     /*0012*/
}

