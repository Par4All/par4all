// hold kernels for gradient_thick_opencl_helper_0_0_0
static cl_kernel gradient_thick_opencl_helper_0_0_0_kernel[2];

// compile kernels for gradient_thick_opencl_helper_0_0_0
static freia_status gradient_thick_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for gradient_thick_opencl_helper_0_0_0
  const char * gradient_thick_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function gradient_thick_opencl_helper_0_0_0\n"
    "KERNEL void gradient_thick_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL t12 = PIXEL_MIN;\n"
    "    t12 = PIXEL_SUP(t12, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);\n"
    "    t12 = PIXEL_SUP(t12, (is_N)? PIXEL_MIN: j0[i-pitch]);\n"
    "    t12 = PIXEL_SUP(t12, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);\n"
    "    t12 = PIXEL_SUP(t12, (is_W)? PIXEL_MIN: j0[i-1]);\n"
    "    t12 = PIXEL_SUP(t12, in0);\n"
    "    t12 = PIXEL_SUP(t12, (is_E)? PIXEL_MIN: j0[i+1]);\n"
    "    t12 = PIXEL_SUP(t12, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);\n"
    "    t12 = PIXEL_SUP(t12, (is_S)? PIXEL_MIN: j0[i+pitch]);\n"
    "    t12 = PIXEL_SUP(t12, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);\n"
    "    PIXEL t11 = PIXEL_MAX;\n"
    "    t11 = PIXEL_INF(t11, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);\n"
    "    t11 = PIXEL_INF(t11, (is_N)? PIXEL_MAX: j0[i-pitch]);\n"
    "    t11 = PIXEL_INF(t11, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);\n"
    "    t11 = PIXEL_INF(t11, (is_W)? PIXEL_MAX: j0[i-1]);\n"
    "    t11 = PIXEL_INF(t11, in0);\n"
    "    t11 = PIXEL_INF(t11, (is_E)? PIXEL_MAX: j0[i+1]);\n"
    "    t11 = PIXEL_INF(t11, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);\n"
    "    t11 = PIXEL_INF(t11, (is_S)? PIXEL_MAX: j0[i+pitch]);\n"
    "    t11 = PIXEL_INF(t11, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);\n"
    "    PIXEL t13 = PIXEL_SUB(t12, t11);\n"
    "    // set output pixels\n"
    "    p0[i] = t13;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(gradient_thick_opencl_helper_0_0_0_source, "gradient_thick_opencl_helper_0_0_0", "-DPIXEL8", &gradient_thick_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(gradient_thick_opencl_helper_0_0_0_source, "gradient_thick_opencl_helper_0_0_0", "-DPIXEL16", &gradient_thick_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function gradient_thick_opencl_helper_0_0_0
freia_status gradient_thick_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= gradient_thick_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = gradient_thick_opencl_helper_0_0_0_kernel[bpp];

  // call kernel gradient_thick_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 1, 0, 0, o0, i0);

  return err;
}
