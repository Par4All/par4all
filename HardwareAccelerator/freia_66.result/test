! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 0 ops and 2 copies, optimized to 0 ops and 0+2+0 copies
// # SPOC calls: 0

// dag 1: 4 ops and 0 copies, optimized to 4 ops and 0+0+0 copies
// split 0: 1 cut
// # SPOC calls: 1

// FREIA-SPoC helper function for module freia_66
freia_status freia_66_spoc_helper_1_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`out [0 input 0] -> [0 poc 0] 24 erode_8c
  // nope
  // poc 24 erode_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_ERODE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];

  // 0`out_1 [0 poc 0] -> [0 alu] 25 sup
  // nope
  // 0`t [0 input 1] -> [0 alu] 25 sup
  // nope
  // alu 25 sup stage 0
  si.alu[0][0].op = SPOC_ALU_SUP_IN0_IN1;


  // STAGE 2
  // 0`t_1 [0 alu] -> [2 poc 0] 26 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN1;
  si.mux[0][2].op = SPOC_MUX_IN0;
  // 0`t_1 [1 input 0] -> [2 poc 0] 26 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`t_1 [2 input 0] -> [2 poc 0] 26 dilate_8c
  // nope
  // poc 26 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];

  // 0`t_1 [1 poc 0] -> [2 alu] 27 sup
  si.mux[1][1].op = SPOC_MUX_IN0;
  si.mux[1][3].op = SPOC_MUX_IN0;
  // 0`t_1 [2 input 1] -> [2 alu] 27 sup
  // nope
  // 0`out [2 poc 0] -> [2 alu] 27 sup
  // nope
  // alu 27 sup stage 2
  si.alu[2][0].op = SPOC_ALU_SUP_IN0_IN1;


  // output image 0`out on 0 and image 0`t on 1
  // 0`out [2 poc 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`out [2 output 0] -> [7 output 0] 0 null
  // 0`out [3 input 0] -> [7 output 0] 0 null
  si.mux[3][0].op = SPOC_MUX_IN0;
  // 0`out [4 input 0] -> [7 output 0] 0 null
  si.mux[4][0].op = SPOC_MUX_IN0;
  // 0`out [5 input 0] -> [7 output 0] 0 null
  si.mux[5][0].op = SPOC_MUX_IN0;
  // 0`out [6 input 0] -> [7 output 0] 0 null
  si.mux[6][0].op = SPOC_MUX_IN0;
  // 0`out [7 input 0] -> [7 output 0] 0 null
  si.mux[7][0].op = SPOC_MUX_IN0;

  // 0`t [2 alu] -> [2 output 1] 0 null
  si.mux[2][1].op = SPOC_MUX_IN1;
  si.mux[2][3].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`t [2 output 1] -> [7 output 1] 0 null
  // 0`t [3 input 1] -> [7 output 1] 0 null
  si.mux[3][3].op = SPOC_MUX_IN1;
  // 0`t [4 input 1] -> [7 output 1] 0 null
  si.mux[4][3].op = SPOC_MUX_IN1;
  // 0`t [5 input 1] -> [7 output 1] 0 null
  si.mux[5][3].op = SPOC_MUX_IN1;
  // 0`t [6 input 1] -> [7 output 1] 0 null
  si.mux[6][3].op = SPOC_MUX_IN1;
  // 0`t [7 input 1] -> [7 output 1] 0 null
  si.mux[7][3].op = SPOC_MUX_IN1;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

