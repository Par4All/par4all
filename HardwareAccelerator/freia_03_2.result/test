! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 8 ops and 0 copies, optimized to 8 ops and 0+0+0 copies

// hold kernels for freia_03_opencl_helper_0_0_0
static cl_kernel freia_03_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_03_opencl_helper_0_0_0
static freia_status freia_03_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_03_opencl_helper_0_0_0
  const char * freia_03_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_03_opencl_helper_0_0_0\n"
    "KERNEL void freia_03_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  int c3,\n"
    "  int c4)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  // no tiling on first dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "  int shift = pitch*get_global_id(0);\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  // loop i upper bound\n"
    "  int workshare = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int last = (get_global_id(1)+1)*workshare;\n"
    "  if (last>width) last = width;\n"
    "\n"
    "  int i;\n"
    "  for (i=get_global_id(1)*workshare; i<last; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t27 = PIXEL_ADD(in0, c0);\n"
    "    PIXEL t28 = PIXEL_ADDSAT(in1, c1);\n"
    "    PIXEL t33 = PIXEL_SET(c2);\n"
    "    PIXEL t29 = PIXEL_ADD(t27, t28);\n"
    "    PIXEL t30 = PIXEL_MUL(t27, c3);\n"
    "    PIXEL t31 = PIXEL_DIV(t29, c4);\n"
    "    PIXEL t32 = PIXEL_AND(t30, t31);\n"
    "    PIXEL t34 = PIXEL_MUL(t32, t33);\n"
    "    // set output pixels\n"
    "    p0[i] = t34;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_03_opencl_helper_0_0_0_source, "freia_03_opencl_helper_0_0_0", "-DPIXEL8", &freia_03_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_03_opencl_helper_0_0_0_source, "freia_03_opencl_helper_0_0_0", "-DPIXEL16", &freia_03_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_03_opencl_helper_0_0_0
freia_status freia_03_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t c2,
  int32_t c3,
  int32_t c4)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_03_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_03_opencl_helper_0_0_0_kernel[bpp];

  // call kernel freia_03_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 2, 5, 0, o0, i0, i1, c0, c1, c2, c3, c4);

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_03

// opencl for dag 0

// opencl function freia_03_opencl_helper_0_0_0
KERNEL void freia_03_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4)
{
  // get id & compute global image shift
  // no tiling on first dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // loop i upper bound
  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;

  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t27 = PIXEL_ADD(in0, c0);
    PIXEL t28 = PIXEL_ADDSAT(in1, c1);
    PIXEL t33 = PIXEL_SET(c2);
    PIXEL t29 = PIXEL_ADD(t27, t28);
    PIXEL t30 = PIXEL_MUL(t27, c3);
    PIXEL t31 = PIXEL_DIV(t29, c4);
    PIXEL t32 = PIXEL_AND(t30, t31);
    PIXEL t34 = PIXEL_MUL(t32, t33);
    // set output pixels
    p0[i] = t34;
  }
}

