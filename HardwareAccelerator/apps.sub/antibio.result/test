  "164 thr" [shape=parallelogram];
  "164 thr" -> "167 D8" [label="w1_0"];
  "162 -" [shape=trapezium,orientation=270];
  "162 -" -> "164 thr" [label="w1_0"];
  "w2_0" -> "162 -";
  "w1_0" -> "162 -";
}
