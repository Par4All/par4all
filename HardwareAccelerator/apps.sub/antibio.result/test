// graph for dag "dag_cleaned_2" of module "main" in dot format
// DAG "dag_cleaned_2": #i=1 #o=0 #c=1 #s=0 #I=0 #B=0 #A=0
digraph "dag_cleaned_2" {
  // inputs
  "w2_0" [shape=circle];

  // outputs

  // computation vertices
  "139 vol" [shape=diamond];
  "w2_0" -> "139 vol";
}
