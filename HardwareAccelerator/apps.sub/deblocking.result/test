  mcu_macro[1][27].addrStart = TERAPIX_UCODE_OR3;


  // inputs:
  // - imagelet 1 is i0 for filter2
  tile_in[0][0].x = imagelet_io_1_0;
  tile_in[0][0].y = 0;
  tile_in[1][0].x = imagelet_io_1_1;
  tile_in[1][0].y = 0;
  // - imagelet 2 is i1 for filter3
  tile_in[0][1].x = imagelet_io_2_0;
