  tile_in[1][2].y = 0;
  // - imagelet 4 is i3 for filter5
  tile_in[0][3].x = imagelet_io_4_0;
  tile_in[0][3].y = 0;
  tile_in[1][3].x = imagelet_io_4_1;
  tile_in[1][3].y = 0;
