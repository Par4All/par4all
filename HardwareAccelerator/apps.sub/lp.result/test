// opencl function main_opencl_helper_0_0_2
KERNEL void main_opencl_helper_0_0_2(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image below
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4,
  int c5)
{
  // get id & compute global image shift
  int height = get_global_size(0);
  int thrid = get_global_id(0) + height*get_global_id(1);
  int shift = pitch*get_global_id(0);

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  // N & S boundaries, one thread on first dimension per row
  // N not needed
  // S not needed

  int workshare = (width+get_global_size(1)-1)/get_global_size(1);
  int last = (get_global_id(1)+1)*workshare;
  if (last>width) last = width;
  int i;
  for (i=get_global_id(1)*workshare; i<last; i++)
  {
    // W & E boundaries, assuming i global index
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t137 = PIXEL_MAX;
    t137 = PIXEL_INF(t137, (is_W)? PIXEL_MAX: j0[i-1]);
    t137 = PIXEL_INF(t137, in0);
    t137 = PIXEL_INF(t137, (is_E)? PIXEL_MAX: j0[i+1]);
    PIXEL t97 = PIXEL_MIN;
    t97 = PIXEL_SUP(t97, (is_W)? PIXEL_MIN: j1[i-1]);
    t97 = PIXEL_SUP(t97, in1);
    t97 = PIXEL_SUP(t97, (is_E)? PIXEL_MIN: j1[i+1]);
    PIXEL t142 = PIXEL_THRESHOLD(t97, c0, c1, c2);
    PIXEL t143 = PIXEL_THRESHOLD(t137, c3, c4, c5);
    PIXEL t144 = PIXEL_AND(t142, t143);
    // set output pixels
    p0[i] = t144;
  }
}

// opencl function main_opencl_helper_D8_511
KERNEL void main_opencl_helper_D8_511(
