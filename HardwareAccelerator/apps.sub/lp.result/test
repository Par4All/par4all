   freia_common_destruct_data(imfilt);                                  /*0208*/
   freia_common_destruct_data(imout);                                   /*0209*/
   freia_common_destruct_data(out);                                     /*0210*/

   freia_common_close_input(&fdin);                                     /*0212*/
   freia_common_close_output(&fdout);                                   /*0213*/

   freia_shutdown();                                                    /*0215*/
   return ret;                                                          /*0216*/
}

