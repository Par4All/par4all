    t195 = PIXEL_SUP(t195, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t195 = PIXEL_SUP(t195, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t195 = PIXEL_SUP(t195, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t195 = PIXEL_SUP(t195, (is_W)? PIXEL_MIN: j0[i-1]);
    t195 = PIXEL_SUP(t195, in0);
    t195 = PIXEL_SUP(t195, (is_E)? PIXEL_MIN: j0[i+1]);
    t195 = PIXEL_SUP(t195, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t195 = PIXEL_SUP(t195, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t195 = PIXEL_SUP(t195, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    // set output pixels
    p0[i] = t195;
  }
}

