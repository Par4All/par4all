    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t227 = PIXEL_MAX;\n"
    "    t227 = PIXEL_INF(t227, (is_N|is_W)? PIXEL_MAX: j1[i-pitch-1]);\n"
    "    t227 = PIXEL_INF(t227, (is_N)? PIXEL_MAX: j1[i-pitch]);\n"
    "    t227 = PIXEL_INF(t227, (is_N|is_E)? PIXEL_MAX: j1[i-pitch+1]);\n"
    "    t227 = PIXEL_INF(t227, (is_W)? PIXEL_MAX: j1[i-1]);\n"
    "    t227 = PIXEL_INF(t227, in1);\n"
    "    t227 = PIXEL_INF(t227, (is_E)? PIXEL_MAX: j1[i+1]);\n"
    "    t227 = PIXEL_INF(t227, (is_W|is_S)? PIXEL_MAX: j1[i+pitch-1]);\n"
    "    t227 = PIXEL_INF(t227, (is_S)? PIXEL_MAX: j1[i+pitch]);\n"
    "    t227 = PIXEL_INF(t227, (is_E|is_S)? PIXEL_MAX: j1[i+pitch+1]);\n"
    "    PIXEL t210 = PIXEL_MAX;\n"
    "    t210 = PIXEL_INF(t210, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);\n"
    "    t210 = PIXEL_INF(t210, (is_N)? PIXEL_MAX: j0[i-pitch]);\n"
