// opencl for dag 0

// opencl function main_opencl_helper_0_0_0
KERNEL void main_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  GLOBAL PIXEL * i2, // const?
  int ofs_i2,
  int width, // of the working area, vs image below
  int pitch)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;
  GLOBAL PIXEL * j2 = i2 + ofs_i2 + shift;

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    PIXEL in2 = j2[i];
    // pixel computations
    PIXEL t220 = PIXEL_MAX;
    t220 = PIXEL_INF(t220, (is_N|is_W)? PIXEL_MAX: j2[i-pitch-1]);
    t220 = PIXEL_INF(t220, (is_N)? PIXEL_MAX: j2[i-pitch]);
    t220 = PIXEL_INF(t220, (is_N|is_E)? PIXEL_MAX: j2[i-pitch+1]);
    t220 = PIXEL_INF(t220, (is_W)? PIXEL_MAX: j2[i-1]);
    t220 = PIXEL_INF(t220, in2);
    t220 = PIXEL_INF(t220, (is_E)? PIXEL_MAX: j2[i+1]);
    t220 = PIXEL_INF(t220, (is_W|is_S)? PIXEL_MAX: j2[i+pitch-1]);
    t220 = PIXEL_INF(t220, (is_S)? PIXEL_MAX: j2[i+pitch]);
    t220 = PIXEL_INF(t220, (is_E|is_S)? PIXEL_MAX: j2[i+pitch+1]);
    PIXEL t221 = PIXEL_MIN;
    t221 = PIXEL_SUP(t221, (is_N|is_W)? PIXEL_MIN: j2[i-pitch-1]);
    t221 = PIXEL_SUP(t221, (is_N)? PIXEL_MIN: j2[i-pitch]);
    t221 = PIXEL_SUP(t221, (is_N|is_E)? PIXEL_MIN: j2[i-pitch+1]);
    t221 = PIXEL_SUP(t221, (is_W)? PIXEL_MIN: j2[i-1]);
    t221 = PIXEL_SUP(t221, in2);
    t221 = PIXEL_SUP(t221, (is_E)? PIXEL_MIN: j2[i+1]);
    t221 = PIXEL_SUP(t221, (is_W|is_S)? PIXEL_MIN: j2[i+pitch-1]);
    t221 = PIXEL_SUP(t221, (is_S)? PIXEL_MIN: j2[i+pitch]);
    t221 = PIXEL_SUP(t221, (is_E|is_S)? PIXEL_MIN: j2[i+pitch+1]);
    PIXEL t203 = PIXEL_MAX;
    t203 = PIXEL_INF(t203, (is_N|is_W)? PIXEL_MAX: j1[i-pitch-1]);
    t203 = PIXEL_INF(t203, (is_N)? PIXEL_MAX: j1[i-pitch]);
    t203 = PIXEL_INF(t203, (is_N|is_E)? PIXEL_MAX: j1[i-pitch+1]);
    t203 = PIXEL_INF(t203, (is_W)? PIXEL_MAX: j1[i-1]);
    t203 = PIXEL_INF(t203, in1);
    t203 = PIXEL_INF(t203, (is_E)? PIXEL_MAX: j1[i+1]);
    t203 = PIXEL_INF(t203, (is_W|is_S)? PIXEL_MAX: j1[i+pitch-1]);
    t203 = PIXEL_INF(t203, (is_S)? PIXEL_MAX: j1[i+pitch]);
    t203 = PIXEL_INF(t203, (is_E|is_S)? PIXEL_MAX: j1[i+pitch+1]);
    PIXEL t204 = PIXEL_MIN;
    t204 = PIXEL_SUP(t204, (is_N|is_W)? PIXEL_MIN: j1[i-pitch-1]);
    t204 = PIXEL_SUP(t204, (is_N)? PIXEL_MIN: j1[i-pitch]);
    t204 = PIXEL_SUP(t204, (is_N|is_E)? PIXEL_MIN: j1[i-pitch+1]);
    t204 = PIXEL_SUP(t204, (is_W)? PIXEL_MIN: j1[i-1]);
    t204 = PIXEL_SUP(t204, in1);
    t204 = PIXEL_SUP(t204, (is_E)? PIXEL_MIN: j1[i+1]);
    t204 = PIXEL_SUP(t204, (is_W|is_S)? PIXEL_MIN: j1[i+pitch-1]);
    t204 = PIXEL_SUP(t204, (is_S)? PIXEL_MIN: j1[i+pitch]);
    t204 = PIXEL_SUP(t204, (is_E|is_S)? PIXEL_MIN: j1[i+pitch+1]);
    PIXEL t197 = PIXEL_MAX;
    t197 = PIXEL_INF(t197, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);
    t197 = PIXEL_INF(t197, (is_N)? PIXEL_MAX: j0[i-pitch]);
    t197 = PIXEL_INF(t197, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);
    t197 = PIXEL_INF(t197, (is_W)? PIXEL_MAX: j0[i-1]);
    t197 = PIXEL_INF(t197, in0);
    t197 = PIXEL_INF(t197, (is_E)? PIXEL_MAX: j0[i+1]);
    t197 = PIXEL_INF(t197, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);
    t197 = PIXEL_INF(t197, (is_S)? PIXEL_MAX: j0[i+pitch]);
    t197 = PIXEL_INF(t197, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);
    PIXEL t198 = PIXEL_MIN;
    t198 = PIXEL_SUP(t198, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t198 = PIXEL_SUP(t198, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t198 = PIXEL_SUP(t198, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t198 = PIXEL_SUP(t198, (is_W)? PIXEL_MIN: j0[i-1]);
    t198 = PIXEL_SUP(t198, in0);
    t198 = PIXEL_SUP(t198, (is_E)? PIXEL_MIN: j0[i+1]);
    t198 = PIXEL_SUP(t198, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t198 = PIXEL_SUP(t198, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t198 = PIXEL_SUP(t198, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    PIXEL t199 = PIXEL_SUB(t197, t198);
    PIXEL t205 = PIXEL_SUB(t203, t204);
    PIXEL t222 = PIXEL_SUB(t220, t221);
    PIXEL t208 = PIXEL_ABSDIFF(t199, t205);
    PIXEL t225 = PIXEL_ABSDIFF(t205, t222);
    // set output pixels
    p0[i] = t208;
    p1[i] = t225;
  }
}

