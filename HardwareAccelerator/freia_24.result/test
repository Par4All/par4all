
! initial function

freia_status freia_24(freia_data2d *o, freia_data2d *i, uint32_t *k)
{
   
   
   
   
   
   freia_data2d *X0 = freia_common_create_data(16, 128, 128), *X1 = freia_common_create_data(16, 128, 128), *tB = freia_common_create_data(16, 128, 128), *tC = freia_common_create_data(16, 128, 128), *tD = freia_common_create_data(16, 128, 128);       /*0010*/
   int sA,  sB,  sC;                                                    /*0011*/
   
   // complex dag for split
   freia_aipo_global_max(i, &sA);                                       /*0014*/
   freia_aipo_threshold(tB, i, sA-10, sA-1, false);                     /*0015*/
   freia_aipo_erode_8c(X0, i, k);                                       /*0016*/
   freia_aipo_global_min(X0, &sB);                                      /*0017*/
   freia_aipo_threshold(tC, X0, sB-10, sB-1, false);                    /*0018*/
   freia_aipo_dilate_8c(X1, tB, k);                                     /*0019*/
   freia_aipo_global_vol(X1, &sC);                                      /*0020*/
   freia_aipo_threshold(tD, X1, sC-10, sC-1, false);                    /*0021*/
   freia_aipo_add(o, tC, tD);                                           /*0022*/

   freia_common_destruct_data(X0);                                      /*0024*/
   freia_common_destruct_data(X1);                                      /*0025*/
   freia_common_destruct_data(tB);                                      /*0026*/
   freia_common_destruct_data(tC);                                      /*0027*/
   freia_common_destruct_data(tD);                                      /*0028*/
   return FREIA_OK;                                                     /*0029*/
}

