! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 8 ops and 0 copies, optimized to 8 ops and 0+0+0 copies

// hold kernels for freia_04_opencl_helper_0_0_1
static cl_kernel freia_04_opencl_helper_0_0_1_kernel[2];

// compile kernels for freia_04_opencl_helper_0_0_1
static freia_status freia_04_opencl_helper_0_0_1_compile(void)
{
  // OpenCL source for freia_04_opencl_helper_0_0_1
  const char * freia_04_opencl_helper_0_0_1_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_04_opencl_helper_0_0_1\n"
    "KERNEL void freia_04_opencl_helper_0_0_1(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  int c1,\n"
    "  int c2,\n"
    "  int c3,\n"
    "  int c4,\n"
    "  int c5,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in1 = j1[i];\n"
    "    PIXEL in0 = j0[i];\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t28 = PIXEL_SUB(in1, in0);\n"
    "    PIXEL t30 = PIXEL_THRESHOLD(in0, c0, c1, c2);\n"
    "    PIXEL t29 = PIXEL_THRESHOLD(t28, c3, c4, c5);\n"
    "    PIXEL_MINIMUM(red32, t30);\n"
    "    PIXEL t33 = PIXEL_ADD(t29, t30);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t29;\n"
    "    p1[i] = t33;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].min = minv;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_04_opencl_helper_0_0_1_source, "freia_04_opencl_helper_0_0_1", "-DPIXEL8", &freia_04_opencl_helper_0_0_1_kernel[0]);
  err |= freia_op_compile_kernel(freia_04_opencl_helper_0_0_1_source, "freia_04_opencl_helper_0_0_1", "-DPIXEL16", &freia_04_opencl_helper_0_0_1_kernel[1]);
  return err;
}

// helper function freia_04_opencl_helper_0_0_1
freia_status freia_04_opencl_helper_0_0_1(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  int32_t c0,
  int32_t c1,
  int32_t c2,
  int32_t c3,
  int32_t c4,
  int32_t c5,
  int32_t * po4)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_04_opencl_helper_0_0_1_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_04_opencl_helper_0_0_1_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_04_opencl_helper_0_0_1
  err |= freia_op_call_kernel(kernel, 2, 2, 6, 1, o0, o1, i0, i1, c0, c1, c2, c3, c4, c5, &redres);

  // return reduction results
  *po4 = redres.minimum;

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_04

// opencl for dag 0

// opencl function freia_04_opencl_helper_0_0_1
KERNEL void freia_04_opencl_helper_0_0_1(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  GLOBAL PIXEL * i1, // const?
  int ofs_i1,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  int c1,
  int c2,
  int c3,
  int c4,
  int c5,
  GLOBAL TMeasure * redX)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;
  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // get input pixels
    PIXEL in1 = j1[i];
    PIXEL in0 = j0[i];

    // pixel computations
    PIXEL t28 = PIXEL_SUB(in1, in0);
    PIXEL t30 = PIXEL_THRESHOLD(in0, c0, c1, c2);
    PIXEL t29 = PIXEL_THRESHOLD(t28, c3, c4, c5);
    PIXEL_MINIMUM(red32, t30);
    PIXEL t33 = PIXEL_ADD(t29, t30);

    // set output pixels
    p0[i] = t29;
    p1[i] = t33;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].min = minv;
}

! updated function

freia_status freia_04(freia_data2d *o, freia_data2d *i, int32_t *k, int32_t inf, int32_t sup, bool bin, int32_t *m, int32_t *v)
{
   
   
   
   
   
   freia_data2d *t0 = freia_common_create_data(16, 128, 128), *t1 = freia_common_create_data(16, 128, 128), *t3 = freia_common_create_data(16, 128, 128);         /*0015*/
   
   // to test operator compaction
   // t0 = erode(i)
   // t1 = dilate(i)
   // t2 = t1 - t0
   // t3 = threshold(t2)
   // t4 = threshold(t0)
   // v  = vol(t3)
   // m  = min(t4)
   // o  = t4 + t3
   freia_aipo_erode_8c(t0, i, k);                                       /*0026*/
   freia_aipo_dilate_6c(t1, i, k);                                      /*0027*/
   freia_04_opencl_helper_0_0_1(t3, o, t0, t1, inf, sup, bin, inf, sup, bin, m);       /*0028*/
   freia_aipo_global_vol(t3, v);                                        /*0031*/

   freia_common_destruct_data(t0);                                      /*0035*/
   freia_common_destruct_data(t1);                                      /*0036*/
   freia_common_destruct_data(t3);                                      /*0038*/

   return FREIA_OK;                                                     /*0041*/
}

! cleaned up function

freia_status freia_04(freia_data2d *o, freia_data2d *i, int32_t *k, int32_t inf, int32_t sup, bool bin, int32_t *m, int32_t *v)
{
   
   
   
   
   
   freia_data2d *t0 = freia_common_create_data(16, 128, 128), *t1 = freia_common_create_data(16, 128, 128), *t3 = freia_common_create_data(16, 128, 128);         /*0015*/
   
   // to test operator compaction
   // t0 = erode(i)
   // t1 = dilate(i)
   // t2 = t1 - t0
   // t3 = threshold(t2)
   // t4 = threshold(t0)
   // v  = vol(t3)
   // m  = min(t4)
   // o  = t4 + t3
   freia_aipo_erode_8c(t0, i, k);                                       /*0026*/
   freia_aipo_dilate_6c(t1, i, k);                                      /*0027*/
   freia_04_opencl_helper_0_0_1(t3, o, t0, t1, inf, sup, bin, inf, sup, bin, m);       /*0028*/
   freia_aipo_global_vol(t3, v);                                        /*0031*/

   freia_common_destruct_data(t0);                                      /*0035*/
   freia_common_destruct_data(t1);                                      /*0036*/
   freia_common_destruct_data(t3);                                      /*0038*/

   return 0;                                                            /*0041*/
}
