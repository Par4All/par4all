   freia_common_tx_image(od, &fdout);                                   /*0081*/
   freia_common_tx_image(og, &fdout);                                   /*0082*/

   freia_common_destruct_data(in);                                      /*0084*/
   freia_common_destruct_data(od);                                      /*0085*/
