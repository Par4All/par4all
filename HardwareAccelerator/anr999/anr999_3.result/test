! acceleration function

#include <freiaCommon.h>
#include <freiaMediumGrain.h>
#include <freiaCoarseGrain.h>
#include <spoc.h>

// dag 0: 33 ops and 0 copies, optimized to 23 ops and 0+0+0 copies
// split 0: 4 cuts
// # SPOC calls: 4

// FREIA-SPoC helper function for module anr999
freia_status anr999_spoc_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  int32_t * po0,
  int32_t * po1,
  const int32_t * pi2)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  spoc_reduction reduc;
  freia_reduction_results redres;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`in [0 input 0] -> [0 measure 0] 0 undefined
  si.mux[0][0].op = SPOC_MUX_IN0;

  // 0`in [0 measure 0] -> [0 measure 0] 0 undefined
  // nope


  // STAGE 1
  // 0`in [0 measure 0] -> [1 poc 0] 41 dilate_8c
  // 0`in [1 input 0] -> [1 poc 0] 41 dilate_8c
  // nope
  // poc 41 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi2[i];


  // STAGE 2
  // 0`od_9 [1 poc 0] -> [2 poc 0] 42 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`od_9 [2 input 0] -> [2 poc 0] 42 dilate_8c
  // nope
  // poc 42 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi2[i];


  // STAGE 0
  // 0`in [0 input 1] -> [0 poc 1] 73 erode_8c
  // nope
  // poc 73 erode_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_ERODE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi2[i];


  // STAGE 1
  // 0`og_10 [0 poc 1] -> [1 poc 1] 74 erode_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`og_10 [1 input 1] -> [1 poc 1] 74 erode_8c
  // nope
  // poc 74 erode_8c stage 1 side 1
  si.poc[1][1].op = SPOC_POC_ERODE;
  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi2[i];


  // STAGE 2
  // 0`og_9 [1 poc 1] -> [2 poc 1] 75 erode_8c
  si.mux[1][3].op = SPOC_MUX_IN1;
  // 0`og_9 [2 input 1] -> [2 poc 1] 75 erode_8c
  // nope
  // poc 75 erode_8c stage 2 side 1
  si.poc[2][1].op = SPOC_POC_ERODE;
  si.poc[2][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][1].kernel[i] = pi2[i];


  // output image 0`od_8 on 0 and image 0`og_8 on 1
  // 0`od_8 [2 poc 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`od_8 [2 output 0] -> [2 output 0] 0 null
  // nope

  // 0`og_8 [2 poc 1] -> [2 output 1] 0 null
  si.mux[2][3].op = SPOC_MUX_IN1;

  // fill in to the end...
  // 0`og_8 [2 output 1] -> [2 output 1] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  redres.raw = (freia_ptr) &reduc;
  redres.size = sizeof(spoc_reduction);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i0);

  // get reductions
  freia_cg_read_reduction_results(&redres);

  // measure 38 global_min stage 0 side 0
  *po0 = (int32_t) reduc.measure[0][0].minimum;

  // measure 39 global_vol stage 0 side 0
  *po1 = (int32_t) reduc.measure[0][0].volume;

  return ret;
}

// FREIA-SPoC helper function for module anr999
freia_status anr999_spoc_helper_0_0_1(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`od_8 [0 input 0] -> [0 poc 0] 43 dilate_8c
  // nope
  // poc 43 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`od_7 [0 poc 0] -> [1 poc 0] 44 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`od_7 [1 input 0] -> [1 poc 0] 44 dilate_8c
  // nope
  // poc 44 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // STAGE 2
  // 0`od_6 [1 poc 0] -> [2 poc 0] 45 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`od_6 [2 input 0] -> [2 poc 0] 45 dilate_8c
  // nope
  // poc 45 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];


  // STAGE 0
  // 0`og_8 [0 input 1] -> [0 poc 1] 76 erode_8c
  // nope
  // poc 76 erode_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_ERODE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];


  // STAGE 1
  // 0`og_7 [0 poc 1] -> [1 poc 1] 77 erode_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`og_7 [1 input 1] -> [1 poc 1] 77 erode_8c
  // nope
  // poc 77 erode_8c stage 1 side 1
  si.poc[1][1].op = SPOC_POC_ERODE;
  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi0[i];


  // STAGE 2
  // 0`og_6 [1 poc 1] -> [2 poc 1] 78 erode_8c
  si.mux[1][3].op = SPOC_MUX_IN1;
  // 0`og_6 [2 input 1] -> [2 poc 1] 78 erode_8c
  // nope
  // poc 78 erode_8c stage 2 side 1
  si.poc[2][1].op = SPOC_POC_ERODE;
  si.poc[2][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][1].kernel[i] = pi0[i];


  // output image 0`od_5 on 0 and image 0`og_5 on 1
  // 0`od_5 [2 poc 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`od_5 [2 output 0] -> [2 output 0] 0 null
  // nope

  // 0`og_5 [2 poc 1] -> [2 output 1] 0 null
  si.mux[2][3].op = SPOC_MUX_IN1;

  // fill in to the end...
  // 0`og_5 [2 output 1] -> [2 output 1] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module anr999
freia_status anr999_spoc_helper_0_0_2(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`od_5 [0 input 0] -> [0 poc 0] 46 dilate_8c
  // nope
  // poc 46 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`od_4 [0 poc 0] -> [1 poc 0] 47 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`od_4 [1 input 0] -> [1 poc 0] 47 dilate_8c
  // nope
  // poc 47 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // STAGE 2
  // 0`od_3 [1 poc 0] -> [2 poc 0] 48 dilate_8c
  si.mux[1][0].op = SPOC_MUX_IN0;
  // 0`od_3 [2 input 0] -> [2 poc 0] 48 dilate_8c
  // nope
  // poc 48 dilate_8c stage 2 side 0
  si.poc[2][0].op = SPOC_POC_DILATE;
  si.poc[2][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][0].kernel[i] = pi0[i];


  // STAGE 0
  // 0`og_5 [0 input 1] -> [0 poc 1] 79 erode_8c
  // nope
  // poc 79 erode_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_ERODE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];


  // STAGE 1
  // 0`og_4 [0 poc 1] -> [1 poc 1] 80 erode_8c
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`og_4 [1 input 1] -> [1 poc 1] 80 erode_8c
  // nope
  // poc 80 erode_8c stage 1 side 1
  si.poc[1][1].op = SPOC_POC_ERODE;
  si.poc[1][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][1].kernel[i] = pi0[i];


  // STAGE 2
  // 0`og_3 [1 poc 1] -> [2 poc 1] 81 erode_8c
  si.mux[1][3].op = SPOC_MUX_IN1;
  // 0`og_3 [2 input 1] -> [2 poc 1] 81 erode_8c
  // nope
  // poc 81 erode_8c stage 2 side 1
  si.poc[2][1].op = SPOC_POC_ERODE;
  si.poc[2][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[2][1].kernel[i] = pi0[i];


  // output image 0`od_2 on 0 and image 0`og_2 on 1
  // 0`od_2 [2 poc 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`od_2 [2 output 0] -> [2 output 0] 0 null
  // nope

  // 0`og_2 [2 poc 1] -> [2 output 1] 0 null
  si.mux[2][3].op = SPOC_MUX_IN1;

  // fill in to the end...
  // 0`og_2 [2 output 1] -> [2 output 1] 0 null
  // nope

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

// FREIA-SPoC helper function for module anr999
freia_status anr999_spoc_helper_0_0_3(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1,
  const int32_t * pi0)
{
  spoc_instr si;
  spoc_param sp;
  freia_microcode mcode;
  freia_dynamic_param dynparam;
  freia_op_param param;
  freia_status ret;
  int i;

  // init pipe to nop
  spoc_init_pipe(&si, &sp, 16);


  // STAGE 0
  // 0`od_2 [0 input 0] -> [0 poc 0] 49 dilate_8c
  // nope
  // poc 49 dilate_8c stage 0 side 0
  si.poc[0][0].op = SPOC_POC_DILATE;
  si.poc[0][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][0].kernel[i] = pi0[i];


  // STAGE 1
  // 0`od_1 [0 poc 0] -> [1 poc 0] 50 dilate_8c
  si.mux[0][0].op = SPOC_MUX_IN0;
  // 0`od_1 [1 input 0] -> [1 poc 0] 50 dilate_8c
  // nope
  // poc 50 dilate_8c stage 1 side 0
  si.poc[1][0].op = SPOC_POC_DILATE;
  si.poc[1][0].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[1][0].kernel[i] = pi0[i];


  // STAGE 0
  // 0`og_2 [0 input 1] -> [0 poc 1] 82 erode_8c
  // nope
  // poc 82 erode_8c stage 0 side 1
  si.poc[0][1].op = SPOC_POC_ERODE;
  si.poc[0][1].grid = SPOC_POC_8_CONNEX;
  for(i=0 ; i<9 ; i++)
    sp.poc[0][1].kernel[i] = pi0[i];


  // STAGE 1
  // 0`od [1 poc 0] -> [1 alu] 87 sub
  // nope
  // 0`og_1 [0 poc 1] -> [1 alu] 87 sub
  si.mux[0][3].op = SPOC_MUX_IN1;
  // 0`og_1 [1 input 1] -> [1 alu] 87 sub
  // nope
  // alu 87 sub stage 1
  si.alu[1][0].op = SPOC_ALU_SUB_IN0_IN1;


  // output image 0`od on 0 and image 0`og on 1
  // 0`od [1 poc 0] -> [1 output 0] 0 null
  si.mux[1][0].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`od [1 output 0] -> [2 output 0] 0 null
  // 0`od [2 input 0] -> [2 output 0] 0 null
  si.mux[2][0].op = SPOC_MUX_IN0;

  // 0`og [1 alu] -> [1 output 1] 0 null
  si.mux[1][1].op = SPOC_MUX_IN1;
  si.mux[1][3].op = SPOC_MUX_IN0;

  // fill in to the end...
  // 0`og [1 output 1] -> [2 output 1] 0 null
  // 0`og [2 input 1] -> [2 output 1] 0 null
  si.mux[2][3].op = SPOC_MUX_IN1;

  mcode.raw = (freia_ptr) &si;
  mcode.size = sizeof(spoc_instr);

  dynparam.raw = (freia_ptr) &sp;
  dynparam.size = sizeof(spoc_param);

  ret = freia_cg_write_microcode(&mcode);
  ret |= freia_cg_write_dynamic_param(&dynparam);

  // actual call of spoc hardware
  freia_cg_template_process_2i_2o(&param, o0, o1, i0, i1);

  return ret;
}

