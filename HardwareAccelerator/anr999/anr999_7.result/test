// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 33 ops and 0 copies, optimized to 23 ops and 0+0+0 copies

// hold kernels for anr999_opencl_helper_0_0_0
static cl_kernel anr999_opencl_helper_0_0_0_kernel[2];

// compile kernels for anr999_opencl_helper_0_0_0
static freia_status anr999_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for anr999_opencl_helper_0_0_0
  const char * anr999_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function anr999_opencl_helper_0_0_0\n"
    "KERNEL void anr999_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  int2 mmin = { PIXEL_MAX, 0 };\n"
    "  int2 mmax = { PIXEL_MIN, 0 };\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    // pixel computations\n"
    "    PIXEL_MINIMUM(red38, in0);\n"
    "    PIXEL_VOLUME(red39, in0);\n"
    "    PIXEL t41 = in0;\n"
    "    t41 = PIXEL_INF(t41, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);\n"
    "    t41 = PIXEL_INF(t41, (is_N)? PIXEL_MAX: j0[i-pitch]);\n"
    "    t41 = PIXEL_INF(t41, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);\n"
    "    t41 = PIXEL_INF(t41, (is_W)? PIXEL_MAX: j0[i-1]);\n"
    "    t41 = PIXEL_INF(t41, (is_E)? PIXEL_MAX: j0[i+1]);\n"
    "    t41 = PIXEL_INF(t41, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);\n"
    "    t41 = PIXEL_INF(t41, (is_S)? PIXEL_MAX: j0[i+pitch]);\n"
    "    t41 = PIXEL_INF(t41, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);\n"
    "    PIXEL t73 = in0;\n"
    "    t73 = PIXEL_SUP(t73, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);\n"
    "    t73 = PIXEL_SUP(t73, (is_N)? PIXEL_MIN: j0[i-pitch]);\n"
    "    t73 = PIXEL_SUP(t73, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);\n"
    "    t73 = PIXEL_SUP(t73, (is_W)? PIXEL_MIN: j0[i-1]);\n"
    "    t73 = PIXEL_SUP(t73, (is_E)? PIXEL_MIN: j0[i+1]);\n"
    "    t73 = PIXEL_SUP(t73, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);\n"
    "    t73 = PIXEL_SUP(t73, (is_S)? PIXEL_MIN: j0[i+pitch]);\n"
    "    t73 = PIXEL_SUP(t73, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);\n"
    "    // set output pixels\n"
    "    p0[i] = t41;\n"
    "    p1[i] = t73;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  redX[threadid].min = mmin.x;\n"
    "  redX[threadid].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(anr999_opencl_helper_0_0_0_source, "anr999_opencl_helper_0_0_0", "-DPIXEL8", &anr999_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(anr999_opencl_helper_0_0_0_source, "anr999_opencl_helper_0_0_0", "-DPIXEL16", &anr999_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function anr999_opencl_helper_0_0_0
freia_status anr999_opencl_helper_0_0_0(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  int32_t * po3,
  int32_t * po4)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= anr999_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = anr999_opencl_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel anr999_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 2, 1, 0, 1, o0, o1, i0, &redres);

  // return reduction results
  *po3 = redres.minimum;
  *po4 = redres.volume;

  return err;
}

