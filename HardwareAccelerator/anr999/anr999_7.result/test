// opencl function anr999_opencl_helper_0_0_0
KERNEL void anr999_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * o1,
  int ofs_o1,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image below
  int pitch,
  GLOBAL TMeasure * redX)
{
  // get id & compute global image shift
  int threadid = get_global_id(0);
  int shift = pitch*threadid;

  // get input & output image pointers
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  // reduction stuff is currently hardcoded...
  int vol = 0;
  int2 mmin = { PIXEL_MAX, 0 };
  int2 mmax = { PIXEL_MIN, 0 };

  // detect N & S boundaries (assuming one thread per row)
  int is_N = (threadid==0);
  int is_S = (threadid==(get_global_size(0)-1));

  // thread's pixel loop
  int i;
  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    // pixel computations
    PIXEL_MINIMUM(red38, in0);
    PIXEL_VOLUME(red39, in0);
    PIXEL t41 = in0;
    t41 = PIXEL_INF(t41, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);
    t41 = PIXEL_INF(t41, (is_N)? PIXEL_MAX: j0[i-pitch]);
    t41 = PIXEL_INF(t41, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);
    t41 = PIXEL_INF(t41, (is_W)? PIXEL_MAX: j0[i-1]);
    t41 = PIXEL_INF(t41, (is_E)? PIXEL_MAX: j0[i+1]);
    t41 = PIXEL_INF(t41, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);
    t41 = PIXEL_INF(t41, (is_S)? PIXEL_MAX: j0[i+pitch]);
    t41 = PIXEL_INF(t41, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);
    PIXEL t73 = in0;
    t73 = PIXEL_SUP(t73, (is_N|is_W)? PIXEL_MIN: j0[i-pitch-1]);
    t73 = PIXEL_SUP(t73, (is_N)? PIXEL_MIN: j0[i-pitch]);
    t73 = PIXEL_SUP(t73, (is_N|is_E)? PIXEL_MIN: j0[i-pitch+1]);
    t73 = PIXEL_SUP(t73, (is_W)? PIXEL_MIN: j0[i-1]);
    t73 = PIXEL_SUP(t73, (is_E)? PIXEL_MIN: j0[i+1]);
    t73 = PIXEL_SUP(t73, (is_W|is_S)? PIXEL_MIN: j0[i+pitch-1]);
    t73 = PIXEL_SUP(t73, (is_S)? PIXEL_MIN: j0[i+pitch]);
    t73 = PIXEL_SUP(t73, (is_E|is_S)? PIXEL_MIN: j0[i+pitch+1]);
    // set output pixels
    p0[i] = t41;
    p1[i] = t73;
  }

  // reduction copy out
  redX[threadid].min = mmin.x;
  redX[threadid].vol = vol;
}

