    t50 = PIXEL_INF(t50, (is_E)? PIXEL_MAX: j0[i+1]);
    t50 = PIXEL_INF(t50, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);
    t50 = PIXEL_INF(t50, (is_S)? PIXEL_MAX: j0[i+pitch]);
    t50 = PIXEL_INF(t50, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);
