// hold kernels for anr999_opencl_helper_0_0_2
static cl_kernel anr999_opencl_helper_0_0_2_kernel[2];

// compile kernels for anr999_opencl_helper_0_0_2
static freia_status anr999_opencl_helper_0_0_2_compile(void)
{
  // OpenCL source for anr999_opencl_helper_0_0_2
  const char * anr999_opencl_helper_0_0_2_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function anr999_opencl_helper_0_0_2\n"
    "KERNEL void anr999_opencl_helper_0_0_2(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * o1,\n"
    "  int ofs_o1,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  GLOBAL PIXEL * i1, // const?\n"
    "  int ofs_i1,\n"
    "  int width, // of the working area, vs image below\n"
    "  int pitch)\n"
    "{\n"
    "  // get id & compute global image shift\n"
    "  int threadid = get_global_id(0);\n"
    "  int shift = pitch*threadid;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * p1 = o1 + ofs_o1 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "  GLOBAL PIXEL * j1 = i1 + ofs_i1 + shift;\n"
    "\n"
    "  // detect N & S boundaries (assuming one thread per row)\n"
    "  int is_N = (threadid==0);\n"
    "  int is_S = (threadid==(get_global_size(0)-1));\n"
    "\n"
    "  // thread's pixel loop\n"
    "  int i;\n"
    "  for (i=0; i<width; i++)\n"
    "  {\n"
    "    // detect W & E boundaries (assuming one row per thread)\n"
    "    int is_W = (i==0);\n"
    "    int is_E = (i==(width-1));\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "    PIXEL in1 = j1[i];\n"
    "    // pixel computations\n"
    "    PIXEL t50 = in0;\n"
    "    t50 = PIXEL_INF(t50, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);\n"
    "    t50 = PIXEL_INF(t50, (is_N)? PIXEL_MAX: j0[i-pitch]);\n"
    "    t50 = PIXEL_INF(t50, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);\n"
    "    t50 = PIXEL_INF(t50, (is_W)? PIXEL_MAX: j0[i-1]);\n"
    "    t50 = PIXEL_INF(t50, (is_E)? PIXEL_MAX: j0[i+1]);\n"
    "    t50 = PIXEL_INF(t50, (is_W|is_S)? PIXEL_MAX: j0[i+pitch-1]);\n"
    "    t50 = PIXEL_INF(t50, (is_S)? PIXEL_MAX: j0[i+pitch]);\n"
    "    t50 = PIXEL_INF(t50, (is_E|is_S)? PIXEL_MAX: j0[i+pitch+1]);\n"
    "    PIXEL t82 = in1;\n"
    "    t82 = PIXEL_SUP(t82, (is_N|is_W)? PIXEL_MIN: j1[i-pitch-1]);\n"
    "    t82 = PIXEL_SUP(t82, (is_N)? PIXEL_MIN: j1[i-pitch]);\n"
    "    t82 = PIXEL_SUP(t82, (is_N|is_E)? PIXEL_MIN: j1[i-pitch+1]);\n"
    "    t82 = PIXEL_SUP(t82, (is_W)? PIXEL_MIN: j1[i-1]);\n"
    "    t82 = PIXEL_SUP(t82, (is_E)? PIXEL_MIN: j1[i+1]);\n"
    "    t82 = PIXEL_SUP(t82, (is_W|is_S)? PIXEL_MIN: j1[i+pitch-1]);\n"
    "    t82 = PIXEL_SUP(t82, (is_S)? PIXEL_MIN: j1[i+pitch]);\n"
    "    t82 = PIXEL_SUP(t82, (is_E|is_S)? PIXEL_MIN: j1[i+pitch+1]);\n"
    "    PIXEL t87 = PIXEL_SUB(t50, t82);\n"
    "    // set output pixels\n"
    "    p0[i] = t50;\n"
    "    p1[i] = t87;\n"
    "  }\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(anr999_opencl_helper_0_0_2_source, "anr999_opencl_helper_0_0_2", "-DPIXEL8", &anr999_opencl_helper_0_0_2_kernel[0]);
  err |= freia_op_compile_kernel(anr999_opencl_helper_0_0_2_source, "anr999_opencl_helper_0_0_2", "-DPIXEL16", &anr999_opencl_helper_0_0_2_kernel[1]);
  return err;
}

// helper function anr999_opencl_helper_0_0_2
freia_status anr999_opencl_helper_0_0_2(
  freia_data2d * o0,
  freia_data2d * o1,
  const freia_data2d * i0,
  const freia_data2d * i1)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= anr999_opencl_helper_0_0_2_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = anr999_opencl_helper_0_0_2_kernel[bpp];

  // call kernel anr999_opencl_helper_0_0_2
  err |= freia_op_call_kernel(kernel, 2, 2, 0, 0, o0, o1, i0, i1);

  return err;
}
