  for (i=0; i<width; i++)
  {
    // detect W & E boundaries (assuming one row per thread)
    int is_W = (i==0);
    int is_E = (i==(width-1));
    // get input pixels
    PIXEL in0 = j0[i];
    PIXEL in1 = j1[i];
    // pixel computations
    PIXEL t50 = in0;
    t50 = PIXEL_INF(t50, (is_N|is_W)? PIXEL_MAX: j0[i-pitch-1]);
    t50 = PIXEL_INF(t50, (is_N)? PIXEL_MAX: j0[i-pitch]);
    t50 = PIXEL_INF(t50, (is_N|is_E)? PIXEL_MAX: j0[i-pitch+1]);
