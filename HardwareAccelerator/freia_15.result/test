! acceleration function

// FREIA OpenCL includes
#include <CL/opencl.h>
#include "freia.h"
#include "private/freia_opencl_runtime.h"


// dag 0: 4 ops and 0 copies, optimized to 4 ops and 0+0+0 copies

// hold kernels for freia_15_opencl_helper_0_0_0
static cl_kernel freia_15_opencl_helper_0_0_0_kernel[2];

// compile kernels for freia_15_opencl_helper_0_0_0
static freia_status freia_15_opencl_helper_0_0_0_compile(void)
{
  // OpenCL source for freia_15_opencl_helper_0_0_0
  const char * freia_15_opencl_helper_0_0_0_source =
    "#include <freia_opencl_runtime.hcl>\n"
    "\n"
    "// opencl function freia_15_opencl_helper_0_0_0\n"
    "KERNEL void freia_15_opencl_helper_0_0_0(\n"
    "  GLOBAL PIXEL * o0,\n"
    "  int ofs_o0,\n"
    "  GLOBAL PIXEL * i0, // const?\n"
    "  int ofs_i0,\n"
    "  int width, // of the working area, vs image pitch below\n"
    "  int height, // of the working area\n"
    "  int pitch,\n"
    "  int c0,\n"
    "  GLOBAL TMeasure * redX)\n"
    "{\n"
    "  // no tiling on height dimension\n"
    "  // assert(height==get_global_size(0));\n"
    "  int j = get_global_id(0);\n"
    "\n"
    "  // reduction stuff is currently hardcoded...\n"
    "  int vol = 0;\n"
    "  PIXEL minv = PIXEL_MAX;\n"
    "  int2 minpos = { 0, 0 };\n"
    "  PIXEL maxv = PIXEL_MIN;\n"
    "  int2 maxpos = { 0, 0 };\n"
    "\n"
    "  // loop i upper bound\n"
    "  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);\n"
    "  int Wlast = Wtile*(get_global_id(1)+1);\n"
    "  if (Wlast>width) Wlast = width;\n"
    "\n"
    "  // get input & output image pointers\n"
    "  int shift = pitch*j;\n"
    "  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;\n"
    "  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;\n"
    "\n"
    "  int i;\n"
    "  for (i=Wtile*get_global_id(1); i<Wlast; i++)\n"
    "  {\n"
    "    // get input pixels\n"
    "    PIXEL in0 = j0[i];\n"
    "\n"
    "    // pixel computations\n"
    "    PIXEL t8 = PIXEL_ADD(in0, c0);\n"
    "    PIXEL_MINIMUM(red9, in0);\n"
    "    PIXEL_MAXIMUM(red10, in0);\n"
    "    PIXEL_VOLUME(red11, in0);\n"
    "\n"
    "    // set output pixels\n"
    "    p0[i] = t8;\n"
    "  }\n"
    "\n"
    "  // reduction copy out\n"
    "  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);\n"
    "  redX[thrid].min = minv;\n"
    "  redX[thrid].max = maxv;\n"
    "  redX[thrid].vol = vol;\n"
    "}\n";
  freia_status err = FREIA_OK;
  err |= freia_op_compile_kernel(freia_15_opencl_helper_0_0_0_source, "freia_15_opencl_helper_0_0_0", "-DPIXEL8", &freia_15_opencl_helper_0_0_0_kernel[0]);
  err |= freia_op_compile_kernel(freia_15_opencl_helper_0_0_0_source, "freia_15_opencl_helper_0_0_0", "-DPIXEL16", &freia_15_opencl_helper_0_0_0_kernel[1]);
  return err;
}

// helper function freia_15_opencl_helper_0_0_0
freia_status freia_15_opencl_helper_0_0_0(
  freia_data2d * o0,
  const freia_data2d * i0,
  int32_t c0,
  int32_t * po2,
  int32_t * po3,
  int32_t * po4)
{
  freia_status err = FREIA_OK;

  // handle on the fly compilation...
  static int to_compile = 1;
  if (to_compile) {
    err |= freia_15_opencl_helper_0_0_0_compile();
    // compilation may have failed
    if (err) return err;
    to_compile = 0;
  }

  // now get kernel, which must be have be compiled
  uint32_t bpp = i0->bpp>>4;
  cl_kernel kernel = freia_15_opencl_helper_0_0_0_kernel[bpp];

  // currently only one reduction structure...
  freia_opencl_measure_status redres;

  // call kernel freia_15_opencl_helper_0_0_0
  err |= freia_op_call_kernel(kernel, 1, 1, 1, 1, o0, i0, c0, &redres);

  // return reduction results
  *po2 = redres.minimum;
  *po3 = redres.maximum;
  *po4 = redres.volume;

  return err;
}
#include <freia_opencl_runtime.hcl>
// generated OpenCL kernels for function freia_15

// opencl for dag 0

// opencl function freia_15_opencl_helper_0_0_0
KERNEL void freia_15_opencl_helper_0_0_0(
  GLOBAL PIXEL * o0,
  int ofs_o0,
  GLOBAL PIXEL * i0, // const?
  int ofs_i0,
  int width, // of the working area, vs image pitch below
  int height, // of the working area
  int pitch,
  int c0,
  GLOBAL TMeasure * redX)
{
  // no tiling on height dimension
  // assert(height==get_global_size(0));
  int j = get_global_id(0);

  // reduction stuff is currently hardcoded...
  int vol = 0;
  PIXEL minv = PIXEL_MAX;
  int2 minpos = { 0, 0 };
  PIXEL maxv = PIXEL_MIN;
  int2 maxpos = { 0, 0 };

  // loop i upper bound
  int Wtile = (width+get_global_size(1)-1)/get_global_size(1);
  int Wlast = Wtile*(get_global_id(1)+1);
  if (Wlast>width) Wlast = width;

  // get input & output image pointers
  int shift = pitch*j;
  GLOBAL PIXEL * p0 = o0 + ofs_o0 + shift;
  GLOBAL PIXEL * j0 = i0 + ofs_i0 + shift;

  int i;
  for (i=Wtile*get_global_id(1); i<Wlast; i++)
  {
    // get input pixels
    PIXEL in0 = j0[i];

    // pixel computations
    PIXEL t8 = PIXEL_ADD(in0, c0);
    PIXEL_MINIMUM(red9, in0);
    PIXEL_MAXIMUM(red10, in0);
    PIXEL_VOLUME(red11, in0);

    // set output pixels
    p0[i] = t8;
  }

  // reduction copy out
  int thrid = get_global_id(0)*get_global_size(1)+get_global_id(1);
  redX[thrid].min = minv;
  redX[thrid].max = maxv;
  redX[thrid].vol = vol;
}

