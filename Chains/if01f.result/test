"initial file:"
C     Fortran version
C     use-def chains with if/else
C     no dependence between if and else case have to be done
      PROGRAM IF01F
      
      INTEGER R, A

      R = -1                                                            0008

      IF (.TRUE.) THEN                                                  0010
         R = 1                                                          0011
      ELSE
         R = 0                                                          0013
      ENDIF

      A = R                                                             0016
      END

"use def graph..."

