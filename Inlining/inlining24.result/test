//  P(_return0,_return0_0,_return0_1,_return1,connexity,i_0,i_1,ret_0,
//    ret_1,ret_2,ret_3,size) {_return0_0==ret_0, _return0_1==ret_3,
//    _return1==ret_2, connexity==8, size==4}

   (void) _return1;
