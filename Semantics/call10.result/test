
Source code with proper effects

double call10(int i)
{
//               <must be written>: x
   double x = 3.;                                                       /*0009*/
//               <must be read   >: i
//               <must be written>: i
   i += 10;                                                             /*0010*/
//               <must be read   >: j
//               <must be written>: j
   j++;                                                                 /*0011*/
//               <must be read   >: x

   return x;                                                            /*0013*/
}

Source code with cumulated effects

//               <must be read   >: j i
//               <must be written>: j
double call10(int i)
{
//               <must be written>: x
   double x = 3.;                                                       /*0009*/
//               <must be read   >: i
//               <must be written>: i
   i += 10;                                                             /*0010*/
//               <must be read   >: j
//               <must be written>: j
   j++;                                                                 /*0011*/
//               <must be read   >: x

   return x;                                                            /*0013*/
}

Source code with transformers for "call10"


//  T(j) {j==j#init+1}

double call10(int i)
{

//  T() {}

   double x = 3.;                                                       /*0009*/

//  T(i) {i==i#init+10}

   i += 10;                                                             /*0010*/

//  T(j) {j==j#init+1}

   j++;                                                                 /*0011*/

//  T() {}


   return x;                                                            /*0013*/
}

Source code with transformers for "main"


//  T(j) {j==j#init+1}

int main()
{

//  T(ai) {ai==3}

   int ai = 3;                                                          /*0018*/

//  T(ai,j) {ai==ai#init+1, j==j#init+1}


   call10(++ai);                                                        /*0020*/

//  T(ai) {ai==0}


   ai = 0;                                                              /*0022*/
}

Source code with preconditions for "call10"


//  P() {i==4, j==0}

double call10(int i)
{

//  P() {i==4, j==0}

   double x = 3.;                                                       /*0009*/

//  P() {i==4, j==0}

   i += 10;                                                             /*0010*/

//  P(i) {i==14, j==0}

   j++;                                                                 /*0011*/

//  P(i,j) {i==14, j==1, j#init==0}


   return x;                                                            /*0013*/
}

Source code with preconditions for "main"


//  P() {j==0}

int main()
{

//  P() {j==0}

   int ai = 3;                                                          /*0018*/

//  P() {ai==3, j==0}


   call10(++ai);                                                        /*0020*/

//  P(ai,j) {ai==4, j==1, j#init==0}


   ai = 0;                                                              /*0022*/
}

