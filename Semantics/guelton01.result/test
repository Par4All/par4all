
Transformers for "FIR"


C  T(FIR) {FIR==0}

int FIR()
{


//  T() {}

   int ifr;

//  T(ifr) {0<=ifr}

   for(ifr = 0; ifr <= 0; ifr += 1)

//  T() {ifr==0}

      if (ifr==0)
         ;

//  T(FIR) {FIR==0, ifr==1}

   return 0;
}

Preconditions for "FIR"


//  P() {}

int FIR()
{


//  P() {}

   int ifr;

//  P() {}

   for(ifr = 0; ifr <= 0; ifr += 1)

//  P(ifr) {ifr==0}

      if (ifr==0)
         ;

//  P(ifr) {ifr==1}

   return 0;
}

Preconditions for "FIR" after partial evaluation


//  P() {}

int FIR()
{


//  P() {}

   int ifr;

//  P() {}

   for(ifr = 0; ifr <= 0; ifr += 1)

//  P(ifr) {ifr==0}

      if (0==0)
         ;

//  P(ifr) {ifr==1}

   return 0;
}

Preconditions for "FIR" after dead code elimination

int FIR()
{

   int ifr;
   ifr = 0;
   return 0;
}
