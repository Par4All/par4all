void update02()
{
   int i = 1;
   int j;
//               <must be read   >: i
//               <must be written>: i j
   
   //i = (i+= 2);
   j = i += 2;
//               <must be read   >: i
//               <must be written>: i j
   j = i++;
//               <must be read   >: i
//               <must be written>: i j
   j = i--;
//               <must be read   >: i
//               <must be written>: i j
   j = ++i;
//               <must be read   >: i
//               <must be written>: i j
   j = --i;
//               <must be read   >: j
   foo(j);
}

