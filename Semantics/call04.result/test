Source code with transformers for "call04"


//  T(call04,j) {call04==i+10, j==j#init+1}

int call04(int i)
{

//  T(i) {i==i#init+10}

   i += 10;                                                             /*0007*/

//  T(j) {j==j#init+1}

   j++;                                                                 /*0008*/

//  T(call04) {call04==i}


   return i;                                                            /*0010*/
}

Source code with transformers for "main"


//  T(j) {j==j#init+1}

int main()
{

//  T(ai) {ai==3}

   int ai = 3;                                                          /*0015*/

//  T(ai,j) {ai==ai#init+10, j==j#init+1}


   ai = call04(ai++);                                                   /*0017*/

//  T(ai) {ai==0}


   ai = 0;                                                              /*0019*/
}

