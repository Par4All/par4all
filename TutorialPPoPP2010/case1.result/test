//  <b[PHI1][PHI2]-R-EXACT-{1<=PHI1, PHI1<=n, 1<=PHI2, PHI2<=m,
//    TOP-LEVEL:2.0==h, m==10, n==10}>

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;

//  <a[PHI1][PHI2]-W-EXACT-{1<=PHI1, PHI1<=n, 1<=PHI2, PHI2<=m,
//    TOP-LEVEL:2.0==h, m==10, n==10}>
//  <b[PHI1][PHI2]-R-EXACT-{1<=PHI1, PHI1<=n, 1<=PHI2, PHI2<=m,
//    TOP-LEVEL:2.0==h, m==10, n==10}>

   for(i = 1; i <= n; i += 1)

//  <a[PHI1][PHI2]-W-EXACT-{PHI1==i, 1<=PHI2, PHI2<=m,
//    TOP-LEVEL:2.0==h, m==10, n==10, 1<=i, i<=n}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==i, 1<=PHI2, PHI2<=m,
//    TOP-LEVEL:2.0==h, m==10, n==10, 1<=i, i<=n}>

      for(j = 1; j <= m; j += 1) {
         x = i*h+j;

//  <a[PHI1][PHI2]-W-EXACT-{PHI1==i, PHI2==j, TOP-LEVEL:2.0==h, m==10,
//    n==10, 1<=i, i<=10, 1<=j, j<=10}>
//  <b[PHI1][PHI2]-R-EXACT-{PHI1==i, PHI2==j, TOP-LEVEL:2.0==h, m==10,
//    n==10, 1<=i, i<=10, 1<=j, j<=10}>

         a[i][j] = b[i][j]*x;
      }
}

