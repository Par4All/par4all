//  P(i,j,x) {2.0==h, m==10, n==10, 1<=i, i<=10, 1<=j, j<=10}
         a[i][j] = b[i][j]*x;
      }
}

Printed files after parallelization

void func1(int n, int m, float a[n][m], float b[n][m], float h)
{
   float x;
   int i, j;
   for(i = 1; i <= 10; i += 1)
      for(j = 1; j <= 10; j += 1) {
         x = i*h+j;
         a[i][j] = b[i][j]*x;
      }
}
