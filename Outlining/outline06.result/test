
Initial code

void foo(int bar[256][256])
{
   int i, j;
   for(i = 0; i <= 255; i += 1)
kernel:
      for(j = 0; j <= 255; j += 1)
         bar[i][j] = K*i*j;
}
int K = 12;
void foo(int bar[256][256]);

Code after outlining

void foo(int bar[256][256])
{
   int i, j;
   for(i = 0; i <= 255; i += 1)
kernel:      kernel(i, bar);
}
void kernel(int i, int bar[256][256])
{
   //PIPS generated variable
   int j;
kernel:
   for(j = 0; j <= 255; j += 1)
      bar[i][j] = K*i*j;
}
//
//     This module was automatically generated by PIPS
//
void kernel(int i, int bar[256][256]);
int K = 12;
void foo(int bar[256][256]);
