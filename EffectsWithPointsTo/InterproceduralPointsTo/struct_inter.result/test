void assignment(struct foo t1, struct foo t2)
{

// Points To:
// t1.ip1 -> _t1_1 , EXACT
// t1.ip2 -> _t1_1 , EXACT
// t2.ip1 -> _t2_2 , EXACT
// t2.ip2 -> _t2_2 , EXACT

   t1 = t2;
}

