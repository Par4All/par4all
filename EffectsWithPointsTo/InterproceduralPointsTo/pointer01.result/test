// Points To IN:
// q1 -> _q1_1 , EXACT
// q2 -> _q2_2 , EXACT
// q3 -> _q3_3 , EXACT


// Points To OUT: none

void pointer_add(pointer q1, const pointer q2, const pointer q3)
{

// Points To:
// q1 -> _q1_1 , EXACT
// q2 -> _q2_2 , EXACT
// q3 -> _q3_3 , EXACT

   *q1 = *q2+*q3;
}

// Points To IN:
// p -> _p_1 , EXACT


// Points To OUT: none

