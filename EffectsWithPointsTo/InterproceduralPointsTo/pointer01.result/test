//             <    is written>: _q1_1
void pointer_add(pointer q1, const pointer q2, const pointer q3)
{
//             <    is read   >: _q2_2 _q3_3 q1 q2 q3
//             <    is written>: _q1_1
   *q1 = *q2+*q3;
}
