void f(int **p, int **q, int **r)
{

// Points To:
// p -> _p_1 , EXACT
// q -> _q_2 , EXACT
// r -> _r_3 , EXACT

   *p = *q;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _q_2_2 , EXACT
// p -> _p_1 , EXACT
// q -> _q_2 , EXACT
// r -> _r_3 , EXACT

   *q = *r;

// Points To:
// _p_1 -> _q_2_2 , EXACT
// _q_2 -> _r_3_3 , EXACT
// _r_3 -> _r_3_3 , EXACT
// p -> _p_1 , EXACT
// q -> _q_2 , EXACT
// r -> _r_3 , EXACT

   return;
}

