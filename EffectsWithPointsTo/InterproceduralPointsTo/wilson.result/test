//             <    is read   >: _q_2 _r_3 p q r
//             <    is written>: _p_1 _q_2
void f(int **p, int **q, int **r)
{
//             <    is read   >: _q_2 p q
//             <    is written>: _p_1
   *p = *q;
//             <    is read   >: _r_3 q r
//             <    is written>: _q_2
   *q = *r;
   return;
}
//             <    is read   >: f:_q_2 f:_r_3
//             <    is written>: f:_p_1 f:_q_2
int main()
{
   int x, y, z;
//             <    is written>: x0 y0 z0
   int *x0 = &x, *y0 = &y, *z0 = &z;
//             <    is read   >: f:_q_2 f:_r_3
//             <    is written>: f:_p_1 f:_q_2

   if (1)
//             <    is read   >: f:_q_2 f:_r_3
//             <    is written>: f:_p_1 f:_q_2
      f(&x0, &y0, &z0);
   else if (2)
//             <    is read   >: f:_q_2 f:_r_3
//             <    is written>: f:_p_1 f:_q_2
      f(&z0, &x0, &y0);
   else
//             <    is read   >: f:_q_2 f:_r_3
//             <    is written>: f:_p_1 f:_q_2
      f(&x0, &y0, &x0);

   return 0;
}
void f(int **p, int **q, int **r);

int main();

