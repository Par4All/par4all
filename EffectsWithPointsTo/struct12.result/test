// Points To:
// _n_1[0].tab2 -> *NULL_POINTER* , MAY
// _n_1[0].tab2 -> _n_1_3__1[0][0] , MAY
// m.tab2 -> *HEAP*_l_39[0] , MAY
// n -> _n_1[0] , EXACT

   (m.tab1)[0] = 10;

// Points To:
// _n_1[0].tab2 -> *NULL_POINTER* , MAY
// _n_1[0].tab2 -> _n_1_3__1[0][0] , MAY
// m.tab2 -> *HEAP*_l_39[0] , MAY
