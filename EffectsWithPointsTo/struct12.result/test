//             <    is written>: i _MALLOC_EFFECTS:_MALLOC_
   for(i = 0; i <= 4; i += 1) {
//             <may be written>: p[*]
//             <    is read   >: i _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
      p[i] = malloc(sizeof(mys));
//             <may be read   >: p[*]
//             <may be written>: *HEAP*_l_77.tab2
//             <    is read   >: i _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_
      p[i]->tab2 = malloc(5*sizeof(int));
   }
//             <may be written>: *HEAP*_l_77.num
//             <    is read   >: p[0]

   p[0]->num = 11;
//             <may be written>: *HEAP*_l_77.tab1[0]
//             <    is read   >: p[0]
   (p[0]->tab1)[0] = 20;
//             <may be read   >: *HEAP*_l_77.tab2
//             <may be written>: *HEAP*_l_77.tab2[0]
//             <    is read   >: p[0]
   (p[0]->tab2)[0] = 30;
//             <may be read   >: *HEAP*_l_77.tab1[0] *HEAP*_l_77.tab2 
//                               p[*]
//             <may be written>: *HEAP*_l_77 *HEAP*_l_77.num 
//                               *HEAP*_l_77.tab1[0]
//             <    is read   >: i
//             <    is written>: i
   
   
   for(i = 1; i <= 4; i += 1) {
//             <may be read   >: p[*]
//             <may be written>: *HEAP*_l_77.num
//             <    is read   >: i
      p[i]->num = 10;
//             <may be read   >: p[*]
//             <may be written>: *HEAP*_l_77.tab1[0]
//             <    is read   >: i
      (p[i]->tab1)[0] = 21;
//             <may be read   >: *HEAP*_l_77.tab1[0] *HEAP*_l_77.tab2 
//                               p[*]
//             <may be written>: *HEAP*_l_77.tab2[0]
//             <    is read   >: i
      (p[i]->tab2)[0] = (p[i]->tab1)[0];
   }
}

