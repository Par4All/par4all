// *HEAP*_l_56.tab2 -> *HEAP*_l_57[0] , MAY
// p -> *HEAP*_l_53[0] , MAY

   (p[0]->tab2)[0] = 30;

// Points To:
// *HEAP*_l_53[*] -> *HEAP*_l_56 , MAY
// *HEAP*_l_56.tab2 -> *HEAP*_l_57[0] , MAY
// p -> *HEAP*_l_53[0] , MAY


