void myprint(int *p)
{
   int i;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <may be read   >: stdout[0]
//             <may be written>: stdout[0]
//             <    is read   >: _p_1[i] i p stdout 
//                               __IO_EFFECTS:LUNS[1]
//             <    is written>: __IO_EFFECTS:LUNS[1]
      printf("\t%d\n", p[i]);
   return;
}

