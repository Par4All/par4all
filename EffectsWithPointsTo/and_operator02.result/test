
PROPER  EFFECTS 

int main()
{
   int i[10], j[20], k[30], l[40];
   _Bool b;
   int *p, *q, *r, *s, *t;
//             <    is written>: p
   p = &k[0];
//             <    is written>: r
   r = &i[0];
//             <    is written>: q
   q = &j[0];
//             <    is written>: s
   s = &l[0];
//             <    is read   >: q s
//             <    is written>: b p r
   /* printf("address of p=%p\n ",p); */
   /*   printf("address of r=%p\n",r); */
   /*   printf("address of q=%p\n",q); */
   /*   printf("address of s=%p\n",s); */
   
   b = (p = q)&&(r = s);
//             <    is read   >: q s
//             <    is written>: p r
   if ((p = q)&&(r = s))
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: p
      *p++;
   else
//             <    is read   >: q s
//             <    is written>: p r
      /*  printf("address of p=%p\n",p); */
      /*   printf("address of r=%p\n",r); */
      if ((p = q)||(r = s))
//             <may be read   >: *ANY_MODULE*:*ANYWHERE*
//             <    is written>: r
         *r++;

   return 0;
}

POINTS-TO


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int i[10], j[20], k[30], l[40];

// Points To: none

   _Bool b;

// Points To: none

   int *p, *q, *r, *s, *t;

// Points To:
// p -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   p = &k[0];

// Points To:
// p -> k[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   r = &i[0];

// Points To:
// p -> k[0] , EXACT
// q -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// r -> i[0] , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   q = &j[0];

// Points To:
// p -> k[0] , EXACT
// q -> j[0] , EXACT
// r -> i[0] , EXACT
// s -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   s = &l[0];

// Points To:
// p -> k[0] , EXACT
// q -> j[0] , EXACT
// r -> i[0] , EXACT
// s -> l[0] , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   /* printf("address of p=%p\n ",p); */
   /*   printf("address of r=%p\n",r); */
   /*   printf("address of q=%p\n",q); */
   /*   printf("address of s=%p\n",s); */
   
   b = (p = q)&&(r = s);

// Points To:
// p -> j[0] , EXACT
// q -> j[0] , EXACT
// r -> l[0] , EXACT
// s -> l[0] , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   if ((p = q)&&(r = s))

// Points To:
// p -> j[0] , EXACT
// q -> j[0] , EXACT
// r -> l[0] , EXACT
// s -> l[0] , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

      *p++;
   else

// Points To: unreachable

      /*  printf("address of p=%p\n",p); */
      /*   printf("address of r=%p\n",r); */
      if ((p = q)||(r = s))

// Points To: unreachable

         *r++;

// Points To:
// p -> j[1] , EXACT
// q -> j[0] , EXACT
// r -> l[0] , EXACT
// s -> l[0] , EXACT
// t -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   return 0;
}

PROPER  EFFECTS WITH POINTS TO

int main()
{
   int i[10], j[20], k[30], l[40];
   _Bool b;
   int *p, *q, *r, *s, *t;
//             <    is written>: p
   p = &k[0];
//             <    is written>: r
   r = &i[0];
//             <    is written>: q
   q = &j[0];
//             <    is written>: s
   s = &l[0];
//             <    is read   >: q s
//             <    is written>: b p r
   /* printf("address of p=%p\n ",p); */
   /*   printf("address of r=%p\n",r); */
   /*   printf("address of q=%p\n",q); */
   /*   printf("address of s=%p\n",s); */
   
   b = (p = q)&&(r = s);
//             <    is read   >: q s
//             <    is written>: p r
   if ((p = q)&&(r = s))
//             <    is read   >: j[0] p
//             <    is written>: p
      *p++;
   else
      /*  printf("address of p=%p\n",p); */
      /*   printf("address of r=%p\n",r); */
      if ((p = q)||(r = s))
         *r++;

   return 0;
}
