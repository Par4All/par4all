void exit02()
{
   int i;
   int *j;
//             <    is written>: i
   i = 1;
//             <    is read   >: i
   if (i) {
//             <    is written>: j
      j = &i;
      exit(0);
   }
   else {
      static int *p;
//             <    is read   >: i _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_
      p = malloc(i*sizeof(int));
   }
//             <    is written>: i

   i = 2;
}

