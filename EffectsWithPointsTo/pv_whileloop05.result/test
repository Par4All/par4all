int main()
{
//             <    is written>: i
   int i = 0;
   int *p, *q;
//             <    is read   >: _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: p _MALLOC_EFFECTS:_MALLOC_

   p = (int *) malloc(10*sizeof(int));
//             <    is read   >: p
//             <    is written>: q
   q = p;
//             <    is read   >: i

   while (i<5) {
//             <may be written>: *HEAP*_l_7[0] *HEAP*_l_7[*]
//             <    is read   >: i q
      *q = i;
//             <    is read   >: q
//             <    is written>: q
      q++;
//             <    is read   >: i
//             <    is written>: i
      i++;
   }
//             <    is read   >: p _MALLOC_EFFECTS:_MALLOC_
//             <    is written>: _MALLOC_EFFECTS:_MALLOC_

   free(p);
   return 0;
}

