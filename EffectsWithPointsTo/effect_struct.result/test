
PROPER  EFFECTS 

float main(int argc, char *argv[])
{
   int i, j, k;
//             <    is written>: i

   for(i = 0; i <= 8; i += 1)
//             <    is written>: j
      for(j = 0; j <= 8; j += 1) {
//             <    is read   >: i j
//             <    is written>: imagein[i][j].re
         imagein[i][j].re = 0.0;
//             <    is read   >: i j
//             <    is written>: imagein[i][j].im
         imagein[i][j].im = 0.0;
      }
//             <    is read   >: imagein[2][1].re
   return imagein[2][1].re;
}

POINTS-TO


// Points To IN:
// argv[*] -> _argv_2[0] , EXACT


// Points To OUT: none

float main(int argc, char *argv[])
{

// Points To:
// argv[*] -> _argv_2[0] , EXACT

   int i, j, k;

// Points To:
// argv[*] -> _argv_2[0] , EXACT


   for(i = 0; i <= 8; i += 1)

// Points To:
// argv[*] -> _argv_2[0] , EXACT

      for(j = 0; j <= 8; j += 1) {

// Points To:
// argv[*] -> _argv_2[0] , EXACT

         imagein[i][j].re = 0.0;

// Points To:
// argv[*] -> _argv_2[0] , EXACT

         imagein[i][j].im = 0.0;
      }

// Points To:
// argv[*] -> _argv_2[0] , EXACT

   return imagein[2][1].re;
}

PROPER  EFFECTS WITH POINTS TO

float main(int argc, char *argv[])
{
   int i, j, k;
//             <    is written>: i

   for(i = 0; i <= 8; i += 1)
//             <    is written>: j
      for(j = 0; j <= 8; j += 1) {
//             <    is read   >: i j
//             <    is written>: imagein[i][j].re
         imagein[i][j].re = 0.0;
//             <    is read   >: i j
//             <    is written>: imagein[i][j].im
         imagein[i][j].im = 0.0;
      }
//             <    is read   >: imagein[2][1].re
   return imagein[2][1].re;
}

