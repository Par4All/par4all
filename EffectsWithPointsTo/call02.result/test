PROPER  EFFECTS WITH POINTS TO

void call02(int i, int j, int y[10], int *q[10], tf_t *p)
{
//             <    is read   >: j
//             <    is written>: i
   /* i can be modified locally, but it won't show in the summary
        effects... which creates a problem for transformer and
        precondition computation. */
   i = j+1;
//             <    is read   >: i
//             <    is written>: y[i]
   y[i] = 0;
//             <    is read   >: p
//             <    is written>: _p_5.one
   p->one = 1;
//             <    is read   >: j p
//             <    is written>: _p_5.two[j]
   (p->two)[j] = 2.;
//             <may be written>: _q_4
//             <    is read   >: i q[i]
   *q[i] = 3;
}
int main()
{
   int a;
   int b;
   int x[10];
   int *ap[10];
   tf_t s;
//             <    is written>: sp
   tf_t *sp = &s;
//             <may be read   >: ap[*]
//             <may be written>: x[*]
//             <    is read   >: a b sp

   call02(a, b, x, ap, sp);
}

