PROPER  EFFECTS WITH POINTS TO

void fpointer01(char c, void *_stream, void (*my_fputc)(const char c, void *_stream), int *col, int indent, int *nbout)
{
//             <    is read   >: c
   if (c=='\n'||c=='\r')
//             <    is read   >: col
//             <    is written>: _col_4
      /* on change de ligne */
      
      *col = 0;
   else {
//             <    is read   >: _col_4 col indent
      /* indentation ok ? */
      
      while (*col<indent) {
         my_fputc(' ', _stream);
//             <    is read   >: _nbout_6 nbout
//             <    is written>: _nbout_6
         (*nbout)++;
//             <    is read   >: _col_4 col
//             <    is written>: _col_4
         (*col)++;
      }
//             <    is read   >: _col_4 col
//             <    is written>: _col_4
      (*col)++;
   }
   
   /* dans tous les cas il faut afficher le caractere passe */
   
   my_fputc(c, _stream);
//             <    is read   >: _nbout_6 nbout
//             <    is written>: _nbout_6
   (*nbout)++;
}
void fpointer01(char c, void *_stream, void (*my_fputc)(const char c, void *_stream), int *col, int indent, int *nbout);

