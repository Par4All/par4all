
Points-to for fpointer01


// Points To IN:
// col -> *NULL_POINTER* , MAY
// col -> _col_4[0] , MAY
// nbout -> *NULL_POINTER* , MAY
// nbout -> _nbout_6[0] , MAY


// Points To OUT:
// col -> _col_4[0] , EXACT
// nbout -> _nbout_6[0] , EXACT

void fpointer01(char c, void *_stream, void (*my_fputc)(const char c, void *_stream), int *col, int indent, int *nbout)
{

// Points To:
// nbout -> *NULL_POINTER* , MAY
// nbout -> _nbout_6[0] , MAY

   if (c=='\n'||c=='\r')

// Points To:
// col -> *NULL_POINTER* , MAY
// col -> _col_4[0] , MAY

      /* on change de ligne */
      
      *col = 0;
   else {

// Points To:
// col -> *NULL_POINTER* , MAY
// col -> _col_4[0] , MAY

      /* indentation ok ? */
      
      while (*col<indent) {

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> _nbout_6[0] , MAY

         my_fputc(' ', _stream);

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> *NULL_POINTER* , MAY
// nbout -> _nbout_6[0] , MAY

         (*nbout)++;

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> _nbout_6[0] , EXACT

         (*col)++;
      }

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> _nbout_6[0] , EXACT

      (*col)++;
   }

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> *NULL_POINTER* , MAY
// nbout -> _nbout_6[0] , MAY

   
   /* dans tous les cas il faut afficher le caractere passe */
   
   my_fputc(c, _stream);

// Points To:
// col -> _col_4[0] , EXACT
// nbout -> *NULL_POINTER* , MAY
// nbout -> _nbout_6[0] , MAY

   (*nbout)++;
}

Proper effects with points-to for fpointer01

void fpointer01(char c, void *_stream, void (*my_fputc)(const char c, void *_stream), int *col, int indent, int *nbout)
{
//             <    is read   >: c
   if (c=='\n'||c=='\r')
//             <    is read   >: col
//             <    is written>: _col_4[0]
      /* on change de ligne */
      
      *col = 0;
   else {
//             <    is read   >: _col_4[0] col indent
      /* indentation ok ? */
      
      while (*col<indent) {
         my_fputc(' ', _stream);
//             <    is read   >: _nbout_6[0] nbout
//             <    is written>: _nbout_6[0]
         (*nbout)++;
//             <    is read   >: _col_4[0] col
//             <    is written>: _col_4[0]
         (*col)++;
      }
//             <    is read   >: _col_4[0] col
//             <    is written>: _col_4[0]
      (*col)++;
   }
   
   /* dans tous les cas il faut afficher le caractere passe */
   
   my_fputc(c, _stream);
//             <    is read   >: _nbout_6[0] nbout
//             <    is written>: _nbout_6[0]
   (*nbout)++;
}
