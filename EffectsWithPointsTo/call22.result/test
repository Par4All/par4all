Points-to for "main"


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[10];

// Points To: none

   int *ap[10];

// Points To:
// ap[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT

   int i;

// Points To:
// ap[*] -> *ANY_MODULE*:*NOWHERE*_b0 , EXACT


   for(i = 0; i <= 9; i += 1)

// Points To:
// ap[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// ap[*] -> a[*] , MAY

      ap[i] = &a[i];

// Points To:
// ap[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// ap[*] -> a[*] , MAY


   call22(ap);

// Points To:
// ap[*] -> *ANY_MODULE*:*NOWHERE*_b0 , MAY
// ap[*] -> a[*] , MAY

   return 0;
}

Proper effects with points-to for "main"

int main()
{
   int a[10];
   int *ap[10];
   int i;
//             <    is written>: i

   for(i = 0; i <= 9; i += 1)
//             <    is read   >: i
//             <    is written>: ap[i]
      ap[i] = &a[i];
//             <    is read   >: ap[4]
//             <    is written>: a[*]

