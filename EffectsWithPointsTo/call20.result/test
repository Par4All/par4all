// Points To IN:
// stdout -> *NULL_POINTER* , MAY
// stdout -> _stdout_[0] , MAY


// Points To OUT: none

int main(void)
{

// Points To: none

   pointer p1, p2, p3;

// Points To:
// p1 -> undefined , EXACT
// p2 -> undefined , EXACT
// p3 -> undefined , EXACT

   
   // could differentiate allocs based on call path?
   p1 = pointer_alloc(13);

// Points To:
// p1 -> pointer_alloc:*HEAP*_l_9 , MAY
// p2 -> undefined , EXACT
// p3 -> undefined , EXACT

   p2 = pointer_alloc(17);

// Points To:
// p1 -> pointer_alloc:*HEAP*_l_9 , MAY
// p2 -> pointer_alloc:*HEAP*_l_9 , MAY
// p3 -> undefined , EXACT

   p3 = pointer_alloc(19);

// Points To:
// p1 -> pointer_alloc:*HEAP*_l_9 , MAY
// p2 -> pointer_alloc:*HEAP*_l_9 , MAY
// p3 -> pointer_alloc:*HEAP*_l_9 , MAY

   
   // no pointer assigned! can keep all points-to
   pointer_add(p1, p2, p3);

// Points To:
// p1 -> pointer_alloc:*HEAP*_l_9 , MAY
// p2 -> pointer_alloc:*HEAP*_l_9 , MAY
// p3 -> pointer_alloc:*HEAP*_l_9 , MAY
// stdout -> *NULL_POINTER* , MAY
// stdout -> _stdout_[0] , MAY

   
   // 36 17 19
