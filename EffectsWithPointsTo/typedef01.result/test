PROPER  EFFECTS WITH POINTS TO

void compute()
{
   int i, j;
//             <    is written>: i

   for(i = 1; i <= 62; i += 1)
//             <    is written>: j
      for(j = 1; j <= 62; j += 1)
//             <    is read   >: i j
//             <    is written>: tab[i][j]
         tab[i][j] = i*j;
}
int main(int argc, char *argv[])
{
   int t;
//             <    is written>: t
   
   
   for(t = 0; t <= 63; t += 1)
//             <may be written>: tab[*][*]
      compute();
   
   
   return 0;
}

