
Transformers for multiply03


//  T(multiply03) {6N==multiply03}

int multiply03(int i, int j, int N)
{

//  T(k) {}

   int k;

//  T(i) {i==2}


   i = 2;

//  T(j) {i==2, j==3}

   j = 3;

//  T(k) {6N==k, i==2, j==3}

   k = i*N*j;

//  T(multiply03) {6N==k, 6N==multiply03, i==2, j==3}

   return k;
}

