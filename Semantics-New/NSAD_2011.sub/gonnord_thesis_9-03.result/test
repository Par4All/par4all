//  T() {0==-1}

                     checking_error();
               }
            }

//  T() {0==-1}

            while (rand_b()) {

//  T() {0==-1}

               if (!(t==3&&b>s+1&&b>s+2))

//  T() {0==-1}

                  deadlock();

//  T() {0==-1}

