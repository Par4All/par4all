//  T() {0==-1}

                           deadlock();

//  T() {0==-1}

                        d++;

//  T() {0==-1}

                        b++;

//  T() {0==-1}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                  else {

//  T() {0==-1}

                     if (!(t==3&&b>s+1&&b==s+2))

//  T() {0==-1}

                        deadlock();

//  T() {0==-1}

                     s++;

//  T() {0==-1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();

//  T() {0==-1}

                     if (!(t==3&&d<9&&d<8))

//  T() {0==-1}

                        deadlock();

//  T() {0==-1}

                     d++;

//  T() {0==-1}

                     b++;

//  T() {0==-1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }

//  T() {0==-1}

               if (rand_b()) {

//  T() {0==-1}

                  if (!(t==3&&b>s+1&&b==s+2))

//  T() {0==-1}

                     deadlock();

//  T() {0==-1}

                  s++;

//  T() {0==-1}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();

//  T() {0==-1}

