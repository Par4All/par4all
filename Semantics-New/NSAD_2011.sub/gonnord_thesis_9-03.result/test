
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T(b,d,s,t) {}

   int b, s, d, t;

//  T(b,d,s) {b==0, d==0, s==0}

   b = s = d = 0;

//  T(t) {b==0, d==0, s==0, t==1}

   t = 1;

//  T() {b==0, d==0, s==0, t==1}

   if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

      checking_error();

//  T(b,d,s,t) {0<=b, b#init<=b,
//    1323b#init+2824s#init+573t#init<=1323b+2824s+573t,
//    182b#init+111s#init+91t<=182b+111s+91t#init,
//    133b#init+54s#init+54t<=133b+54s+54t#init,
//    98b#init+158s#init+29t<=98b+158s+29t#init,
//    98b#init+152s#init+31t<=98b+152s+31t#init,
//    78b#init+198s#init+t#init<=78b+198s+t,
//    58b#init+158s#init+31t#init<=58b+158s+31t,
//    32b#init+42s#init+13t<=32b+42s+13t#init,
//    29b#init+72s#init+24t#init<=29b+72s+24t,
//    19b#init+53s#init+7t#init<=19b+53s+7t, s<=b+10, b<=s+19, 1<=t}

   while (rand_b())

//  T(b,d,s,t) {b+s==b#init+s#init+1, s<=b+10, b<=s+19,
//    77s+3t+31t#init<=9b+68s#init+195,
//    20s#init+10t#init<=b+19s+10t+10, 10s#init+10t<=b+9s+15t#init+5,
//    2s+3t#init<=b+s#init+3t+10, 2s+t#init<=b+s#init+t+10,
//    6s+3t#init<=b+5s#init+t+16, 63s+82t<=b+62s#init+51t#init+185,
//    b+18s#init+9t#init<=19s+14t+4, b+8s+9t<=9s#init+9t#init+28,
//    b+48s+29t<=49s#init+29t#init+68, 2b+20s#init<=22s+t+9t#init+8,
//    3b+30s#init+19t#init<=33s+34t+12, 23s+10t#init<=3b+20s#init+60,
//    2b+18s#init<=20s+9t#init+11, 3b+19t#init<=3s+19t+57, s#init<=s,
//    s<=s#init+1, 4s#init+2t#init<=4s+t+3, s+2t<=s#init+t#init+5,
//    3s+3t<=3s#init+2t#init+7, s#init+t#init<=s+3, 1<=t, 1<=t#init}

      if (rand_b()) {

//  T() {t==1, s<=b+8}

         if (!(t==1&&b>s-9))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9}

         s++;

//  T() {t==1, s<=b+9, b<=s+19}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {b==s+9, t==1}

         if (!(t==1&&b==s+9))

//  T() {0==-1}

            deadlock();

//  T(t) {b==s+9, t==3, t#init==1}

         t = 3;

//  T(b) {b==b#init+1, b==s+10, t==3}

         b++;

//  T(d) {b==s+10, d==0, t==3}

         d = 0;

//  T() {b==s+10, d==0, t==3}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {b==s+1, t==3}

         if (!(t==3&&b==s+1))

//  T() {0==-1}

            deadlock();

//  T(t) {b==s+1, t==1, t#init==3}

         t = 1;

//  T(s) {b==s, b==s#init+1, t==1}

         s++;

//  T(d) {b==s, d==0, t==1}

         d = 0;

//  T() {b==s, d==0, t==1}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {b==s-1, t==2}

         if (!(t==2&&b==s-1))

//  T() {0==-1}

            deadlock();

//  T(t) {b==s-1, t==1, t#init==2}

         t = 1;

//  T(b) {b==b#init+1, b==s, t==1}

         b++;

//  T() {b==s, t==1}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {b==s-9, t==1}

         if (!(t==1&&b==s-9))

//  T() {0==-1}

            deadlock();

//  T(t) {b==s-9, t==2, t#init==1}

         t = 2;

//  T(s) {b==s-10, b==s#init-9, t==2}

         s++;

//  T() {b==s-10, t==2}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {t==2, b+2<=s}

         if (!(t==2&&b<s-1))

//  T() {0==-1}

            deadlock();

//  T(b) {b==b#init+1, t==2, b+1<=s}

         b++;

//  T() {t==2, s<=b+10, b+1<=s}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {d==9, t==3}

         if (!(t==3&&d==9))

//  T() {0==-1}

            deadlock();

//  T(t) {d==9, t==4, t#init==3}

         t = 4;

//  T(b) {b==b#init+1, d==9, t==4}

         b++;

//  T() {d==9, t==4, s<=b+10, b<=s+19}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {t==3, s+2<=b}

         if (!(t==3&&b>s+1))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, t==3, s+1<=b}

         s++;

//  T() {t==3, s+1<=b, b<=s+19}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {t==3, d<=8}

         if (!(t==3&&d<9))

//  T() {0==-1}

            deadlock();

//  T(d) {d==d#init+1, t==3, d<=9}

         d++;

//  T(b) {b==b#init+1, t==3, d<=9}

         b++;

//  T() {t==3, s<=b+10, b<=s+19, d<=9}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {t==4, s+2<=b}

         if (!(t==4&&b>s+1))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, t==4, s+1<=b}

         s++;

//  T() {t==4, s+1<=b, b<=s+19}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {b==s+1, t==4}

         if (!(t==4&&b==s+1))

//  T() {0==-1}

            deadlock();

//  T(t) {b==s+1, t==1, t#init==4}

         t = 1;

//  T(s) {b==s, b==s#init+1, t==1}

         s++;

//  T(d) {b==s, d==0, t==1}

         d = 0;

//  T() {b==s, d==0, t==1}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {t==1, b<=s+8}

         if (!(t==1&&b<s+9))

//  T() {0==-1}

            deadlock();

//  T(b) {b==b#init+1, t==1, b<=s+9}

         b++;

//  T() {t==1, s<=b+10, b<=s+9}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T(b,d,s,t) {}

   int b, s, d, t;

//  T(b,d,s) {b==0, d==0, s==0}

   b = s = d = 0;

//  T(t) {b==0, d==0, s==0, t==1}

   t = 1;

//  T() {b==0, d==0, s==0, t==1}

   if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

      checking_error();

//  T(b,s) {d==0, t==1, 21b#init+s<=21b+s#init,
//    19b#init+121s#init<=19b+121s, 11b#init+s<=11b+s#init,
//    2b+37s#init<=2b#init+37s}

   while (rand_b())

//  T(b,s) {t==1, 9b#init+29s<=29b+9s#init+180,
//    21b#init+s+20<=21b+s#init, 3b#init+32s#init+35<=3b+32s,
//    2s+9s#init<=2b+9b#init+99, b+63s#init<=18b#init+46s+108,
//    11b+9b#init<=11s+9s#init+180, 20b+9s#init<=9b#init+20s+99,
//    35b+63s#init<=18b#init+80s+108}

      if (rand_b())

//  T(b,s) {t==1, 9b#init+29s<=29b+9s#init+180,
//    21b#init+s+20<=21b+s#init, 3b#init+32s#init+35<=3b+32s,
//    2s+9s#init<=2b+9b#init+99, b+63s#init<=18b#init+46s+108,
//    11b+9b#init<=11s+9s#init+180, 20b+9s#init<=9b#init+20s+99,
//    35b+63s#init<=18b#init+80s+108}

         if (rand_b())

//  T(b,s) {t==1, 9b#init+29s<=29b+9s#init+180,
//    21b#init+s+20<=21b+s#init, 3b#init+32s#init+35<=3b+32s,
//    2b+9s#init<=9b#init+2s+63, 11b+9b#init<=11s+9s#init+180,
//    16b+9s#init<=9b#init+16s+63}

            if (rand_b()) {

//  T() {t==1, b<=s+8}

               if (!(t==1&&b<s+9))

//  T() {0==-1}

                  deadlock();

//  T(b) {b==b#init+1, t==1, b<=s+9}

               b++;

//  T() {t==1, s<=b+10, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();

//  T() {t==1, s<=b+8, b<=s+9}

               if (!(t==1&&b>s-9))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+8}

               s++;

//  T() {t==1, s<=b+9, b<=s+8}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else if (rand_b()) {

//  T() {t==1, s<=b+8}

               if (!(t==1&&b>s-9))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9}

               s++;

//  T() {t==1, s<=b+9, b<=s+19}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();

//  T() {t==1, s<=b+9, b<=s+8}

               if (!(t==1&&b<s+9))

//  T() {0==-1}

                  deadlock();

//  T(b) {b==b#init+1, t==1, s<=b+8, b<=s+9}

               b++;

//  T() {t==1, s<=b+8, b<=s+9}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
            else {

//  T() {t==1, s<=b+8}

               if (!(t==1&&b>s-9))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9}

               s++;

//  T() {t==1, s<=b+9, b<=s+19}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();

//  T() {t==1, s<=b+8, b<=s+19}

               if (!(t==1&&b>s-9))

//  T() {0==-1}

                  deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+18}

               s++;

//  T() {t==1, s<=b+9, b<=s+18}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();

//  T(b,s) {t==1, b#init<=b, 8b#init+27s#init<=8b+27s, s<=b+10,
//    b<=s+19}

               while (rand_b())

//  T(b,s) {t==1, b<=b#init+1, 22b<=21b#init+s+19,
//    b#init+s#init+1<=b+s, 3s<=b+2s#init+10, s<=s#init+1}

                  if (rand_b()) {

//  T() {b==s-2, t==1}

                     if (!(t==1&&b<s+9&&b==s-2))

//  T() {0==-1}

                        deadlock();

//  T(b) {b==b#init+1, b==s-1, t==1}

                     b++;

//  T() {b==s-1, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();

//  T() {b==s-1, t==1}

                     if (!(t==1&&b>s-9))

//  T() {0==-1}

                        deadlock();

//  T(s) {b==s-2, b==s#init-1, t==1}

                     s++;

//  T() {b==s-2, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else if (rand_b())

//  T(b,s) {b+s==b#init+s#init+1, t==1, b#init<=b, b<=b#init+1,
//    2b#init+s<=3b+8, 22b<=21b#init+s+19}

                     if (rand_b()) {

//  T() {t==1, b+3<=s}

                        if (!(t==1&&b<s+9&&b<s-2))

//  T() {0==-1}

                           deadlock();

//  T(b) {b==b#init+1, t==1, b+2<=s}

                        b++;

//  T() {t==1, s<=b+10, b+2<=s}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                     else {

//  T() {t==1, s<=b+7}

                        if (!(t==1&&b>s-9&&b>s-8))

//  T() {0==-1}

                           deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+8}

                        s++;

//  T() {t==1, s<=b+8, b<=s+19}

                        if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                           checking_error();
                     }
                  else {

//  T() {b==s-8, t==1}

                     if (!(t==1&&b>s-9&&b==s-8))

//  T() {0==-1}

                        deadlock();

//  T(s) {b==s-9, b==s#init-8, t==1}

                     s++;

//  T() {b==s-9, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();

//  T() {b==s-9, t==1}

                     if (!(t==1&&b<s+9))

//  T() {0==-1}

                        deadlock();

//  T(b) {b==b#init+1, b==s-8, t==1}

                     b++;

//  T() {b==s-8, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }

//  T() {b==s-2, t==1}

               if (!(t==1&&b<s+9&&b==s-2))

//  T() {0==-1}

                  deadlock();

//  T(b) {b==b#init+1, b==s-1, t==1}

               b++;

//  T() {b==s-1, t==1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();

//  T() {b==s-1, t==1}

               if (!(t==1&&b<s+9))

//  T() {0==-1}

                  deadlock();

//  T(b) {b==b#init+1, b==s, t==1}

               b++;

//  T() {b==s, t==1}

               if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                  checking_error();
            }
         else {

//  T() {t==1, b<=s+8}

            if (!(t==1&&b<s+9))

//  T() {0==-1}

               deadlock();

//  T(b) {b==b#init+1, t==1, b<=s+9}

            b++;

//  T() {t==1, s<=b+10, b<=s+9}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();

//  T() {t==1, s<=b+10, b<=s+8}

            if (!(t==1&&b<s+9))

//  T() {0==-1}

               deadlock();

//  T(b) {b==b#init+1, t==1, s<=b+9, b<=s+9}

            b++;

//  T() {t==1, s<=b+9, b<=s+9}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();

//  T(b,s) {t==1, b#init<=b, s<=b+10, b<=s+19, s#init<=s}

            while (rand_b())

//  T(b,s) {t==1, b<=b#init+1, 12b<=11b#init+s+19,
//    b#init+s#init+1<=b+s, 13s<=b+12s#init+10, s<=s#init+1}

               if (rand_b())

//  T(b,s) {t==1, b<=b#init+1, 12b<=11b#init+s+19,
//    b#init+s#init+1<=b+s, 5b+19s<=6b#init+18s#init+16, s<=s#init+1}

                  if (rand_b()) {

//  T() {b==s+8, t==1}

                     if (!(t==1&&b<s+9&&b==s+8))

//  T() {0==-1}

                        deadlock();

//  T(b) {b==b#init+1, b==s+9, t==1}

                     b++;

//  T() {b==s+9, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();

//  T() {b==s+9, t==1}

                     if (!(t==1&&b>s-9))

//  T() {0==-1}

                        deadlock();

//  T(s) {b==s+8, b==s#init+9, t==1}

                     s++;

//  T() {b==s+8, t==1}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else if (rand_b()) {

//  T() {t==1, b<=s+7}

                     if (!(t==1&&b<s+9&&b<s+8))

//  T() {0==-1}

                        deadlock();

//  T(b) {b==b#init+1, t==1, b<=s+8}

                     b++;

//  T() {t==1, s<=b+10, b<=s+8}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
                  else {

//  T() {t==1, s+3<=b}

                     if (!(t==1&&b>s-9&&b>s+2))

//  T() {0==-1}

                        deadlock();

//  T(s) {s==s#init+1, t==1, s+2<=b}

                     s++;

//  T() {t==1, s+2<=b, b<=s+19}

                     if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                        checking_error();
                  }
               else {

//  T() {b==s+2, t==1}

                  if (!(t==1&&b>s-9&&b==s+2))

//  T() {0==-1}

                     deadlock();

//  T(s) {b==s+1, b==s#init+2, t==1}

                  s++;

//  T() {b==s+1, t==1}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();

//  T() {b==s+1, t==1}

                  if (!(t==1&&b<s+9))

//  T() {0==-1}

                     deadlock();

//  T(b) {b==b#init+1, b==s+2, t==1}

                  b++;

//  T() {b==s+2, t==1}

                  if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

                     checking_error();
               }

//  T() {b==s+2, t==1}

            if (!(t==1&&b>s-9&&b==s+2))

//  T() {0==-1}

               deadlock();

//  T(s) {b==s+1, b==s#init+2, t==1}

            s++;

//  T() {b==s+1, t==1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();

//  T() {b==s+1, t==1}

            if (!(t==1&&b>s-9))

//  T() {0==-1}

               deadlock();

//  T(s) {b==s, b==s#init+1, t==1}

            s++;

//  T() {b==s, t==1}

            if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

               checking_error();
         }
      else if (rand_b()) {

//  T() {t==1, s<=b+8}

         if (!(t==1&&b>s-9))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9}

         s++;

//  T() {t==1, s<=b+9, b<=s+19}

         if (!(b-s>=-10&&b-s<=19))

//  T() {0==-1}

            checking_error();

//  T() {t==1, s<=b+8, b<=s+19}

         if (!(t==1&&b>s-9))

//  T() {0==-1}

            deadlock();

//  T(s) {s==s#init+1, t==1, s<=b+9, b<=s+18}

         s++;

//  T() {t==1, s<=b+9, b<=s+18}

         if (!(b-s>=-10&&b-s<=19))

