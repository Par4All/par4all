PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int b, s, d, t;

//  P(b,d,s,t) {}

   b = s = d = 0;

//  P(b,d,s,t) {b==0, d==0, s==0}

   t = 1;

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}

   if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

      checking_error();

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}

   while (rand_b())

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

      if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==1&&b>s-9))

//  P(b,d,s,t) {0<=b, 9<=12b+s, b<=s+19, 20t<=27b+2s+20,
//    50<=3b+3s+23t, 4t<=3b+3s+4, 15<=3b+s+6t, 3t<=3b+s+3, s<=b+t+8,
//    s+t<=b+12, b+32<=s+23t, b<=s+5t+4, t<=b+2, 0<=d, d<=9, 0<=s,
//    18<=s+9t, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==1, 0<=b, s<=b+8, b<=s+9, 0<=d, d<=9, 0<=s}

         s++;

//  P(b,d,s,t) {t==1, 0<=b, s<=b+9, b<=s+8, 0<=d, d<=9, 1<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==1&&b==s+9))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b+2<=s+10t, b<=s+5t+4, t<=b+2,
//    0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {b==s+9, t==1, 9<=b, 0<=d, d<=9}

         t = 3;

//  P(b,d,s,t) {b==s+9, t==3, 9<=b, 0<=d, d<=9}

         b++;

//  P(b,d,s,t) {b==s+10, t==3, 10<=b, 0<=d, d<=9}

         d = 0;

//  P(b,d,s,t) {b==s+10, d==0, t==3, 10<=b}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==3&&b==s+1))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {b==s+1, t==3, 2<=b, 0<=d, d<=9}

         t = 1;

//  P(b,d,s,t) {b==s+1, t==1, 2<=b, 0<=d, d<=9}

         s++;

//  P(b,d,s,t) {b==s, t==1, 2<=b, 0<=d, d<=9}

         d = 0;

//  P(b,d,s,t) {b==s, d==0, t==1, 2<=b}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==2&&b==s-1))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {b==s-1, t==2, 1<=b, 0<=d, d<=9}

         t = 1;

//  P(b,d,s,t) {b==s-1, t==1, 1<=b, 0<=d, d<=9}

         b++;

//  P(b,d,s,t) {b==s, t==1, 2<=b, 0<=d, d<=9}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==1&&b==s-9))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+2t+6, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2,
//    0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {b==s-9, t==1, 0<=b, 0<=d, d<=9}

         t = 2;

//  P(b,d,s,t) {b==s-9, t==2, 0<=b, 0<=d, d<=9}

         s++;

//  P(b,d,s,t) {b==s-10, t==2, 0<=b, 0<=d, d<=9}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==2&&b<s-1))

//  P(b,d,s,t) {0<=b, s<=b+9, b<=s+19, s+80t<=161b+89, 20t<=27b+2s+20,
//    4t<=3b+3s+4, 3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4,
//    t<=2b+1, t<=b+2, 0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==2, 0<=b, 20<=27b+2s, 3<=3b+s, 2<=b+s, s<=b+10,
//    b+2<=s, 0<=d, d<=9, 0<=s}

         b++;

//  P(b,d,s,t) {t==2, 1<=b, 47<=27b+2s, 6<=3b+s, 3<=b+s, s<=b+9,
//    b+1<=s, 0<=d, d<=9, 0<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==3&&d==9))

//  P(b,d,s,t) {0<=b, 2b+5d<=2s+5t+63, b<=s+19, 20t<=27b+2s+20,
//    4t<=3b+3s+4, 3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2,
//    0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {d==9, t==3, 1<=b, 40<=27b+2s, 6<=3b+s, 3<=b+s, s<=b+9,
//    b<=s+19, 0<=s}

         t = 4;

//  P(b,d,s,t) {d==9, t==4, 1<=b, 40<=27b+2s, 6<=3b+s, 3<=b+s, s<=b+9,
//    b<=s+19, 0<=s}

         b++;

//  P(b,d,s,t) {d==9, t==4, 2<=b, 67<=27b+2s, 9<=3b+s, 4<=b+s, s<=b+8,
//    b<=s+20, 0<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P(b,d,s,t) {b==s+20, d==9, t==4, 20<=b}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==3&&b>s+1))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, 2b<=2s+5t+18,
//    t<=b+2, 0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==3, 1<=b, 40<=27b+2s, 6<=3b+s, 3<=b+s, s+2<=b,
//    b<=s+19, 0<=d, d<=9, 0<=s}

         s++;

//  P(b,d,s,t) {t==3, 1<=b, 42<=27b+2s, 7<=3b+s, 4<=b+s, s+1<=b,
//    b<=s+18, 0<=d, d<=9, 1<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==3&&d<9))

//  P(b,d,s,t) {0<=b, 18b<=5d+18s+45t+162, b<=s+19, 20t<=27b+2s+20,
//    4t<=3b+3s+4, 3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2,
//    0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==3, 1<=b, 40<=27b+2s, 6<=3b+s, 3<=b+s, s<=b+9,
//    b<=s+19, 0<=d, d<=8, 0<=s}

         d++;

//  P(b,d,s,t) {t==3, 1<=b, 40<=27b+2s, 6<=3b+s, 3<=b+s, s<=b+9,
//    b<=s+19, 1<=d, d<=9, 0<=s}

         b++;

//  P(b,d,s,t) {t==3, 2<=b, 67<=27b+2s, 9<=3b+s, 4<=b+s, s<=b+8,
//    b<=s+20, 1<=d, d<=9, 0<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P(b,d,s,t) {b==s+20, t==3, 20<=b, 1<=d, d<=9}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==4&&b>s+1))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, b+18t<=s+73, t<=b+2,
//    0<=d, d<=9, 0<=s, 3t<=2s+9, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==4, 2<=b, 60<=27b+2s, 9<=3b+s, 4<=b+s, s+2<=b,
//    b<=s+19, 0<=d, d<=9, 0<=s}

         s++;

//  P(b,d,s,t) {t==4, 2<=b, 62<=27b+2s, 10<=3b+s, 5<=b+s, s+1<=b,
//    b<=s+18, 0<=d, d<=9, 1<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==4&&b==s+1))

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 20t<=15b+9s+26,
//    4t<=3b+3s+4, 3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2,
//    0<=d, d<=9, 0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {b==s+1, t==4, 3<=b, 0<=d, d<=9}

         t = 1;

//  P(b,d,s,t) {b==s+1, t==1, 3<=b, 0<=d, d<=9}

         s++;

//  P(b,d,s,t) {b==s, t==1, 3<=b, 0<=d, d<=9}

         d = 0;

//  P(b,d,s,t) {b==s, d==0, t==1, 3<=b}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(b,d,s,t) {0<=b, b<=s+19, 20t<=27b+2s+20, 4t<=3b+3s+4,
//    3t<=3b+s+3, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

         if (!(t==1&&b<s+9))

//  P(b,d,s,t) {0<=b, b<=s+19, 466<=27b+2s+223t, 20t<=27b+2s+20,
//    50<=3b+3s+23t, 4t<=3b+3s+4, 51<=3b+s+24t, 3t<=3b+s+3,
//    s+28<=b+19t, s<=b+t+8, s+t<=b+12, b<=s+5t+4, t<=b+2, 0<=d, d<=9,
//    0<=s, 1<=t, t<=4}

            deadlock();

//  P(b,d,s,t) {t==1, 0<=b, s<=b+9, b<=s+8, 0<=d, d<=9, 0<=s}

         b++;

//  P(b,d,s,t) {t==1, 1<=b, s<=b+8, b<=s+9, 0<=d, d<=9, 0<=s}

         if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int b, s, d, t;

//  P(b,d,s,t) {}

   b = s = d = 0;

//  P(b,d,s,t) {b==0, d==0, s==0}

   t = 1;

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}

   if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

      checking_error();

//  P(b,d,s,t) {b==0, d==0, s==0, t==1}

   while (rand_b())

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

      if (rand_b())

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

         if (rand_b())

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

            if (rand_b()) {

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               if (!(t==1&&b<s+9))

//  P() {0==-1}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               b++;

//  P(b,d,s,t) {d==0, t==1, 19s+59<=59b, s<=b+7, b<=s+9,
//    911b<=2871s+911}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();

//  P(b,d,s,t) {d==0, t==1, 19s+59<=59b, s<=b+7, b<=s+9,
//    911b<=2871s+911}

               if (!(t==1&&b>s-9))

//  P() {0==-1}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s+59<=59b, s<=b+7, b<=s+9,
//    911b<=2871s+911}

               s++;

//  P(b,d,s,t) {d==0, t==1, 19s+40<=59b, s<=b+8, b<=s+8,
//    911b+1960<=2871s}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else if (rand_b()) {

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               if (!(t==1&&b>s-9))

//  P() {0==-1}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               s++;

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+9, b<=s+7,
//    911b+2871<=2871s}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+9, b<=s+7,
//    911b+2871<=2871s}

               if (!(t==1&&b<s+9))

//  P() {0==-1}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+9, b<=s+7,
//    911b+2871<=2871s}

               b++;

//  P(b,d,s,t) {d==0, t==1, 19s+40<=59b, s<=b+8, b<=s+8,
//    911b+1960<=2871s}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();
            }
            else {

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               if (!(t==1&&b>s-9))

//  P() {0==-1}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b, s<=b+8, b<=s+8, 911b<=2871s}

               s++;

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+9, b<=s+7,
//    911b+2871<=2871s}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+9, b<=s+7,
//    911b+2871<=2871s}

               if (!(t==1&&b>s-9))

//  P(b,d,s,t) {b==s-9, d==0, t==1, 4<=b}

                  deadlock();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+19, s<=b+8, b<=s+7,
//    911b+2871<=2871s}

               s++;

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+38, s<=b+9, b<=s+6,
//    911b+5742<=2871s}

               if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                  checking_error();

//  P(b,d,s,t) {d==0, t==1, 19s<=59b+38, s<=b+9, b<=s+6,
//    911b+5742<=2871s}

               while (rand_b())

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                  if (rand_b()) {

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                     if (!(t==1&&b<s+9&&b==s-2))

//  P(b,d,s,t) {d==0, t==1, 0<=b, 8613<=1049b+2871s, 67s<=87b+536,
//    s<=b+9, b<=s+6, 911b+5742<=2871s, 2<=s}

                        deadlock();

//  P(b,d,s,t) {b==s-2, d==0, t==1, 0<=b}

                     b++;

//  P(b,d,s,t) {b==s-1, d==0, t==1, 1<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();

//  P(b,d,s,t) {b==s-1, d==0, t==1, 1<=b}

                     if (!(t==1&&b>s-9))

//  P() {0==-1}

                        deadlock();

//  P(b,d,s,t) {b==s-1, d==0, t==1, 1<=b}

                     s++;

//  P(b,d,s,t) {b==s-2, d==0, t==1, 1<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();
                  }
                  else if (rand_b())

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                     if (rand_b()) {

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                        if (!(t==1&&b<s+9&&b<s-2))

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+2, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                           deadlock();

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b+3<=s,
//    911b+5742<=2871s, 2<=s}

                        b++;

//  P(b,d,s,t) {d==0, t==1, 1<=b, 67s<=87b+449, s<=b+8, b+2<=s,
//    911b+4831<=2871s, 2<=s}

                        if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                           checking_error();
                     }
                     else {

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                        if (!(t==1&&b>s-9&&b>s-8))

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b+8<=s,
//    911b+5742<=2871s, 2<=s}

                           deadlock();

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+7, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                        s++;

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+603, s<=b+8, b<=s+5,
//    911b+8613<=2871s, 3<=s}

                        if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                           checking_error();
                     }
                  else {

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=87b+536, s<=b+9, b<=s+6,
//    911b+5742<=2871s, 2<=s}

                     if (!(t==1&&b>s-9&&b==s-8))

//  P(b,d,s,t) {d==0, t==1, 0<=b, 67s<=107b+469, 67s<=87b+536, s<=b+9,
//    b<=s+6, 911b+5742<=2871s, 2<=s}

                        deadlock();

//  P(b,d,s,t) {b==s-8, d==0, t==1, 0<=b}

                     s++;

//  P(b,d,s,t) {b==s-9, d==0, t==1, 0<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();

//  P(b,d,s,t) {b==s-9, d==0, t==1, 0<=b}

                     if (!(t==1&&b<s+9))

//  P() {0==-1}

                        deadlock();

//  P(b,d,s,t) {b==s-9, d==0, t==1, 0<=b}

                     b++;

//  P(b,d,s,t) {b==s-8, d==0, t==1, 1<=b}

                     if (!(b-s>=-10&&b-s<=19))

//  P() {0==-1}

                        checking_error();
                  }

//  P(b,d,s,t) {d==0, t==1, 0<=b, 133s<=173b+1064, s<=b+9, b<=s+6,
//    911b+5742<=2871s}

               if (!(t==1&&b<s+9&&b==s-2))

//  P(b,d,s,t) {d==0, t==1, 0<=b, 8613<=1049b+2871s, 133s<=173b+1064,
//    s<=b+9, b<=s+6, 911b+5742<=2871s}

                  deadlock();

