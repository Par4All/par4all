
TRANSFORMERS


//  T() {}

void ts_singlestate(void)
{

//  T(i,j,k) {}

   int i, j, k;

//  T(i,j,k) {i==0, j==0, k==0}

   i = j = k = 0;

//  T() {i==0, j==0, k==0}

   if (20*k>11*i)

//  T() {0==-1}

      checking_error();

//  T(i,j,k) {i<=102, i#init+2j<=i+2j#init,
//    11i#init+2j+20k<=11i+2j#init+20k#init,
//    i+2j#init+10k#init<=i#init+2j+10k, 2j<=i, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, j<=9}

   while (rand_b())

//  T(i,j,k) {i==i#init+2, i<=102, 20k<=11i, j<=j#init+1,
//    j+9j#init<=81, j#init+5k#init+1<=j+5k,
//    j+10k<=j#init+10k#init+11}

      if (rand_b()) {

//  T() {i<=100, j<=8}

         if (!(i<=100&&j<9))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, i<=102, j<=8}

         i += 2;

//  T(k) {k==k#init+1, i<=102, j<=8}

         k++;

//  T(j) {j==j#init+1, i<=102, j<=9}

         j++;

//  T() {i<=102, 20k<=11i, j<=9}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {i<=100, j<=8}

         if (!(i<=100&&j<9))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, i<=102, j<=8}

         i += 2;

//  T(j) {j==j#init+1, i<=102, j<=9}

         j++;

//  T() {i<=102, 20k<=11i, j<=9}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {j==9, i<=100}

         if (!(i<=100&&j==9))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, j==9, i<=102}

         i += 2;

//  T(k) {j==9, k==k#init+2, i<=102}

         k += 2;

//  T(j) {j==0, j#init==9, i<=102}

         j = 0;

//  T() {j==0, i<=102, 20k<=11i}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
}

//  T() {}

void ts_restructured(void)
{

//  T(i,j,k) {}

   int i, j, k;

//  T(i,j,k) {i==0, j==0, k==0}

   i = j = k = 0;

//  T() {i==0, j==0, k==0}

   if (20*k>11*i)

//  T() {0==-1}

      checking_error();

//  T(i,j,k) {i<=100, 2i#init+j#init<=2i+j, i#init+2j<=i+2j#init,
//    11i#init+2j+20k<=11i+2j#init+20k#init,
//    i+2j#init+10k#init<=i#init+2j+10k, 0<=2i+j, 2j<=i, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, j<=8}

   while (rand_b())

//  T(i,j,k) {9i+2j==9i#init+2j#init+20, i<=100, 20k<=11i,
//    j#init<=j+8, j<=j#init+1, j+8j#init<=64,
//    2j#init+9k#init+2<=2j+9k, 2j+9k<=2j#init+9k#init+11}

      if (rand_b()) {

//  T() {i<=98, j<=7}

         if (!(i<=98&&j<8))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, i<=100, j<=7}

         i += 2;

//  T(k) {k==k#init+1, i<=100, j<=7}

         k++;

//  T(j) {j==j#init+1, i<=100, j<=8}

         j++;

//  T() {i<=100, 20k<=11i, j<=8}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {i<=98, j<=7}

         if (!(i<=98&&j<8))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, i<=100, j<=7}

         i += 2;

//  T(j) {j==j#init+1, i<=100, j<=8}

         j++;

//  T() {i<=100, 20k<=11i, j<=8}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  T() {j==8, i<=98}

         if (!(i<=98&&j==8))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, j==8, i<=100}

         i += 2;

//  T(k) {j==8, k==k#init+1, i<=100}

         k++;

//  T(j) {j==9, j#init==8, i<=100}

         j++;

//  T() {j==9, i<=100, 20k<=11i}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();

//  T() {j==9, i<=98, 20k<=11i}

         if (!(i<=98&&j==9))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, j==9, i<=100, 20k+22<=11i}

         i += 2;

//  T(k) {j==9, k==k#init+2, i<=100, 20k<=11i+18}

         k += 2;

//  T(j) {j==0, j#init==9, i<=100, 20k<=11i+18}

         j = 0;

//  T() {j==0, i<=100, 20k<=11i}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }
      else {

//  T() {j==8, i<=98}

         if (!(i<=98&&j==8))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, j==8, i<=100}

         i += 2;

//  T(j) {j==9, j#init==8, i<=100}

         j++;

//  T() {j==9, i<=100, 20k<=11i}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();

//  T() {j==9, i<=98, 20k<=11i}

         if (!(i<=98&&j==9))

//  T() {0==-1}

            deadlock();

//  T(i) {i==i#init+2, j==9, i<=100, 20k+22<=11i}

         i += 2;

//  T(k) {j==9, k==k#init+2, i<=100, 20k<=11i+18}

         k += 2;

//  T(j) {j==0, j#init==9, i<=100, 20k<=11i+18}

         j = 0;

//  T() {j==0, i<=100, 20k<=11i}

         if (20*k>11*i)

//  T() {0==-1}

            checking_error();
      }

//  T(i,j,k) {9i+2j==9i#init+2j#init+20, 101<=i, i<=102,
//    197j#init+242<=18i+206j, 17j#init+60k#init+2804<=60i+11j,
//    14j#init+60k#init+74<=33i+8j, j#init+3k#init+139<=3i+j,
//    3i<=2j+4j#init+30k#init+4, k+46<=i, 199j#init<=208j+1592,
//    j<=j#init+1, 2j#init+9k#init+2<=2j+9k,
//    2j+9k<=2j#init+9k#init+11, 35j#init+44<=44j+36k#init,
//    898<=7j+11j#init+90k#init, j#init<=8}

   if (rand_b()) {

//  T() {99<=i, i<=100, 0<=2i+j, 2j+20k<=11i, i<=2j+10k, j<=8}

      if (!(i<=100&&j<9&&i>98))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, 101<=i, i<=102, 4<=2i+j, 2j+20k+22<=11i,
//    i<=2j+10k+2, j<=8}

      i += 2;

//  T(k) {k==k#init+1, 101<=i, i<=102, 4<=2i+j, 2j+20k+2<=11i,
//    i+8<=2j+10k, j<=8}

      k++;

//  T(j) {j==j#init+1, 101<=i, i<=102, 5<=2i+j, 2j+20k<=11i,
//    i+10<=2j+10k, j<=9}

      j++;

//  T() {101<=i, i<=102, 5<=2i+j, 2j+20k<=11i, i+10<=2j+10k, k+46<=i,
//    j<=9}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();
   }
   else if (rand_b()) {

//  T() {99<=i, i<=100, 0<=2i+j, 2j+20k<=11i, i<=2j+10k, j<=8}

      if (!(i<=100&&j<9&&i>98))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, 101<=i, i<=102, 4<=2i+j, 2j+20k+22<=11i,
//    i<=2j+10k+2, j<=8}

      i += 2;

//  T(j) {j==j#init+1, 101<=i, i<=102, 5<=2i+j, i<=2j+10k,
//    2j+20k+20<=11i, j<=9}

      j++;

//  T() {101<=i, i<=102, 5<=2i+j, i<=2j+10k, 2j+20k+20<=11i, k+46<=i,
//    j<=9}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();
   }
   else if (rand_b()) {

//  T() {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

      if (!(i<=98&&j==8))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      i += 2;

//  T(k) {j==8, k==k#init+1, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      k++;

//  T(j) {j==9, j#init==8, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      j++;

//  T() {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();

//  T() {j==9, 99<=i, i<=100, k+46<=i, 10<=k}

      if (!(i<=100&&j==9&&i>98))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, j==9, 101<=i, i<=102, k+48<=i, 10<=k}

      i += 2;

//  T(k) {j==9, k==k#init+2, 101<=i, i<=102, k+46<=i, 12<=k}

      k += 2;

//  T(j) {j==0, j#init==9, 101<=i, i<=102, k+46<=i, 12<=k}

      j = 0;

//  T() {j==0, 101<=i, i<=102, k+46<=i, 12<=k}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();
   }
   else {

//  T() {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

      if (!(i<=98&&j==8))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      i += 2;

//  T(j) {j==9, j#init==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      j++;

//  T() {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();

//  T() {j==9, 99<=i, i<=100, k+47<=i, 9<=k}

      if (!(i<=100&&j==9&&i>98))

//  T() {0==-1}

         deadlock();

//  T(i) {i==i#init+2, j==9, 101<=i, i<=102, k+49<=i, 9<=k}

      i += 2;

//  T(k) {j==9, k==k#init+2, 101<=i, i<=102, k+47<=i, 11<=k}

      k += 2;

//  T(j) {j==0, j#init==9, 101<=i, i<=102, k+47<=i, 11<=k}

      j = 0;

//  T() {j==0, 101<=i, i<=102, k+47<=i, 11<=k}

      if (20*k>11*i)

//  T() {0==-1}

         checking_error();
   }
}

PRECONDITIONS


//  P() {}

void ts_singlestate(void)
{

//  P() {}

   int i, j, k;

//  P(i,j,k) {}

   i = j = k = 0;

//  P(i,j,k) {i==0, j==0, k==0}

   if (20*k>11*i)

//  P() {0==-1}

      checking_error();

//  P(i,j,k) {i==0, j==0, k==0}

   while (rand_b())

//  P(i,j,k) {i<=102, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=9}

      if (rand_b()) {

//  P(i,j,k) {i<=102, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=9}

         if (!(i<=100&&j<9))

//  P(i,j,k) {i<=102, 2j<=i, 19998<=945i+332j, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, 1660k+1818<=931i, j<=9}

            deadlock();

//  P(i,j,k) {i<=100, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=8}

         i += 2;

//  P(i,j,k) {i<=102, 2j+2<=i, 2j+20k+22<=11i, i<=2j+10k+2,
//    20k+22<=11i, j<=8}

         k++;

//  P(i,j,k) {i<=102, 2j+2<=i, 2j+20k+2<=11i, i+8<=2j+10k, 20k+2<=11i,
//    j<=8}

         j++;

//  P(i,j,k) {i<=102, 2j<=i, 2j+20k<=11i, i+10<=2j+10k, 20k+2<=11i,
//    j<=9}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(i,j,k) {i<=102, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=9}

         if (!(i<=100&&j<9))

//  P(i,j,k) {i<=102, 2j<=i, 19998<=945i+332j, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, 1660k+1818<=931i, j<=9}

            deadlock();

//  P(i,j,k) {i<=100, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=8}

         i += 2;

//  P(i,j,k) {i<=102, 2j+2<=i, 2j+20k+22<=11i, i<=2j+10k+2,
//    20k+22<=11i, j<=8}

         j++;

//  P(i,j,k) {i<=102, 2j<=i, i<=2j+10k, 2j+20k+20<=11i, 20k+22<=11i,
//    j<=9}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(i,j,k) {i<=102, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i, j<=9}

         if (!(i<=100&&j==9))

//  P(i,j,k) {i<=102, 2j<=i, 85j<=i+664, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=9}

            deadlock();

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+18}

         i += 2;

//  P(i,j,k) {j==9, 20<=i, i<=102, 20k+40<=11i, i<=10k+20}

         k += 2;

//  P(i,j,k) {j==9, 20<=i, i<=102, 20k<=11i, i<=10k}

         j = 0;

//  P(i,j,k) {j==0, 20<=i, i<=102, 20k<=11i, i<=10k}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
}

//  P() {}

void ts_restructured(void)
{

//  P() {}

   int i, j, k;

//  P(i,j,k) {}

   i = j = k = 0;

//  P(i,j,k) {i==0, j==0, k==0}

   if (20*k>11*i)

//  P() {0==-1}

      checking_error();

//  P(i,j,k) {i==0, j==0, k==0}

   while (rand_b())

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=8}

      if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=8}

         if (!(i<=98&&j<8))

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 3960<=206i+83j, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, 1660k+1584<=929i, j<=8}

            deadlock();

//  P(i,j,k) {i<=98, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i,
//    j<=7}

         i += 2;

//  P(i,j,k) {i<=100, 4<=2i+j, 2j+2<=i, 2j+20k+22<=11i, i<=2j+10k+2,
//    20k+22<=11i, j<=7}

         k++;

//  P(i,j,k) {i<=100, 4<=2i+j, 2j+2<=i, 2j+20k+2<=11i, i+8<=2j+10k,
//    20k+2<=11i, j<=7}

         j++;

//  P(i,j,k) {i<=100, 2j<=i, 5<=2i+j, 2j+20k<=11i, i+10<=2j+10k,
//    20k+2<=11i, j<=8}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=8}

         if (!(i<=98&&j<8))

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 3960<=206i+83j, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, 1660k+1584<=929i, j<=8}

            deadlock();

//  P(i,j,k) {i<=98, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k, 20k<=11i,
//    j<=7}

         i += 2;

//  P(i,j,k) {i<=100, 4<=2i+j, 2j+2<=i, 2j+20k+22<=11i, i<=2j+10k+2,
//    20k+22<=11i, j<=7}

         j++;

//  P(i,j,k) {i<=100, 2j<=i, 5<=2i+j, i<=2j+10k, 2j+20k+20<=11i,
//    20k+22<=11i, j<=8}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=8}

         if (!(i<=98&&j==8))

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 85j<=i+581, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, j<=8}

            deadlock();

//  P(i,j,k) {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

         i += 2;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         k++;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         j++;

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

         if (!(i<=98&&j==9))

//  P(i,j,k) {j==9, 99<=i, i<=100, k+46<=i, 10<=k}

            deadlock();

//  P(i,j,k) {j==9, 18<=i, i<=98, 20k+18<=11i, i<=10k+8}

         i += 2;

//  P(i,j,k) {j==9, 20<=i, i<=100, 20k+40<=11i, i<=10k+10}

         k += 2;

//  P(i,j,k) {j==9, 20<=i, i<=100, 20k<=11i, i+10<=10k}

         j = 0;

//  P(i,j,k) {j==0, 20<=i, i<=100, 20k<=11i, i+10<=10k}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 2j+20k<=11i, i<=2j+10k,
//    20k<=11i, j<=8}

         if (!(i<=98&&j==8))

//  P(i,j,k) {i<=100, 0<=2i+j, 2j<=i, 85j<=i+581, 2j+20k<=11i,
//    i<=2j+10k, 20k<=11i, j<=8}

            deadlock();

//  P(i,j,k) {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

         i += 2;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         j++;

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

         if (!(i<=98&&j==9))

//  P(i,j,k) {j==9, 99<=i, i<=100, k+47<=i, 9<=k}

            deadlock();

//  P(i,j,k) {j==9, 18<=i, i<=98, 20k+38<=11i, i<=10k+18}

         i += 2;

//  P(i,j,k) {j==9, 20<=i, i<=100, 20k+60<=11i, i<=10k+20}

         k += 2;

//  P(i,j,k) {j==9, 20<=i, i<=100, i<=10k, 20k+20<=11i}

         j = 0;

//  P(i,j,k) {j==0, 20<=i, i<=100, i<=10k, 20k+20<=11i}

         if (20*k>11*i)

//  P() {0==-1}

            checking_error();
      }

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

   if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      if (!(i<=100&&j<9&&i>98))

//  P(i,j,k) {i<=98, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

         deadlock();

//  P(i,j,k) {99<=i, i<=100, 5<=2i+j, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      i += 2;

//  P(i,j,k) {101<=i, i<=102, 9<=2i+j, 3900k+4290<=2145i+2j,
//    2j+20k+22<=11i, i<=2j+10k+2, j<=8}

      k++;

//  P(i,j,k) {101<=i, i<=102, 9<=2i+j, 3900k+390<=2145i+2j,
//    2j+20k+2<=11i, i+8<=2j+10k, j<=8}

      j++;

//  P(i,j,k) {101<=i, i<=102, 10<=2i+j, 2j+20k<=11i,
//    3900k+392<=2145i+2j, i+10<=2j+10k, j<=9}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();
   }
   else if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      if (!(i<=100&&j<9&&i>98))

//  P(i,j,k) {i<=98, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

         deadlock();

//  P(i,j,k) {99<=i, i<=100, 5<=2i+j, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      i += 2;

//  P(i,j,k) {101<=i, i<=102, 9<=2i+j, 3900k+4290<=2145i+2j,
//    2j+20k+22<=11i, i<=2j+10k+2, j<=8}

      j++;

//  P(i,j,k) {101<=i, i<=102, 10<=2i+j, i<=2j+10k,
//    3900k+4292<=2145i+2j, 2j+20k+20<=11i, j<=9}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();
   }
   else if (rand_b()) {

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      if (!(i<=98&&j==8))

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 85j<=i+581, 3900k<=2145i+2j,
//    2j+20k<=11i, i<=2j+10k, j<=8}

         deadlock();

//  P(i,j,k) {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

      i += 2;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      k++;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      j++;

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+18<=11i, i<=10k+8}

      if (!(i<=100&&j==9&&i>98))

//  P(i,j,k) {j==9, 18<=i, i<=98, 20k+18<=11i, i<=10k+8}

         deadlock();

//  P(i,j,k) {j==9, 99<=i, i<=100, k+46<=i, 10<=k}

      i += 2;

//  P(i,j,k) {j==9, 101<=i, i<=102, k+48<=i, 10<=k}

      k += 2;

//  P(i,j,k) {j==9, 101<=i, i<=102, k+46<=i, 12<=k}

      j = 0;

//  P(i,j,k) {j==0, 101<=i, i<=102, k+46<=i, 12<=k}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();
   }
   else {

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 3900k<=2145i+2j, 2j+20k<=11i,
//    i<=2j+10k, j<=8}

      if (!(i<=98&&j==8))

//  P(i,j,k) {i<=100, 0<=39i+20j, 2j<=i, 85j<=i+581, 3900k<=2145i+2j,
//    2j+20k<=11i, i<=2j+10k, j<=8}

         deadlock();

//  P(i,j,k) {j==8, 16<=i, i<=98, 20k+16<=11i, i<=10k+16}

      i += 2;

//  P(i,j,k) {j==8, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      j++;

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();

//  P(i,j,k) {j==9, 18<=i, i<=100, 20k+38<=11i, i<=10k+18}

      if (!(i<=100&&j==9&&i>98))

//  P(i,j,k) {j==9, 18<=i, i<=98, 20k+38<=11i, i<=10k+18}

         deadlock();

//  P(i,j,k) {j==9, 99<=i, i<=100, k+47<=i, 9<=k}

      i += 2;

//  P(i,j,k) {j==9, 101<=i, i<=102, k+49<=i, 9<=k}

      k += 2;

//  P(i,j,k) {j==9, 101<=i, i<=102, k+47<=i, 11<=k}

      j = 0;

//  P(i,j,k) {j==0, 101<=i, i<=102, k+47<=i, 11<=k}

      if (20*k>11*i)

//  P() {0==-1}

         checking_error();
   }
}
