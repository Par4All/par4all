
Points-to


// Points To IN: none


// Points To OUT: none

int mainInt()
{

// Points To: none

   int a[10];

// Points To: none

   int *p, *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT


   q = &a[0];

// Points To:
// p -> undefined , EXACT
// q -> a[0] , EXACT

   p = q+1;

// Points To:
// p -> a[1] , EXACT
// q -> a[0] , EXACT


   *q = 0;

// Points To:
// p -> a[1] , EXACT
// q -> a[0] , EXACT

   *p = 0;

// Points To:
// p -> a[1] , EXACT
// q -> a[0] , EXACT


   return 0;
}

PROPER  EFFECTS

int mainInt()
{
   int a[10];
   int *p, *q;
//             <    is written>: q

   q = &a[0];
//             <    is read   >: q
//             <    is written>: p
   p = q+1;
//             <    is read   >: q
//             <    is written>: a[0]

   *q = 0;
//             <    is read   >: p
//             <    is written>: a[1]
   *p = 0;

   return 0;
}

Transformers with point-to


//  T(mainInt) {mainInt==0}

int mainInt()
{

//  T() {}

   int a[10];

//  T(p,q) {}

   int *p, *q;

//  T(q) {&a[0]==q}


   q = &a[0];

//  T(p) {&a[0]+sizeof(int)==p, &a[0]==q}

   p = q+1;

//  T(a[0]) {&a[0]+sizeof(int)==p, &a[0]==q, a[0]==0}


   *q = 0;

//  T(a[1]) {&a[0]+sizeof(int)==p, &a[0]==q, a[0]==0, a[1]==0}

   *p = 0;

//  T(mainInt) {&a[0]+sizeof(int)==p, &a[0]==q, a[0]==0, a[1]==0,
//    mainInt==0}


   return 0;
}

Preconditions with point-to


//  P() {}

int mainInt()
{

//  P() {}

   int a[10];

//  P() {}

   int *p, *q;

//  P(p,q) {}


   q = &a[0];

//  P(p,q) {&a[0]==q}

   p = q+1;

//  P(p,q) {&a[0]+sizeof(int)==p, &a[0]==q}


   *q = 0;

//  P(a[0],p,q) {&a[0]+sizeof(int)==p, &a[0]==q, a[0]==0}

   *p = 0;

//  P(a[0],a[1],p,q) {&a[0]+sizeof(int)==p, &a[0]==q, a[0]==0,
//    a[1]==0}


   return 0;
}

