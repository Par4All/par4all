
Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   int a[20], i = 2, j = 3;

// Points To: none

   int *p, *q;

// Points To:
// p -> undefined , EXACT
// q -> undefined , EXACT


   q = &a[0];

// Points To:
// p -> undefined , EXACT
// q -> a[0] , EXACT

   p = q+i*j;

// Points To:
// p -> a[*] , MAY
// q -> a[0] , EXACT


   return 0;
}

PROPER  EFFECTS

int main()
{
//             <    is written>: i j
   int a[20], i = 2, j = 3;
   int *p, *q;
//             <    is written>: q

   q = &a[0];
//             <    is read   >: i j q
//             <    is written>: p
   p = q+i*j;

   return 0;
}

Transformers with point-to


//  T(main) {main==0}

int main()
{

//  T(i,j) {i==2, j==3}

   int a[20], i = 2, j = 3;

//  T(p,q) {i==2, j==3}

   int *p, *q;

//  T(q) {&a[0]==q, i==2, j==3}


   q = &a[0];

//  T(p) {&a[0]+6sizeof(int)==p, &a[0]==q, i==2, j==3}

   p = q+i*j;

//  T(main) {&a[0]+6sizeof(int)==p, &a[0]==q, i==2, j==3, main==0}


   return 0;
}

Preconditions with point-to


//  P() {}

int main()
{

//  P() {}

   int a[20], i = 2, j = 3;

//  P(i,j) {i==2, j==3}

   int *p, *q;

//  P(i,j,p,q) {i==2, j==3}


   q = &a[0];

//  P(i,j,p,q) {&a[0]==q, i==2, j==3}

   p = q+i*j;

//  P(i,j,p,q) {&a[0]+6sizeof(int)==p, &a[0]==q, i==2, j==3}


   return 0;
}

