
Points-to


// Points To IN: none


// Points To OUT: none

int main()
{

// Points To: none

   struct Mastruct toto;

// Points To: none

   toto.first = 0;

// Points To: none

   toto.first = 1;

// Points To: none


   return 0;
}

PROPER  EFFECTS

int main()
{
   struct Mastruct toto;
//             <    is written>: toto.first
   toto.first = 0;
//             <    is written>: toto.first
   toto.first = 1;

   return 0;
}

Transformers with point-to


//  T(main) {main==0}

int main()
{

//  T() {}

   struct Mastruct toto;

//  T(toto[first]) {toto[first]==0}

   toto.first = 0;

//  T(toto[first]) {toto[first]==1, toto[first]#init==0}

   toto.first = 1;

//  T(main) {main==0, toto[first]==1}


   return 0;
}

Preconditions with point-to


//  P() {}

int main()
{

//  P() {}

   struct Mastruct toto;

//  P() {}

   toto.first = 0;

//  P(toto[first]) {toto[first]==0}

   toto.first = 1;

//  P(toto[first]) {toto[first]==1}


   return 0;
}
