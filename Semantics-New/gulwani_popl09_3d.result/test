//  P() {0==-1}

         deadlock();

//  P() {0==-1}

      x++;

//  P() {0==-1}

      y++;

//  P() {0==-1}

      c++;

//  P() {0==-1}

      if (!(c<=n))

//  P() {0==-1}

         checking_error();
   }

//  P() {0==-1}

   if (!(x>=n&&y==m-1))

//  P() {0==-1}

      deadlock();

//  P() {0==-1}

   x++;

//  P() {0==-1}

   y++;

//  P() {0==-1}

   c++;

//  P() {0==-1}

   if (!(c<=n))

//  P() {0==-1}

      checking_error();
   goto L4;
l99999:   ;

