//  P(a,b,s1,s2) {a==0, b==0, s1==3, s2==1}

         s1 = 1;

//  P(a,b,s1,s2) {a==0, b==0, s1==1, s2==1}

         if (s1==3&&s2==3)

//  P() {0==-1}

            checking_error();
      }
      else {

//  P(a,b,s1,s2) {b==0, s1==1, s2==1}

