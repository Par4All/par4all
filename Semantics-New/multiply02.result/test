
Transformers for multiply02


//  T(multiply02) {}

int multiply02(int i, int j, int N)
{

//  T(k) {}

   int k;

//  T(i) {i==2}


   i = 2;

//  T(j) {j==3}

   j = 3;

//  T(k) {}

   k = i*N*j;

//  T(multiply02) {k==multiply02}

   return k;
}

Preconditions for multiply02


//  P() {}

int multiply02(int i, int j, int N)
{

//  P() {}

   int k;

//  P(k) {}


   i = 2;

//  P(i,k) {i==2}

   j = 3;

//  P(i,j,k) {i==2, j==3}

   k = i*N*j;

//  P(i,j,k) {i==2, j==3}

   return k;
}
